.searchIcon {
    width: 28rpx;
    height: 28rpx;
}

.search {
    background: #fff;
    height: 98rpx;
    display: flex;
    align-items: center;
    padding: 0rpx 20rpx;
}

.search input {
    height: 64rpx;
    width: 556rpx;
    border: 2rpx solid #ececec;
    background: #f8f8f8;
    border-radius: 10rpx;
    font-size: 30rpx;
    padding-left: 20rpx;
    margin-right: 20rpx;
}

.search button {
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-right: 0rpx;
    padding-left: 0rpx;
    width: 110rpx;
    height: 64rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #ff5e5e;
    border-radius: 10rpx;
}

.icon {
    width: 41rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

.block {
    background: #fff;
    margin-top: 20rpx;
    padding: 24rpx 20rpx 30rpx;
}

.blockTil {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-weight: 700;
}

.blockList {
    margin-top: 24rpx;
    display: flex;
    width: 710rpx;
    flex-wrap: wrap;
}

.auditions {
    flex-wrap: nowrap;
    white-space: nowrap;
    overflow: hidden;
}

.blockItem {
    display: inline-block;
    margin-right: 25rpx;
}

.blockList>view {
    width: 220rpx;
    margin-right: 25rpx;
}

.blockList>view:nth-child(3n) {
    margin-right: 0rpx;
}

.storyImg {
    width: 220rpx;
    height: 220rpx;
    border-radius: 10rpx;
}

.storyTitle {
    width: 220rpx;
    height: 28rpx;
    font-size: 26rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-top: 20rpx;
    text-align: center;
}

.storyDet {
    position: relative;
}

.listen {
    display: inline-block;
    height: 30rpx;
    padding: 0rpx 10rpx;
    border-radius: 6rpx;
    background: rgba(0,0,0,0.5);
    position: absolute;
    bottom: 10rpx;
    right: 10rpx;
    color: #fff;
    font-size: 26rpx;
    line-height: 30rpx;
}

.listenIcon {
    width: 23rpx;
    height: 22rpx;
    margin-right: 16rpx;
}

.album {
    height: 36rpx;
    width: 92rpx;
    background: #ff5e5e;
    color: #fff;
    position: absolute;
    top: 20rpx;
    left: 0rpx;
    text-align: center;
    line-height: 36rpx;
    border-radius: 0rpx 6rpx 6rpx 0rpx;
}

.icn {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.vipWrap {
    margin-top: 10rpx;
    background: #fff;
    padding: 10rpx 25rpx;
}

.vip {
    padding: 10rpx 20rpx;
    width: 660rpx;
    height: 160rpx;
    background: #ff8383;
    color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-radius: 10rpx;
}

.vipicon {
    height: 120rpx;
    width: 120rpx;
    border-radius: 50%;
}

.vipLeft {
    display: flex;
}

.vipDet {
    margin-left: 24rpx;
    width: 360rpx;
    display: flex;
    flex-direction: column;
    font-size: 26rpx;
}

.vipDet>text:first-child {
    display: inline-block;
    margin-top: 20rpx;
    font-weight: 700;
    font-size: 30rpx;
    margin-bottom: 28rpx;
    color: #d03939;
}

.vipJoin {
    width: 172rpx;
    height: 52rpx;
    background: #b93036;
    border-radius: 10rpx;
    text-align: center;
    line-height: 52rpx;
}

.classify {
    white-space: nowrap;
    width: 710rpx;
    height: 92rpx;
    padding: 0rpx 20rpx;
    line-height: 92rpx;
    border-bottom: 1rpx solid #ececec;
    background: #fff;
    margin-top: 10rpx;
}

.classify view {
    display: inline-block;
    padding-right: 60rpx;
    height: 92rpx;
    font-size: 30rpx;
}

.classify text {
    display: inline-block;
    height: 92rpx;
    box-sizing: border-box;
}

.classify .in {
    color: #ff5e5e;
    border-bottom: 4rpx solid #ff5e5e;
}

.session {
    margin-top: 0rpx;
    padding-top: 0rpx;
    padding-bottom: 0rpx;
}

.sessionList {
    padding-top: 20rpx;
    margin-top: 0rpx;
}

.sessionList>view {
    margin-bottom: 30rpx;
}