<tab-bar chooseNav="0"></tab-bar>
<view class="container">
    <view class="sort">
        <view bindtap="toMember" class="header" style="order:{{sort[0].order}}" wx:if="{{sort[0].status}}">
            <image class="bg" src="{{bg}}"></image>
            <view animation="{{animationData}}" class="memberCard" style="background-image:url({{cardsBg}});">
                <view class="cardsCont">
                    <image class="weblogo" src="{{imgroot}}{{qzCards.weblogo}}" wx:if="{{qzCards.weblogo!=''}}"></image>
                    <image class="weblogo" src="{{weblogo}}" wx:if="{{qzCards.weblogo==''}}"></image>
                    <view class="cardsOpera">
                        <view wx:for="{{qzCards.privilege_identifier}}">
                            <image class="iconOprea" src="{{imgroot}}{{item.pic}}"></image>
                            <text>{{item.title}}</text>
                        </view>
                    </view>
                </view>
            </view>
            <view class="headerBot"></view>
            <view class="cover"></view>
            <view class="join">
                <text>{{qzCards.money}}/年  </text>
                <text> 加入亲子会员卡</text>
            </view>
        </view>
        <view class="notice" style="order:{{sort[2].order}}" wx:if="{{sort[2].status}}">
            <view>
                <image src="../../../style/images/notice.png"></image>
                <text>公告</text>
            </view>
            <view>
                <swiper autoplay="true" class="noticeMsg" duration="800" interval="2500" vertical="true">
                    <swiper-item wx:for="{{notice}}">
              {{item.title}}
            </swiper-item>
                </swiper>
            </view>
        </view>
        <swiper autoplay="true" circular="true" class="swiper" duration="800" indicatorActiveColor="#ff5e5e" indicatorColor="rgba(255,255,255,.7)" indicatorDots="true" interval="3000" style="order:{{sort[1].order}}" wx:if="{{sort[1].status}}">
            <swiper-item wx:for="{{imgUrls}}" wx:key="{{index}}">
                <image bindtap="toSwiperAd" class="slide-image" data-url="{{item.url}}" mode="scaleToFill" src="{{imgroot}}{{item.pic}}"></image>
            </swiper-item>
        </swiper>
        <view class="userOpe" style="order:{{sort[3].order}}" wx:if="{{sort[3].status}}">
            <view bindtap="toInner" class="opeartion" data-url="{{item.url}}" wx:for="{{operation}}">
                <image mode="scaleToFill" src="{{imgroot}}{{item.pic}}"></image>
                <text>{{item.title}}</text>
            </view>
        </view>
        <view class="member" style="order:{{sort[4].order}}" wx:if="{{sort[4].status}}">
            <image class="coupon" src="{{couponBg}}"></image>
            <view class="isMember" wx:if="{{vipType==1}}">
                <view class="memberLeft">
                    <image class="uthumb" src="{{user.img}}"></image>
                    <view class="nickname">
                        <view class="uname">{{user.name}}</view>
                        <view>{{user.end_time_d}}</view>
                    </view>
                </view>
                <view bindtap="toJoinMember" class="memberRight">
          我要续费
        </view>
            </view>
            <view class="isMember" wx:if="{{vipType==0}}">
                <view class="memberLeft">
                    <image class="uthumb" src="{{user.img}}"></image>
                    <view class="nickname">
                        <view class="uname">您还没有加入亲子卡</view>
                    </view>
                </view>
                <view bindtap="toJoinMember" class="memberRight">
          立即开卡
        </view>
            </view>
        </view>
        <view class="block" style="order:{{sort[5].order}}" wx:if="{{sort[5].status}}">
            <view class="blockTil">
                <view>
                    <image class="icon" src="../../../style/images/icon1.png"></image>
                    <text>小朋友故事会精选试听</text>
                </view>
                <view bindtap="toStoryList">
                    <text>查看更多</text>
                    <image class="right16" src="../../../style/images/right.png"></image>
                </view>
            </view>
            <view class="blockList">
                <view bindtap="toVipStory" data-album="{{item.is_album}}" data-albumid="{{item.album_id}}" data-flink="{{item.file_link}}" data-id="{{item.id}}" data-index="{{index}}" data-src="{{item.file_path}}" wx:for="{{story}}">
                    <view class="storyDet">
                        <image class="storyImg" src="{{imgroot}}{{item.album_pic}}" wx:if="{{item.is_album==1}}"></image>
                        <image class="storyImg" src="{{imgroot}}{{item.pic}}" wx:if="{{item.is_album==0}}"></image>
                        <view class="listen">
                            <image class="listenIcon" src="../../../style/images/icon2.png"></image>
                            <text wx:if="{{item.is_album==0}}">{{item.access_num}}</text>
                            <text wx:if="{{item.is_album==1}}">{{item.album_access_num}}</text>
                        </view>
                        <view class="album" wx:if="{{item.is_album==1}}">专辑</view>
                    </view>
                    <view class="storyTitle" wx:if="{{item.is_album==0}}">{{item.title}}</view>
                    <view class="storyTitle" wx:if="{{item.is_album==1}}">{{item.album_title}}</view>
                </view>
            </view>
            <view bindtap="toStoryList" class="blockTil library">
                <view>
                    <image class="icn" src="../../../style/images/icon4.png"></image>
                    <text>亲子会员卡专属故事库</text>
                </view>
                <view>
                    <text>查看更多</text>
                    <image class="right16" src="../../../style/images/right2.png"></image>
                </view>
            </view>
        </view>
        <view class="session" style="order:{{sort[6].order}}" wx:if="{{sort[6].status}}">
            <view class="sessionTitle">
                <image class="licon" src="../../../style/images/icon3.png"></image>
                <text>精选推荐</text>
                <image class="licon ricon" src="../../../style/images/icon3.png"></image>
            </view>
            <view bindtap="toParentingdet" class="listStory" data-id="{{item.id}}" wx:for="{{list}}">
                <view class="listUp">
                    <image class="" src="{{imgroot}}{{item.pic}}"></image>
                    <view class="tip2" wx:if="{{item.qzk_status==0}}">
                        <view>￥{{item.common_price}}</view>
                    </view>
                    <view class="tip1" wx:if="{{item.qzk_status==0}}">
                        <view>亲子卡价</view>
                        <view>￥{{item.qzk_price}}</view>
                    </view>
                    <view class="tip1" wx:if="{{item.qzk_status==1}}">
                        <view>亲子卡</view>
                        <view>免费</view>
                    </view>
                    <view class="mask" wx:if="{{item.hd_status==3}}">
                        <image src="../../../style/images/over.png"></image>
                    </view>
                </view>
                <view class="listDown">
                    <view class="title">
                        <text class="titleIcon">亲子卡</text>
                        <text class="titl">{{item.title}}</text>
                    </view>
                    <view class="times">
                        <view>{{item.bm_begin_time_d}}至{{item.bm_end_time_d}}</view>
                        <view>已售：{{item.sale_num}}份</view>
                    </view>
                    <view class="tags">
                        <text wx:if="{{item.tag_arr!=''}}" wx:for="{{item.tag_arr}}" wx:for-item="itm">{{itm}}</text>
                    </view>
                </view>
            </view>
        </view>
        <view class="technical" style="order:99" wx:if="{{setting.is_techzhichi==1}}">
            <view>
                <image class="supportIcon" src="{{imgroot}}{{setting.jszc_img}}"></image>
            </view>
            <view class="tecSup">
                <view wx:if="{{setting.pt_name}}">{{setting.jszc_tdcp}}</view>
                <view>合作电话：{{setting.hz_tel}}</view>
            </view>
            <view bindtap="callphone" class="dialogPhone" data-phone="{{setting.hz_tel}}">
                <image class="supportPhone" src="../../../style/images/support2.png"></image>
                <text>拨打</text>
            </view>
        </view>
        <view bindtap="toggleAd" id="Ad" wx:if="{{showAd==1}}">
            <image catchtap="toggleAd" class="closeAd" src="../../../style/images/closead.png"></image>
            <image class="adImg" src="{{imgroot}}{{setting.index_adv_img}}"></image>
        </view>
        <view class="foot" style="order:100"></view>
        <is-login bindtogetuserinfo="updateUserInfo" dUserId="{{d_user_id}}"></is-login>
    </view>
</view>
