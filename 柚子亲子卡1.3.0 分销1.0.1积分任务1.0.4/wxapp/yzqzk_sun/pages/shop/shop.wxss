.swiper {
    width: 750rpx;
    height: 374rpx;
}

.swiper image {
    width: 750rpx;
    height: 374rpx;
}

.searchIcon {
    width: 28rpx;
    height: 28rpx;
}

.search {
    background: #fff;
    height: 98rpx;
    display: flex;
    align-items: center;
    padding: 0rpx 20rpx;
}

.search input {
    height: 60rpx;
    width: 556rpx;
    border: 2rpx solid #ececec;
    background: #f8f8f8;
    border-radius: 10rpx;
    font-size: 30rpx;
    padding-left: 20rpx;
    margin-right: 20rpx;
}

.search button {
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-right: 0rpx;
    padding-left: 0rpx;
    width: 110rpx;
    height: 64rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #ff5e5e;
    border-radius: 10rpx;
}

.notice {
    width: 100%;
    height: 98rpx;
    background-color: #fff;
    line-height: 98rpx;
    display: flex;
    align-items: center;
    margin-top: 20rpx;
    border-bottom: 1rpx solid #f5f3f3;
}

.notice view {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.notice image {
    display: flex;
    width: 52rpx;
    height: 46rpx;
    margin: 0rpx 20rpx 0rpx 24rpx;
}

.notice .notice-msg {
    margin-left: 30rpx;
    font-size: 28rpx;
    color: #666;
    line-height: 30rpx;
    font-weight: 500;
}

.noticeMsg {
    height: 98rpx;
    width: 480rpx;
    padding-right: 8rpx;
    font-size: 28rpx;
    color: #666;
    font-weight: 500;
}

.enter {
    height: 58rpx;
    width: 146rpx;
    color: #fff;
    border-radius: 10rpx;
    background: #ff5e5e;
    line-height: 58rpx;
    justify-content: center;
}

.block {
    position: relative;
}

.cate {
    display: flex;
    justify-content: space-between;
    padding: 0rpx 20rpx;
    width: 710rpx;
    align-items: center;
    height: 90rpx;
    background: #fff;
    margin-top: 20rpx;
}

.cate>view {
    display: flex;
    align-items: center;
    color: #666;
}

.down {
    width: 12rpx;
    height: 10rpx;
    margin-left: 16rpx;
    transform: rotate(180deg);
}

.up {
    transform: rotate(0deg);
}

.cateCont {
    background: #fff;
    position: absolute;
    top: 98rpx;
    left: 0rpx;
    z-index: 1001;
    width: 750rpx;
}

.cateCont>view {
    height: 72rpx;
    border-bottom: 1rpx solid #ececec;
    padding-left: 20rpx;
    color: #666;
    line-height: 72rpx;
}

.shopimg {
    width: 200rpx;
    height: 200rpx;
    border-radius: 10rpx;
    margin-right: 22rpx;
}

.goodimg {
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
}

.shop {
    padding: 30rpx 20rpx;
    background: #fff;
    margin-top: 10rpx;
}

.list {
    border: 2rpx solid #ff5e5e;
    border-radius: 10rpx;
    box-sizing: border-box;
    padding: 18rpx 20rpx 20rpx;
    width: 710rpx;
    margin-bottom: 30rpx;
}

.list:last-child {
    margin-bottom: 0rpx;
}

.shopUp {
    display: flex;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.shopdet {
    width: 448rpx;
    font-size: 26rpx;
    color: #666;
}

.shopdet>view {
    width: 448rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.shopname {
    color: #222;
    font-size: 30rpx;
    font-weight: 700;
    margin-top: 6rpx;
    margin-bottom: 22rpx;
    line-height: 34rpx;
}

.address {
    margin-top: 30rpx;
    margin-bottom: 24rpx;
}

.tags {
    display: flex;
    align-items: center;
}

.shopDown {
    width: 670rpx;
    box-sizing: border-box;
}

.cards {
    display: flex;
    width: 670rpx;
    height: 166rpx;
    background: #fff6f7;
    border: 1rpx solid #ff5e5e;
    position: relative;
    margin-top: 20rpx;
    box-sizing: border-box;
}

.cards::before,.cards::after {
    content: " ";
    display: block;
    width: 16rpx;
    height: 16rpx;
    background: #fff;
    position: absolute;
    right: 182rpx;
    top: -8rpx;
    border-bottom: 1rpx solid #ff5e5e;
    border-left: 1rpx solid #ff5e5e;
    border-radius: 50%;
    z-index: 99;
    transform: rotate(-45deg);
}

.cards::after {
    top: 152rpx;
    transform: rotate(135deg);
}

.cardsLeft {
    width: 492rpx;
    padding-left: 26rpx;
    border-right: 1rpx dashed #DADCE3;
    box-sizing: border-box;
}

.cardsUp {
    height: 105rpx;
    box-sizing: border-box;
    border-bottom: 1rpx dashed #DADCE3;
    display: flex;
    padding-top: 30rpx;
}

.money {
    font-size: 36rpx;
    font-weight: 700;
    margin-right: 20rpx;
    margin-top: 10rpx;
}

.money text:first-child {
    font-size: 72rpx;
}

.day {
    color: #999;
    font-size: 24rpx;
    margin-top: 8rpx;
    letter-spacing: 2rpx;
}

.remark {
    height: 60rpx;
    line-height: 60rpx;
    color: #666;
    font-size: 24rpx;
}

.cardsRight {
    height: 166rpx;
    width: 198rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.cardsRight view:first-child {
    width: 164rpx;
    height: 54rpx;
    background: #ff5e5e;
    border-radius: 26rpx;
    text-align: center;
    line-height: 54rpx;
    color: #fff;
    margin-top: 56rpx;
}

.cardsRight view.isVip {
    margin-top: 14rpx;
    display: flex;
    align-items: center;
    font-size: 24rpx;
    font-weight: 700;
    color: #ffae00;
}

.vipIcon {
    margin-right: 4rpx;
    width: 24rpx;
    height: 20rpx;
}

.cardsRight .out {
    opacity: 0.35;
}

.mask {
    position: fixed;
    height: 100%;
    width: 100%;
    top: 0rpx;
    right: 0rpx;
    left: 0rpx;
    bottom: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.joinIco {
    width: 360rpx;
    height: 180rpx;
    margin-top: 74rpx;
}

.joinCont {
    z-index: 1001;
    position: fixed;
    padding-bottom: 30rpx;
    background: #fff;
    width: 600rpx;
    top: 4%;
    left: 75rpx;
    border-radius: 20rpx;
    overflow: hidden;
}

.show {
    animation: showModel 0.5s 1;
}

@keyframes showModel {
    from {
        transform: scale(0.6);
    }

    to {
        transform: scale(1);
    }
}

.joinHead {
    height: 284rpx;
    width: 720rpx;
    border-radius: 50%;
    background: #ff5e5e;
    display: flex;
    justify-content: center;
    margin-left: -60rpx;
    margin-top: -58rpx;
}

.joinBody {
    padding: 0rpx 20rpx;
    width: 560rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.joinTitle {
    padding: 50rpx 0rpx 30rpx;
    font-weight: 700;
    text-align: center;
}

.joinBody>view {
    margin-bottom: 30rpx;
    width: 560rpx;
    background: #f6f5f5;
    border-radius: 10rpx;
}

.joinBody input {
    width: 540rpx;
    height: 72rpx;
    padding-left: 20rpx;
    font-size: 24rpx;
    background: #f6f5f5;
    border-radius: 10rpx;
}

.join-time {
    position: relative;
    display: flex;
    align-items: center;
    margin-bottom: 30rpx;
    width: 560rpx;
    background: #f6f5f5;
    border-radius: 10rpx;
}

.join-time>text {
    margin-left: 20rpx;
}

.join-time>input {
    width: 350rpx;
}

.subMsg {
    padding-left: 35rpx;
    padding-right: 35rpx;
    background: #ff5e5e;
    font-size: 30rpx;
    color: #fff;
    height: 68rpx;
    line-height: 68rpx;
    text-align: center;
    border-radius: 10rpx;
    font-weight: 700;
    margin-top: 14rpx;
}

.joinBody>view.pay {
    text-align: center;
    background: #fff;
}

.joinBody>view.joinTitle {
    background: #fff;
}

.pay>text {
    font-size: 34rpx;
    font-weight: 700;
    color: #ff5e5e;
}