.header {
    height: 420rpx;
    width: 750rpx;
    background-size: 750rpx 420rpx;
    background-repeat: no-repeat;
    position: relative;
}

.bg {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    height: 420rpx;
    width: 750rpx;
}

.memberCard {
    width: 670rpx;
    height: 420rpx;
    position: absolute;
    top: 50rpx;
    left: 40rpx;
    border-radius: 16rpx;
    background-size: 670rpx 420rpx;
    background-repeat: no-repeat;
}

.cardsCont {
    width: 670rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.iconOprea {
    width: 60rpx;
    height: 60rpx;
    margin-bottom: 14rpx;
}

.cardsOpera>view {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 24rpx;
    color: #ff5e5e;
    font-weight: 700;
    margin: 0rpx 50rpx;
}

.weblogo {
    width: 280rpx;
    height: 80rpx;
    margin-top: 50rpx;
}

.cardsOpera {
    margin-top: 20rpx;
    display: flex;
    align-items: center;
}

.blcok {
    background: #fff;
    height: 200rpx;
    text-align: center;
}

.webname {
    padding-top: 82rpx;
    font-size: 34rpx;
    font-weight: 700;
}

.price {
    display: flex;
    justify-content: center;
    margin-top: 24rpx;
}

.curprice {
    font-size: 30rpx;
    font-weight: 700;
}

.curprice>text {
    color: #ff5e5e;
}

.oldprice {
    font-size: 24rpx;
    color: #999;
    position: relative;
    display: inline-block;
    margin-left: 30rpx;
}

.oldprice::after {
    content: " ";
    display: inline-block;
    width: 100%;
    height: 2rpx;
    background: #999;
    position: absolute;
    top: 10rpx;
    left: 0rpx;
    transform: rotate(3deg);
}

.personMsg {
    height: 80prx;
    line-height: 80rpx;
    padding-left: 20rpx;
    font-weight: 700;
    color: #999;
}

.session {
    background: #fff;
    padding-left: 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.list {
    height: 90rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #ececec;
}

.list>text {
    display: inline-block;
    width: 148rpx;
    color: #666;
}

.fixNav {
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    height: 128rpx;
    width: 750rpx;
    background: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-top: 1rpx solid #acacac;
    z-index: 99999;
}

.fixNav>button {
    margin-left: 0rpx;
    margin-right: 0rpx;
    width: 710rpx;
    height: 90rpx;
    background: #ff5e5e;
    color: #fff;
    font-size: 34rpx;
    line-height: 90rpx;
    border-radius: 6rpx;
}

.addressdet {
    padding-right: 20rpx;
    z-index: -1;
}

.addressdet,.addressdet>textarea {
    height: 180rpx;
    z-index: -1;
}

.rightIcn {
    width: 12rpx;
    height: 21rpx;
}

.babyIcon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 22rpx;
}

.baby {
    background: #fff;
    margin-top: 10rpx;
    display: flex;
    align-items: center;
    width: 710rpx;
    padding: 0rpx 20rpx;
    justify-content: space-between;
    height: 90rpx;
}

.baby>view {
    display: flex;
    align-items: center;
    width: 680rpx;
    font-size: 26rpx;
    color: #666;
}

.baby input {
    color: #222;
}