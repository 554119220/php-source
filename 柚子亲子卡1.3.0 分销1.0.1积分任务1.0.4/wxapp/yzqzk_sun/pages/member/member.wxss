.header {
    height: 420rpx;
    width: 750rpx;
    background-size: 750rpx 420rpx;
    background-repeat: no-repeat;
    position: relative;
}

.bg {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    height: 420rpx;
    width: 750rpx;
}

.memberCard {
    width: 670rpx;
    height: 406rpx;
    position: absolute;
    top: 50rpx;
    left: 40rpx;
    border-radius: 16rpx;
    background-size: 670rpx 406rpx;
    background-repeat: no-repeat;
    box-shadow: 1rpx 2rpx 12rpx 0rpx rgba(247,91,91,0.3);
}

.uThumb {
    height: 120rpx;
    width: 120rpx;
    border: 2rpx solid #fff;
    border-radius: 50%;
    margin-top: 50rpx;
    margin-bottom: 20rpx;
}

.cardsCont {
    width: 670rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #b93036;
    font-size: 25rpx;
    font-weight: 700;
}

.userMsg {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.userMsg>text:last-child {
    font-weight: 500;
    margin-top: 10rpx;
}

.order-nav {
    width: 750rpx;
    height: 92rpx;
    display: flex;
    justify-content: space-between;
    background: #fff;
    line-height: 92rpx;
    margin-top: 68rpx;
    border-bottom: 1rpx solid #dcdcdc;
}

.order-nav view {
    width: 375rpx;
    height: 92rpx;
    text-align: center;
    font-size: 30rpx;
    color: #999;
    font-weight: 700;
}

.nav {
    display: inline-block;
    height: 92rpx;
}

.order-nav .on {
    display: inline-block;
    color: #ff5e5e;
    box-sizing: border-box;
    border-bottom: 4rpx solid #ff5e5e;
}

.session {
    padding: 0rpx 20rpx 50rpx 20rpx;
    background: #fff;
}

.secWelfare {
    padding-top: 30rpx;
}

.list {
    display: flex;
    border: 2rpx solid #ff5e5e;
    border-radius: 10rpx;
    width: 674rpx;
    padding-left: 18rpx;
    box-shadow: 0rpx 0rpx 12rpx 0rpx rgba(255,94,94,0.3);
    margin-bottom: 30rpx;
}

.gimg {
    padding: 18rpx 0rpx;
    width: 250rpx;
    height: 180rpx;
    border-right: 2rpx dotted #ff5e5e;
    padding-right: 20rpx;
}

.gimg>image {
    width: 250rpx;
    height: 180rpx;
}

.listDet {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 18rpx 0rpx 18rpx 18rpx;
    height: 180rpx;
    width: 382rpx;
}

.title {
    color: #666;
    font-size: 26rpx;
    line-height: 40rpx;
    display: -webkit-box!important;
    width: 382rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.time {
    font-size: 26rpx;
    color: #666;
}

.privilege {
    width: 200rpx;
    color: #fff;
    background: #ff5e5e;
    border-radius: 6rpx;
    height: 46rpx;
    text-align: center;
    line-height: 46rpx;
    margin-top: 12rpx;
}

.btn {
    width: 750rpx;
    height: 128rpx;
    border-top: 1rpx solid #acacac;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    z-index: 11;
}

.btn>button {
    height: 98rpx;
    width: 710rpx;
    color: #fff;
    font-size: 34rpx;
    background: #ff5e5e;
    line-height: 98rpx;
    border-radius: 6rpx;
}

.notesTil {
    padding-top: 30rpx;
    display: flex;
    justify-content: center;
}

.notes {
    display: flex;
}

.notes>image:first-child {
    margin-right: -38rpx;
    transform: rotateY(180deg);
}

.notes>image:last-child {
    margin-left: -38rpx;
}

.notes>image {
    height: 93rpx;
    width: 122rpx;
    margin-top: 22rpx;
}

.notes>view {
    height: 90rpx;
    padding: 0rpx 46rpx;
    line-height: 90rpx;
    background: #ff5e5e;
    color: #fff;
    font-size: 34rpx;
}

.rules {
    width: 670rpx;
    padding: 50rpx 20rpx;
    background: #ffaeae;
    color: #fff;
    line-height: 42rpx;
    font-weight: 700;
    letter-spacing: 2rpx;
    word-break: break-all;
}

.priImg>image {
    width: 710rpx;
}

.toIndex {
    background: rgba(255,255,255,0.9);
    z-index: 111;
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0rpx 0rpx 22rpx 0rpx rgba(184,38,38,0.1);
    position: fixed;
    left: 604rpx;
    bottom: 300rpx;
}

.toIndex>image {
    width: 52rpx;
    height: 50rpx;
}