.tab {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    background: #fff;
    border-top: 2rpx solid #d5d5d5;
    height: 92rpx;
    padding-bottom: 4rpx;
}

.ipx {
    padding-bottom: 68rpx;
}

.tabnav {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.tablist {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #8c8c8c;
}

.active {
    color: #ff5e5e;
}

.tabimg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 8rpx;
    margin-bottom: 0rpx;
}

.user {
    height: 230rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(255,94,94) 0%,rgb(255,203,203) 100%);
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 30rpx 0rpx 22rpx;
    font-size: 28rpx;
    color: #fff;
}

.user-info {
    display: flex;
}

.thumb-img {
    border-radius: 50%;
    width: 160rpx;
    height: 160rpx;
}

.nickname {
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin-left: 30rpx;
}

.user-name {
    margin-bottom: 12rpx;
}

.thumb {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    border: 2rpx solid #fff;
}

.scan {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.icon {
    margin-bottom: 14rpx;
    width: 38rpx;
    height: 38rpx;
}

.session {
    background: #fff;
    font-size: 28rpx;
    color: #222;
    border-bottom: 1px solid #cbcbcb;
}

.session-title {
    height: 90rpx;
    padding: 0rpx 20rpx;
    display: flex;
    align-items: center;
    border-bottom: 1px solid #cbcbcb;
    justify-content: space-between;
}

.addManager {
    display: inline-block;
    width: 170rpx;
    height: 68rpx;
    font-weight: 700;
    color: #fff;
    background: #ff5e5e;
    border-radius: 10rpx;
    text-align: center;
    line-height: 68rpx;
}

.add {
    width: 28rpx;
    height: 28rpx;
    margin-right: 24rpx;
}

.session-cont {
    display: flex;
    flex-wrap: wrap;
}

.list {
    height: 120rpx;
    width: 335rpx;
    border-bottom: 1px solid #cbcbcb;
    padding: 20rpx;
    font-size: 28rpx;
    color: #222;
    display: flex;
    flex-direction: column;
}

.list:nth-child(odd) {
    width: 334rpx;
    border-right: 1px solid #cbcbcb;
}

.list-item {
    font-size: 52rpx;
    line-height: 86rpx;
    font-weight: 700;
    text-align: center;
    margin-top: 6rpx;
}

.finance {
    margin-top: 20rpx;
    margin-bottom: 150rpx;
}

.cash {
    display: flex;
    flex-direction: row;
    justify-content: center;
    font-size: 30rpx;
    color: #ff5e5e;
    font-weight: 700;
    align-items: center;
}

.cash image {
    width: 32rpx;
    height: 28rpx;
    margin-right: 24rpx;
}

.wrap {
    background: #fff;
    margin-bottom: 20rpx;
}

.wrapTitle {
    height: 90rpx;
    padding-left: 20rpx;
    border-bottom: 1rpx solid #cbcbcb;
    line-height: 90rpx;
}

.navIcon {
    width: 82rpx;
    height: 82rpx;
    margin-bottom: 22rpx;
}

.tabNav {
    height: 160rpx;
    width: 710rpx;
    padding: 0 20rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    font-size: 24rpx;
}

.itemNav {
    display: flex;
    flex-direction: column;
    align-items: center;
}