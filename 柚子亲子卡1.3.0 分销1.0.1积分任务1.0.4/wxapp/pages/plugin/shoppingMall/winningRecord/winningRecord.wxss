.menu_box {
    width: 750rpx;
    line-height: 80rpx;
    font-size: 30rpx;
}

.menu {
    display: inline-block;
    width: 109rpx;
    color: #7d7d7d;
    text-align: center;
}

.menu:nth-child(1) {
    margin-left: 94rpx;
}

.menu:nth-child(2) {
    margin-left: 117rpx;
}

.menu:nth-child(3) {
    margin-left: 117rpx;
}

.borders {
    color: #000000;
    border-bottom: 5rpx solid #000000;
}

.tab1,.tab2 {
    height: 300rpx;
    background: #23bff3;
}

.tab2 {
    background: #ccc;
}

.list1 {
    height: 200rpx;
    display: flex;
    flex-direction: row;
    box-sizing: border-box;
    width: 750rpx;
}

.tu2 {
    width: 126rpx;
    height: 114rpx;
    margin-left: 37rpx;
    margin-top: 40rpx;
}

.product-Text {
    margin-left: 35rpx;
}

.product-name {
    font-size: 30rpx;
    margin-top: 25rpx;
    display: block;
}

.integral-points {
    font-size: 24rpx;
    display: block;
    margin-top: 10rpx;
}

.count {
    margin-left: 17rpx;
}

.bargain {
    font-size: 24rpx;
    margin-top: 29rpx;
    display: block;
    color: #e20004;
}

.cut {
    margin-left: 10rpx;
}

.in-progress {
    height: 80rpx;
    width: 750rpx;
    border-bottom: solid 1px #f5f5f5;
    box-sizing: border-boxp;
    display: flex;
    flex-direction: row;
    align-items: center;
    padding-left: 20rpx;
    border-top: solid 7rpx #f2f2f2;
}

.underway {
    font-size: 24rpx;
    font-weight: bold;
}

.bg {
    height: 17rpx;
    background: #f2f2f2;
}

.scroll {
    height: 1300rpx;
}