.tabbar_box {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 9998;
    width: 100%;
    height: 120rpx;
    background: #fff;
}

.tabbar_nav {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 26rpx;
    height: 100%;
    position: relative;
}

.tabbar_icon {
    width: 51rpx;
    height: 51rpx;
}

.cartnum {
    position: absolute;
    top: 5rpx;
    right: 0rpx;
    background: #f00;
    border-radius: 50%;
    min-width: 40rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    padding: 3rpx;
    color: #fff;
    font-size: 8pt;
}