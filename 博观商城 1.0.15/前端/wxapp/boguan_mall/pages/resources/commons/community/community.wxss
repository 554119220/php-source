.community_swicth {
    background: #fff;
    padding: 0rpx 30rpx;
    height: 90rpx;
    font-family: PingFang-SC-Bold;
    box-sizing: border-box;
}

.community_address image {
    width: 36rpx;
    height: 36rpx;
}

.community_address text {
    padding-left: 10rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
    font-size: 31rpx;
    color: #000;
    padding-right: 15rpx;
    position: relative;
    height: 40rpx;
    line-height: 40rpx;
}

.community_address {
    height: 50rpx;
}

.community_address text::after {
    content: "";
    width: 0;
    height: 0;
    border-left: 10rpx solid #747474;
    border-right: 10rpx solid transparent;
    border-top: 10rpx solid transparent;
    border-bottom: 10rpx solid transparent;
    position: absolute;
    z-index: 106;
    top: 12rpx;
    right: -5rpx;
}

.community_buyers {
    width: 45%;
    padding-left: 15rpx;
}

.community_buyers_list {
    background: #808080;
    border-radius: 60rpx;
    width: 100%;
    height: 50rpx;
    padding: 0 10rpx;
    box-sizing: border-box;
    overflow: hidden;
}

.buyers_list_banner,.community_buyers_list_box {
    height: 50rpx;
}

.community_buyers_list_box image {
    width: 38rpx;
    height: 38rpx;
    border-radius: 38rpx;
}

.community_buyers_list_box text {
    font-size: 22rpx;
    color: #fff;
    padding-left: 10rpx;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
}