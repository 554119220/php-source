.m_title {
    padding: 25rpx 20rpx;
    font-size: 32rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.m_title .title_name {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 32rpx;
}

.m_title .more {
    font-size: 25rpx;
    display: block;
    position: absolute;
}

.m_title_navigator {
    position: relative;
}

.m_title_navigator .m_title_after {
    content: "";
    width: 15rpx;
    height: 15rpx;
    position: absolute;
    right: 0rpx;
    top: 50%;
    margin-top: -8rpx;
    display: block;
    transform: rotate(135deg);
    border-left: 3rpx solid #666;
    border-top: 3rpx solid #666;
}

.m_title_right {
    position: absolute;
    top: 0;
    font-size: 28rpx;
}