@import "../../../../../__wuBaseWxss__/18.wxss";

page {
    height: 100%;
    width: 100%;
}

.boguan {
    height: 100%;
    width: 100%;
}

.tab {
    background: #fff;
    text-align: center;
    position: fixed;
    width: 100%;
    z-index: 9999;
}

.tab_box text {
    display: inline-block;
    padding: 20rpx 0;
}

.tab_box.active text {
    position: relative;
}

.tab_box.active text::after {
    content: "";
    height: 2px;
    width: 100%;
    background: #ad0e11;
    color: #ad0e11;
    position: absolute;
    left: 0;
    bottom: 0;
}

.tab-swiper {
    height: 100%;
}

.tab-swiper swiper {
    padding-top: 80rpx;
    height: 100%;
    box-sizing: border-box;
}

.scroll-view_H {
    height: 100%;
}

.not_coupon_title {
    text-align: center;
    color: #666;
    margin-top: 30rpx;
}