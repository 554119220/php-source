@import "../../resources/commons/search/search.wxss";

page {
    height: 100%;
    width: 100%;
    overflow-y: hidden;
    position: fixed;
    background: #fff;
    z-index: 1;
}

.boguan {
    width: 100%;
    height: 100%;
    position: fixed;
    background: #fff;
}

.tabbar_box+.boguan {
    height: calc(100% - 120rpx);
}

.category_search {
    padding-bottom: 20rpx;
}

.category {
    border-top: 1rpx solid #dedede;
    height: calc(100% - 100rpx);
}

.category_left {
    width: 170rpx;
    border-right: 1rpx solid #dedede;
}

.category_left,.category_right {
    height: 100%;
}

.scroll-view_H {
    height: 100%;
    width: 170rpx;
}

.category_item {
    text-align: center;
    padding: 15px 0;
    font-size: 28rpx;
    color: #7f7f7f;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.category_item.active {
    color: #ad0e11;
}

.categorySelset {
    width: 13rpx;
    height: 30rpx;
    background: #ad0e11;
    position: absolute;
    top: 33rpx;
    left: 0;
}

.category_right {
    box-sizing: border-box;
}

.category_right_paading {
    padding: 30rpx 30rpx 30rpx 30rpx;
}

.category_right .banner {
    width: 100%;
    height: 260rpx;
    border-radius: 5rpx;
    overflow: hidden;
}

.category_right .banner image,.category_right .banner .banner_link {
    width: 100%;
    height: 100%;
    display: block;
}

.second_cate .title {
    padding-top: 50rpx;
    text-align: center;
    font-size: 24rpx;
}

.second_cate .title text {
    position: relative;
    height: 40rpx;
    line-height: 40rpx;
    display: inline-block;
    padding: 0 10rpx;
}

.second_cate .title text::after {
    width: 25rpx;
    height: 1px;
    content: '';
    background: #cacaca;
    position: absolute;
    top: 20rpx;
    left: -25rpx;
    z-index: 1;
}

.second_cate .title text::before {
    width: 25rpx;
    height: 1px;
    content: '';
    background: #ccc;
    position: absolute;
    top: 20rpx;
    right: -25rpx;
    z-index: 1;
}

.nav {
    flex-wrap: wrap;
}

.nav_item {
    width: 33.3333%;
    text-align: center;
    font-size: 22rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
    margin-bottom: 20rpx;
}

.nav_pic {
    width: 130rpx;
    height: 130rpx;
    margin: 20rpx auto;
}

.nav_pic image {
    width: 100%;
    height: 100%;
    display: block;
    margin: 0 auto;
}

.scroll-category_H {
    height: 100%;
    overflow-y: hidden;
}

.cateBanner {
    height: 200rpx;
    margin-bottom: 20rpx;
}

.cateBanner navigator {
    width: 100%;
    height: 100%;
}

.cateBanner image {
    height: 100%;
    width: 100%;
}

.cate_nav .nav_item {
    width: 25%;
}

.category_product {
    padding: 20rpx 0;
}

.product_item {
    margin-bottom: 20rpx;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #ededed;
}

.category_product .product_pic {
    width: 180rpx;
    height: 180rpx;
}

.category_product .product_pic image {
    width: 100%;
    height: 100%;
}

.product_info {
    padding-left: 20rpx;
    position: relative;
}

.product_title {
    text-align: left;
    font-size: 28rpx;
    color: #333;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
    font-family: PingFangSC-Regular;
}

.product_describe {
    font-size: 22rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    color: #999;
    margin-top: 5rpx;
}

.product_bottom {
    position: absolute;
    bottom: 0;
    width: 100%;
}

.product_price .price {
    color: #ad0e11;
    font-size: 26rpx;
}

.product_price .o_price {
    font-size: 20rpx;
    color: #666;
    text-decoration: line-through;
    margin-left: 10rpx;
}

.bug {
    background: #ad0e11;
    color: #fff;
    padding: 10rpx 12rpx;
    border-radius: 5rpx;
    margin-right: 30rpx;
}

.spec_bottom {
    position: relative;
}

.spec_bottom .auth_btn {
    position: absolute;
    width: 100%;
    height: 80rpx;
    top: 0;
    left: 0;
    background: transparent;
    border-radius: 0;
}

.spec_bottom .auth_btn::after {
    border: none;
}

.pay_nav {
    padding-left: 20rpx;
}

.spec_bottom .pay_nav {
    padding-left: 0rpx;
}

.pay_nav .add_cart {
    width: 50%;
    background: #222222;
    color: #fff;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
    border-bottom-left-radius: 10rpx;
}

.pay_nav .to_pay {
    width: 50%;
    background: #c13b38;
    color: #fff;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
    border-bottom-right-radius: 10rpx;
}

.spec_popup {
    width: 80%;
    background: #fff;
    border-radius: 10rpx;
}

.mask_center {
    height: calc(100% - 120rpx);
}

.spec_header {
    padding: 30rpx 30rpx 0 30rpx;
    height: 200rpx;
    border-bottom: 1rpx solid #f1f1f1;
    position: relative;
}

.spec_pic image {
    width: 170rpx;
    height: 170rpx;
}

.spec_info {
    font-size: 24rpx;
    padding: 0 0rpx 0 30rpx;
}

.spec_product_title {
    text-align: left;
    font-size: 24rpx;
    color: #333;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
    font-family: PingFangSC-Regular;
    margin-bottom: 5rpx;
    padding-right: 20rpx;
}

.spec_price {
    height: 40rpx;
    color: #c13b38;
}

.spec_price .vip_discount {
    font-size: 18rpx;
    padding: 2rpx 10rpx;
    display: inline-block;
    border: 1px solid #c13b38;
    border-radius: 5rpx;
    margin-left: 10rpx;
}

.colse image {
    width: 30rpx;
    height: 30rpx;
    padding: 20rpx;
    position: absolute;
    top: 10rpx;
    right: 10rpx;
}

.spec_content {
    padding: 0rpx 30rpx;
}

.spec_tit {
    padding-bottom: 20rpx;
    font-size: 26rpx;
    padding-top: 20rpx;
}

.spec_select {
    flex-wrap: wrap;
    border-bottom: 1rpx solid #f1f1f1;
}

.spec_select:last-child {
    border-bottom: 0;
}

.spec_box {
    border: 1px solid #ededed;
    border-radius: 6rpx;
    padding: 10rpx 20rpx;
    font-size: 24rpx;
    margin-right: 25rpx;
    margin-bottom: 15rpx;
}

.spec_box.active {
    color: #c13b38;
    border-color: #c13b38;
}

.spec_box.disabled {
    border: 1rpx dashed #c7c7c7;
    color: #c7c7c7;
    pointer-events: auto;
}

.scroll-view_X {
    max-height: 430rpx;
    overflow: hidden;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    z-index: 10;
}

.buy_num {
    padding: 30rpx;
    background: #fff;
    border-top: 1rpx solid #ededed;
}

.cart_btn .sum,.cart_btn .add {
    border: 1px solid #dddddd;
    border-radius: 50%;
}

.cart_btn .number {
    height: 45rpx;
    line-height: 45rpx;
    padding: 0 20rpx;
    font-size: 28rpx;
}

.cart_btn .sum {
    width: 45rpx;
    height: 45rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.cart_btn .not_sum {
    pointer-events: none;
    width: 45rpx;
    height: 45rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.cart_btn .add {
    width: 45rpx;
    height: 45rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.buylimit_goods {
    padding: 20rpx 0;
    box-sizing: border-box;
    width: 100%;
}

.buylimit_goods_pic {
    width: 180rpx;
    height: 180rpx;
}

.buylimit_goods_pic image {
    width: 100%;
    height: 100%;
}

.buylimit_goods_info {
    position: relative;
    width: calc(100% - 180rpx);
    height: 180rpx;
    margin-left: 20rpx;
}

.buylimit_goods_name {
    text-align: left;
    font-size: 24rpx;
    color: #333;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
    font-family: PingFangSC-Regular;
}

.buylimit_goods_price {
    position: absolute;
    bottom: 0rem;
    left: 0;
    color: #ad0e11;
}

.buylimit_goods_price-txt {
    font-size: 24rpx;
    color: #ad0e11;
}

.buylimit_goods_price-num {
    font-family: PingFangSC-Regular;
    font-size: 36rpx;
    margin-left: 4rpx;
    margin-right: 4rpx;
}

.buylimit_goods_addition-info {
    position: absolute;
    font-size: 24rpx;
    color: #999;
    right: 0;
    bottom: 62rpx;
    text-align: right;
}

.buylimit_goods_addition-btn {
    position: absolute;
    bottom: 0;
    right: 0;
    height: 40rpx;
    width: 110rpx;
    background-color: #c13b38;
    border-radius: 6rpx;
}

.buylimit_goods_addition-btn-txt {
    width: 100%;
    line-height: 40rpx;
    text-align: center;
    color: #fff;
    font-size: 22rpx;
    display: block;
}

.buylimit_goods_addition-will-btn {
    font-family: PingFangSC-Regular;
    width: 130rpx;
    height: 52rpx;
    border-radius: 6rpx;
    font-size: 26rpx;
    color: #fff;
    background: #7db49d;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.buylimit_goods_addition-will-btn-txt {
    font-size: 33rpx;
    transform: scale(0.8);
    transform-origin: 0 center;
}

.icon_remind {
    display: inline-block;
    margin-right: 10rpx;
    width: 28rpx;
    height: 28rpx;
}

.buylimit_goods_ellips text {
    display: inline-block;
    background: #d87672;
    border-radius: 3rpx;
    padding: 4rpx 5rpx;
    color: #fff;
    margin: 2rpx 4rpx 0 0;
    font-size: 20rpx;
    line-height: 1;
}

.vip {
    border: 1px solid #e0ce94;
    border-radius: 5rpx;
    color: #c13b38;
    font-size: 20rpx;
    display: table;
}

.vip .vip_tit {
    display: table-cell;
    padding-left: 6rpx;
    background: linear-gradient(rgba(250,250,250,1),rgba(240,230,190,1));
    font-size: 19rpx;
}

.vip_price {
    padding: 0 4rpx;
    line-height: 19rpx;
    overflow: hidden;
    font-size: 19rpx;
}

.product_price {
    padding: 12rpx 0;
    line-height: 1;
    font-family: PingFangSC-Regula;
    color: #ad0e11;
}

.product_price .price_icon {
    font-size: 22rpx;
}

.spec_val {
    flex-flow: wrap;
}

.spec_val .selected_Spec {
    width: 60rpx;
}

.spec_val .value_box {
    padding: 0 3rpx;
}

.spec_val .value_box:last-child text {
    display: none;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}