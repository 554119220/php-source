@import "../../Home/coupon/coupon_goods/coupon_goods.wxss";

page {
    background: #f3f3f3;
}

.boguan {
    padding-bottom: 100rpx;
}

.tabbar_box+.boguan {
    padding-bottom: 221rpx;
}

.store_cart {
    border-bottom: 1rpx solid #ededed;
}

.store_cart:last-child {
    background: #000;
    border-bottom: 0rpx solid #ededed;
}

.cart_box {
    padding: 20rpx 30rpx 20rpx 0rpx;
    background: #fff;
}

.product_pic {
    width: 158rpx;
    height: 158rpx;
}

.product_pic navigator {
    width: 158rpx;
    height: 158rpx;
}

.product_pic image {
    width: 100%;
    height: 100%;
}

.orderProduct_tit {
    padding: 0 20rpx;
    font-size: 26rpx;
    line-height: 40rpx;
}

.orderProduct_tit .name {
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
}

.orderProduct_tit .spec {
    line-height: 30rpx;
    margin-top: 10rpx;
}

.orderProduct_tit .spec text {
    font-size: 24rpx;
    color: #999;
}

.orderProduct_tit .spec text:last-child {
    margin-right: 0rpx;
    display: none;
}

.orderProduct_tit .vip_price {
    color: #ad0e11;
    font-size: 26rpx;
}

.vip_price .price {
    color: #c1c1c1;
    font-size: 22rpx;
    margin-left: 10rpx;
}

.tabbar_box+.boguan .cart_bottom {
    bottom: 120rpx;
}

.cart_bottom {
    width: 100%;
    position: fixed;
    bottom: 0rpx;
    background: #fff;
    border-top: 1rpx solid #dddddd;
    padding: 20rpx 30rpx 20rpx 0rpx;
    box-sizing: border-box;
}

.all_checked_padding {
    padding-left: 30rpx;
}

.total_price .price {
    color: #c13b38;
    margin-right: 30rpx;
}

.total_pay .topay {
    padding: 15rpx 35rpx;
    background: #c13b38;
    border-radius: 8rpx;
    color: #fff;
}

.not_order {
    padding: 200rpx 0;
}

.not_order image {
    width: 250rpx;
    height: 250rpx;
    display: block;
    margin: 0 auto;
}

.not_order_tips {
    display: block;
    font-size: 28rpx;
    color: #999;
    text-align: center;
    margin-top: 30rpx;
}

.not_order_btn {
    border: 1rpx solid #999;
    font-size: 26rpx;
    padding: 8rpx 10rpx;
    text-align: center;
    display: block;
    width: 131rpx;
    margin: 30rpx auto;
    border-radius: 5rpx;
    background: #fff;
    color: #999;
}

.CantBuy_head {
    padding: 15rpx 30rpx;
    background: #fff;
    margin: 20rpx 0 0 0;
    border-bottom: 1rpx solid #ededed;
}

.CantBuy_detele_btn {
    font-size: 24rpx;
    color: #c13b38;
    padding: 10rpx 20rpx;
    border: 1rpx solid #c13b38;
    border-radius: 5rpx;
}

.CantBuy_title {
    text-align: left;
    font-size: 26rpx;
    color: #999;
}

.CantBuy_goods_tips {
    font-size: 24rpx;
    color: #c13b38;
    margin-top: 30rpx;
}

.check_padding {
    padding: 30rpx 30rpx;
}

.box_left .check_box {
    margin-top: 30rpx;
}

.check_box {
    width: 40rpx;
    height: 40rpx;
    display: block;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.check_box.active {
    width: 40rpx;
    height: 40rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.cart_btn {
    margin-top: 20rpx;
}

.cart_btn .sum,.cart_btn .add {
    border: 1px solid #dddddd;
    border-radius: 50%;
}

.cart_btn .number {
    height: 40rpx;
    line-height: 40rpx;
    padding: 0 20rpx;
    font-size: 28rpx;
}

.cart_btn .sum {
    width: 40rpx;
    height: 40rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.cart_btn .not_sum {
    pointer-events: none;
    width: 40rpx;
    height: 40rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.cart_btn .add {
    width: 40rpx;
    height: 40rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.cart_btn .not_add {
    pointer-events: none;
    width: 40rpx;
    height: 40 rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.detele_btn {
    width: 120rpx;
    justify-content: flex-end;
    font-size: 24rpx;
}

.detele_padding {
    padding: 20rpx 30rpx;
}

.delete {
    width: 30rpx;
    height: 30rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}