.tabbar_box+.boguan {
    padding-bottom: 120rpx;
}

.add_program_tip {
    width: 400rpx;
    height: 50rpx;
    position: absolute;
    right: 20rpx;
    top: 15rpx;
    background: #c13b38;
    border-radius: 60rpx;
    z-index: 107;
    font-size: 23rpx;
    text-align: center;
    line-height: 50rpx;
    font-family: PingFang-SC-Bold;
    padding: 5rpx 0;
    color: #fff;
}

.add_program_tip::after {
    content: "";
    width: 0;
    height: 0;
    border-left: 15rpx solid transparent;
    border-right: 15rpx solid transparent;
    border-top: 15rpx solid transparent;
    border-bottom: 15rpx solid #c13b38;
    position: absolute;
    z-index: 106;
    top: -25rpx;
    right: 95rpx;
}

.add_program_tip image {
    width: 11rpx;
    height: 22rpx;
    margin-left: 5rpx;
}

.spec_bottom {
    position: relative;
}

.spec_bottom .auth_btn {
    position: absolute;
    width: 100%;
    height: 80rpx;
    top: 0;
    left: 0;
    background: transparent;
    border-radius: 0;
}

.spec_bottom .auth_btn::after {
    border: none;
}

.pay_nav {
    padding-left: 20rpx;
}

.spec_bottom .pay_nav {
    padding-left: 0rpx;
}

.pay_nav .add_cart {
    width: 50%;
    background: #222222;
    color: #fff;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
    border-bottom-left-radius: 10rpx;
}

.pay_nav .to_pay {
    width: 50%;
    background: #c13b38;
    color: #fff;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
    border-bottom-right-radius: 10rpx;
}

.spec_popup {
    width: 80%;
    background: #fff;
    border-radius: 10rpx;
}

.mask_center {
    height: calc(100% - 120rpx);
}

.spec_header {
    padding: 30rpx 30rpx 0 30rpx;
    height: 200rpx;
    border-bottom: 1rpx solid #f1f1f1;
    position: relative;
}

.spec_pic image {
    width: 170rpx;
    height: 170rpx;
}

.spec_info {
    font-size: 24rpx;
    padding: 0 0rpx 0 30rpx;
}

.spec_product_title {
    text-align: left;
    font-size: 24rpx;
    color: #333;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
    font-family: PingFangSC-Regular;
    margin-bottom: 5rpx;
    padding-right: 20rpx;
}

.spec_price {
    height: 40rpx;
    color: #c13b38;
}

.spec_price .vip_discount {
    font-size: 18rpx;
    padding: 2rpx 10rpx;
    display: inline-block;
    border: 1px solid #c13b38;
    border-radius: 5rpx;
    margin-left: 10rpx;
}

.colse image {
    width: 30rpx;
    height: 30rpx;
    padding: 20rpx;
    position: absolute;
    top: 10rpx;
    right: 10rpx;
}

.spec_content {
    padding: 0rpx 30rpx;
}

.spec_tit {
    padding-bottom: 20rpx;
    font-size: 26rpx;
    padding-top: 20rpx;
}

.spec_select {
    flex-wrap: wrap;
    border-bottom: 1rpx solid #f1f1f1;
}

.spec_select:last-child {
    border-bottom: 0;
}

.spec_box {
    border: 1px solid #ededed;
    border-radius: 6rpx;
    padding: 10rpx 20rpx;
    font-size: 24rpx;
    margin-right: 25rpx;
    margin-bottom: 15rpx;
}

.spec_box.active {
    color: #c13b38;
    border-color: #c13b38;
}

.spec_box.disabled {
    border: 1rpx dashed #c7c7c7;
    color: #c7c7c7;
    pointer-events: auto;
}

.scroll-view_X {
    max-height: 430rpx;
    overflow: hidden;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    z-index: 9999;
}

.buy_num {
    padding: 30rpx;
    background: #fff;
    border-top: 1rpx solid #ededed;
}

.cart_btn .sum,.cart_btn .add {
    border: 1px solid #dddddd;
    border-radius: 50%;
}

.cart_btn .number {
    height: 45rpx;
    line-height: 45rpx;
    padding: 0 20rpx;
    font-size: 28rpx;
}

.cart_btn .sum {
    width: 45rpx;
    height: 45rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.cart_btn .not_sum {
    pointer-events: none;
    width: 45rpx;
    height: 45rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.cart_btn .add {
    width: 45rpx;
    height: 45rpx;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}