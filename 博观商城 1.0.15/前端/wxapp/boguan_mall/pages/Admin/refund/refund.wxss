.boguan {
    padding-bottom: 120rpx;
}

.refund_tips {
    padding: 20rpx;
    color: #c13b38;
    text-align: center;
    background: #f9f9f9;
}

.send_order_info {
    margin-top: 20rpx;
    padding: 30rpx;
    background: #fff;
}

.send_order_info .send_box {
    margin-bottom: 20rpx;
}

.send_order_info .title {
    width: 180rpx;
}

.send_order_info .send_text {
    text-align: right;
    color: #666;
}

.send_order_info .send_price {
    color: #c13b38;
}

.img_box {
    margin-top: 20rpx;
}

.img_box image {
    width: 150rpx;
    height: 150rpx;
    margin-right: 20rpx;
}

.refund_options {
    padding: 20rpx 30rpx;
    position: fixed;
    bottom: 0;
    background: #fff;
    width: 100%;
    box-sizing: border-box;
    text-align: center;
}

.options_refuse,.options_agreed {
    border: 1rpx solid #ad0e11;
    padding: 15rpx 0;
    border-radius: 10rpx;
    color: #ad0e11;
}

.options_refuse {
    margin-right: 15rpx;
}

.options_agreed {
    margin-left: 15rpx;
}

.refund_options .active {
    background: #ad0e11;
    color: #fff;
}

.refund_address {
    padding: 20rpx 30rpx;
    margin-top: 20rpx;
    background: #fff;
}

.section__title {
    width: 180rpx;
}

.picker {
    padding: 10rpx 30rpx 10rpx 0;
    position: relative;
    overflow: hidden;
    color: #666;
}

.picker::after {
    content: "";
    width: 15rpx;
    height: 15rpx;
    position: absolute;
    right: 10rpx;
    top: 50%;
    margin-top: -8rpx;
    display: block;
    transform: rotate(135deg);
    border-left: 3rpx solid #666;
    border-top: 3rpx solid #666;
}

.options_agreed.options_disabled {
    background: #f2f2f2;
    pointer-events: none;
    border-color: #f2f2f2;
    color: #333;
}

.user_send_header {
    padding: 20rpx 30rpx;
    background: #fff;
    margin: 20rpx 0;
}

.express-content {
    background: #fff;
    padding: 20rpx 30rpx;
}

.express-jd {
    background: #fff;
    padding-left: 20rpx;
    font-size: 26rpx;
    color: #999999;
}

.express-jd.active {
    color: #06bf04;
}

.express-jd .kd-jd {
    border-bottom: 1px solid #ededed;
    padding: 20rpx 0;
}

.express-jd .kd-jd .time {
    margin-top: 20rpx;
}

.jd-ico {
    width: 40rpx;
    position: relative;
    border-left: 1px solid #ededed;
}

.ico-dot {
    width: 20rpx;
    height: 20rpx;
    background: #ededed;
    border-radius: 50%;
    position: absolute;
    top: 30%;
    left: -10rpx;
}

.express-jd.active .ico-dot {
    width: 40rpx;
    height: 40rpx;
    display: block;
    left: -20rpx;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,);
}

.expressMsg {
    color: #06bf04;
}