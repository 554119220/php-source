<view class="boguan">
    <view class="share_info flex-row">
        <view class="can_price flex-grow-1">
            <text>{{shareData.share_money}}元</text>
            <view class="tit">可提现佣金</view>
        </view>
        <view class="end_price flex-grow-1">
            <text>{{shareData.withdraw_money}}元</text>
            <view class="tit">已提现佣金</view>
        </view>
        <view class="hava_price flex-grow-1">
            <text>{{shareData.not_settle_money}}元</text>
            <view class="tit">未结算佣金</view>
        </view>
    </view>
    <view class="stay_pay flex-row">
        <view class="stay_title flex-grow-1">待打款佣金</view>
        <view class="stay_price flex-grow-0">
            <text>{{shareData.wait_pay_money}}</text> 元</view>
    </view>
    <view class="forward_btn">
        <navigator url="../forward/forward?share_money={{shareData.share_money}}&min_withdraw={{shareData.min_withdraw}}">立即提现</navigator>
    </view>
</view>
