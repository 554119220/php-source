.share_header {
    height: 270rpx;
    background: #f2f2fa;
    padding: 30rpx;
}

.user_logo {
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-right: 20rpx;
    margin-left: 10rpx;
}

.user_logo image {
    width: 100%;
    height: 100%;
}

.user_name,.share_user {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.share_user {
    font-size: 26rpx;
}

.share_info {
    text-align: center;
    border-radius: 10rpx;
    margin-top: 30rpx;
}

.can_price {
    padding: 20rpx 0;
    border-right: 1rpx solid #d3d6d9;
}

.end_price {
    padding: 20rpx 0;
    border-right: 1rpx solid #d3d6d9;
}

.hava_price {
    padding: 20rpx 0;
}

.my_service {
    padding: 0rpx 30rpx 0rpx 30rpx;
    background: #fff;
    margin-top: 30rpx;
}

.my_service_tit {
    font-size: 30rpx;
    padding: 30rpx 30rpx;
    border-bottom: 1rpx solid #e9e9e9;
}

.service_list {
    flex-wrap: wrap;
}

.icon_service {
    width: 33.3333%;
    text-align: center;
    font-size: 26rpx;
    color: #666;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #ededed;
}

.icon_service image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 0 auto;
    margin-bottom: 0rpx;
}

.tixian {
    background: #ad0e11;
    color: #fff;
    padding: 0 25rpx;
    font-size: 26rpx;
    border-radius: 5rpx;
}

.icon_service .price {
    color: #ad0e11;
    font-size: 26rpx;
}