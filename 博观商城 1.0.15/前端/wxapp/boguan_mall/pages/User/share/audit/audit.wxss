.share_banner {
    background: #fff;
}

.share_banner image {
    width: 100%;
    height: 100%;
}

.audit_form {
    background: #fff;
    margin-top: 20rpx;
}

.audit_tit {
    text-align: center;
    padding: 30rpx;
    font-weight: bold;
    border-bottom: 1rpx solid #ededed;
}

.audit_box {
    height: 1.4rem;
    border-bottom: 1rpx solid #ededed;
    padding: 20rpx 30rpx;
}

.form_label {
    width: 150rpx;
}

.parent_name {
    color: #c13b38;
}

.submit {
    padding: 50rpx;
}

.agree {
    margin-top: 30rpx;
    font-size: 26rpx;
}

.agree_text navigator {
    display: inline-block;
    color: #c13b38;
}

checkbox .wx-checkbox-input {
    border-radius: 3rpx;
    height: 26rpx;
    width: 26rpx;
    margin-top: -4rpx;
}

.audit_tips {
    padding: 30rpx;
}

.audit_tips .tips {
    text-align: center;
    color: #999;
}

.toIndex {
    width: 100%;
    height: 75rpx;
    line-height: 75rpx;
    color: #fff;
    border-radius: 10rpx;
    background: #c13b38;
    text-align: center;
    margin-top: 50rpx;
}