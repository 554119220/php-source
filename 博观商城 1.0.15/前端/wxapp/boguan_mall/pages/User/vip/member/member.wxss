.cards-wrapper {
    background-color: #fff;
    overflow-x: hidden;
    padding-top: 40rpx;
    position: relative;
}

.card-bg {
    position: absolute;
    top: 0;
    width: 100%;
    height: 540rpx;
    background-image: -webkit-linear-gradient(225deg,#666 0%,#111 100%);
}

.bg-img {
    height: 540rpx;
    background: url("//img.yzcdn.cn/memberlevel/pexels-photo.png") no-repeat center;
    background-size: cover;
    opacity: 1;
}

.swipe-wrapper {
    margin-left: 70rpx;
    position: relative;
    z-index: 1;
    min-height: 330rpx;
}

.swipe-wrapper swiper {
    height: 330rpx;
}

.swipe-wrapper swiper swiper-item {
    width: 618rpx!important;
    padding: 20rpx 10rpx;
    color: #fff;
    box-sizing: border-box;
    cursor: pointer;
    position: relative;
    border-radius: 8px;
}

.card-wrap {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    padding: 10rpx 30rpx;
    border-radius: 8px;
}

.container {
    padding: 10px;
    background-color: #fff;
    position: relative;
}

.curve {
    width: 140%;
    height: 150rpx;
    position: absolute;
    left: -20%;
    top: -100rpx;
    border: solid 10rpx #fff;
    border-color: #fff transparent transparent transparent;
    background-color: #fff;
    border-radius: 40%/150rpx 150rpx 0 0;
}

.title {
    padding-bottom: 20rpx;
    text-align: center;
    font-size: 32rpx;
    color: #caab65;
    line-height: 36rpx;
    position: relative;
}

.title .dot {
    width: 16rpx;
    height: 16rpx;
    background-image: linear-gradient(-225deg,#f0e2c0 0%,#c4ae7c 100%);
    display: inline-block;
    border-radius: 16rpx;
    margin: 0 10rpx 4rpx 10rpx;
}

.title .dot.small {
    width: 12rpx;
    height: 12rpx;
    border-radius: 12rpx;
    margin-bottom: 6rpx;
}

.title_text {
    padding: 0 10rpx;
}

.interests {
    justify-content: center;
    -webkit-box-pack: center;
}

.interests {
    flex-wrap: wrap;
    padding-bottom: 10rpx;
}

.interests_box {
    width: 25%;
}

.interests_box image {
    width: 80rpx;
    height: 80rpx;
    display: block;
    margin: 10rpx auto;
}

.discount {
    font-size: 28rpx;
    text-align: center;
}

.remark {
    font-size: 22rpx;
    color: #999;
    text-align: center;
}

.level-wrapper {
    background-color: #fff;
    margin-top: 20rpx;
    padding: 40rpx;
    position: relative;
}

.level-table {
    font-size: 26rpx;
}

.table_thead {
    background-color: #f6f1e4;
    padding: 15rpx 0;
}

.table_name {
    padding-left: 30rpx;
    width: 200rpx;
}

.table_value {
    padding-left: 30rpx;
}

.table_tbody {
    background-color: #fcfbf7;
}

.table_tbody .flex-row {
    padding: 15rpx 0;
}

.answer-group {
    background: #fff;
    padding: 40rpx;
}

.answer {
    padding: 20rpx 0 20rpx 0;
}

.answer .answer-title {
    vertical-align: top;
    font-size: 28rpx;
    color: #333;
}

.answer .answer-content {
    font-size: 24rpx;
    color: #999;
    text-align: justify;
    line-height: 36rpx;
    padding: 20rpx 0 0 43rpx;
}

.answer .answer-title .icon {
    background-image: url("data:image/png;base64,");
    display: inline-block;
    width: 28rpx;
    height: 28rpx;
    background-size: 28rpx 28rpx;
    vertical-align: top;
}

.answer .answer-title .answer-title-name {
    padding-left: 20rpx;
    display: inline-block;
    vertical-align: top;
}

.card-level image,.card-code image {
    width: 50rpx;
    height: 50rpx;
}

.card-top {
    font-size: 28rpx;
    font-weight: bold;
    position: relative;
}

.card-middle {
    height: 180rpx;
}

.card-bottom {
    font-size: 24rpx;
}

.card-gift {
    font-size: 24rpx;
}