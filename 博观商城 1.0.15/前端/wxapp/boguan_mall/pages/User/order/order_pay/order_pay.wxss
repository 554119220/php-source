@import "../../../../../__wuBaseWxss__/18.wxss";

.boguan {
    padding-bottom: 160rpx;
}

.order_header {
    background: #fff;
}

.order_address,.user_ziti {
    padding: 30rpx 30rpx;
    background-repeat: repeat-x;
    background-size: 104rpx;
    background-position: bottom left;
    background-image: url('data:image/png;base64,');
}

.order_address navigator {
    width: 100%;
}

.not_address {
    border-bottom: 1rpx solid #ededed;
}

.not_address navigator {
    padding: 30rpx 30rpx;
    text-align: center;
}

.map {
    padding: 0 20rpx 0 0;
}

.map image {
    width: 30rpx;
    height: 28rpx;
}

.order_icon {
    padding-left: 20rpx;
}

.order_icon image {
    width: 12rpx;
    height: 22rpx;
}

.address {
    color: #666;
    margin-top: 10rpx;
    padding-left: 50rpx;
    font-size: 22rpx;
}

.order_concent {
    border-top: 1rpx solid #ededed;
    border-bottom: 1rpx solid #ededed;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    background: #fff;
    padding: 20rpx 0;
}

.orderProducts {
    padding: 10rpx 30rpx;
    background: #fafafa;
    margin-bottom: 10rpx;
}

.orderProducts:last-child {
    margin-bottom: 0rpx;
}

.orderProduct_pic {
    width: 150rpx;
    height: 150rpx;
}

.orderProduct_pic image {
    width: 100%;
    height: 100%;
}

.orderProduct_tit {
    padding: 0 20rpx;
    font-size: 24rpx;
    line-height: 40rpx;
}

.orderProduct_tit .name {
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    white-space: normal;
    word-break: break-all;
    text-overflow: ellipsis;
}

.orderProduct_tit .spec {
    line-height: 35rpx;
    margin-top: 10rpx;
}

.orderProduct_tit .spec text {
    color: #8e8e8e;
    font-size: 24rpx;
}

.orderProduct_tit .spec text:last-child {
    display: none;
}

.orderProduct_tit .spec text:last-child {
    margin-right: 0rpx;
}

.orderProduct_info {
    width: 200rpx;
    text-align: right;
    line-height: 40rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    font-size: 24rpx;
}

.orderProduct_info .o_price {
    color: #c13b38;
    font-size: 24rpx;
}

.orderProduct_info .price {
    font-size: 24rpx;
}

.orderProduct_info .freight {
    color: #969696;
    font-size: 22rpx;
}

.orderProduct_info .number {
    font-size: 24rpx;
}

.all_price {
    padding: 10rpx 30rpx;
    font-size: 26rpx;
}

.all_price .flex-grow-0 {
    font-size: 24rpx;
}

.xjbox {
    text-align: right;
}

.xj {
    color: #000;
    font-weight: bold;
}

.xjprice {
    color: #c13b38;
    font-weight: bold;
    font-size: 35rpx;
}

.order_caozuo {
    width: 100%;
    text-align: right;
    border-top: 1rpx solid #ededed;
    background: #fff;
    line-height: 79rpx;
    font-size: 30rpx;
    position: fixed;
    bottom: 0;
    left: 0;
}

.order_caozuo .price {
    color: #c13b38;
}

.confirmOrder {
    background: #c13b38;
    display: inline-block;
    color: #fff;
    padding: 0 30rpx;
    margin-left: 30rpx;
    border-radius: 10rpx;
}

.order_caozuo .flex-grow-1 {
    padding: 20rpx 30rpx;
}

.mask,.notSupport_mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    z-index: 1;
}

.vip_pop {
    position: fixed;
    top: calc(50% - 250rpx);
    left: calc(50% - 300rpx);
    width: 600rpx;
    height: 400rpx;
    background: #fff;
    border-radius: 10rpx;
    z-index: 10;
    box-sizing: border-box;
    padding: 30rpx 30rpx;
}

.vip_title {
    text-align: center;
    border-bottom: 1rpx solid #ededed;
    padding: 10rpx;
}

.vip_title image {
    width: 60rpx;
    height: 60rpx;
}

.vip_content {
    padding: 20rpx 20rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #ededed;
    font-size: 26rpx;
}

.vip_content image {
    width: 50rpx;
    height: 50rpx;
}

.vip_content input {
    height: 50rpx;
    line-height: 50rpx;
    margin-left: 20rpx;
}

.wechat_iphone {
    background: #ff7d55;
    color: #fff;
    display: block;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    border-radius: 8rpx;
    padding: 5rpx 10rpx;
    font-size: 26rpx;
}

.wechat_iphone::after {
    border: none;
}

.protocol image {
    width: 40rpx;
    height: 40rpx;
}

.protocol {
    padding: 20rpx 20rpx;
    font-size: 26rpx;
}

.protocol .tips {
    padding: 0 10rpx;
    color: #999;
}

.protocol .vip_xueyi {
    color: #ff7d55;
}

.submit {
    background: #ff7d55;
    width: 150rpx;
    text-align: center;
    color: #fff;
    margin: 0 auto;
    padding: 12rpx 0;
    border-radius: 10rpx;
    font-size: 26rpx;
    margin-top: 20rpx;
}

.iphonetips {
    font-size: 26rpx;
}

.close_btn {
    position: absolute;
    top: 10rpx;
    right: 10rpx;
    padding: 10rpx;
}

.close_btn image {
    width: 50rpx;
    height: 50rpx;
}

.store_info {
    background: #fff;
    padding: 20rpx 20rpx 0rpx 20rpx;
}

.shop-logo image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
}

.shop-sm image {
    width: 12rpx;
    height: 22rpx;
}

.coupon,.integral {
    background: #fff;
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #ededed;
}

.select_icon {
    width: 12rpx;
    height: 22rpx;
    margin-left: 20rpx;
}

.coupon_num text {
    color: #c13b38;
}

.coupon_content .coupon_price {
