.express {
    padding: 30rpx;
    background: #fff;
    border-bottom: 1px solid #ededed;
    margin-bottom: 20rpx;
}

.goods-pic {
    width: 130rpx;
    height: 130rpx;
    padding-right: 30rpx;
}

.goods-pic image {
    width: 100%;
    height: 100%;
}

.order-info {
    font-size: 28rpx;
    color: #999999;
}

.order-info view {
    margin-bottom: 10rpx;
}

.order-info .wx {
    color: #000;
}

.order-info .qs {
    color: #06bf04;
}

.copy {
    font-size: 24rpx;
    border: 1rpx solid #ededed;
    padding: 5rpx 10rpx;
    border-radius: 5rpx;
}

.express-content {
    background: #fff;
    padding: 20rpx 30rpx;
}

.express-jd {
    background: #fff;
    padding-left: 20rpx;
    font-size: 26rpx;
    color: #999999;
}

.express-jd.active {
    color: #06bf04;
}

.express-jd .kd-jd {
    border-bottom: 1px solid #ededed;
    padding: 20rpx 0;
}

.express-jd .kd-jd .time {
    margin-top: 20rpx;
}

.jd-ico {
    width: 40rpx;
    position: relative;
    border-left: 1px solid #ededed;
}

.ico-dot {
    width: 20rpx;
    height: 20rpx;
    background: #ededed;
    border-radius: 50%;
    position: absolute;
    top: 30%;
    left: -10rpx;
}

.express-jd.active .ico-dot {
    width: 40rpx;
    height: 40rpx;
    display: block;
    left: -20rpx;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,);
}