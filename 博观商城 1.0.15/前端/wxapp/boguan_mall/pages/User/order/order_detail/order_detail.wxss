.order_status {
    padding: 35rpx 35rpx 20rpx 35rpx;
    background: #f8f9ff;
    text-align: center;
    color: #ae0e11;
}

.orderTimeEnd {
    padding: 0 35rpx;
    background: #fff;
    font-size: 26rpx;
    color: #999;
    text-align: center;
}

.orderTimeEnd text {
    color: #c13b38;
}

.order_address {
    padding: 30rpx;
    background: #fff;
    font-size: 26rpx;
    margin-bottom: 20rpx;
}

.address_ico image {
    width: 30rpx;
    height: 38rpx;
}

.address_ico {
    padding-right: 30rpx;
    margin-top: 66rpx;
}

.address_info {
    margin-top: 30rpx;
}

.orderProducts {
    padding: 20rpx 20rpx;
    background: #f9f9f9;
}

.orderProducts navigator {
    width: 100%;
}

.orderProduct_pic {
    width: 158rpx;
    height: 158rpx;
}

.orderProduct_pic image {
    width: 100%;
    height: 100%;
}

.orderProduct_tit {
    padding: 0 20rpx;
    font-size: 26rpx;
    line-height: 40rpx;
}

.orderProduct_tit .name {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.orderProduct_tit .spec text {
    display: inline-block;
    font-size: 24rpx;
    color: #525252;
}

.orderProduct_tit .spec text:last-child {
    display: none;
}

.orderProduct_info {
    width: 150rpx;
    text-align: right;
    line-height: 40rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.orderProduct_info .freight {
    color: #969696;
    font-size: 22rpx;
}

.orderProduct_info .number {
    font-size: 24rpx;
}

.refund {
    border: 1px solid #666666;
    color: #333;
    padding: 3rpx 10rpx;
    font-size: 22rpx;
    border-radius: 6rpx;
    margin-top: 10rpx;
    text-align: center;
}

.all_price {
    padding: 30rpx;
    background: #fff;
    border-bottom: 1rpx solid #ededed;
}

.all_price .flex-grow-0 {
    color: #999999;
}

.can_pay {
    padding: 30rpx;
    background: #fff;
}

.can_price {
    color: #c13b38;
}

.order_info {
    margin-top: 20rpx;
    background: #fff;
}

.order_tit {
    padding: 30rpx;
    border-bottom: 1rpx solid #ededed;
}

.order_on_time {
    padding: 30rpx;
}

.order_on_time {
    color: #999999;
}

.copy {
    color: #666666;
    border: 1px solid #666666;
    border-radius: 5rpx;
    padding: 5rpx 15rpx;
    font-size: 24rpx;
}

.contact {
    padding: 30rpx;
    margin-top: 20rpx;
    background: #fff;
}

.contact button {
    background: transparent;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
}

.contact button::after {
    border: none;
}

.contact_btn {
    color: #333333;
    border: 1px solid #333333;
    border-radius: 5rpx;
}

.contact_time {
    text-align: right;
    color: #999999;
}

.express {
    padding: 30rpx;
    background: #fff;
    font-size: 26rpx;
    margin-bottom: 20rpx;
}

.kd-ico image {
    width: 60rpx;
    height: 60rpx;
}

.kd-number {
    padding: 0 20rpx;
    color: #06bf04;
    margin-bottom: 10rpx;
    font-size: 26rpx;
}

.kd-jd {
    padding: 0 20rpx;
    font-size: 26rpx;
}

.kd-img {
    width: 12rpx;
    height: 22rpx;
}

.express_time {
    margin-top: 10rpx;
}

.store_info {
    background: #fff;
    padding: 20rpx 20rpx 0rpx 20rpx;
}

.shop-logo image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
}

.shop-sm image {
    width: 12rpx;
    height: 22rpx;
}

.ziti {
    background: #fff;
    font-size: 26rpx;
    margin-bottom: 20rpx;
}

.ziti .label {
    padding: 30rpx 30rpx;
    border-bottom: 1rpx solid #ededed;
}

.ziti .label:last-child {
    border-bottom: 0rpx solid #ededed;
}

.label_tit {
    width: 150rpx;
}

.label_address {
    word-break: break-all;
}

.label_address image {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
}

.label_address,.label_qrcode {
    text-align: right;
    color: #666;
}

.ziti_address {
    display: flex;
    flex-direction: row-reverse;
    width: 100%;
    text-align: right;
}

.label_qrcode image {
    width: 44rpx;
    height: 44rpx;
}

.contact_iphone {
    padding: 20rpx 0;
}

.contact_tihuo {
    border: 1rpx solid #ccc;
    width: 199rpx;
    padding: 15rpx 20rpx;
    border-radius: 10rpx;
    margin: 0 auto;
    text-align: center;
}

.contact_iphone image {
    width: 34rpx;
    height: 39rpx;
}

.navigation image {
    width: 39rpx;
    height: 39rpx;
}

.contact_iphone text {
    margin-left: 20rpx;
}

.store_ziti {
    text-align: right;
}

.store_ziti view {
    font-size: 28rpx;
}