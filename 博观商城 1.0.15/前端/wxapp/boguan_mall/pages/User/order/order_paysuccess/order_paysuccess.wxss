.pay_result {
    padding: 30rpx 30rpx 50rpx 30rpx;
    border-top: 1rpx solid #ededed;
    border-bottom: 1rpx dashed #ededed;
    text-align: center;
    background: #fff;
}

.result {
    color: #0e9f07;
    font-size: 40rpx;
}

.result_tips {
    color: #999;
    font-size: 28rpx;
    margin: 10rpx 0;
}

.result_tips text {
    color: #f00;
}

.result_btn {
    margin-top: 30rpx;
}

.look_order text,.to_index text {
    border: 1rpx solid #999;
    display: inline-block;
    width: 150rpx;
    text-align: center;
    line-height: 60rpx;
    border-radius: 8rpx;
    margin: 0 30rpx;
}

.look_order text {
    float: right;
}

.to_index text {
    float: left;
}

.buy_get {
    background: #fff;
}

.get_title {
    text-align: center;
    padding: 30rpx 0;
    color: #999;
}

.address_info {
    padding: 30rpx;
    margin-top: 20rpx;
    background: #fff;
}

.address_info .user,.address_info .pay_tit {
    width: 150rpx;
    display: inline-block;
}

.address_info .price {
    color: #0e9f07;
}