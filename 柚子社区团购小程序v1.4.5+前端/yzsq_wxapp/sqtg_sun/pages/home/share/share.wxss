.share {
    display: flex;
    justify-content: space-between;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    padding: 0 30rpx;
}

.share input {
    height: 60rpx;
    width: 560rpx;
    background-color: #f8f8f8;
    border-top: 1px solid #eaeaea;
    border-radius: 5px;
    padding-left: 20rpx;
}

.share .share-btn {
    width: 100rpx;
    height: 60rpx;
    background-color: #fe433f;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 5px;
}

.share .share-btn image {
    width: 30rpx;
    height: 30rpx;
}

.share-bg {
    height: 30rpx;
    background-color: #f8f8f8;
}

.tel-hd {
    height: 88rpx;
    border-bottom: 1rpx solid #eee;
    padding: 0 20rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-around;
}

.tel-hd .hd-content {
    height: 88rpx;
    position: relative;
}

.tel-hd text {
    font-size: 30rpx;
    line-height: 88rpx;
}

.tel-hd .current {
    position: absolute;
    bottom: 0;
    left: 0;
    border-bottom: 2rpx solid #f6ad41;
    width: 100%;
}

.tel-hd .current1 {
    color: #fe322e;
}

.tel-bd .tel-bd-list {
    padding: 20rpx;
    border-bottom: 1px solid #e9e9e9;
    display: flex;
}

.list-img {
    margin-right: 30rpx;
}

.list-img image {
    width: 200rpx;
    height: 200rpx;
}

.tel-bd-list .list-content {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.tel-bd-list .list-content .content-t {
    font-size: 28rpx;
    color: #0b0b0b;
}

.tel-bd-list .list-content .content-b {
    color: #fe332e;
}

.noMore {
    width: 100%;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    color: #999;
}