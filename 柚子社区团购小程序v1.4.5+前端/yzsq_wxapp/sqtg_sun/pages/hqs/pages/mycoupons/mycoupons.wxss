page {
    background: #f2f2f2;
}

.swiper-tab {
    position: relative;
}

.shouldFiexd {
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    z-index: 10;
    animation: headFix 0.3s 1;
}

@keyframes headFix {
    0% {
        top: -100rpx;
    }

    100% {
        top: 0;
    }
}

.session {
    padding-bottom: 30rpx;
}

.couponsItem {
    display: flex;
    margin: 0 30rpx;
    margin-top: 30rpx;
}

.couponsLef {
    width: 200rpx;
    height: 150rpx;
    background: #f87d6d;
    text-align: center;
    color: #fff;
}

.couponsLefPass {
    background-color: #666;
}

.couponsRig {
    flex: 1;
    background: #ffdbd6;
    padding-left: 30rpx;
    overflow: hidden;
}

.couponsRigPass {
    background-color: #ccc;
}

.couponsSale {
    margin-top: 30rpx;
    font-size: 42rpx;
}

.couponsSale text,.couponsSatisfy,.couponsTime,.couponsLimit {
    font-size: 24rpx;
}

.couponsTit {
    margin-top: 20rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.couponsTime,.couponsLimit {
    margin-top: 6rpx;
}

.couponsLimit {
    width: 90%;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}