.foot-paddings {
    bottom: 240rpx;
}

.foot-paddingss {
    padding-bottom: 100rpx;
}

page,.hei100 {
    height: 100%;
}

.buyFastLef {
    flex: 1;
}

.buyFastRig {
    flex: 4;
}

.bfScrolllef {
    width: 100%;
    height: 100%;
    background: #f3f3f3;
    padding-bottom: 130rpx;
}

.bfScrollrig {
    width: 100%;
    height: 100%;
}

.bfScrolllef view {
    display: flex;
    align-items: center;
    height: 100rpx;
    justify-content: center;
}

.bfScrolllef .active {
    background: #fafafa;
}

.bfgoodsList {
    display: flex;
    align-items: center;
    padding: 20rpx;
    border-bottom: 1rpx solid #eee;
}

.bflLefimg {
    width: 160rpx;
    height: 160rpx;
    margin-right: 20rpx;
}

.bflRig {
    flex: 1;
}

.choiceNum {
    display: flex;
    align-items: center;
}

.choiceNum image {
    width: 50rpx;
    height: 50rpx;
}

.bfgoodsTit {
    color: #999;
    padding: 20rpx 0 0 20rpx;
}

.cnInput {
    height: 50rpx;
    line-height: 50rpx;
    padding: 0 12rpx;
    margin: 0 5rpx;
}

.bfchoiceRule {
    position: relative;
    height: 40rpx;
    line-height: 40rpx;
    background: #f87d6d;
    padding: 0 20rpx;
    color: #fff;
    border-radius: 20rpx;
    font-size: 24rpx;
}

.bfchoiceRule text {
    position: absolute;
    right: -5rpx;
    top: -5rpx;
    background: #fff;
    height: 20rpx;
    line-height: 20rpx;
    border-radius: 20rpx;
    font-size: 16rpx;
    font-weight: 600;
    padding: 0 10rpx;
    color: #f87d6d;
    font-family: Arial,Helvetica,sans-serif;
    border: 1rpx solid #f87d6d;
}

.popUp {
    position: fixed;
    width: 90%;
    left: 5%;
    top: 50%;
    transform: translateY(-50%);
    z-index: 11;
    background: #fff;
    border-radius: 20rpx;
}

.popUptit {
    width: 70%;
    height: 36rpx;
    margin-left: 15%;
    margin-top: 50rpx;
    text-align: center;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.specificationList {
    float: left;
    margin: 0 0 10px 30rpx;
    padding: 6rpx 20rpx;
    background: #f7f7f7;
    color: #999;
}

.specificationTit {
    margin: 30rpx 0 0 30rpx;
    color: #999;
    margin-bottom: 14rpx;
}

.popFooter {
    clear: both;
    padding: 0 30rpx;
    display: flex;
    height: 100rpx;
    align-items: center;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
    border-top: 1rpx solid #eee;
}

.popfMoney {
    flex: 1;
    font-size: 30rpx;
    color: #f87d6d;
}

.popfJioncar {
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 20rpx;
    background: #f87d6d;
    color: #fff;
    border-radius: 10rpx;
}

.popUpclose {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 30rpx;
    height: 30rpx;
}

.specificationList.active {
    color: #f87d6d;
}

.bfCarimg {
    position: relative;
    z-index: 9;
}

.bfCarimg image {
    position: relative;
    margin: -20rpx 0 0 20rpx;
    width: 110rpx;
    height: 110rpx;
    z-index: 9;
}

.bfCartxt {
    flex: 1;
    box-sizing: border-box;
    padding-left: 30rpx;
    font-family: Arial,Helvetica,sans-serif;
}

.bfCartxt text:nth-child(1) {
    color: #fff;
    font-size: 30rpx;
    font-weight: 600;
}

.cf {
    color: #fff;
    font-size: 30rpx;
    font-weight: 600;
}

.bfFooter {
    position: fixed;
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
    left: 0;
    bottom: 0;
    background: #b0cafd;
    box-shadow: 0 -1rpx 5rpx rgba(0,0,0,0.1);
}

.bfCarbtn {
    width: 180rpx;
    height: 100rpx;
}

.bfbtnGray,.bfbtnRed {
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 30rpx;
}

.bfbtnGray {
    background: #666;
    color: #bbb;
}

.bfbtnRed {
    background: #8db4ff;
    color: #fff;
}

.bfCarnum {
    position: absolute;
    right: 6rpx;
    top: -15rpx;
    height: 30rpx;
    line-height: 30rpx;
    padding: 0 20rpx;
    background: #f87d6d;
    color: #fff;
    font-size: 20rpx;
    border-radius: 15rpx;
    z-index: 9;
}

.bfHadchoice {
    position: fixed;
    bottom: 100rpx;
    left: 0;
    width: 100%;
    background: #fff;
}

.bfHadchoiceTit {
    height: 60rpx;
    border-bottom: 1rpx solid #eee;
}

.bfHadchoiceTit image {
    width: 24rpx;
    height: 24rpx;
    margin-right: 6rpx;
}

.bfHadchoiceTit view:nth-child(1) {
    height: 32rpx;
    line-height: 32rpx;
    margin-left: 30rpx;
    border-left: 5rpx solid #f87d6d;
    padding-left: 10rpx;
    flex: 1;
}

.bfHadchoiceTit view:nth-child(2) {
    margin-right: 30rpx;
}

.bfHadchoiceCont {
    padding: 10rpx 30rpx;
    height: 80rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #eee;
}

.bfcTit,.goodsSpec {
    width: 360rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.bfHadchoiceCont image {
    width: 40rpx;
    height: 40rpx;
}

.bfcMoney {
    font-size: 30rpx;
    color: #f87d6d;
    flex: 1;
    margin-left: 10rpx;
}

.posImg {
    position: absolute;
    z-index: 1;
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
}

.bftitWrap {
    width: 360rpx;
}

.popAddcar {
    background: #f87d6d;
    padding: 10rpx 20rpx;
    border-radius: 6rpx;
    color: #fff;
}

.goodsSpec {
    font-size: 22rpx;
    color: #999;
}

.clb {
    clear: both;
}