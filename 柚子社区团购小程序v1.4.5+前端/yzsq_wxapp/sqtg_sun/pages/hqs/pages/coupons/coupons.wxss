page {
    background: #f2f2f2;
}

.couponsList {
    margin: 20rpx 20rpx 0 20rpx;
    background: #fff;
    border-radius: 15rpx;
}

.couponsList:last-child {
    margin-bottom: 20rpx;
}

.cpyelBg {
    height: 50rpx;
    padding-left: 30rpx;
    line-height: 50rpx;
    font-size: 20rpx;
    color: #fff;
    background: #f87d6d;
    border-top-left-radius: 15rpx;
    border-top-right-radius: 15rpx;
    border-bottom: 1rpx solid #eee;
}

.couponsCont {
    margin-top: 35rpx;
    display: flex;
    align-items: center;
    padding: 0 30rpx 30rpx 30rpx;
    border-bottom: 1rpx solid #eee;
}

.couponsBtn {
    height: 56rpx;
    line-height: 56rpx;
    padding: 0 20rpx;
    color: #fff;
    background: #f87d6d;
    border-radius: 10rpx;
}

.bigMoney {
    margin: 0 30rpx 0 10rpx;
}

.couponsBot {
    height: 60rpx;
    width: 680rpx;
    line-height: 60rpx;
    text-align: right;
    padding-right: 30rpx;
    font-size: 24rpx;
    color: #999;
    overflow: hidden;
    text-overflow: ellipsis;
}

.couponsCont .litMoney {
    margin-top: 15rpx;
}