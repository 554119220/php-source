page {
    background: #f2f2f2;
}

.swiper-tab {
    position: relative;
}

.shouldFiexd {
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    z-index: 10;
    animation: headFix 0.3s 1;
}

@keyframes headFix {
    0% {
        top: -100rpx;
    }

    100% {
        top: 0;
    }
}

.session {
    padding-bottom: 30rpx;
}

.couponsItem {
    display: flex;
    margin: 0 30rpx;
    margin-top: 30rpx;
}

.couponsLef {
    width: 200rpx;
    height: 150rpx;
    background: #e9472c;
    text-align: center;
    color: #fff;
}

.couponsLefPass {
    background-color: #666;
}

.couponsRig {
    flex: 1;
    background: #ffdbd6;
    padding-left: 30rpx;
    overflow: hidden;
}

.couponsRigPass {
    background-color: #ccc;
}

.couponsSale {
    margin-top: 30rpx;
    font-size: 42rpx;
}

.couponsSale text,.couponsSatisfy,.couponsTime,.couponsLimit {
    font-size: 24rpx;
}

.couponsTit {
    margin-top: 20rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.couponsTime,.couponsLimit {
    margin-top: 6rpx;
}

.couponsLimit {
    width: 90%;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.swiper-tab {
    width: 100%;
    display: flex;
    height: 89rpx;
    line-height: 89rpx;
    text-align: center;
    border-bottom: 1px solid #eee;
    justify-content: space-around;
    background: #fff;
    font-size: 30rpx;
    color: #666;
    box-sizing: border-box;
}

.swiper-tab .active {
    height: 86rpx;
    line-height: 86rpx;
    border-bottom: 5rpx solid #fe5624;
    color: #e9472c;
    box-sizing: border-box;
}

.ciList {
    margin-top: 30rpx;
    display: flex;
    height: 188rpx;
    border-radius: 8rpx;
    background: #fff;
    margin: 30rpx 30rpx 0;
}

.cilLef {
    width: 209rpx;
    height: 188rpx;
    box-sizing: border-box;
    color: #fff;
    text-align: center;
    position: relative;
}

.cilLef view:nth-of-type(1) {
    font-size: 48rpx;
    position: absolute;
    top: 45rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 100%;
}

.cilLef view:nth-of-type(2) {
    font-size: 18rpx;
    position: absolute;
    bottom: 55rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 100%;
}

.cilLef image {
    width: 209rpx;
    height: 188rpx;
    display: block;
}

.cilRig {
    flex: 1;
    padding-left: 20rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: center;
    position: relative;
}

.cilRig view:nth-child(1) {
    font-size: 30rpx;
    font-weight: 700;
    color: #333;
}

.cilRig view:nth-child(2) {
    margin-top: 37rpx;
    font-size: 20rpx;
    color: #999;
}

.cilRig view:nth-child(3) {
    margin-top: 10rpx;
    font-size: 20rpx;
    color: #999;
}

.ciBtn {
    margin: 0 auto;
    margin-top: 40rpx;
    width: 300rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #fff;
    background: #e9472c;
    border-radius: 40rpx;
    font-size: 30rpx;
    text-align: center;
}

.ciList::before {
    position: absolute;
    content: '';
    left: 135rpx;
    top: -15rpx;
    width: 30rpx;
    height: 30rpx;
    background: #f2f2f2;
    border-radius: 50%;
}

.ciList::after {
    position: absolute;
    content: '';
    left: 135rpx;
    bottom: -15rpx;
    width: 30rpx;
    height: 30rpx;
    background: #f2f2f2;
    border-radius: 50%;
}

.cilRig image {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 0;
    width: 137rpx;
    height: 157rpx;
}

.outdate {
    background: #999;
}