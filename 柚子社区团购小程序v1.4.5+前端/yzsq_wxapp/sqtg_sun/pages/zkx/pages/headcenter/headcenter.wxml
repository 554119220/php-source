<view class="apply" wx:if="{{!is_leader}}">
    <image mode="widthFix" src="{{apply_bgm?img_root+apply_bgm:'../../resource/images/leaderapply.png'}}"></image>
    <view bindtap="toApply" class="btn-apply">申请加入</view>
</view>
<view class="{{padding?'foot-padding':''}}" wx:if="{{show&&is_leader}}">
    <view class="header {{setting.personcenter_color_b?'customer-header':''}}" style="--cusbg:{{setting.personcenter_color_b}};">
        <image bindtap="myScan" class="scan" src="../../../../../zhy/resource/images/scan.png"></image>
        <image class="headerLef" mode="aspectFill" src="{{myleader.pic}}"></image>
        <view class="haderRig">
            <view class="name">{{setting.leader_replace}}：{{myleader.name}}</view>
            <view class="trange">{{myleader.community}}</view>
            <view class="address">{{myleader.address}}</view>
        </view>
    </view>
    <view class="myOrder">
        <view class="myOrderTit">
            <view>团员订单</view>
            <navigator url="/sqtg_sun/pages/zkx/pages/memberorder/memberorder?id=2&myleaderid={{myleader.id}}">
        查看全部
      </navigator>
            <image src="../../../../../zhy/resource/images/arrowRig.png"></image>
        </view>
        <view class="myOrderCont">
            <navigator class="mocList1" url="/sqtg_sun/pages/zkx/pages/memberorder/memberorder?id=2&myleaderid={{myleader.id}}">
                <image src="../../../../../zhy/resource/images/my/order2.png"></image>
                <view>待配送</view>
                <text class="orderCount" wx:if="{{myleader.order_count.state2}}">{{myleader.order_count.state2}}</text>
            </navigator>
            <navigator class="mocList1" url="/sqtg_sun/pages/zkx/pages/memberorder/memberorder?id=3&myleaderid={{myleader.id}}">
                <image src="../../../../../zhy/resource/images/my/order3.png"></image>
                <view>配送中</view>
                <text class="orderCount" wx:if="{{myleader.order_count.state3}}">{{myleader.order_count.state3}}</text>
            </navigator>
            <navigator class="mocList1" url="/sqtg_sun/pages/zkx/pages/memberorder/memberorder?id=4&myleaderid={{myleader.id}}">
                <image src="../../../../../zhy/resource/images/my/order4.png"></image>
                <view>待自提</view>
                <text class="orderCount" wx:if="{{myleader.order_count.state4}}">{{myleader.order_count.state4}}</text>
            </navigator>
            <navigator class="mocList1" url="/sqtg_sun/pages/zkx/pages/memberorder/memberorder?id=5&myleaderid={{myleader.id}}">
                <image src="../../../../../zhy/resource/images/my/order5.png"></image>
                <view>已完成</view>
            </navigator>
        </view>
    </view>
    <view class="myOrderItemCysc" wx:if="{{spell}}">
        <navigator url="../../../plugin/spell/memberorder/memberorder?myleaderid={{myleader.id}}">
            <image src="/zhy/resource/images/nav1.png"></image>
            <view>拼团</view>
        </navigator>
        <navigator url="../../../plugin/spell/commissiondetails/commissiondetails?myleaderid={{myleader.id}}">
            <image src="/zhy/resource/images/nav11.png"></image>
            <view>佣金明细</view>
        </navigator>
    </view>
    <view class="headTable">
        <view class="htList">
            <view class="htTit">今日订单</view>
            <view class="htNum">{{myleader.today_count}}</view>
        </view>
        <view class="htList">
            <view class="htTit">昨日订单</view>
            <view class="htNum">{{myleader.yesterday_count}}</view>
        </view>
        <view class="htList">
            <view class="htTit">累计订单</view>
            <view class="htNum">{{myleader.total_count}}</view>
        </view>
    </view>
    <view class="myFinance" wx:if="{{usersearch||users&&users.length}}">
        <view class="myOrderTit">待核销用户</view>
        <view style="max-height: 300rpx; overflow: auto;">
            <view bindtap="chooseUser" data-userid="{{item.user_id}}" style="display:flex;padding:10rpx 20rpx;" wx:for="{{users}}">
                <image src="{{item.img}}" style="width:100rpx;height:100rpx;border-radius:100rpx;margin-left:30rpx;"></image>
                <view style="padding:20rpx 40rpx;flex:1;">
                    <view>{{item.user_name}}</view>
                    <view>{{item.tel}}</view>
                </view>
            </view>
        </view>
        <view class="mfBot">
            <view class="mfbLef">
                <input bindconfirm="userSearch" bindinput="userKeyInput" confirmType="search" placeholder="输入用户名称/电话" style="width:100%"></input>
            </view>
            <view bindtap="userSearch" class="myfContRig withdrawal">搜索</view>
        </view>
    </view>
    <view class="myFinance" wx:if="{{userSearchSending||sending_users&&sending_users.length}}">
        <view class="myOrderTit">配送中用户</view>
        <view style="max-height: 300rpx; overflow: auto;">
            <view bindtap="chooseSendingUser" data-userid="{{item.user_id}}" style="display:flex;padding:10rpx 20rpx;" wx:for="{{sending_users}}" wx:key="index">
                <image src="{{item.img}}" style="width:100rpx;height:100rpx;border-radius:100rpx;margin-left:30rpx;"></image>
                <view style="padding:20rpx 40rpx;flex:1;">
                    <view>{{item.user_name}}</view>
                    <view>{{item.tel}}</view>
                </view>
            </view>
        </view>
        <view class="mfBot">
            <view class="mfbLef">
                <input bindconfirm="userSearch" bindinput="userKeyInput" confirmType="search" placeholder="输入用户名称/电话" style="width:100%"></input>
            </view>
            <view bindtap="userSearchSending" class="myfContRig withdrawal">搜索</view>
        </view>
    </view>
    <view class="myFinance" wx:if="{{myleader.is_leader}}">
        <view class="myOrderTit">我的财务</view>
        <view class="myfCont flexAlignc">
            <view class="myfContLef flexAlignc">
                <view class="myflItem">
                    <view>{{myleader.today_sum||0}}</view>
                    <view>今日收入</view>
                </view>
                <view class="myflItem">
                    <view>{{myleader.yesterday_sum||0}}</view>
                    <view>昨日收入</view>
                </view>
                <view class="myflItem" wx:if="{{storeinfo.id!=0}}">
                    <view>{{myleader.total_sum||0}}</view>
                    <view>累计收入</view>
                </view>
            </view>
        </view>
        <view class="mfBot">
            <view class="mfbLef">
        可提现金额：
        <view>￥{{myleader.money||0}}</view>
            </view>
            <navigator class="myfContRig" url="/sqtg_sun/pages/zkx/pages/withdrawal/withdrawal" wx:if="{{myleader.withdraw_switch&&myleader.withdraw_switch!=0&&myleader.money>0}}">
                <view class="withdrawal">提现</view>
            </navigator>
        </view>
    </view>
    <view class="myFinance" wx:if="{{myleader.is_leader}}">
        <view class="myOrderTit">商品</view>
        <view class="goodsCont">
            <navigator class="addGoodslef" url="/sqtg_sun/pages/zkx/pages/headgoods/headgoods?state=1&leader_id={{myleader.id}}">
                <view class="v1">{{myleader.goodses.count}}</view>
                <view>普通在售</view>
            </navigator>
            <view class="goodsLine"></view>
            <view class="addGoodsrig">
                <navigator class="addGoods" url="/sqtg_sun/pages/zkx/pages/headgoods/headgoods?state=0&leader_id={{myleader.id}}" wx:if="{{myleader.is_leader&&myleader.choosegoods_switch==1}}">
          添加
        </navigator>
                <navigator class="addGoods" url="/sqtg_sun/pages/zkx/pages/headgoods/headgoods?state=0&leader_id={{myleader.id}}" wx:if="{{myleader.choosegoods_switch==0}}">
          查看
        </navigator>
            </view>
            <navigator class="addGoodslef" url="/sqtg_sun/pages/zkx/pages/headpingoods/headpingoods?state=1&leader_id={{myleader.id}}" wx:if="{{spell}}">
                <view class="v1">{{myleader.goodses.pinCount}}</view>
                <view>拼团在售</view>
            </navigator>
            <view class="goodsLine"></view>
            <view class="addGoodsrig" wx:if="{{spell}}">
                <navigator class="addGoods" url="/sqtg_sun/pages/zkx/pages/headpingoods/headpingoods?state=0&leader_id={{myleader.id}}" wx:if="{{myleader.is_leader&&myleader.choosegoods_switch==1}}">
          添加
        </navigator>
                <navigator class="addGoods" url="/sqtg_sun/pages/zkx/pages/headpingoods/headpingoods?state=0&leader_id={{myleader.id}}" wx:if="{{myleader.choosegoods_switch==0}}">
          查看
        </navigator>
            </view>
        </view>
    </view>
    <view class="verificationMember" wx:if="{{myleader.is_leader}}">
        <view class="vmLef">
      核销人员：
      <text>{{myleader.users.count}}</text>
      人
    </view>
        <navigator class="vmRig" url="/sqtg_sun/pages/zkx/pages/verificationmember/verificationmember?leader_id={{myleader.id}}">
      添加核销人员
    </navigator>
    </view>
</view>
<import src="../../../../../zhy/template/temloading/temloading.wxml"></import>
<template is="temloading" data wx:if="{{!show}}"></template>
<com-footer bind:padding="getPadding" cartCount="{{cartCount}}" currURL="/sqtg_sun/pages/zkx/pages/headapplication/headapplication" wx:if="{{showFoot}}"></com-footer>
