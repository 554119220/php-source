page {
    background-color: #f0eff4;
}

.more-list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 20rpx;
    border-top: 1px solid #f0eff4;
    background-color: #fff;
}

.more-list .more-list-l {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 290rpx;
    color: #151516;
    font-size: 30rpx;
}

.more-list .more-list-l image {
    width: 80rpx;
    height: 80rpx;
}

.more-list .more-list-r {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 400rpx;
}

.more-list .more-list-r .join-num-txt1 {
    font-size: 26rpx;
    color: #151516;
}

.more-list .more-list-r .join-num-txt2 {
    font-size: 26rpx;
    color: #9a9a9a;
    margin-top: 5rpx;
}

.more-list .more-list-r .join-num text {
    color: #ff4414;
}

.more-list .more-list-r .join-group {
    width: 150rpx;
    height: 60rpx;
    background: -webkit-linear-gradient(left,#ff934a,#fc6770);
    border-radius: 5px;
    color: #fff;
    text-align: center;
    line-height: 60rpx;
}

.ellipsis1 {
    width: 190rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}