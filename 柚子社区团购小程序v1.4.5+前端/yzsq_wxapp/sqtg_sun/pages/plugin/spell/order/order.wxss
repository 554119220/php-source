page {
    background: #f2f2f2;
    padding-bottom: 200rpx;
}

.qdd {
    display: flex;
    align-items: center;
    background: #fff;
    padding: 20rpx 30rpx;
    border-top: 1rpx solid #eee;
}

.qddLefImg {
    width: 130rpx;
    height: 130rpx;
    margin-right: 30rpx;
}

.qddRigCont {
    display: flex;
    justify-content: space-between;
}

.qddRig {
    flex: 1;
}

.qddRigTit,.qddRigCont>view:nth-child(1) {
    font-size: 30rpx;
}

.qddRigCont>view:nth-child(1) {
    color: #f87d6d;
}

.qddChoice {
    background: #fff;
    border-top: 20rpx solid #f2f2f2;
}

.courier,.invite {
    height: 100rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #eee;
    justify-content: space-between;
    font-size: 30rpx;
    padding: 0 30rpx;
}

.qddCoupons,.qddMessage,.inviteSty {
    border-top: 20rpx solid #f2f2f2;
    height: 100rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    padding: 0 30rpx;
    align-items: center;
    font-size: 30rpx;
}

.qddMessage input,.inviteSty input {
    text-align: right;
    color: #999;
}

.inviteSty {
    margin-top: 0;
}

.inviteSty:first-child {
    border-bottom: 1rpx solid #eee;
}

.qddWin {
    position: fixed;
    z-index: 11;
    left: 50%;
    top: 50%;
    width: 80%;
    height: 280rpx;
    background: #fff;
    margin: -140rpx 0 0 -40%;
    border-radius: 10rpx;
    text-align: center;
    font-size: 30rpx;
}

.qddWin view:nth-child(1) {
    color: #999;
}

.qddWin view:nth-child(2) {
    color: #f87d6d;
}

.qddWin view:nth-child(1),.qddWin view:nth-child(2) {
    margin-top: 70rpx;
}

.qddFooter,.qddFooter2 {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    background: #fff;
    display: flex;
}

.qddFooter>view:nth-child(2),.qddfRig,.qddDctive,.qddBefore,.qddAfter {
    width: 260rpx;
    background: #999;
    color: #fff;
    font-size: 34rpx;
    line-height: 100rpx;
    text-align: center;
}

.qddFooter>view:nth-child(1),.qddfLef {
    flex: 1;
    color: #f87d6d;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
}

.qddFooter>view:nth-child(1)>text:nth-child(2),.qddfLef>text:nth-child(2) {
    font-size: 34rpx;
}

.qddDctive,.qddAfter {
    background: #f87d6d;
}

.qddBefore,.qddAfter {
    border-radius: 0;
}

.express {
    display: flex;
    flex-direction: column;
}

.radio {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding: 0 30rpx;
    line-height: 100rpx;
}

.express label:first-child {
    border-bottom: 1px solid #eee;
}

.choose-address {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    height: 90rpx;
    line-height: 90rpx;
}

.choose-address view {
    font-size: 28rpx;
    color: #333;
}

.choose-address image {
    width: 10rpx;
    height: 20rpx;
}

.btn {
    background: #f87d6d;
}

.fee {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 90rpx;
    line-height: 90rpx;
    width: 690rpx;
    padding: 0 30rpx;
    position: fixed;
    bottom: 100rpx;
    background: #fff;
    border-bottom: 1px solid #eee;
}

.fee view:last-child {
    color: #f87d6d;
}

.shopflex {
    display: flex;
    flex-direction: row;
}

.shoplist {
    flex: 1;
}

.shoplist-in {
    display: flex;
    flex-direction: row;
    width: 100%;
}

.picker {
    flex: 1;
}

.shoplist image {
    margin-top: 6rpx;
    width: 14rpx;
    height: 28rpx;
}

.address {
    width: 690rpx;
}

.button-hover {
    background: #fff;
}

.webChat,.meetPay {
    height: 120rpx;
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    border-top: 1rpx #eee solid;
}

.payIcon {
    width: 60rpx;
    height: 60rpx;
    margin-right: 30rpx;
}

.choicepayTit {
    line-height: 80rpx;
    text-align: center;
    font-size: 30rpx;
}

.iconRight {
    width: 34rpx;
    height: 24rpx;
}

.webChat view:nth-child(2),.meetPay view:nth-child(2) {
    flex: 1;
}

.pay-footer {
    border-top: 1rpx solid #ececec;
    padding: 15rpx 20rpx;
}

.pay-footer>button {
    width: 710rpx;
    height: 90rpx;
    line-height: 90rpx;
    background-color: #f87d6d;
    font-size: 34rpx;
    color: #fff;
    border-radius: 16rpx;
}

.icon25 {
    width: 20rpx;
    height: 25rpx;
}

.dishesLine {
    width: 100%;
    height: 10rpx;
    margin-top: 30rpx;
}

.tab-hd {
    height: 108rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    background-color: #fff;
}

.tab-hd .tab-hd-title {
    width: 50%;
    color: #666;
    font-size: 28rpx;
    height: 108rpx;
    line-height: 108rpx;
    display: block;
    border-right: 1rpx solid #DCDCDC;
    text-align: center;
}

.maAddress {
    display: flex;
    align-items: center;
    margin-top: 10rpx;
}

.maCont {
    padding: 0 30rpx;
}

.maLef {
    margin-right: 15rpx;
}

.maName {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.isMaskPhone {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
}

#isPhone {
    position: fixed;
    width: 480rpx;
    height: 250rpx;
    background: #fff;
    top: 50%;
    left: 50%;
    margin-left: -265rpx;
    margin-top: -185rpx;
    z-index: 1001;
    padding: 50rpx 50rpx;
    border-radius: 12rpx;
}

.isLoginBtn {
    display: flex;
    justify-content: space-around;
}

.isLoginBtn button,.isLoginBtn navigator,.isLoginBtn view {
    background: #fff;
    height: 68rpx;
    line-height: 68rpx;
    font-size: 28rpx;
    width: 260rpx;
    text-align: center;
    color: #4bad37;
    border: 1rpx solid #eee;
    border-radius: 12rpx;
}

.isLoginBtn navigator,.isLoginBtn view {
    height: 65rpx;
    line-height: 65rpx;
    margin-right: 20rpx;
    background: #f2f2f2;
    color: #999;
}

.loginTitle {
    font-size: 34rpx;
}

.shopImg {
    height: 80rpx;
    display: flex;
    align-items: center;
    padding: 20rpx 0rpx;
    justify-content: center;
    font-size: 28rpx;
}

.shopImg>image {
    margin: 0 14rpx 0 0;
    width: 68rpx;
    height: 68rpx;
    border-radius: 50%;
}