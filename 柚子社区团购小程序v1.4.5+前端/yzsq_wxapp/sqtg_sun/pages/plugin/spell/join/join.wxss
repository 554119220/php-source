.count-down {
    width: 750rpx;
    height: 200rpx;
    border-bottom: 20rpx solid #efeff4;
}

.count-time {
    height: 120rpx;
    line-height: 60rpx;
    font-weight: 600;
    font-size: 50rpx;
    text-align: center;
}

.count-tilte {
    height: 100rpx;
    line-height: 140rpx;
    text-align: center;
}

.qmItem {
    background: #fff;
    padding: 30rpx 30rpx 20rpx 30rpx;
    border-bottom: 1rpx solid #eee;
}

.qiLef image {
    border-radius: 0;
    width: 220rpx;
    height: 180rpx;
    display: block;
    margin-right: 20rpx;
}

.qiCont {
    padding: 0;
    display: flex;
    align-items: center;
    position: relative;
}

.qiMoney .txtRed {
    position: absolute;
    right: 20rpx;
    bottom: 10rpx;
}

.qiMoney view:nth-child(1) {
    color: #999;
}

.imgWrap image {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
}

.imgWrap>view {
    width: 70rpx;
    position: relative;
    margin: 0 10rpx;
}

.imgWrap {
    padding-top: 20rpx;
    display: flex;
    height: 110rpx;
    align-items: center;
    justify-content: center;
}

.qmIcon {
    position: absolute;
    top: -8rpx;
    right: -2rpx;
    width: 40rpx;
    height: 20rpx;
    line-height: 20rpx;
    text-align: center;
    color: #fff;
    background: #ffb825;
    font-size: 15rpx;
}

.imgWrap>view .qmIcon {
    display: none;
}

.imgWrap>view:first-child .qmIcon {
    display: block;
}

.timeRemain {
    text-align: center;
    line-height: 50rpx;
}

.timeRemain text {
    color: #f87d6d;
    margin: 0 3rpx;
}

.btnGrayBig {
    color: rgba(0,0,0,0.6);
    background-color: #dedede;
}

.deleteLine {
    margin: 10rpx 0;
}