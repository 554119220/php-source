page {
    padding: 90rpx 0 100rpx 0;
}

.orderList {
    border-bottom: 20rpx solid #f2f2f2;
}

.goodsImg {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}

.wtteamItemTit,.wtteamItem {
    margin: 0 30rpx;
}

.wtteamItemTit {
    display: flex;
    align-items: center;
    height: 90rpx;
    border-bottom: 1rpx solid #eee;
}

.wtteamItemTit view {
    flex: 1;
    text-align: center;
}

.wtteamItem {
    padding: 15rpx 0;
    display: table;
    border-bottom: 1rpx solid #eee;
}

.wtteamItem view,.wtteamItem navigator {
    display: table-cell;
    vertical-align: middle;
    width: 25%;
    text-align: center;
    overflow: hidden;
}

.swiper-tab {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    display: flex;
    height: 89rpx;
    border-bottom: 1px solid #eee;
    justify-content: space-around;
}

.tab-item {
    line-height: 89rpx;
    font-size: 30rpx;
}

.on,.swiper-tab .active {
    height: 90rpx;
    line-height: 89rpx;
    border-bottom: 2rpx solid #e9472c;
    color: #e9472c;
}

.noMore {
    padding: 30rpx 0;
    text-align: center;
    color: #999;
}