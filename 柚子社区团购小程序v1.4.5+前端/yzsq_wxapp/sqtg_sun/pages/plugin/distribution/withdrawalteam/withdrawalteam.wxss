page {
    padding: 0 0 100rpx 0;
}

.orderList {
    border-bottom: 0;
}

.wtteamItem {
    padding: 20rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #eee;
}

.wtteamItemlef {
    width: 100rpx;
    height: 100rpx;
    margin-right: 20rpx;
    border-radius: 50%;
}

.wtteamItemrig {
    flex: 1;
}

.userImg {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    margin-right: 40rpx;
}

.myMessage {
    height: 220rpx;
    display: flex;
    align-items: center;
    box-sizing: border-box;
    padding: 0 30rpx;
    color: #fff;
}

.flex1 {
    flex: 1;
}

.lowerLevel {
    display: flex;
}

.userName {
    font-size: 32rpx;
}

.jionTime,.lowerLevel {
    margin-top: 10rpx;
    font-size: 24rpx;
}

.lowerLevel view {
    margin-right: 20rpx;
}

.gayTit {
    padding-left: 30rpx;
    line-height: 88rpx;
    border-bottom: 1rpx solid #eee;
    font-size: 28rpx;
}

.noMore {
    padding: 30rpx 0;
    text-align: center;
    color: #999;
}