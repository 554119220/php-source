page {
    background: #f2f2f2;
}

.dcHeader {
    margin: 20rpx;
    box-sizing: border-box;
    background: #e9472c;
    padding: 30rpx;
    color: #fff;
    border-radius: 15rpx;
}

.dchTop {
    padding-bottom: 30rpx;
    border-bottom: 1rpx solid #fff;
}

.dchTop,.dchBot {
    display: flex;
    align-items: center;
}

.dchTop image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    margin-right: 40rpx;
}

.dchtRig view:nth-child(1) {
    font-size: 32rpx;
}

.dchBot {
    padding-top: 10rpx;
}

.dchbLef {
    flex: 1;
}

.dchbLef view {
    margin-top: 20rpx;
}

.dchbRig {
    height: 56rpx;
    line-height: 56rpx;
    padding: 0 20rpx;
    border: 1rpx solid #fff;
    border-radius: 10rpx;
}

.dcWallet {
    margin: 20rpx;
    background: #fff;
    display: flex;
    box-sizing: border-box;
    padding: 40rpx 30rpx;
    font-size: 30rpx;
    border-radius: 20rpx;
}

.dcWalletLef,.dcWalletRig {
    flex: 1;
    text-align: center;
}

.dcWalletLef {
    border-right: 1rpx solid #eee;
}

.dcWalletLef view:nth-child(1) {
    color: #e9472c;
}

.dcWalletRig view:nth-child(1) {
    color: #8db4ff;
}

.dcWalletLef view:nth-child(2),.dcWalletRig view:nth-child(2) {
    margin-top: 10rpx;
}

.dcItem {
    margin-top: 20rpx;
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
}

.dcList {
    width: 223rpx;
    height: 220rpx;
    background: #fff;
    margin: 0 0 20rpx 20rpx;
    text-align: center;
    border-radius: 15rpx;
}

.dcList image {
    display: block;
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    margin: 0 auto;
    margin-top: 30rpx;
}

.dcList>view:nth-child(2) {
    margin-top: 15rpx;
    font-size: 30rpx;
}

.dcList>view:nth-child(3) {
    margin-top: 15rpx;
    color: #e9472c;
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 200px;
}

swiper {
    height: 374rpx;
}

.slide-image {
    width: 750rpx;
