}

.notice {
    width: 100%;
    height: 72rpx;
    background-color: #ffffff;
    line-height: 72rpx;
    font-size: 30rpx;
    font-weight: 700;
    color: #222;
    display: flex;
    align-items: center;
}

.notice image {
