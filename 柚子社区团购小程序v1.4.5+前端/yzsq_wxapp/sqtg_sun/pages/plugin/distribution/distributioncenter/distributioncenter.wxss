}

.coupon {
    margin-top: 10rpx;
    background: #fff;
}

.coupon_title {
    padding: 30rpx 0rpx 50rpx;
    font-size: 32rpx;
    color: #222;
    display: flex;
    justify-content: center;
    align-content: center;
}

.coupon_title:before,.coupon_title:after {
    content: " ";
    display: inline-block;
    width: 62rpx;
    height: 1px;
    background: #cbcbcb;
    margin-right: 24rpx;
    margin-top: 22rpx;
}

.coupon_title:after {
    margin-right: 0rpx;
    margin-left: 24rpx;
}

.coupon-list {
    padding: 0rpx 20rpx 30rpx;
    display: flex;
}

.coupon-item {
    width: 710rpx;
    height: 200rpx;
    border-radius: 10rpx;
    background-color: rgb(255,98,128);
    box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(4,0,0,0.11);
    display: flex;
    flex-direction: row;
}

.coupon-cont {
    width: 510rpx;
    padding-left: 20rpx;
    color: #fff;
    font-size: 22rpx;
}

.coupon-cont view {
    margin-top: 10rpx;
}

.coupon-cont .coupon-money {
    font-size: 58rpx;
    font-weight: 700;
}

.coupon-money text {
    font-size: 30rpx;
    margin-right: 4rpx;
}

.coupon-status {
    width: 200rpx;
    border-left: 1px dashed #fff;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    line-height: 200rpx;
}

.coupon-money {
    margin-top: 30rpx;
}

.hair {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
}

.hair-list {
    flex: 0 0 50%;
    height: 366rpx;
}

.hair-item {
    height: 366rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-right: 1rpx solid #cbcbcb;
    border-bottom: 1rpx solid #cbcbcb;
    font-size: 30rpx;
    color: #222;
}

.hair-list:nth-child(1) .hair-item,.hair-list:nth-child(2) .hair-item {
    border-top: 1rpx solid #cbcbcb;
}

.hair-item image {
    height: 112rpx;
    width: 112rpx;
    margin-top: 20rpx;
    border-radius: 50%;
}

.hair-name {
    margin: 30rpx 0rpx 0rpx;
}

.hair-name text {
    display: inline-block;
