@import "../components/wxParse/wxParse.wxss";

.wrap {
    margin: 30rpx;
    padding: 30rpx;
    background: #fff;
    border-radius: 10rpx;
}

.title {
    margin-bottom: 20rpx;
    font-size: 30rpx;
    color: #383838;
}

.time {
    padding-bottom: 30rpx;
    border-bottom: 1px solid #f2f2f2;
    font-size: 26rpx;
    color: #969696;
}

.content {
    padding: 20rpx 0 0;
    font-size: 26rpx;
    color: #969696;
    overflow: hidden;
}