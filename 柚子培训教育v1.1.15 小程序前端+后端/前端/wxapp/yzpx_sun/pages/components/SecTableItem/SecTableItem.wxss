.list {
    position: relative;
    display: flex;
    flex-direction: column;
    padding: 30rpx;
    background: #fff;
    border-radius: 10rpx;
}

.list>view {
    position: relative;
    padding-left: 60rpx;
}

.list-top {
    position: relative;
    line-height: 56rpx;
    font-size: 30rpx;
    color: #383838;
}

.list-center,.list-bottom {
    line-height: 46rpx;
    font-size: 26rpx;
    color: #969696;
}

.list-bottom {
    width: 420rpx;
}

.yu {
    position: absolute;
    left: 0;
    top: 10rpx;
    width: 40rpx;
    height: 40rpx;
}

.btn {
    position: absolute;
    bottom: 30rpx;
    right: 30rpx;
    width: 150rpx;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 30rpx;
    color: #fff;
    background: #de5656;
}

.ellips1 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}