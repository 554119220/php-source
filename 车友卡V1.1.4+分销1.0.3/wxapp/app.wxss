page {
    background-color: #f8f8f8;
    height: 100%;
    font-family: "Microsoft YaHei","Helvetica Neue","Hiragino Sans GB","\9ED1\4F53",Arial;
    font-size: 0rpx;
}

.container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    box-sizing: border-box;
    font-size: 28rpx;
    line-height: 26rpx;
    color: #222;
}

.rightIcon {
    width: 12rpx;
    height: 18rpx;
}

.tab {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    background: #fff;
    box-shadow: 0 -3rpx 3rpx 0rpx rgba(180,179,179,0.118);
    height: 92rpx;
    padding-bottom: 4rpx;
    z-index: 99999;
}

.tabnav {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.tablist {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #8c8c8c;
}

.tabOn {
    position: relative;
    top: -42rpx;
}

.tabimg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 8rpx;
    margin-bottom: 0rpx;
}

.tabOn .tabimg {
    width: 89rpx;
    height: 89rpx;
    position: absolute;
    padding: 8rpx;
    border-radius: 50%;
    background: #fff;
    box-shadow: 0 -3rpx 3rpx 0rpx rgba(180,179,179,0.118);
}

.tabOn text {
    position: absolute;
    bottom: -84rpx;
}

.foot {
    height: 200rpx;
}

button::after {
    border: none;
}