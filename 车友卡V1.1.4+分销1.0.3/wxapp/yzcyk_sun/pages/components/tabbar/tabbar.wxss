.tab {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    height: 94rpx;
    background: fff;
    box-shadow: 0 -3rpx 3rpx 0rpx rgba(180,179,179,0.118);
    padding-bottom: 4rpx;
    z-index: 9999;
    background: #fff;
}

.ipx {
    padding-bottom: 72rpx;
}

.tabnav {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.tablist {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #8c8c8c;
}

.tabOn {
    position: relative;
    height: 94rpx;
}

.tabimg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 8rpx;
    margin-bottom: 0rpx;
}

.tabOn .tabimg {
    width: 90rpx;
    height: 90rpx;
    position: absolute;
    top: -40rpx;
    padding: 7rpx;
    border-radius: 50%;
    background: #fff;
    box-shadow: 0 -3rpx 3rpx 0rpx rgba(180,179,179,0.118);
}

.tabOn text {
    position: absolute;
    bottom: 0rpx;
}