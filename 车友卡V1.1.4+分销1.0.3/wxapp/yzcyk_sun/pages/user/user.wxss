.head {
    position: relative;
    height: 346rpx;
    width: 750rpx;
    display: flex;
    justify-content: center;
    color: #222222;
    overflow: hidden;
}

.userbg {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    z-index: -1;
    width: 750rpx;
    height: 346rpx;
}

.uthumb {
    border: 2rpx solid #fff;
}

.uthumb,.uthumb image {
    width: 156rpx;
    height: 156rpx;
    border-radius: 50%;
}

.uthumb {
    margin-bottom: 36rpx;
}

.head-thumb {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 40rpx;
    letter-spacing: 2rpx;
    z-index: 999;
    line-height: 42rpx;
}

.block {
    background: #fff;
}

.block-up {
    padding: 0rpx 30rpx;
    display: flex;
    height: 92rpx;
    justify-content: space-between;
    align-items: center;
}

.rightIcon {
    margin-left: 20rpx;
    width: 16rpx;
    height: 24rpx;
}

.block-up view {
    display: flex;
    align-items: center;
    color: #666;
}

.block-down {
    display: flex;
    justify-content: space-around;
    align-items: center;
    height: 154rpx;
    border-top: 1rpx solid #ececec;
    font-size: 22rpx;
    padding-right: 20rpx;
    margin-left: 20rpx;
}

.block-down view {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-weight: 700;
}

.icn {
    width: 50rpx;
    height: 50rpx;
    margin-bottom: 20rpx;
}

.icon {
    width: 56rpx;
    height: 56rpx;
    margin-right: 34rpx;
}

.toOrder {
    color: #666;
    font-size: 24rpx;
}

.session {
    background: #fff;
    padding-left: 30rpx;
    margin-top: 20rpx;
}

.session .list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 20rpx;
    border-bottom: 1rpx solid #ececec;
    height: 92rpx;
}

.session .list:last-child {
    border-bottom: 1rpx solid #fff;
}

.list view {
    display: flex;
    align-items: center;
}

.concat {
    display: flex;
    align-items: center;
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-right: 0rpx;
    padding-left: 0rpx;
    width: 660rpx;
    background: #fff;
    font-size: 28rpx;
    color: #222;
}

.cardMask {
    position: fixed;
    height: 100%;
    width: 100%;
    top: 0rpx;
    right: 0rpx;
    left: 0rpx;
    bottom: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.cardModel {
    z-index: 1001;
    position: fixed;
    background: #fff;
    top: 100rpx;
    left: 75rpx;
    width: 600rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 12rpx;
}

.title {
    font-size: 34rpx;
    font-weight: 700;
    margin-top: 52rpx;
    line-height: 34rpx;
    margin-bottom: 40rpx;
}

.modelInput>input {
    border: 1rpx solid #dcdcdc;
    width: 520rpx;
    height: 72rpx;
    font-size: 24rpx;
    border-radius: 8rpx;
    padding-left: 20rpx;
}

.modelInput {
    margin-bottom: 20rpx;
    margin-left: 30rpx;
}

.codeInput {
    margin-top: 30rpx;
}

.cardModelFoot {
    display: flex;
    margin-top: 40rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    height: 98rpx;
    line-height: 98rpx;
    border-top: 1rpx solid #ececec;
}

.cardModelFoot>button {
    width: 300rpx;
    background: #fff;
    box-sizing: border-box;
    border-radius: 12rpx;
    font-size: 34rpx;
    color: #3cc51f;
    line-height: 98rpx;
}

.cardModelFoot>button:first-child {
    border-right: 1rpx solid #ececec;
    color: #999;
}

.mask {
    position: fixed;
    height: 100%;
    width: 100%;
    top: 0rpx;
    right: 0rpx;
    left: 0rpx;
    bottom: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.joinIco {
    width: 360rpx;
    height: 180rpx;
    margin-top: 74rpx;
}

.joinCont {
    z-index: 1001;
    position: fixed;
    padding-bottom: 30rpx;
    background: #fff;
    width: 600rpx;
    top: 4%;
    left: 75rpx;
    border-radius: 20rpx;
    overflow: hidden;
}

.show {
    animation: showModel 0.5s 1;
}

@keyframes showModel {
    from {
        transform: scale(0.6);
    }

    to {
        transform: scale(1);
    }
}

.joinHead {
    height: 284rpx;
    width: 720rpx;
    border-radius: 50%;
    background: #f9cf01;
    display: flex;
    justify-content: center;
    margin-left: -60rpx;
    margin-top: -58rpx;
}

.joinBody {
    padding: 0rpx 20rpx;
    width: 560rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.joinTitle {
    padding: 50rpx 0rpx 30rpx;
    font-weight: 700;
    text-align: center;
}

.joinBody>view {
    margin-bottom: 30rpx;
    width: 560rpx;
    background: #f6f5f5;
    border-radius: 10rpx;
}

.joinBody input {
    width: 540rpx;
    height: 72rpx;
    padding-left: 20rpx;
    font-size: 24rpx;
    background: #f6f5f5;
    border-radius: 10rpx;
}

.join-time {
    position: relative;
    display: flex;
    align-items: center;
    margin-bottom: 30rpx;
    width: 560rpx;
    background: #f6f5f5;
    border-radius: 10rpx;
}

.join-time>text {
    margin-left: 20rpx;
}

.join-time>input {
    width: 350rpx;
}

.subMsg {
    padding-left: 35rpx;
    padding-right: 35rpx;
    background: #f9cf01;
    font-size: 30rpx;
    color: #fff;
    height: 68rpx;
    line-height: 68rpx;
    text-align: center;
    border-radius: 10rpx;
    font-weight: 700;
    margin-top: 14rpx;
}

.joinBody>view.pay {
    text-align: center;
    background: #fff;
}

.joinBody>view.joinTitle {
    background: #fff;
}

.pay>text {
    font-size: 34rpx;
    font-weight: 700;
    color: #f9cf01;
}

#styleSecond .session {
    width: 650rpx;
    margin-left: 20rpx;
    background: #fff;
    box-shadow: 0rpx 0rpx 14rpx 0rpx rgba(5,2,2,0.078);
    border-radius: 10rpx;
}

#styleSecond .session1 {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 30rpx 26rpx;
    margin-top: 30rpx;
}

#styleSecond .session1 .lst {
    display: flex;
    align-items: center;
}

#styleSecond .session1 .lst>image {
    height: 88rpx;
    width: 88rpx;
    margin-right: 70rpx;
    border-radius: 50%;
    box-shadow: 1.5rpx 2.598rpx 8rpx 0rpx #f9cf01;
    border: 2rpx solid #fff;
}

#styleSecond .session1 .toFx {
    width: 128rpx;
    height: 56rpx;
    line-height: 56rpx;
    border-radius: 28rpx;
    background: #f9cf01;
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    margin-right: 0px;
}