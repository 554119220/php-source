.personMsg {
    height: 70prx;
    line-height: 70rpx;
    padding-left: 30rpx;
    font-weight: 700;
    color: #999;
}

.session {
    background: #fff;
    padding-left: 20rpx;
}

.list {
    height: 90rpx;
    border-bottom: 1rpx solid #ececec;
    padding-right: 20rpx;
    padding-left: 10rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.list>text {
    white-space: nowrap;
}

.list input {
    text-align: right;
}

.btn {
    margin-top: 140rpx;
}

.btn>button {
    width: 676rpx;
    height: 92rpx;
    line-height: 92rpx;
    background: #f9cf01;
    color: #fff;
    font-size: 34rpx;
    border-radius: 10rpx;
    margin-bottom: 30rpx;
}

.btn>button.cancel {
    background: #f9f9f9;
    color: #222;
    border: 1rpx solid #cbcbcb;
}