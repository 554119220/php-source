.container {
    margin-bottom: 300rpx;
}

.date {
    width: 710rpx;
    margin: 20rpx 20rpx 0rpx 20rpx;
    background: #fff;
    box-sizing: border-box;
    border: 2rpx solid #ff5e5e;
    border-radius: 10rpx;
}

.date-header {
    background: #ff5e5e;
    color: #fefefe;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 32rpx;
    font-weight: 700;
    position: relative;
}

.date-header>view>text {
    color: #ffe826;
}

.record {
    position: absolute;
    right: 20rpx;
    top: 15rpx;
    border: 2rpx solid #fff;
    width: 132rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    font-weight: 500;
    font-size: 26rpx;
    border-radius: 23rpx;
}

.arrowIcn {
    width: 18rpx;
    height: 32rpx;
}

.month-choose {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 90rpx;
}

.wrap {
    padding: 0rpx 40rpx;
    border-top: 0rpx solid #ff5e5e;
    border-radius: 0rpx 0rpx 10rpx 10rpx;
    color: #222;
    font-weight: 500;
}

.row {
    display: flex;
    flex-wrap: wrap;
}

.row>view {
    font-size: 28rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin: 5rpx;
    border-radius: 50%;
    text-align: center;
}

.body>view.current {
    background: #f4e4e2;
}

.body>view.old {
    color: #999;
}

.body>view.signin {
    box-shadow: 0rpx 3rpx 3rpx 0rpx #ff5e5e;
}

.body {
    border-bottom: 1rpx solid #ff5e5e;
}

.punch {
    height: 92rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.punch>view {
    width: 142rpx;
    height: 46rpx;
    border-radius: 23rpx;
    border: 2rpx solid #ff5e5e;
    background: #ffd7d7;
    color: #ff5e5e;
    font-weight: 700;
    font-size: 26rpx;
    text-align: center;
    line-height: 46rpx;
}

.bind {
    position: relative;
}

.bind>view {
    position: absolute;
    top: -26rpx;
}

.bind>view.leftbind {
    left: 70rpx;
}

.bind>view.rightbind {
    right: 70rpx;
}

.icon25 {
    width: 16rpx;
    height: 62rpx;
}

.progress {
    width: 710rpx;
    margin: 10rpx 20rpx 0rpx 20rpx;
    background: #fff;
    box-sizing: border-box;
    border: 2rpx solid #ff5e5e;
    border-radius: 10rpx;
    height: 200rpx;
    padding: 0rpx 30rpx;
}

.giftDay {
    display: flex;
    margin-top: 35rpx;
    position: relative;
    height: 24rpx;
}

.giftDay>view {
    width: 68rpx;
    height: 24rpx;
    color: #ff5e5e;
    font-size: 20rpx;
    border: 1rpx solid #ff5e5e;
    line-height: 24rpx;
    text-align: center;
    border-radius: 12rpx;
    position: absolute;
    margin-left: -34rpx;
    top: 0rpx;
}

.progressBar {
    margin-top: 10rpx;
    width: 650rpx;
    height: 10rpx;
    background: #e6e6e6;
    border-radius: 5rpx;
    border: 1rpx solid #d9d9d9;
    position: relative;
}

.progressBar>view {
    height: 10rpx;
    position: absolute;
    left: 0rpx;
    background: #ff5e5e;
}

.gift {
    display: flex;
    position: relative;
    margin-top: 16rpx;
}

.gift>image {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    margin-left: -25rpx;
}

.session {
    width: 710rpx;
    margin: 10rpx 20rpx 0rpx 20rpx;
    background: #fff;
    box-sizing: border-box;
    border: 2rpx solid #ff5e5e;
    border-radius: 10rpx;
    padding: 0rpx 20rpx;
}

.username {
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-bottom: 1rpx solid #ff5e5e;
}

.target {
    padding-top: 32rpx;
    font-weight: 700;
    font-size: 30rpx;
    width: 670rpx;
    text-align: center;
    line-height: 46rpx;
}

.target>view {
    display: inline-block;
    text-align: left;
    word-break: break-all;
}

.createTime {
    text-align: right;
    font-size: 24rpx;
    color: #666;
    padding: 10rpx 0rpx 20rpx;
}

.mask {
    position: fixed;
    height: 100%;
    width: 100%;
    top: 0rpx;
    right: 0rpx;
    left: 0rpx;
    bottom: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.giftModel {
    position: fixed;
    height: 418rpx;
    width: 670rpx;
    top: 212rpx;
    left: 40rpx;
    background: #fff;
    border-radius: 16rpx;
    display: flex;
    align-items: center;
    flex-direction: column;
    z-index: 1001;
}

.modelTitle>image {
    width: 306rpx;
    height: 64rpx;
    margin-top: -20rpx;
}

.welare {
    font-size: 32rpx;
    padding: 24rpx 0rpx 30rpx;
}

.coupon {
    width: 630rpx;
    height: 192rpx;
    box-sizing: border-box;
    border: 1rpx solid #ff5e5e;
    background: #ff5e5e;
    border-radius: 16rpx;
    position: relative;
}

.bgCards {
    width: 570rpx;
    height: 152rpx;
    position: absolute;
    top: 20rpx;
    left: 20rpx;
}

.modelLeft {
    height: 152rpx;
    width: 200rpx;
    position: absolute;
    top: 20rpx;
    left: 20rpx;
    font-size: 30rpx;
    color: #ff5e5e;
    font-weight: 700;
    line-height: 152rpx;
    text-align: center;
    box-sizing: border-box;
    border-right: 1rpx dotted #ff5e5e;
}

.modelRight {
    width: 350rpx;
    padding: 0rpx 10rpx;
    position: absolute;
    top: 20rpx;
    left: 220rpx;
    font-size: 26rpx;
}

.cardName {
    text-align: center;
    margin-top: 24rpx;
}

.cardTime {
    color: #666;
    font-size: 24rpx;
    line-height: 36rpx;
    margin-top: 14rpx;
}

.cardsTip {
    color: #666;
    margin-top: 52rpx;
    font-size: 20rpx;
}

.close {
    position: relative;
}

.closeIcon {
    width: 52rpx;
    height: 52rpx;
    position: absolute;
    top: 62rpx;
    margin-left: -28rpx;
}