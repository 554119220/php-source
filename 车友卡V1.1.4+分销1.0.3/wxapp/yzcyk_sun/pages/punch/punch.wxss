.list {
    margin: 50rpx 30rpx 0rpx 30rpx;
    background: #fff;
    padding: 10rpx 30rpx;
    width: 630rpx;
    height: 160rpx;
    display: flex;
    align-items: center;
    box-shadow: 0rpx 4rpx 6rpx 0rpx rgba(3,3,3,0.063);
    border-radius: 10rpx;
}

.micon {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
}

.listRight {
    margin-left: 34rpx;
    width: 436rpx;
    height: 160rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin-top: 10rpx;
}

.mission {
    display: flex;
    justify-content: space-between;
    font-size: 22rpx;
    color: #999;
}

.target {
    color: #222;
    font-size: 30rpx;
}

.progress {
    margin: 20rpx 0rpx;
}

.progress>progress {
    border-radius: 5rpx;
}

.create {
    display: inline-block;
    width: 142rpx;
    height: 46rpx;
    border-radius: 23rpx;
    color: #ff5e5e;
    background: #ffd7d7;
    border: 1rpx solid #ff5e5e;
    font-size: 22rpx;
    text-align: center;
    line-height: 46rpx;
}

.punch {
    background: #ff5e5e;
    color: #fff;
}

.add {
    display: flex;
    background: #fff;
    margin-top: 10rpx;
    padding: 0rpx 20rpx;
    width: 710rpx;
    height: 120rpx;
    justify-content: space-between;
    align-items: center;
}

.addLeft {
    line-height: 48rpx;
    color: #ff5e5e;
}

.addLeft>view:last-child {
    font-size: 24rpx;
    color: #999;
}

.addIcon {
    width: 62rpx;
    height: 62rpx;
}