.header {
    background: #fff;
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 32rpx;
    border-top: 1rpx solid #ececec;
    border-bottom: 1rpx solid #ececec;
    margin-top: 10rpx;
}

.session {
    background: #fff;
    padding-left: 30rpx;
}

.blockTitle {
    padding: 24rpx 0rpx;
    font-size: 30rpx;
}

textarea {
    height: 120rpx;
    width: 650rpx;
    border-radius: 10rpx;
    padding: 20rpx;
    background: #ececec;
    margin-bottom: 20rpx;
    font-size: 26rpx;
    color: #666;
}

.list {
    display: flex;
    justify-content: space-between;
    height: 90rpx;
    border-top: 1rpx solid #ececec;
    align-items: center;
    width: 700rpx;
    padding-right: 20rpx;
}

.list>text {
    white-space: nowrap;
}

.rightIcn {
    width: 12rpx;
    height: 21rpx;
    margin-left: 20rpx;
}

.babyIcon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 22rpx;
}

.baby>view {
    display: flex;
    align-items: center;
    width: 680rpx;
    font-size: 26rpx;
    color: #666;
}

.listRight {
    display: flex;
    align-items: center;
}

.listRight>input {
    text-align: right;
}

.remark {
    padding: 20rpx 20rpx;
    color: #999;
    line-height: 42rpx;
    font-size: 26rpx;
}

.btn {
    width: 750rpx;
    height: 128rpx;
    border-top: 1rpx solid #d2d2d2;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    z-index: 11;
}

.btn>button {
    height: 98rpx;
    width: 710rpx;
    color: #fff;
    font-size: 34rpx;
    background: #ff5e5e;
    line-height: 98rpx;
    border-radius: 12rpx;
}