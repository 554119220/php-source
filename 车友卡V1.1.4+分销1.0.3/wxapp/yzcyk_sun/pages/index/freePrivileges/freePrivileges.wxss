.swiper {
    width: 750rpx;
    height: 440rpx;
}

.swiper image {
    width: 750rpx;
    height: 440rpx;
}

.block {
    background: #fff;
    padding: 24rpx 20rpx 34rpx;
}

.title {
    font-size: 30rpx;
    font-weight: 700;
    letter-spacing: 1rpx;
    line-height: 42rpx;
}

.num {
    margin-top: 24rpx;
    display: flex;
    justify-content: space-between;
    color: #ff5e5e;
}

.num>text:last-child {
    color: #999;
    font-size: 24rpx;
}

.list {
    background: #fff;
    margin-top: 30rpx;
    padding-left: 20rpx;
}

.list>view {
    font-size: 30rpx;
    width: 710rpx;
    height: 82rpx;
    line-height: 82rpx;
    border-bottom: 1rpx solid #ececec;
    padding-right: 20rpx;
}

.list>view:last-child {
    border-bottom: 0rpx solid #ececec;
}

.list>view>text:last-child {
    color: #999;
    margin-left: 24rpx;
}

.wrap {
    background: #fff;
    padding-left: 30rpx;
    margin-top: 10rpx;
}

.shopname {
    font-size: 30rpx;
    height: 92rpx;
    line-height: 92rpx;
    font-weight: 700;
    text-align: center;
}

.shopList {
    padding-right: 50rpx;
    display: flex;
    justify-content: space-between;
    width: 670rpx;
    border-top: 1rpx solid #ececec;
    height: 82rpx;
    align-items: center;
}

.icon {
    width: 26rpx;
    height: 34rpx;
    margin-right: 20rpx;
}

.shopList>view {
    display: flex;
    align-items: center;
}

.icna {
    width: 42rpx;
    height: 42rpx;
}

.rules {
    background: #FFF;
    margin-top: 10rpx;
}

.notes {
    border-top: 1rpx solid #ececec;
    padding: 24rpx 20rpx 30rpx;
}

.session {
    margin-top: 10rpx;
    background: #fff;
    padding-bottom: 30rpx;
}

.sessionTit {
    font-size: 32rpx;
    color: #ff5e5e;
    font-weight: 700;
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
}

.detail {
    line-height: 36rpx;
}

.detail>image {
    width: 100%;
}

.icn {
    width: 60rpx;
    height: 60rpx;
}

.fixNav {
    display: flex;
    height: 98rpx;
    align-items: center;
    position: fixed;
    bottom: 0rpx;
    background: #fff;
    box-shadow: 0rpx -3rpx 3rpx 0rpx rgba(180,180,180,.21);
    z-index: 99;
}

.fixLeft {
    width: 278rpx;
    display: flex;
    justify-content: space-around;
    font-size: 20rpx;
    color: #8c8c8c;
}

.nav-list {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    line-height: 28rpx;
    background-color: #fff;
    font-size: 20rpx;
    color: #8c8c8c;
    padding-left: 0rpx;
    padding-right: 0rpx;
    border-radius: 0rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
}

.nav-list::after {
    border: none;
}

.fixRight {
    display: flex;
    width: 452rpx;
    font-weight: 700;
    padding-right: 20rpx;
}

.fixRight>view {
    width: 452rpx;
    height: 82rpx;
    background: #999;
    color: #FFF;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    line-height: 38rpx;
    border-radius: 10rpx;
    font-size: 32rpx;
}

.fixRight>view.toJoin {
    background: #ff5e5e;
}