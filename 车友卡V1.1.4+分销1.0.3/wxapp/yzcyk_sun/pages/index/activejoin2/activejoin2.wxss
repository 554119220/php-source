.goods {
    background: #FFF;
    padding: 20rpx;
    height: 180rpx;
    display: flex;
    margin-top: 10rpx;
    margin-bottom: 20rpx;
}

.gimg {
    width: 180rpx;
    height: 180rpx;
    border-radius: 12rpx;
}

.gdet {
    display: flex;
    margin-left: 20rpx;
    width: 510rpx;
    height: 180rpx;
    flex-direction: column;
    justify-content: space-between;
    font-weight: 700;
    font-size: 26rpx;
    color: #f9cf01;
}

.gdet>view:first-child {
    font-size: 30rpx;
    color: #222;
    line-height: 42rpx;
    margin-top: 6rpx;
}

.rightIcn {
    width: 12rpx;
    height: 21rpx;
}

.babyIcon {
    width: 32rpx;
    height: 21rpx;
    margin-right: 22rpx;
}

.baby {
    background: #fff;
    margin-top: 10rpx;
    display: flex;
    align-items: center;
    width: 710rpx;
    padding: 0rpx 20rpx;
    justify-content: space-between;
    height: 90rpx;
}

.baby>view {
    display: flex;
    align-items: center;
    width: 680rpx;
    font-size: 26rpx;
    color: #666;
}

.baby input {
    color: #222;
}

.personMsg {
    height: 70rpx;
    padding-left: 20rpx;
    font-weight: 700;
    color: #999;
    line-height: 70rpx;
}

.session {
    background: #fff;
    padding-left: 20rpx;
}

.list {
    height: 90rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #ececec;
}

.list>text {
    display: inline-block;
    width: 148rpx;
    color: #666;
}

.list:last-child {
    border-bottom: 0rpx solid #fff;
}

.fixNav {
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    height: 98rpx;
    width: 750rpx;
    background: #fff;
    display: flex;
    justify-content: flex-end;
    box-shadow: 0rpx -3rpx 3rpx 0rpx rgba(180,180,180,0.078);
    line-height: 98rpx;
}

.fixNav>view {
    margin-right: 50rpx;
}

.fixNav>view>text {
    color: #f9cf01;
    font-weight: 700;
}

.fixNav>button {
    margin-left: 0rpx;
    margin-right: 0rpx;
    width: 220rpx;
    height: 98rpx;
    background: #f9cf01;
    color: #fff;
    font-size: 32rpx;
    font-weight: 700;
    line-height: 98rpx;
    border-radius: 0rpx;
}

._list {
    display: flex;
    justify-content: space-between;
    padding-right: 20rpx;
    height: 90rpx;
    align-items: center;
    color: #666;
}

._list:first-child {
    border-bottom: 1rpx solid #ececec;
}

.icn {
    width: 29rpx;
    height: 3rpx;
}

.addIcn {
    height: 30rpx;
}

.listNum {
    display: flex;
}

.listNum>view {
    width: 60rpx;
    height: 50rpx;
    border: 1rpx solid #cbcbcb;
    display: flex;
    align-items: center;
    justify-content: center;
}

.listNum>input {
    width: 92rpx;
    height: 50rpx;
    margin: 0rpx 10rpx;
    border: 1rpx solid #cbcbcb;
    text-align: center;
    color: #222;
    font-weight: 700;
}

.totalPrice {
    font-size: 30rpx;
    font-weight: 700;
    color: #222;
}

.cardIcon {
    width: 32rpx;
    height: 27rpx;
    margin-right: 22rpx;
}

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.drawer_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

.drawer_title {
    font-size: 32rpx;
    color: #222;
    padding: 24rpx 20rpx;
    display: flex;
    justify-content: space-between;
}

.cancel {
    width: 30rpx;
    height: 30rpx;
}

.drawer_content {
    height: 500rpx;
    overflow-y: scroll;
}

.cards {
    margin: 20rpx 20rpx 30rpx;
    font-size: 24rpx;
    box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(109,205,253,0.11);
    background: #ffeded;
}

.card-prices {
    padding: 6rpx 20rpx 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: cneter;
    border: 1rpx solid #f9cf01;
    color: #f9cf01;
    border-bottom: 1rpx dashed #f9cf01;
    border-radius: 12rpx 12rpx 0rpx 0rpx;
    position: relative;
}

.card-prices:before,.card-prices:after {
    content: " ";
    display: block;
    width: 16rpx;
    height: 16rpx;
    background: #f8f8f8;
    border-radius: 50%;
    position: absolute;
    bottom: -12rpx;
    border: 1rpx solid #f9cf01;
    z-index: 99;
    border-left: 1rpx solid transparent;
    border-bottom: 1rpx solid transparent;
}

.card-prices:before {
    left: -10rpx;
    transform: rotate(45deg);
}

.card-prices:after {
    transform: rotate(-135deg);
    right: -10rpx;
}

.card-time {
    border: 1rpx solid #f9cf01;
    border-top: 1rpx dashed #f9cf01;
    border-radius: 0rpx 0rpx 12rpx 12rpx;
    height: 118rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding-left: 20rpx;
    line-height: 40rpx;
    color: #666;
}

.price {
    display: flex;
    line-height: 92rpx;
    margin-top: 16rpx;
}

.price text:nth-child(1) {
    font-size: 42rpx;
    margin-top: 16rpx;
}

.price text:nth-child(2) {
    font-size: 92rpx;
}

.right {
    width: 12rpx;
    height: 22rpx;
    margin-left: 22rpx;
}

.use {
    margin-top: 54rpx;
    font-weight: 700;
}

.coupon {
    justify-content: space-between;
}

.coupon>view {
    width: 300rpx;
}

.coupon>view:last-child {
    display: flex;
    justify-content: flex-end;
    font-size: 28rpx;
    color: #222;
}

.rightIcn {
    margin-left: 16rpx;
}