.header {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 160rpx;
    background: #fff;
}

.header>view {
    display: flex;
    align-items: center;
    font-size: 34rpx;
}

.header>view>icon {
    margin-right: 20rpx;
    margin-top: 14rpx;
}

.addrIcon {
    height: 46rpx;
    width: 33rpx;
    margin-right: 20rpx;
}

.session {
    width: 710rpx;
    margin: 20rpx;
    border-radius: 16rpx;
}

.shopname {
    width: 708rpx;
    box-sizing: border-box;
    background: #fff;
    height: 92rpx;
    text-align: center;
    line-height: 92rpx;
    font-size: 34rpx;
    border-radius: 16rpx 16rpx 0 0;
    border: 1rpx solid #f9cf01;
    border-bottom: 0rpx solid #f9cf01;
    font-weight: 700;
}

.wrap {
    position: relative;
    width: 708rpx;
    box-sizing: border-box;
    border-radius: 0 0 16rpx 16rpx;
    border: 1rpx solid #f9cf01;
    border-top: 1rpx dashed #f9cf01;
}

.discount {
    background: #fff;
    text-align: center;
}

.dsicountMoney {
    font-size: 42rpx;
    color: #f9cf01;
    line-height: 42rpx;
    padding: 34rpx 0rpx 44rpx;
}

.wrap:before,.wrap:after {
    content: " ";
    display: block;
    width: 16rpx;
    height: 16rpx;
    background: #f8f8f8;
    border-radius: 50%;
    position: absolute;
    top: -8rpx;
    border: 1px solid #f9cf01;
    z-index: 99;
    border-left: 1px solid transparent;
    border-bottom: 1px solid transparent;
}

.wrap:before {
    left: -10rpx;
    transform: rotate(45deg);
}

.wrap:after {
    transform: rotate(-135deg);
    right: -8rpx;
}

.time {
    font-size: 24rpx;
    color: #666;
}

.time text {
    margin-left: 24rpx;
}

.use {
    background: #fff;
    border-radius: 0 0 16rpx 16rpx;
}

.useTitle {
    text-align: center;
    padding: 50rpx 0rpx 28rpx;
    font-weight: 700;
    color: #666;
}

.explain {
    line-height: 39rpx;
    color: #666;
    padding: 0rpx 20rpx 50rpx;
}

.fbot {
    height: 128rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    background: #fff;
    z-index: 10;
    border-top: 1rpx solid #cdcdcd;
    width: 750rpx;
}

.fbot>button {
    width: 710rpx;
    height: 98rpx;
    background: #f9cf01;
    font-size: 34rpx;
    color: #fff;
    line-height: 98rpx;
}

.btnGroup>button {
    width: 340rpx;
}

.btnGroup>button:last-child {
    background: #999;
}