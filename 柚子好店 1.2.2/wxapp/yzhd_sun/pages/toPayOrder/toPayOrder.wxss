.order_box {
    background-color: #fff;
}

.order_title {
    height: 90rpx;
    padding: 0 30rpx;
    background-color: #fff;
}

.order_icon {
    width: 30rpx;
    height: 35rpx;
    margin: 0 20rpx 0 0;
}

.order_mes {
    position: relative;
    padding: 30rpx;
    background-color: #fff;
}

.orderImg-box {
    width: 260rpx;
    height: 180rpx;
    margin: 0 30rpx 0 0;
}

.order-img {
    width: 260rpx;
    height: 180rpx;
}

.na_price_jia {
}

.name {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.price {
    color: #ff5555;
}

.num-box {
    bottom: 20rpx;
    right: 30rpx;
    position: absolute;
    display: flex;
    z-index: 9;
}

.num-box .num-jian,.num-box .num-input,.num-box .num-jia {
    width: 66rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    border: 1px solid #eee;
    font-size: 40rpx;
    margin: 3rpx;
}

.num-box .num-jian,.num-box .num-jia {
    background-color: #fff;
}

.num-box .num-jian.disabled,.num-box .num-jia.disabled {
    background-color: #f5f5f9;
}

.num-box .num-input {
    font-size: 28rpx;
}

.num-box .num-input input {
    height: 100%;
}

.order_guige {
    height: 80rpx;
    color: #999;
    background-color: #fff;
    margin: 0 30rpx;
}

.buyer_box {
    margin: 30rpx 0;
    background-color: #fff;
}

.connect_box {
    margin: 0 30rpx;
    padding: 10rpx 0 20rpx;
    color: #999;
}

.notice_box {
    background-color: #fff;
    margin: 0 0 30rpx 0;
}

.notice_contents {
    font-size: 26rpx;
    color: #999;
    margin: 0 30rpx;
    padding: 30rpx 0;
}

.card_yh {
    background-color: #fff;
    height: 90rpx;
    padding: 0 30rpx;
    margin: 30rpx 0 0;
}

.arrow {
    width: 12rpx;
    height: 25rpx;
    margin: 0 0 0 20rpx;
}

.save {
    color: #999;
}

.save_money {
    color: #ff5555;
}

.label_row {
    height: 90rpx;
    margin: 0 0 30rpx;
    background-color: #f2f2f2;
    padding: 0 30rpx;
}

.diyholder {
    color: #999;
}

.inputBox {
    width: 100%;
    color: #333;
}

.payNow-btn {
    width: 600rpx;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    border-radius: 10rpx;
    background-color: #ff5555;
    font-size: 34rpx;
    color: #fff;
    margin: 30rpx auto 50rpx;
}

button::after {
    border: none;
}