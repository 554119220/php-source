.bigWrap {
    margin: 30rpx;
    background-color: #fff;
    padding: 20rpx;
    border-radius: 10rpx;
}

.inputRow {
    height: 80rpx;
    padding: 0 20rpx;
    margin: 0 0 30rpx 0;
    border: 1rpx solid #999;
}

.starRow {
    height: 80rpx;
    padding: 0 20rpx;
    margin: 0 0 30rpx 20rpx;
    border: 1rpx solid #999;
}

.input {
    width: 100%;
    flex: 1;
}

.posBtn {
    width: 90rpx;
    height: 50rpx;
    background: rgba(255,85,85,1);
    color: #fff;
    border-radius: 10rpx;
    text-align: center;
    line-height: 50rpx;
}

.checkbox {
    margin: 0 50rpx 0 0;
}

.picker {
    flex: 1;
    text-align: center;
}

checkbox {
    width: 30rpx;
    height: 30rpx;
    margin: 0 10rpx 0 0;
}

checkbox .wx-checkbox-input {
    width: 30rpx;
    height: 30rpx;
    margin-top: -5rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    background: #ffb62b;
    border: none;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}

.addImgBox {
    margin: 50rpx 0 0 0;
}

.content_add {
    width: 150rpx;
    height: 150rpx;
}

.add_img {
    width: 150rpx;
    height: 150rpx;
    border-radius: 10rpx;
    display: flex;
    border: 1rpx solid #eee;
}

.noBorder {
    border: none;
}

.upload {
    height: auto;
    background: #fff;
    padding: 0rpx 0rpx 10rpx;
    margin: 30rpx 0 0 0;
}

.content_add {
    width: 25%;
    height: 100%;
    display: inline-block;
}

.banner {
    font-size: 26rpx;
    color: #000;
}

.border {
    border-bottom: 1rpx solid #eee;
}

.banner_list {
    height: 90rpx;
    padding: 0rpx 30rpx;
}

.confirm {
    width: 600rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #ffb825;
    color: #fff;
    line-height: 90rpx;
    text-align: center;
    margin-top: 40rpx;
    margin-bottom: 30rpx;
}

button::after {
    border: none;
}

.delete {
    width: 40rpx;
    height: 40rpx;
    right: 10rpx;
    top: 0rpx;
}

.ab {
    width: 40rpx;
    display: flex;
}

.deterPush {
    width: 600rpx;
    height: 100rpx;
    line-height: 100rpx;
    background: rgba(239,68,68,1);
    border-radius: 10rpx;
    color: #fff;
    margin: 30rpx auto;
    font-size: 34rpx;
}

button::after {
    border: none;
}