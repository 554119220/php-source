.vipTypeBox {
    padding: 0 30rpx;
    z-index: 9;
}

.classic_item {
    width: 210rpx;
    height: 130rpx;
    background: rgba(251,236,225,1);
    border-radius: 10rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.classic_item.select {
    border: 1px solid #FF9D55;
    box-sizing: border-box;
}

.classic_text {
    font-size: 26rpx;
    color: #FF5555;
}

.classic_money {
    color: #FF5555;
    font-size: 36rpx;
    font-weight: bold;
}

.activeBtn {
    width: 110rpx;
    height: 60rpx;
    background: rgba(255,85,85,1);
    border-radius: 10rpx;
    color: #fff;
    text-align: center;
    line-height: 60rpx;
}

.label_row:last-child {
    margin: 0 30rpx 40rpx;
}

.bbj {
    width: 100%;
}

.mbj {
    width: 100%;
    height: 400rpx;
    position: absolute;
    border-radius: 10rpx;
}

.sbj {
    width: 100%;
    height: 184rpx;
    position: absolute;
}

page {
    background-color: #fff;
}

.bigBj {
    width: 100%;
    height: 440rpx;
    position: relative;
}

.midBj {
    width: 92%;
    height: 400rpx;
    position: absolute;
    top: 90rpx;
    left: 30rpx;
    border-radius: 10rpx;
}

.smaBj {
    width: 100%;
    height: 184rpx;
    z-index: 5;
    position: absolute;
    top: 350rpx;
}

.userMes {
    margin: 10rpx 20rpx 50rpx 20rpx;
    position: relative;
}

.userinfo-avatar {
    width: 90rpx;
    height: 90rpx;
    border-radius: 10rpx;
    margin: 10rpx 20rpx 0 0;
    display: flex;
}

.tequan_icon {
    width: 30rpx;
    height: 30rpx;
    margin: 0 10rpx 0 0;
}

.joinbtn {
    min-width: 200rpx;
    height: 50rpx;
    border-radius: 10rpx;
    background-color: #ff5555;
    text-align: center;
    line-height: 50rpx;
    color: #fff;
    padding: 0;
    font-size: 30rpx;
}

.viptequan {
    text-align: center;
    color: #999;
    font-size: 26rpx;
    position: relative;
}

.tequan_row {
    margin: 10rpx 48rpx;
    position: relative;
    text-align: center;
    justify-content: center;
}

.tequan_word {
    width: 200rpx;
    font-size: 30rpx;
    color: #333;
    flex: 1;
}

.currStatus {
    font-size: 26rpx;
    color: #999;
}

.nextStatus {
    font-size: 26rpx;
    color: #ff9d55;
}

.vip-title {
    margin: 80rpx 30rpx 30rpx;
    position: relative;
}

.vipt-title {
    margin: 50rpx 30rpx 30rpx;
    position: relative;
}

.vipth-title {
    margin: 50rpx 30rpx 30rpx;
    position: relative;
}

.mes_icon {
    width: 30rpx;
    height: 35rpx;
    margin: 0 20rpx 0 0;
}

.formBox {
    width: 100%;
    display: inline-block;
    margin: 250rpx 0 0 0;
    background-color: #fff;
    z-index: 9;
    position: relative;
}

.label_row {
    height: 90rpx;
    background-color: #f2f2f2;
    margin-bottom: 20rpx;
    padding: 0 30rpx;
    margin: 0 30rpx 20rpx;
}

.inputbox {
    width: 100%;
    color: #333;
}

.payNow-btn {
    width: 600rpx;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    border-radius: 10rpx;
    background-color: #ff5555;
    font-size: 34rpx;
    color: #fff;
    margin: 20rpx auto 40rpx;
}

button::after {
    border: none;
}

.vipRule {
    width: 400rpx;
    height: 200rpx;
    border-radius: 10rpx;
    position: fixed;
    z-index: 13;
    background-color: #fff;
    top: 200rpx;
    left: 175rpx;
    padding: 20rpx;
    overflow-y: scroll;
}