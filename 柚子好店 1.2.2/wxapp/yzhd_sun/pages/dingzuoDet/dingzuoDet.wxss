page {
    background-color: #fff;
}

.big_box {
    padding: 30rpx;
}

.successSta {
    font-size: 48rpx;
}

.tishi {
    font-size: 26rpx;
    color: #999;
}

.mid_box {
    padding: 30rpx 0;
}

.sel_mes {
    margin: 20rpx 30rpx 0 0;
}

.logo_box {
    width: 80rpx;
    height: 80rpx;
    border-radius: 100%;
    margin: 0 20rpx 0 0;
}

.mes_row {
    font-size: 26rpx;
}

.classic {
    color: #999;
}

.shop_logo {
    width: 80rpx;
    height: 80rpx;
    border-radius: 100%;
}

.call {
    width: 100rpx;
    height: 40rpx;
    border-radius: 10rpx;
    background-color: #ffde55;
    text-align: center;
    line-height: 40rpx;
    margin: 20rpx 20rpx 0 0;
}

.btn_row {
    justify-content: flex-end;
    margin: 30rpx 0 0 0;
}

.btn {
    width: 180rpx;
    height: 50rpx;
    padding: 0;
    margin: 0;
    line-height: 50rpx;
    text-align: center;
    font-size: 30rpx;
    background-color: #fff;
    border-radius: 10rpx;
}

.cancle-btn {
    border: 1rpx solid #ff5555;
    color: #ff5555;
    margin: 0 40rpx 0 0;
}

.tell-friend {
    border: 1rpx solid #ff9d55;
    color: #ff9d55;
}

button:after {
    border: none;
}