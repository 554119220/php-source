[is="yzhd_sun/pages/SecFooter/SecFooter"] {
    font-size: 20rpx;
}

[is="yzhd_sun/pages/SecFooterold/SecFooter"] {
    font-size: 20rpx;
}

page {
    background-color: #f2f2f2;
    font-size: 30rpx;
    color: #333;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.footerWrap {
    height: 98rpx;
}

.flex {
    display: flex;
}

.flex-center {
    display: flex;
    align-items: center;
}

.flex-center-col {
    display: flex;
    align-items: center;
    flex-direction: column;
}

.flex-start {
    display: flex;
    align-items: flex-start;
}

.flex-center-spa {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.flex-center-aro {
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.border_b {
    border-bottom: 1rpx solid #eee;
}

.border_t {
    border-top: 1rpx solid #eee;
}

.left_b {
    display: inline-block;
    width: 8rpx;
    height: 30rpx;
    border-radius: 4rpx;
    background-color: #ffde55;
    margin-right: 20rpx;
}

.right_b {
    display: inline-block;
    width: 8rpx;
    height: 30rpx;
    border-radius: 4rpx;
    background-color: #ffde55;
    margin-left: 20rpx;
}

.border {
    border: 1rpx solid #eee;
}

.rel {
    position: relative;
}

.ab {
    position: absolute;
}

.flex1 {
    flex: 1;
}

.dot {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.wall {
    width: 100%;
    height: 20rpx;
    background-color: #f2f2f2;
}

.twall {
    width: 100%;
    height: 30rpx;
    background-color: #f2f2f2;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 10;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 11;
}

.shareFriend {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 30rpx;
    background-color: #fff;
}

.actionList {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 30rpx;
    background-color: #fff;
}

.actionList:last-child {
    margin: 10rpx 0 0 0;
}

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
    background: #000;
    opacity: 0.1;
    overflow: hidden;
}

.drawer_box {
    width: 100%;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 12;
    background: #fff;
    font-size: 30rpx;
    background-color: #f2f2f2;
}

.icon {
    display: block;
    width: 54rpx;
    height: 54rpx;
}

.tabBar {
    display: flex;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 98rpx;
    background: #fff;
    z-index: 11;
    justify-content: space-between;
    align-items: center;
    box-shadow: 6rpx 6rpx 6rpx 6rpx #ddd;
}

.tabBar-item {
    color: #333;
    text-align: center;
}

.tabBar-item view:nth-child(2) {
    font-size: 20rpx;
}

.tabBartext {
    color: #333;
}

.tabBar-item:nth-child(1) {
    margin-left: 30rpx;
}

.tabBar-item:last-child {
    margin-right: 30rpx;
}

.tabBar-item:nth-child(3) view:nth-child(1) {
    position: relative;
}

.tabBar-item:nth-child(3) view:nth-child(1) image {
    position: absolute;
    left: -10rpx;
    top: -60rpx;
    width: 60rpx;
    height: 60rpx;
}

.tabBar-item:nth-child(3) view:nth-child(2) {
    margin-top: 52rpx;
}