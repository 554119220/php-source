.title {
    height: 90rpx;
}

.title>view {
    padding: 0 60rpx;
    height: 89rpx;
    line-height: 89rpx;
}

.list {
    padding: 30rpx;
    line-height: 60rpx;
}

.img {
    flex: 0 0 auto;
    width: 230rpx;
    height: 170rpx;
    margin-right: 30rpx;
}

.tip>view {
    display: inline-block;
    font-size: 24rpx;
    padding: 0 10rpx;
    height: 30rpx;
    line-height: 30rpx;
    margin-right: 20rpx;
    margin-top: 10rpx;
}

.tip_1 {
    border: 1rpx solid #f2507d;
    color: #f2507d;
}

.tip_2 {
    border: 1rpx solid #36c5f7;
    color: #36c5f7;
}

.tip_3 {
    border: 1rpx solid #558af7;
    color: #558af7;
}

.none {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
}