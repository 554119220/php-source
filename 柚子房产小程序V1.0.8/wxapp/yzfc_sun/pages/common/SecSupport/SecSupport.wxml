<view class="zhichi" wx:if="{{show}}">
    <view class="logob_box">
        <image class="logo_b" src="{{imgLink+msg.sup_logo}}" wx:if="{{msg.sup_logo}}"></image>
    </view>
    <view class="center_word">
        <view class="logo_name" wx:if="{{msg.sup_name}}">{{msg.sup_name}}</view>
        <view wx:if="{{msg.sup_tel}}">合作电话:{{msg.sup_tel}}</view>
    </view>
    <view bindtap="_onCallTab" class="callphone" wx:if="{{msg.sup_tel}}">拨打</view>
</view>
