<view class="sec-footer" style="background:{{color.foot_color}}" wx:if="{{passFlag==0}}">
    <button bindtap="_onNavTab" class="item" data-index="{{index}}" wx:for="{{foot}}" wx:key="{{index}}">
        <image src="{{item.choose?item.imgh:item.img}}"></image>
        <text style="color:{{item.choose?color.foot_font_color_two:color.foot_font_color_one}}">{{item.txt}}</text>
    </button>
</view>
