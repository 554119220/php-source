.container {
    padding-bottom: 98rpx;
}

.header-img {
    height: 96rpx;
    width: 96rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.circle-top {
    padding: 20rpx;
    line-height: 46rpx;
}

.circle-msg {
    line-height: 50rpx;
    margin-bottom: 17rpx;
}

.img-box {
    flex-wrap: wrap;
    padding: 20rpx 0 20rpx 20rpx;
    line-height: 0;
}

.img {
    width: 70%;
}

.imgs {
    width: 222rpx;
    height: 222rpx;
    margin-right: 15rpx;
    margin-bottom: 15rpx;
}

.icon-img {
    width: 36rpx;
    height: 36rpx;
    margin-right: 10rpx;
}

.circle-tip {
    height: 60rpx;
}

.circle-tip>view {
    width: 140rpx;
}

.add-icon {
    position: fixed;
    bottom: 130rpx;
    right: 30rpx;
    z-index: 1;
    width: 80rpx;
    height: 80rpx;
}

.nav {
    width: 750rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    white-space: nowrap;
}

.nav-view {
    display: inline-block;
    flex: 0 0 auto;
    padding: 0 30rpx;
    text-align: center;
    position: relative;
}

.nav-view:before {
    content: "";
    position: absolute;
    right: 0;
    top: 10%;
    height: 80%;
    background: #ddd;
    width: 1rpx;
}

.nav-view:last-child:before {
    background: #fff;
}

.nav-view image {
    width: 36rpx;
    height: 36rpx;
    margin-right: 20rpx;
    vertical-align: -5rpx;
}

.find-con {
    padding: 0 30rpx 30rpx 30rpx;
}

.find-con-img {
    width: 230rpx;
    height: 170rpx;
    margin-right: 30rpx;
}

.icon-img1 {
    vertical-align: -7rpx;
}

.icon-img2 {
    vertical-align: -5rpx;
}

.find-r {
    position: relative;
    height: 170rpx;
}

.circle-tip {
    position: absolute;
    bottom: 0;
}

.none {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
}