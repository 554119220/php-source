.header-img {
    height: 96rpx;
    width: 96rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.circle-top {
    padding: 30rpx;
    line-height: 26rpx;
}

.circle-msg {
    line-height: 50rpx;
    margin-bottom: 20rpx;
}

.img-box {
    flex-wrap: wrap;
    padding: 20rpx 0 20rpx 20rpx;
    line-height: 0;
}

.img {
    width: 70%;
}

.imgs {
    width: 222rpx;
    height: 222rpx;
    margin-right: 15rpx;
    margin-bottom: 15rpx;
}

.icon-img {
    width: 36rpx;
    height: 36rpx;
    margin-right: 10rpx;
}

.circle-tip {
    height: 60rpx;
}

.circle-tip>view {
    width: 140rpx;
}

.img1 {
    width: 150rpx;
    height: 150rpx;
}

.img2 {
    width: 70rpx;
    height: 150rpx;
}

.img2:first-child {
    margin-right: 10rpx;
}

.img3:first-child {
    width: 70rpx;
    height: 150rpx;
    margin-right: 10rpx;
}

.img3:nth-child(2) {
    width: 70rpx;
    height: 70rpx;
    margin-bottom: 10rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.img3:nth-child(3) {
    width: 70rpx;
    height: 70rpx;
    bottom: 0;
    right: 0;
    position: absolute;
}

.img4:first-child {
    width: 70rpx;
    height: 70rpx;
    left: 0;
    top: 0;
    position: absolute;
}

.img4:nth-child(2) {
    width: 70rpx;
    height: 70rpx;
    right: 0;
    top: 0;
    position: absolute;
}

.img4:nth-child(3) {
    width: 70rpx;
    height: 70rpx;
    left: 0;
    bottom: 0;
    position: absolute;
}

.img4:nth-child(4) {
    width: 70rpx;
    height: 70rpx;
    right: 0;
    bottom: 0;
    position: absolute;
}

.img4:nth-child(n+5) {
    width: 0;
    height: 0;
}

.new-list {
    padding: 0 30rpx 30rpx;
}

.new-list-right {
    margin-right: 20rpx;
    position: relative;
    width: 150rpx;
    height: 150rpx;
}