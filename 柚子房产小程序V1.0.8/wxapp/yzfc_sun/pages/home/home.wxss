.container {
    padding-bottom: 98rpx;
}

.slide-image {
    height: 350rpx;
    width: 100%;
}

swiper {
    height: 350rpx;
}

.icon_48 {
    height: 48rpx;
    width: 48rpx;
}

.nav {
    height: 136rpx;
    box-shadow: 0 0 10rpx rgba(0,0,0,0.3);
    border-radius: 20rpx;
    margin: -40rpx 30rpx 20rpx 30rpx;
    position: relative;
    background: #fff;
}

.nav>view {
    width: 25%;
}

.card_box {
    padding: 20rpx;
}

.card_list {
    width: 100%;
    height: 250rpx;
    position: relative;
    box-shadow: 0 0 10rpx rgba(0,0,0,0.3);
}

.card_img {
    width: 100%;
    height: 250rpx;
}

.card_mask {
    position: absolute;
    background: rgba(0,0,0,0.3);
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 2;
}

.card_time {
    position: absolute;
    top: 20rpx;
    left: 20rpx;
    color: #fff;
    z-index: 3;
}

.card_time .flex-start {
    margin-top: 10rpx;
}

.time_detail {
    background: #fff;
    height: 32rpx;
    line-height: 32rpx;
    min-width: 40rpx;
    text-align: center;
}

.small-mao {
    margin: 0 10rpx;
}

.card_tip {
    color: #fff;
    height: 36rpx;
    width: 60rpx;
    padding-right: 20rpx;
    line-height: 36rpx;
    position: absolute;
    top: 0;
    right: 20rpx;
    font-size: 24rpx;
    z-index: 3;
}

.card_tip image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.card_tip view {
    position: absolute;
    top: 0;
    left: 10rpx;
    width: 100%;
    height: 100%;
    z-index: 1;
}

.card_tips {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background: rgba(255,255,255,.5);
    padding: 10rpx;
}

.card_tips>view {
    background: rgba(223,35,35,.5);
    color: #fff;
    padding: 10rpx 20rpx;
    font-size: 36rpx;
}

.card_name {
    font-size: 28rpx;
    font-weight: bold;
    height: 60rpx;
    line-height: 60rpx;
}

.head_box {
    padding-left: 10rpx;
}

.head_box image {
    width: 36rpx;
    height: 36rpx;
    border-radius: 50%;
    margin-left: -10rpx;
}

.head_box image:last-child {
    margin-right: 20rpx;
}

.card_btn {
    width: 140rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #fff;
    border-radius: 40rpx;
    margin: 0;
    padding: 0;
    font-size: 26rpx;
}

.title {
    height: 80rpx;
    padding: 0 20rpx;
}

.icon_next {
    width: 14rpx;
    height: 24rpx;
    margin-left: 20rpx;
}

.news>view {
    position: relative;
    margin-left: 20rpx;
    font-size: 0;
    line-height: 0;
    margin-bottom: 20rpx;
}

.news>view:first-child {
    margin-right: 10rpx;
}

.news image {
    width: 340rpx;
    height: 200rpx;
}

.news_name {
    height: 40rpx;
    line-height: 40rpx;
    position: absolute;
    bottom: 0;
    right: 0;
    left: 0;
    background: rgba(0,0,0,0.3);
    font-size: 28rpx;
    color: #fff;
    padding: 0 20rpx;
}

.tip>view {
    font-size: 24rpx;
    padding: 0 10rpx;
    height: 30rpx;
    line-height: 30rpx;
    margin-right: 20rpx;
    margin-top: 2rpx;
}

.tip_1 {
    border: 1rpx solid #f2507d;
    color: #f2507d;
}

.tip_2 {
    border: 1rpx solid #36c5f7;
    color: #36c5f7;
}

.tip_3 {
    border: 1rpx solid #558af7;
    color: #558af7;
}

.today {
    padding: 20rpx;
    line-height: 55rpx;
}

.today image {
    width: 100%;
    height: 250rpx;
}

.youzi {
    display: flex;
    justify-content: center;
    font-size: 26rpx;
    padding: 20rpx;
    align-items: center;
    text-align: center;
}

.youzi image {
    width: 72rpx;
    height: 72rpx;
    margin-right: 30rpx;
}

.youzi button {
    margin: 0;
    padding: 0;
    font-size: 24rpx;
    width: 80rpx;
    text-align: center;
    height: 46rpx;
    line-height: 46rpx;
    margin-left: 30rpx;
    color: #fff;
    background: #df2323;
}

.new_ad_box image {
    width: 500rpx;
    height: 680rpx;
    position: fixed;
    top: 50%;
    left: 50%;
    margin-left: -250rpx;
    margin-top: -340rpx;
    border-radius: 20rpx;
    z-index: 11;
}

.card_img-a,.card_img-a>image {
    height: 200rpx;
}

.new_ad_box icon {
    position: fixed;
    z-index: 11;
    top: 50%;
    margin-top: -360rpx;
    left: 50%;
    margin-left: 270rpx;
}