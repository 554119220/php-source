@import "../common/wxParse/wxParse.wxss";

.logo {
    margin: 0 30rpx;
    font-size: 36rpx;
    padding-top: 25rpx;
    line-height: 0;
}

.logo image {
    width: 250rpx;
    height: 250rpx;
}

.title {
    display: flex;
    flex-direction: row;
    height: 80rpx;
    line-height: 80rpx;
}

.title>view:nth-child(1) {
    flex: 1;
    margin-left: 140rpx;
}

.title>view:nth-child(2) {
    width: 140rpx;
    font-size: 26rpx;
    color: #005aff;
}

.msg {
    padding: 30rpx;
}

.list {
    padding: 0 30rpx;
    margin-top: 30rpx;
}

button {
    background: #005aff;
    margin: 60rpx 30rpx;
}