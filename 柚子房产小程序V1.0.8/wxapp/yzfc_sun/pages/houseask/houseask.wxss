.search_box>view {
    background: #f5f5f5;
    height: 70rpx;
    margin: 20rpx 30rpx;
    border-radius: 10rpx;
}

.search_box icon {
    margin: 0 20rpx;
}

.icon_44 {
    width: 48rpx;
    height: 48rpx;
}

.que {
    padding: 20rpx;
}

.interlocution {
    padding: 20rpx 30rpx;
    line-height: 50rpx;
}

.interlocution text {
    width: 48rpx;
    height: 48rpx;
    border-radius: 50%;
    flex: 0 0 auto;
    line-height: 48rpx;
    margin-right: 20rpx;
}

.top {
    display: flex;
}

.bottom {
    display: flex;
}

.container {
    padding-bottom: 98rpx;
}

button {
    position: fixed;
    bottom: 0;
    right: 0;
    left: 0;
    border-radius: 0;
    height: 98rpx;
    line-height: 98rpx;
}

.none {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
}