.container {
    padding-top: 198rpx;
    padding-bottom: 98rpx;
}

.search_box {
    position: fixed;
    background: #fff;
    height: 112rpx;
    left: 0;
    right: 0;
    top: 0;
    z-index: 11;
}

.search_box>view {
    background: #f5f5f5;
    height: 70rpx;
    margin: 20rpx 30rpx;
    border-radius: 10rpx;
}

.search_box icon {
    margin: 0 20rpx;
}

.icon_44 {
    width: 48rpx;
    height: 48rpx;
}

.treeOne {
    display: flex;
    flex-direction: row;
    height: 90rpx;
    position: fixed;
    left: 0;
    right: 0;
    top: 114rpx;
    background: #fff;
    z-index: 11;
}

.txt-center {
    flex: 1;
}

.menu {
    position: fixed;
    left: 0;
    right: 0;
    top: 206rpx;
    background: red;
    height: 578rpx;
    overflow: auto;
    background: #fff;
    font-size: 28rpx;
    z-index: 11;
}

.menu-con {
    display: flex;
    flex-direction: column;
}

.h_578 {
    height: 578rpx;
    box-sizing: border-box;
}

.border-r {
    border-right: 1rpx solid #ddd;
}

.fix-bottom {
    position: fixed;
    background: #fff;
    left: 0;
    right: 0;
    height: 131rpx;
    top: 784rpx;
    border-top: 1rpx solid #ddd;
}

.fix-bottom>button {
    width: 340rpx;
    height: 70rpx;
    font-size: 32rpx;
    line-height: 70rpx;
}

.menu-list {
    display: flex;
    align-items: center;
    height: 90rpx;
    justify-content: center;
    box-sizing: border-box;
}

radio {
    display: none;
}

.san {
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-top: 16rpx solid #ddd;
    margin-left: 20rpx;
    line-height: 0;
}

.san1 {
    display: block;
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-top: 16rpx solid #df2323;
    margin-left: 20rpx;
    line-height: 0;
}

.houseList {
    display: flex;
    padding: 20rpx 30rpx;
    line-height: 50rpx;
    position: relative;
}

.houseList-img {
    width: 230rpx;
    height: 170rpx;
    margin-right: 20rpx;
}

.tip {
    margin: 3rpx 0;
}

.tip>view {
    font-size: 24rpx;
    padding: 0 10rpx;
    height: 30rpx;
    line-height: 30rpx;
    margin-right: 20rpx;
    margin-bottom: 10rpx;
}

.tip_1 {
    border: 1rpx solid #f2507d;
    color: #f2507d;
}

.tip_2 {
    border: 1rpx solid #36c5f7;
    color: #36c5f7;
}

.tip_3 {
    border: 1rpx solid #558af7;
    color: #558af7;
}

.margin-top-50 {
    margin-top: -50rpx;
}

.w_440 {
    width: 440rpx;
}

.lock {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: fixed;
}

.rotatea {
    display: block;
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-bottom: 16rpx solid #df2323;
    margin-left: 20rpx;
    line-height: 0;
    transition: All 0.4s ease-in-out;
}

.rotateb {
    transform: rotate(180deg);
}

.none {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
}