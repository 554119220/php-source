.font-46 {
    font-size: 46rpx;
}

.top {
    height: 300rpx;
    position: relative;
}

.top_img {
    width: 100%;
    height: 300rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.top-mask {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.3);
    z-index: 2;
}

.top_name {
    position: absolute;
    z-index: 3;
    left: 0;
    right: 0;
    text-align: center;
    top: 55rpx;
}

.leadMsg {
    box-shadow: 0 0 10rpx rgba(0,0,0,0.3);
    margin: 0 30rpx;
    line-height: 44rpx;
    padding: 20rpx;
    border-radius: 20rpx;
    transform: translateY(-50%);
    position: relative;
    z-index: 4;
    background: #fff;
}

.houseList {
    padding: 20rpx 30rpx;
    line-height: 50rpx;
}

.houseList-img {
    width: 230rpx;
    height: 170rpx;
    margin-right: 20rpx;
}

.tip>view {
    font-size: 24rpx;
    padding: 0 10rpx;
    height: 30rpx;
    line-height: 30rpx;
    margin-right: 20rpx;
}

.tip_1 {
    border: 1rpx solid #f2507d;
    color: #f2507d;
    margin-bottom: 10rpx;
}

.tip_2 {
    border: 1rpx solid #36c5f7;
    color: #36c5f7;
    margin-bottom: 10rpx;
}

.tip_3 {
    border: 1rpx solid #558af7;
    color: #558af7;
    margin-bottom: 10rpx;
}

.margin-top-50 {
    margin-top: -50rpx;
}

.fuck {
    display: flex;
    flex-direction: row;
}