<view wx:if="{{nclass==4}}">
    <view class="top">
        <text class="t-left">入账金额</text>
        <text class="t-right">+{{info.deposit}}</text>
    </view>
    <view class="box">
        <text class="b-left">交易类型</text>
        <text class="b-right">{{info.nclass}}</text>
    </view>
    <view class="box">
        <text class="b-left">交易时间</text>
        <text class="b-right">{{info.overtime}}</text>
    </view>
    <view class="box">
        <text class="b-left">充值方式</text>
        <text class="b-right">微信</text>
    </view>
    <view bindtap="del" class="box-bottom">
        <text>删除本条记录</text>
    </view>
</view>
