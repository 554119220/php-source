<view class="top">
    <text class="t-left">审核情况</text>
    <text class="t-right">{{info.audit==1?'申请中':info.audit==2?'已通过':info.audit==3?'已拒绝':'已过期'}}</text>
</view>
<view class="box">
    <text class="b-left">起点</text>
    <text class="b-right">{{info.starting_place}}</text>
</view>
<view class="box">
    <text class="b-left">终点</text>
    <text class="b-right">{{info.end_place}}</text>
</view>
<view class="box">
    <text class="b-left">最迟出发时间</text>
    <text class="b-right">{{info.begin_time}}</text>
</view>
<view class="box">
    <text class="b-left">最迟到达时间</text>
    <text class="b-right">{{info.end_time}}</text>
</view>
<view class="box">
    <text class="b-left">是否往返</text>
    <text class="b-right">{{info.flag==2?'往返双程':'单程'}}</text>
</view>
<view class="box">
    <text class="b-left">投放时间</text>
    <text class="b-right">{{info.put_time}} 天</text>
</view>
<view class="box">
    <text class="b-left">真实姓名</text>
    <text class="b-right">{{info.truename}}</text>
</view>
<view class="box">
    <text class="b-left">拒绝原因</text>
    <text class="b-right">{{info.reason==null?'暂无原因':info.reason}}</text>
</view>
<view bindtap="del" class="box-bottom">
    <text>删除本条记录</text>
</view>
