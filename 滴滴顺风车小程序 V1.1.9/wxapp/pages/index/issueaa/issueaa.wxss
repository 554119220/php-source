.navbar {
    height: 100rpx;
    line-height: 100rpx;
    flex: none;
    display: flex;
    background: #fff;
    font-size: 30rpx;
    font-weight: bold;
}

.navbar .item {
    position: relative;
    flex: auto;
    text-align: center;
    color: #575758;
}

.navbar .item.active {
    color: #64a9f6;
}

.navbar .item.active:after {
    content: "";
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4rpx;
    background: #64a9f6;
}

