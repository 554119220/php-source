.zan-dialog__mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 999;
    opacity: 0.3;
    display: none;
}

.zan-dialog__container {
    transition: all 0.4s ease;
    z-index: 999;
    border-radius: 20rpx;
    text-align: center;
}

.zan-dialog--show .zan-dialog__container {
    transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
    display: block;
}

