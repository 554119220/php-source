page {
    background: #fff;
}

.search {
    display: flex;
    padding: 15rpx 35rpx;
    width: 630rpx;
    background: #f9f9f9;
    align-items: center;
    border-radius: 60rpx;
    margin: 0 auto;
}

.search input {
    width: 530rpx;
    margin-left: 45rpx;
}

.swiper-block {
    height: 350rpx;
    width: 100%;
}

.swiper-item {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    overflow: unset;
}

.slide-image {
    height: 280rpx;
    width: 500rpx;
    border-radius: 9rpx;
    margin: 0rpx 30rpx;
    z-index: 1;
}

.active {
    transform: scale(1.1);
    transition: all 0.2s ease-in 0s;
    z-index: 20;
}

.nav {
    display: flex;
    padding: 0 20rpx;
    justify-content: space-around;
}

.nav-list {
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
    font-weight: bold;
    color: #999;
    font-size: 30rpx;
    letter-spacing: 2rpx;
}

.on {
    height: 88rpx;
    color: #f4533f;
    line-height: 88rpx;
    border-bottom: 2rpx solid;
}

.cut-line {
    height: 20rpx;
    background: #f9f9f9;
}

.live {
}

.live-list {
    height: 430rpx;
    width: 100%;
    margin-bottom: 20rpx;
    position: relative;
}

.live-list .live-bg {
    height: 430rpx;
    width: 100%;
}

.list-main {
    color: #fff;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.list-head {
    display: flex;
    flex-direction: row-reverse;
    padding: 20rpx;
}

.live-list .list-bottom {
    height: 120rpx;
    width: 100%;
    background: rgba(0,0,0,0.7);
    position: absolute;
    bottom: 0;
}

.live-list .list-bottom .list-item {
    padding: 20rpx;
    display: flex;
}

.live-list .list-bottom .list-item .avatar {
    width: 80rpx;
    height: 80rpx;
    border: 4rpx solid #fff;
    border-radius: 50%;
}

.live-list .list-bottom .list-item .text {
    margin-left: 20rpx;
}

.live-list .list-bottom .list-item .text .text-top {
    color: #fff;
    font-size: 4vw;
    width: 590rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.live-list .list-bottom .list-item .text .text-bottom {
    color: #999;
    font-size: 3.5vw;
    display: flex;
    justify-content: space-between;
}

.live-list .list-bottom .list-item .text .text-bottom .nick {
    color: #999;
    width: 350rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.live-list .list-bottom .list-item .tip {
    display: flex;
    align-items: center;
}

.live-list .list-bottom .list-item .tip image {
    width: 36rpx;
    margin-right: -16rpx;
}

.live-list .list-bottom .list-item .tip .num {
    color: #f4533f;
    padding-left: 4rpx;
    padding-right: 4rpx;
}

.list-head .list-head-left {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.list-head-left .user-head {
    width: 55rpx;
    height: 55rpx;
    margin-right: 15rpx;
}

.pos {
    display: flex;
    align-items: center;
}

.pos image {
    width: 15rpx;
    height: 20rpx;
    margin-left: 10rpx;
    margin-right: 5rpx;
}

.list-head-right {
    display: flex;
    align-items: center;
    background: #f59980;
    padding: 0 10rpx;
    height: 50rpx;
    line-height: 50rpx;
    border-radius: 30rpx;
}

.point {
    font-weight: bold;
    color: #f4533f;
    margin-right: 5rpx;
    line-height: 35rpx;
    height: 35rpx;
}

.live-satte {
    margin-right: 15rpx;
}

.goods-img {
    padding: 0 20rpx;
    display: flex;
    margin-top: 180rpx;
}

.goods-img view:first-child {
    margin: 0;
}

.goods-img view {
    margin-left: 20rpx;
    height: 90rpx;
    width: 90rpx;
    position: relative;
}

.goods-img view image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}

.goods-img view text {
    position: absolute;
    left: 5rpx;
    bottom: 5rpx;
    color: #f5f7f8;
}

.live-title {
    height: 40rpx;
    line-height: 40rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-top: 15rpx;
    padding: 0 20rpx;
}