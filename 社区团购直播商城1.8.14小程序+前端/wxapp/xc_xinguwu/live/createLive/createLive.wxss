page {
    background: #fff;
    color: #444;
}

.user-info {
    text-align: center;
    border-top: 4rpx solid #f9f9f9;
    padding-top: 80rpx;
}

.user-info image {
    margin: 0 auto;
    width: 175rpx;
    height: 175rpx;
    border-radius: 50%;
    margin-bottom: 35rpx;
}

.live-name {
    width: 630rpx;
    height: 90rpx;
    padding: 0 30rpx;
    background: #fafafa;
    margin: 0 auto;
    border-radius: 10rpx;
    margin-top: 80rpx;
}

.upload {
    display: flex;
    color: #888;
    padding: 0 30rpx;
    margin-top: 60rpx;
    align-items: baseline;
}

.weui-uploader__file {
    width: 245rpx;
    height: 180rpx;
    margin-right: 40rpx;
}

.file {
    position: relative;
}

.clear {
    position: absolute;
    right: 0;
    top: 0;
}

.load {
    width: 245rpx;
    height: 180rpx;
}

.chooseImage {
    width: 245rpx;
    height: 180rpx;
    margin-right: 40rpx;
    border: 1rpx dashed #d6d6d6;
    display: flex;
    align-items: center;
    justify-content: center;
}

.addgoods {
    margin-top: 60rpx;
    padding: 0 30rpx;
}

.addgoods .choose-goods {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 40rpx;
    background: #f9f9f9;
    border-radius: 10rpx;
    font-weight: bold;
}

.addgoods .choose-goods image {
    width: 20rpx;
    height: 30rpx;
}

.mygoods {
    padding: 20rpx;
    background: #f9f9f9;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.mygoods .main-img {
    width: 90rpx;
    height: 90rpx;
    border-radius: 10rpx;
}

.content {
    width: 525rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.content-left {
    width: 60%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.arrows {
    width: 20rpx;
    height: 30rpx;
}

.price {
    margin-top: 10rpx;
    color: #f4604c;
    font-weight: bold;
}

.create-bottom {
    display: flex;
    justify-content: space-between;
    padding: 10rpx 20rpx;
    width: 710rpx;
}

.goodsSitting {
    width: 210rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    color: #fff;
    background: #ff7e14;
    border-radius: 60rpx;
}

.createLive {
    width: 470rpx;
    background: #f93d3d;
    height: 90rpx;
    color: #fff;
    line-height: 90rpx;
    border-radius: 60rpx;
}

.createLive::after {
    border: 0;
}

.sitting {
    padding: 0 25rpx;
    border-top: 1rpx solid #e2e2e2;
    position: fixed;
    left: 0;
    top: 0;
    width: 700rpx;
    height: 100vh;
    background: #fff;
    z-index: 6;
}

.sitting .sitting-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #e2e2e2;
    padding: 30rpx 0;
}

.sitting .main-img {
    width: 180rpx;
    height: 180rpx;
}

.sitting .content {
    display: flex;
    align-items: center;
    width: 490rpx;
    justify-content: space-between;
}

.sitting .content-main {
    width: 390rpx;
}

.sitting .title {
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    height: 76rpx;
    line-height: 38rpx;
}

.sitting .price {
    color: #f23921;
    font-weight: bold;
    margin-top: 50rpx;
}

.sitting .handle {
    width: 100%;
    background: #fff;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 6;
}

.sitting .remove {
    display: flex;
    justify-content: center;
    align-items: center;
    border-bottom: 1rpx solid #eaeaea;
    padding: 30rpx 25rpx;
    font-weight: bold;
}

.sitting .remove image {
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.sitting .cancel {
    padding: 30rpx 25rpx;
    text-align: center;
    color: #999;
}

.sitting .close {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    color: #fff;
    background: #f23921;
    text-align: center;
    position: fixed;
    left: 0;
    bottom: 0;
}

.addDetail {
    width: 100%;
    height: 100vh;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 5;
    background: #fff;
}

.addDetail .choose-head {
    border-top: 2rpx solid #f8f8f8;
    padding: 25rpx 20rpx;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.addDetail .selectAll {
}

.addDetail .select {
    width: 40rpx;
    height: 40rpx;
    border-radius: 10rpx;
    border: 1rpx solid #d3d3d3;
    margin-right: 20rpx;
}

.addDetail .selected {
    width: 44rpx;
    height: 44rpx;
    border-radius: 10rpx;
    margin-right: 20rpx;
    overflow: hidden;
}

.addDetail .selected image {
    width: 100%;
    height: 100%;
}

.addDetail .makesure {
    padding: 0;
    margin: 0 auto;
    border-radius: 0;
    color: #f94a4a;
    width: 75rpx;
    background: #fff;
    height: 35rpx;
    line-height: 35rpx;
}

.addDetail .makesure::after {
    border: 0;
}

.addDetail .weui-search-bar__form {
    position: relative;
    -webkit-box-flex: 1;
    flex: auto;
    border-radius: 5px;
    background: #fff;
    border: 0;
}

.addDetail .weui-search-bar {
    position: relative;
    padding: 8px 10px;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    box-sizing: border-box;
    background-color: #f8f8f8;
    border-top: 0;
    border-bottom: 0;
}

.addDetail .weui-search-bar__label {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 2;
    border-radius: 3px;
    text-align: center;
    color: #9b9b9b;
    background: #fff;
    line-height: 28px;
}

.addDetail .goods {
    background: #fff;
    padding: 0 20rpx;
}

.addDetail .goods-list {
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #e2e2e2;
    padding: 20rpx 0;
}

.addDetail .content {
    display: flex;
    align-items: center;
    width: 630rpx;
    justify-content: space-between;
}

.addDetail .content .content-img {
    width: 180rpx;
    height: 180rpx;
}

.addDetail .content .content-right {
    width: 420rpx;
}

.addDetail .content-right .goods-name {
    font-weight: bold;
    height: 72rpx;
    line-height: 36rpx;
}

.addDetail .content-right .content-bottom {
    display: flex;
    align-items: center;
    margin-top: 60rpx;
}

.addDetail .goods-price {
    color: #f94a4a;
    font-weight: bold;
    margin-right: 50rpx;
}

.addDetail .saled {
    color: #5c5c5c;
}