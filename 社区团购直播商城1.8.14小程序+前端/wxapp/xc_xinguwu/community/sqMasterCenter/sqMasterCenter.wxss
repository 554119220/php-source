.master-banner {
    width: 100%;
    height: 320rpx;
    position: relative;
}

.banner-bg {
    width: 100%;
    height: 100%;
}

.banner-main {
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    position: absolute;
}

.banner-center {
    display: flex;
    align-items: center;
    color: #fff;
    justify-content: space-between;
    font-size: 20rpx;
    margin-top: 45rpx;
}

.banner-main .head-img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border: 4rpx solid #fff;
    margin-left: 30rpx;
}

.banner-content {
    width: 530rpx;
    margin-right: 30rpx;
    font-size: 3.5vw;
}

.banner-content-head {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.banner-content-head image {
    width: 30rpx;
    height: 30rpx;
}

.user-info {
    display: flex;
    align-items: center;
    padding: 10rpx 0;
}

.user-info image {
    width: 18rpx;
    height: 20rpx;
    margin-right: 16rpx;
}

.user-info .info-detail {
    display: flex;
    align-items: center;
}

.user-info .user-name {
    margin-right: 30rpx;
}

.user-address {
    display: flex;
    align-items: center;
}

.user-address image {
    width: 16rpx;
    height: 18rpx;
}

.user-address view {
    margin-left: 20rpx;
    width: 95%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.master-body {
    padding: 0 20rpx;
    position: relative;
    top: -90rpx;
    left: 0;
    z-index: 5;
}

.price-box {
    background: #fff;
    border-radius: 20rpx;
}

.price-head {
    display: flex;
    justify-content: space-between;
    padding: 0 25rpx;
    padding-top: 30rpx;
}

.price-main {
    display: flex;
    justify-content: space-between;
    padding: 0 25rpx;
    padding-bottom: 20rpx;
    align-items: baseline;
}

.total-price {
    font-size: 40rpx;
}

.last-day {
    color: #F04043;
}

.price-bottom {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 25rpx;
    border-top: 1rpx solid #e5e5e5;
}

.toDep {
    border: 1rpx solid;
    padding: 5rpx 15rpx;
    color: #F04043;
    border-radius: 10rpx;
}

.center-list {
    padding: 30rpx 0;
    display: flex;
    background: #fff;
    border-radius: 20rpx;
    margin-top: 20rpx;
}

.center-list .list {
    width: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    height: 70rpx;
    border-right: 1rpx solid #D7D7D7;
}

.center-list .list image {
    margin-right: 30rpx;
}

.center-list .list:last-child {
    border: 0;
}

.master-box {
    background: #fff;
    margin-top: 20rpx;
    border-radius: 20rpx;
}

.master-box .list:last-child {
    border: 0;
}

.master-box .list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 40rpx;
    border-bottom: 1rpx solid #E9E9E9;
}

.scan {
    width: 30rpx;
    height: 30rpx;
}

.one-key {
    width: 26rpx;
    height: 34rpx;
}

.sq-con {
    width: 34rpx;
    height: 26rpx;
}

.sq-sta {
    width: 26rpx;
    height: 30rpx;
}

.sq-group {
    width: 34rpx;
    height: 26rpx;
}

.master-box .list .list-content {
    width: 570rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.master-box .list .list-content image {
    width: 15rpx;
    height: 25rpx;
}

.alter-list {
    margin-top: 20rpx;
    background: #fff;
    padding: 0 30rpx;
}

.alter-head {
    display: flex;
    align-items: center;
    padding: 30rpx 0;
}

.alter-head image {
    width: 32rpx;
    height: 31rpx;
    margin-right: 23rpx;
}

.alter-list .list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-top: 40rpx;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.alter-list .list1 {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.alter-list .list1:last-child {
    border: 0;
}

.alter-list .list:last-child {
    border: 0;
}

.alter-list input {
    text-align: right;
    width: 560rpx;
}

.plc {
    text-align: right;
}

.list1 image {
    width: 15rpx;
    height: 25rpx;
}

.add {
    width: 500rpx;
    text-align: right;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.alter-box {
    width: 100%;
    height: 100vh;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 5;
    background: #F9F9F9;
}

.sure-alter {
    background: #FF3636;
    width: 620rpx;
    height: 90rpx;
    border-radius: 45rpx;
    margin: 0 auto;
    margin-top: 220rpx;
    color: #fff;
    letter-spacing: 5rpx;
    text-align: center;
    line-height: 90rpx;
}

.deposit-money {
    width: 710rpx;
    margin: 0 auto;
    border-radius: 20rpx;
    background: #fff;
    overflow: hidden;
    margin-top: 100rpx;
}

.total-money {
    background: #f93d3d;
    padding: 30rpx;
    color: #fff;
}

.title {
    padding: 0rpx 45rpx 20rpx 45rpx;
}

.ipt {
    width: 620rpx;
    margin: 0 auto;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #e3e3e3;
}

.ipt input {
    height: 75rpx;
    font-size: 6vw;
    font-weight: bold;
}

.sure {
    height: 90rpx;
    line-height: 90rpx;
    color: #fff;
    background: #f04043;
    width: 620rpx;
    margin: 0 auto;
    border-radius: 60rpx;
    margin-top: 270rpx;
    margin-bottom: 30rpx;
    font-size: 3.3vw;
    letter-spacing: 5rpx;
}

.sure::after {
    border: 0;
}

.deposit-box {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 30rpx 45rpx;
}

.deposit {
    display: flex;
    align-items: center;
}

.select {
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 1rpx solid #e2e2e2;
    margin-right: 10rpx;
}

.select view {
    width: 10rpx;
    height: 10rpx;
    background: #f93d3d;
    border-radius: 50%;
}

.toDeposit {
    color: #f93d3d;
}

.deposit-money icon {
    position: relative;
    left: 650rpx;
    top: -80rpx;
}

.auit {
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background: #f9f9f9;
    z-index: 10;
}

.auit>image {
    width: 400rpx;
    margin: 300rpx auto 50rpx auto;
}

.auit .text {
    text-align: center;
    font-size: 4.5vw;
    letter-spacing: 6rpx;
    font-weight: bold;
}

.auit .btn {
    width: 600rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    position: absolute;
    bottom: 80rpx;
    left: 75rpx;
    background: #FF3636;
    border-radius: 40rpx;
    color: #fff;
    font-size: 30rpx;
    letter-spacing: 6rpx;
}