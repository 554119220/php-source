.claim-head {
    background: #fff;
    padding: 0 30rpx;
    width: 690rpx;
    position: fixed;
    left: 0;
    top: 0;
}

.search-box {
    display: flex;
    align-items: center;
    justify-content: space-between;
    border: 1rpx solid #ebebeb;
    padding: 10rpx 30rpx;
    border-radius: 45rpx;
}

.search-box image {
    width: 34rpx;
    height: 32rpx;
}

.search-box input {
    width: 545rpx;
}

.nav {
    display: flex;
    align-items: center;
    height: 100rpx;
    line-height: 100rpx;
}

.nav-list {
    width: 33.3%;
    text-align: center;
}

.on .child {
    color: #ff3636;
    padding: 10rpx 30rpx;
    box-shadow: 0px 1px 5px 0px rgba(55,55,55,0.12);
    border-radius: 28rpx;
}

.child text {
    color: #ff3636;
}

.line {
    height: 180rpx;
}

.order-box {
    padding: 0 20rpx;
}

.order-list {
    background: #fff;
    border-radius: 20rpx;
    margin-top: 20rpx;
}

.order-list .list-head {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #ececec;
}

.order-list .head-left {
    display: flex;
}

.id {
    margin-right: 30rpx;
}

.state {
    color: #ff3636;
}

.list-main {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 30rpx;
}

.list-main image {
    width: 210rpx;
    height: 210rpx;
    border-radius: 20rpx;
}

.list-main .content {
    width: 370rpx;
    font-size: 24rpx;
}

.list-main .goods-name {
    height: 80rpx;
    line-height: 40rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.size {
    font-size: 3vw;
}

.content-bottom {
    display: flex;
    align-items: center;
    margin-top: 10rpx;
    justify-content: space-between;
}

.nowp {
    margin-right: 10rpx;
    color: #ff3636;
    font-size: 3.5vw;
}

.oldp {
    color: #cdcdcd;
    font-size: 20rpx;
    text-decoration: line-through;
}

.user-info {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    padding: 20rpx 30rpx;
    border-top: 1rpx solid #ececec;
}

.user-head {
    width: 56rpx;
    height: 56rpx;
}

.user-content {
    width: 540rpx;
}

.user-content-head {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.user-tell {
    display: flex;
    align-items: center;
}

.user-tell image {
    width: 20rpx;
    height: 21rpx;
    margin-left: 30rpx;
}

.user-content-bottom {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 10rpx;
}

.address {
    width: 95%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.list-bottom {
    padding: 30rpx;
    border-top: 1rpx solid #ececec;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: row-reverse;
}

.user-content-bottom image {
    width: 19rpx;
    height: 23rpx;
}

.make-money text {
    color: #ff3636;
}

.btn-box {
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.btn-box .btn:last-child {
    margin-left: 20rpx;
}

.btn {
    padding: 5rpx 20rpx;
    border: 1px solid rgba(236,236,236,1);
    border-radius: 25rpx;
    color: #acacac;
}

.bottom-line {
    height: 60rpx;
}

.sure-get {
    position: fixed;
    left: 60rpx;
    width: 630rpx;
    top: 400rpx;
    background: #fff;
    border-radius: 20rpx;
    z-index: 6;
    text-align: center;
}

.sure-get-title {
    padding: 60rpx 0;
    font-size: 4vw;
    letter-spacing: 4rpx;
}

.sure-get-bottom {
    padding: 0 60rpx 50rpx 60rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.cancel {
    width: 235rpx;
    height: 85rpx;
    border-radius: 45rpx;
    line-height: 85rpx;
    border: 1rpx solid #ff3636;
    color: #ff3636;
}

.sure {
    width: 235rpx;
    height: 85rpx;
    border-radius: 45rpx;
    line-height: 85rpx;
    border: 1rpx solid #ff3636;
    color: #fff;
    background: #ff3636;
}

.ver-model {
    width: 750rpx;
    position: fixed;
    left: 0rpx;
    top: 0px;
    z-index: 5;
    background: #fff;
    height: 100vh;
}

.ver-content {
    padding: 30rpx 0;
    height: calc(100vh - 330rpx);
    overflow-y: scroll;
}

.ver-title {
    text-align: center;
    position: relative;
    padding: 40rpx 0;
    border-bottom: 1rpx solid #e7e7e7;
}

.ver-close {
    position: absolute;
    right: 40rpx;
    top: 20rpx;
    font-size: 6vw;
}

.ver-list {
    width: 470rpx;
    margin: 0 auto;
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    margin-top: 10rpx;
}

.ver-info {
    width: 310rpx;
}

.info-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.ver-ipt {
    padding: 0 35rpx;
    background: #f9f9f9;
    height: 80rpx;
    min-height: 80rpx;
    line-height: 80rpx;
    width: 400rpx;
    margin: 0 auto;
    border-radius: 10rpx;
}

.ver-bottom {
    padding: 50rpx 60rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    text-align: center;
}