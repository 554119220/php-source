page {
    color: #444;
}

.nav {
    display: flex;
    align-items: center;
    height: 90rpx;
    line-height: 90rpx;
    background: #fff;
}

.nav-list {
    width: 50%;
    text-align: center;
}

.on text {
    padding: 10rpx 30rpx;
    box-shadow: 0 0 5rpx rgba(0,0,0,0.2);
    border-radius: 30rpx;
    color: #ff3636;
}

.brokerage-box {
    margin: 0 auto;
    margin-top: 20rpx;
    width: 710rpx;
    background: #fff;
    padding-bottom: 70rpx;
    border-radius: 20rpx;
}

.get-list {
    padding: 35rpx 40rpx;
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    border-bottom: 1rpx solid #ececec;
}

.get-list image {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
}

.get-list .content {
    width: 360rpx;
}

.get-list .get-price {
    width: 150rpx;
    text-align: right;
}

.get-list .method {
    font-size: 3vw;
    padding: 5rpx 0;
    color: #888;
}

.get-list .date {
    font-size: 3vw;
    color: #888;
}

.brokerage-list {
    padding: 35rpx 40rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #ececec;
}

.list-right {
    display: flex;
    align-items: center;
    text-align: right;
}

.go-back {
    margin-right: 50rpx;
    color: #ff3636;
    font-size: 3vw;
}

.state1 {
    color: #ffa031;
}

.state2 {
    color: #ff3636;
}

.state3 {
    color: #888;
}

.right-content {
    font-size: 3vw;
}