.n-sq-change-banner {
    width: 100%;
    height: 360rpx;
    position: relative;
}

.change-bg {
    width: 100%;
    height: 100%;
}

.change-main {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

.change-head {
    padding: 30rpx 20rpx;
    background: #f89a79;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.head-left {
    display: flex;
    align-items: center;
}

.head-left image {
    width: 24rpx;
    height: 30rpx;
    margin-left: 10rpx;
    margin-right: 5rpx;
}

.head-left view {
    width: 550rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.hint {
    text-align: center;
    font-size: 40rpx;
    margin-top: 30rpx;
    color: #edee38;
}

.apply-join {
    background: #edee38;
    border-radius: 25rpx;
    height: 45rpx;
    width: 200rpx;
    margin: 0 auto;
    text-align: center;
    line-height: 45rpx;
    color: #f77037;
    margin-top: 20rpx;
}

.service-box {
    width: 710rpx;
    margin: 0 auto;
    position: relative;
    left: 0;
    top: -90rpx;
    z-index: 0;
    border-radius: 20rpx;
    overflow: hidden;
}

.service-box .service-list:last-child {
    margin: 0;
}

.service-box .service-list {
    background: #fff;
    margin-bottom: 10rpx;
    padding: 30rpx 40rpx;
}

.service-head {
    display: flex;
    align-items: center;
    font-size: 30rpx;
    font-weight: bold;
}

.service-address text {
    color: #d4d4d4;
}

.address {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: 10rpx;
    width: 540rpx;
}

.service-head image {
    width: 35rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

.service-bottom {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 10rpx;
}

.service-dis {
}

.dis-text {
    color: #aaa;
}

.dis-detail {
    color: #f78162;
    font-weight: bold;
    margin-top: 10rpx;
}