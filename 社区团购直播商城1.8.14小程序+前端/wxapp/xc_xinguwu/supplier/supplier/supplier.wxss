page {
    background: #fff;
}

.supplier-banner {
    background: #fe4025;
    padding: 50rpx 0 90rpx 0;
    text-align: center;
    color: #fff;
}

.supplier-banner image {
    width: 150rpx;
    height: 150rpx;
    margin: 0 auto;
    border-radius: 50%;
}

.user-name {
    font-size: 28rpx;
    margin-top: 10rpx;
}

.today-detail {
    display: flex;
    align-items: center;
    width: 705rpx;
    margin: 0 auto;
    margin-top: -70rpx;
    background: #fff;
    border-radius: 10rpx;
    box-shadow: 0rpx 3rpx 6rpx 0rpx rgba(59,59,59,0.12);
}

.id {
    font-size: 20rpx;
}

.today-detail {
    padding: 30rpx 0;
}

.detail-list {
    width: 49.9%;
    text-align: center;
}

.detail-line {
    height: 62rpx;
    width: 1rpx;
    background: #e1e1e1;
}

.detail-num {
    font-size: 32rpx;
}

.list-name {
    font-size: 20rpx;
    color: #888;
    margin-top: 10rpx;
}

.supplier-nav {
    display: flex;
    align-items: center;
    padding: 35rpx 0;
}

.nav-list {
    width: 25%;
    text-align: center;
    font-size: 24rpx;
}

.nav-list image {
    margin: 0 auto;
}

.supplier1 {
    width: 48rpx;
    height: 50rpx;
    padding-bottom: 10rpx;
}

.supplier2 {
    width: 42rpx;
    height: 42rpx;
    padding-bottom: 18rpx;
}

.supplier3 {
    width: 33rpx;
    height: 40rpx;
    padding-bottom: 20rpx;
}

.supplier4 {
    width: 40rpx;
    height: 40rpx;
    padding-bottom: 20rpx;
}

.cut-line {
    height: 20rpx;
    background: #f9f9f9;
}

.product-head {
    display: flex;
    padding: 30rpx 20rpx;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #EBEBEB;
}

.my-product {
    font-size: 28rpx;
}

.see-more {
    font-size: 24rpx;
    letter-spacing: 5rpx;
    color: #888;
}

.porduct-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 27rpx 25rpx;
    border-bottom: 1rpx solid #EBEBEB;
}

.porduct-list image {
    width: 138rpx;
    height: 115rpx;
}

.list-content {
    width: 530rpx;
    font-size: 24rpx;
}

.content-name {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    max-height: 60rpx;
    line-height: 30rpx;
}

.content-bottom {
    display: flex;
    align-items: center;
    margin-top: 20rpx;
}

.content-bottom text {
    color: #FC1F1F;
}

.inventory {
    margin-left: 30rpx;
    color: #888;
}

.bottom-line {
    height: 50rpx;
}