<form bindsubmit="myfrom">
    <view class="apply-box">
        <view class="apply-title">请填写申请信息</view>
        <view class="apply-ipt">
            <input name="name" placeholder="输入你的姓名"></input>
        </view>
        <view class="apply-ipt">
            <input bindinput="phoneNum" name="phone" placeholder="输入你的手机号码" type="number" value="{{phoneNum}}"></input>
        </view>
        <button class="submit" formType="submit">提交申请</button>
    </view>
</form>
