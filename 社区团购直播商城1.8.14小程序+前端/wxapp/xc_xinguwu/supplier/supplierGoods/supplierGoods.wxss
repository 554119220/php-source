page {
    background: #fff;
}

.goods-box {
    border-top: 20rpx solid #f9f9f9;
    padding: 0 25rpx;
}

.goods-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 25rpx 0;
    border-bottom: 1rpx solid #ebebeb;
    font-size: 24rpx;
}

.goods-list image {
    width: 138rpx;
    height: 115rpx;
}

.goods-content {
    width: 530rpx;
}

.godds-name {
    max-height: 60rpx;
    line-height: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.content-bottom {
    display: flex;
    align-items: center;
    margin-top: 10rpx;
}

.sailed {
    margin-right: 30rpx;
}

.sailed text {
    color: #FC1F1F;
}