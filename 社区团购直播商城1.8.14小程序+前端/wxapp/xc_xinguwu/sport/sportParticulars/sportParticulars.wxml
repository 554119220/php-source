<view class="sportParticulars-box">
    <view class="list-box">
        <view class="list" wx:for="{{list}}">
            <view class="detail-left">
                <view class="name">{{item.title}}</view>
                <view class="date">{{item.createtime}}</view>
            </view>
            <view class="cost" wx:if="{{item.fee<=0}}">-{{item.fee}}</view>
            <view class="get" wx:else>+{{item.fee}}</view>
        </view>
    </view>
</view>
