.content-out {
    position: absolute;
    height: 150rpx;
    width: 166.6666rpx;
    background-color: #f5f0fc;
    border-radius: 15rpx;
    box-shadow: 0 5px 0 #b0adae;
    box-sizing: border-box;
    border: 8rpx solid transparent;
}

.start-btn {
    border-radius: 15rpx;
    height: 145rpx;
    width: 166.6666rpx;
    background-color: #ffe400;
    color: #f6251e;
    text-align: center;
    font-weight: bolder;
    position: absolute;
    left: 205rpx;
    top: 190rpx;
    overflow: hidden;
    box-shadow: 0 5px 0 #af9505;
}

.select-content {
    box-sizing: border-box;
    border: 8rpx solid #ffe368;
}

.start-btn-box {
    width: 100%;
    height: 100%;
    position: relative;
}

.start-btn-bg {
    width: 100%;
    height: 100%;
}

.start-btn-box view {
    width: 100%;
    height: 50rpx;
