}

.text {
    font-size: 3vw;
    color: #fe9292;
    width: 85%;
    margin: 0 auto;
    margin-top: 10rpx;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
