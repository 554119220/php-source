.l-nav {
    display: flex;
    align-items: center;
    height: 90rpx;
    line-height: 90rpx;
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    background: #fff;
}

.l-nav-list {
    width: 50%;
    text-align: center;
}

.on text {
    color: #f93d3d;
    border-bottom: 2rpx solid #f93d3d;
    padding-bottom: 24rpx;
}

.line {
    height: 92rpx;
}

.ver-box {
    background: #fff;
    padding: 0 20rpx;
}

.ver-list {
    padding: 30rpx 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #e3e3e3;
}

.ver-list image {
    width: 155rpx;
    height: 140rpx;
    border-radius: 10rpx;
}

.ver-content {
    width: 520rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.content-name {
    font-size: 28rpx;
}

.content-date {
    color: #888;
    font-size: 3vw;
    margin-top: 20rpx;
}

.ver-code {
    width: 115rpx;
    height: 50rpx;
    border-radius: 25rpx;
    line-height: 50rpx;
    text-align: center;
    color: #f93d3d;
    border: 1rpx solid #f93d3d;
}

.change {
    color: #f93d3d;
    margin-top: -60rpx;
}

.model {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.4);
    z-index: 5;
}

.ver-code-box {
    width: 545rpx;
    height: 695rpx;
    margin: 0 auto;
    margin-top: 180rpx;
    position: relative;
}

.ver-code-box .code-bg {
    width: 100%;
    height: 100%;
}

.code-main {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

.code-head {
    height: 145rpx;
    display: flex;
    line-height: 145rpx;
    padding: 0 50rpx;
    justify-content: space-between;
    align-items: center;
}

.code-title {
    width: 30rpx;
    line-height: 24rpx;
    text-align: center;
    font-size: 3vw;
    margin-bottom: 15rpx;
}

.code-head-right {
    display: flex;
    align-items: baseline;
    width: 400rpx;
    justify-content: space-between;
    letter-spacing: 5rpx;
}

.copy {
    color: #ff201a;
    text-align: right;
}

.model .my-code {
    width: 390rpx;
    height: 390rpx;
    margin: 0 auto;
    margin-top: 47rpx;
    border: 1rpx solid;
    border-radius: 0;
}

.hint {
    text-align: center;
    margin-top: 20rpx;
}

.ask-for-head {
    display: flex;
    align-items: center;
    padding: 30rpx 35rpx;
    border-bottom: 1rpx solid #ecebeb;
    justify-content: space-between;
}

.ask-for-head image {
    width: 27rpx;
    height: 27rpx;
}

.ask-for-main {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 30rpx 35rpx;
}

.ask-for-main image {
    width: 145rpx;
    height: 145rpx;
    border-radius: 20rpx;
}

.ask-for-content {
    width: 370rpx;
}

.ask-for-name {
    font-weight: bold;
}

.ask-for-content-list {
    display: flex;
    align-items: center;
    color: #888;
    margin-top: 5rpx;
}

.ask-for-user {
    display: flex;
    align-items: center;
}

.ask-for-user view {
    width: 14rpx;
}

.ask-for-address {
    padding: 25rpx 35rpx;
    border-top: 1rpx solid #e4e4e4;
}

.model-box {
    width: 620rpx;
    margin: 0 auto;
    background: #fff;
    border-radius: 10rpx;
    margin-top: 100rpx;
    padding-bottom: 40rpx;
}

.get-method {
    display: flex;
    align-items: center;
    padding: 30rpx 35rpx;
    border-top: 1rpx solid #e4e4e4;
    justify-content: space-between;
}

.get-method-item {
    display: flex;
}

.method-box {
    display: flex;
    align-items: center;
}

.method-list {
    display: flex;
    align-items: center;
    margin-right: 35rpx;
    color: #b2b1b1;
}

.method-list icon {
    margin-right: 15rpx;
}

.get-on {
    color: #ff263a;
}

.address-box {
    display: flex;
    align-items: center;
    padding: 30rpx 35rpx;
    border-top: 1rpx solid #e4e4e4;
}

.address-detail {
    display: flex;
    align-items: center;
    width: 410rpx;
    justify-content: space-between;
}

.address-detail view {
    width: 340rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.address-box image {
    width: 13rpx;
    height: 22rpx;
}

.m-address-box {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    padding: 30rpx 35rpx;
    border-top: 1rpx solid #e4e4e4;
}

.m-address-box .m-add-tell image {
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    margin-left: 20rpx;
}

.m-address-content {
    width: 410rpx;
}

.m-add-name {
    font-weight: bold;
    font-size: 28rpx;
}

.m-address-content-head {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.m-add-tell {
    display: flex;
    align-items: center;
}

.m-address-detail {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 10rpx;
}

.m-address-box .m-address-detail image {
    width: 15rpx;
    height: 20rpx;
    margin-left: 25rpx;
}

.m-address-detail view {
    width: 390rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.remark-box {
    display: flex;
    align-items: center;
    padding: 30rpx 35rpx;
    border-top: 1rpx solid #e4e4e4;
    border-bottom: 1rpx solid #e4e4e4;
}

.remark-left {
    display: flex;
    align-items: center;
}

.remark-left view {
    width: 55rpx;
}

.remark-box input {
    height: 40rpx;
    line-height: 40rpx;
    min-height: 40rpx;
}

.make-sure-get {
    width: 560rpx;
    height: 90rpx;
    background: #f93d3d;
    color: #fff;
    text-align: center;
    line-height: 90rpx;
    margin: 0 auto;
    margin-top: 40rpx;
    border-radius: 45rpx;
}