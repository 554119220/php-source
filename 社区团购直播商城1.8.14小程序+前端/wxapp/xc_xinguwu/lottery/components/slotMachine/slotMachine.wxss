.machine_wp {
    display: flex;
    margin-left: 55rpx;
    margin-top: 220rpx;
}

.machine_wp .machine_item_wp {
    width: 160rpx;
    height: 210rpx;
    position: relative;
    border-radius: 2px;
    overflow: hidden;
    margin-right: 20rpx;
}

.machine_wp .machine_item_wp:last-child {
    margin-right: 0;
}

.machine_wp .machine_item_wp .machine_item_slide {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}

.machine_wp .machine_item_wp .machine_item_ele {
    width: 150rpx;
    height: 180rpx;
    color: #facf3d;
    text-align: center;
    padding-top: 30rpx;
}

.machine_item_ele image {
    width: 30rpx;
    height: 65rpx;
    margin: 0 auto;
    margin-top: 20rpx;
}