<template name="scratch">
    <view class="scratch_wp" style="position:relative;width: {{scratch.width}}px; height: {{scratch.height}}px;margin: 0 auto;z-index:5">
        <canvas bindtouchend="touchEnd" bindtouchmove="touchMove" bindtouchstart="touchStart" canvasId="scratch" disableScroll="{{isScroll}}" style="margin: 0 auto;width: {{scratch.width}}px;height: {{scratch.height}}px;margin-top:20rpx; background: transparent"></canvas>
        <view class="scratch_txt" style="position:absolute;top:0;left:0;width: 100%;height:100%;text-align: center;line-height: {{scratch.awardTxtLineHeight}}px;color: {{scratch.awardTxtColor}};font-size: {{scratch.awardTxtFontSize}};pointer-events:none;z-index: -1">{{scratch.awardTxt}}
    </view>
    </view>
</template>
