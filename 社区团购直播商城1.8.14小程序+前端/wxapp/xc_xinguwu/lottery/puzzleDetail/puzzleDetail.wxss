.puzzle-detail {
    width: 100%;
    height: 100vh;
    position: relative;
}

.puzzle-bg {
    width: 100%;
    height: 100vh;
}

.puzzle-main {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

.puzzle-chance {
    width: 320rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    color: #fff;
    font-size: 28rpx;
    letter-spacing: 5rpx;
    background: #febc26;
    border-radius: 35rpx;
    position: fixed;
    top: 80rpx;
    left: 215rpx;
}

.progress {
    width: 600rpx;
    height: 40rpx;
    border-radius: 25rpx;
    position: fixed;
    left: 75rpx;
    top: 260rpx;
    background: #ff3064;
    overflow: hidden;
    border: 5rpx solid #FF3064;
}

.progress-img {
    width: 600rpx;
    height: 40rpx;
    position: absolute;
    top: 0rpx;
    border-radius: 25rpx;
}

.puzzle-box {
    width: 710rpx;
    height: 835rpx;
    margin: 0 auto;
    background: #ff6845;
    border-radius: 30rpx;
    padding-top: 10rpx;
    margin-top: 40rpx;
}

.game-box1 image {
    width: 640rpx;
    height: 640rpx;
    border-radius: 20rpx;
}

.puzzle-box-bg {
    position: absolute;
    top: 180rpx;
    width: 700rpx;
    height: 820rpx;
    background: #ffbed0;
    left: 15rpx;
    border-radius: 20rpx;
    border: 10rpx solid #ff6845;
}

.border {
    width: 670rpx;
    height: 660rpx;
    position: absolute;
    left: 40rpx;
    top: 320rpx;
    background: #ff6845;
    border-radius: 20rpx;
}

.count {
    color: #fff;
    text-align: center;
    padding-top: 20rpx;
    letter-spacing: 5rpx;
}

.game-box {
    background: #ff6845;
    border-radius: 20rpx;
    overflow: hidden;
}

.tui-puzzle-li {
    width: 33.33%;
    height: 33.33%;
    position: absolute;
    box-sizing: border-box;
    overflow: hidden;
    border-radius: 20rpx;
    color: #ff6845;
}

.btn-bottom {
    width: 550rpx;
    height: 88rpx;
    position: fixed;
    left: 100rpx;
    bottom: 58rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.btn-bottom image {
    width: 245rpx;
    height: 88rpx;
}

.puzzle-shadow {
    width: 100%;
    height: 100vh;
    background: rgba(32,32,32,0.4);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 11;
}

.prize-box {
    width: 570rpx;
    height: 530rpx;
    position: absolute;
    left: 90rpx;
    top: 290rpx;
    z-index: 11;
}

.prize-box-main {
    width: 100%;
    height: 100%;
    position: relative;
}

.prize-bg {
    width: 100%;
    height: 100%;
}

.prize-content {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

.clost-img {
    width: 26rpx;
    height: 26rpx;
    position: absolute;
    right: 45rpx;
    top: 50rpx;
}

.prize-title {
    text-align: center;
    margin-top: 55rpx;
    color: #fd3062;
    font-weight: bold;
    letter-spacing: 5rpx;
    font-size: 40rpx;
}

.all-prize {
    width: 370rpx;
    margin: 0 auto;
    margin-top: 20rpx;
    height: 340rpx;
}

.prize-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 30rpx;
}

.prize-name {
    color: #fd3062;
}

.prize-right {
    display: flex;
    align-items: center;
    width: 230rpx;
    justify-content: flex-end;
    color: #fe901e;
}

.prize-right image {
    width: 83rpx;
    height: 40rpx;
    margin-left: 10rpx;
}

.puzzle-rank {
    width: 570rpx;
    height: 610rpx;
    position: fixed;
    left: 90rpx;
    top: 250rpx;
    z-index: 11;
}

.rank-main {
    width: 100%;
    height: 100%;
    position: relative;
}

.rank-bg {
    width: 570rpx;
    height: 610rpx;
}

.rank-content {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

.all-rank {
    margin-top: 30rpx;
    height: 360rpx;
}

.rank-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 40rpx;
    margin-top: 20rpx;
}

.m-rank-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 10rpx 40rpx;
    box-shadow: 0px -2rpx 3rpx 0rpx rgba(64,64,64,0.1);
    margin-top: 0rpx;
}

.rank-title {
    text-align: center;
    margin-top: 55rpx;
    color: #fd3062;
    font-weight: bold;
    letter-spacing: 5rpx;
    font-size: 40rpx;
}

.list-left {
    display: flex;
    align-items: center;
}

.list-left image {
    width: 48rpx;
    height: 48rpx;
    border-radius: 50%;
}

.list-left view {
    width: 200rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list-right {
    display: flex;
    align-items: center;
    width: 220rpx;
    justify-content: space-between;
}

.normal-rank {
    color: #888;
}

.red-rank {
    color: #fd3062;
}

.second {
    color: #febc26;
}

.finished {
    width: 540rpx;
    height: 380rpx;
    position: fixed;
    left: 105rpx;
    top: 325rpx;
    z-index: 11;
}

.finished-content {
    width: 100%;
    height: 100%;
    position: relative;
}

.toast-bg {
    width: 100%;
    height: 100%;
}

.finished-main {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.close-img {
    width: 26rpx;
    height: 26rpx;
    position: absolute;
    right: 30rpx;
    top: 30rpx;
}

.finish-title {
    text-align: center;
    font-size: 36rpx;
    color: #ff3064;
    margin-top: 45rpx;
    margin-bottom: 30rpx;
}

.finish-list {
    display: flex;
    align-items: center;
    padding-left: 150rpx;
    margin-bottom: 20rpx;
}

.finish-list view:nth-child(1) {
    color: #888;
}

.finish-list view:nth-child(2) {
    color: #ff3064;
    font-size: 30rpx;
}

.finish-bottom {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 78rpx;
    margin-top: 30rpx;
}

.finish-bottom button {
    width: 160rpx;
    height: 65rpx;
    padding: 0;
    color: #fff;
    line-height: 65rpx;
    font-size: 3.5vw;
}

.invite {
    background: #FF46E8;
}

.play-again {
    background: #FF3064;
}

.failed-text {
    width: 350rpx;
    text-align: center;
    margin: 0 auto;
    margin-top: 45rpx;
    color: #888;
}

.failed-text text {
    color: #FF3064;
}

.chance-text {
    color: #888;
    width: 330rpx;
    margin: 0 auto;
    margin-top: 115rpx;
    font-size: 26rpx;
    text-align: center;
    margin-bottom: 60rpx;
}