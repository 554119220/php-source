.team-tab-box-title {
    position: fixed;
    left: 0;
    top: 0;
    display: flex;
    background: #fff;
    z-index: 5;
    width: 100%;
    border-top: 1rpx solid #f9f9f9;
}

.team-tab-nav {
    display: inline-block;
    width: 33.3%;
    height: 90rpx;
    line-height: 90rpx;
    box-sizing: border-box;
    text-align: center;
    color: #444;
    background-color: #fff;
}

.on view {
    width: 30%;
    margin: 0 auto;
    color: #f23921;
    border-bottom: 2rpx solid #f23921;
    height: 85rpx;
}

.on1 view {
    color: #f23921;
}

.nav-box {
    height: 130rpx;
    width: 100%;
    background: #f8f8f8;
    position: fixed;
    top: 90rpx;
    left: 0;
    display: flex;
    align-items: center;
    z-index: 5;
}

.all-nav {
    display: flex;
    height: 90rpx;
    width: 94%;
    margin: 0 auto;
    background: #fff;
    line-height: 90rpx;
    border-radius: 15rpx;
}

.all-nav .nav:last-child {
    margin: 0;
}

.nav {
    width: 33%;
    text-align: center;
    color: #888;
    margin-right: 23%;
}

.nav image {
    width: 10rpx;
    height: 20rpx;
}

.nav view {
    text-align: center;
}

.contribution view {
    margin-left: 100rpx;
}

.sort view {
    display: flex;
    align-items: center;
    margin: 0 auto;
    margin-left: 110rpx;
}

.sort view image {
    margin-left: 20rpx;
}

.all-team {
    position: absolute;
    left: 0;
    width: 100%;
    top: 220rpx;
}

.team-list {
    display: flex;
    align-items: center;
    margin: 0 auto;
    border-radius: 15rpx;
    margin-bottom: 20rpx;
    background: #fff;
    width: 94%;
    padding: 20rpx 0;
}

.team-list image {
    width: 105rpx;
    height: 105rpx;
    margin-left: 20rpx;
}

.content {
    display: flex;
    width: 76%;
    margin-left: 20rpx;
    align-items: center;
    justify-content: space-between;
}

.name {
    width: 160rpx;
}

.content-sum {
    width: 240rpx;
    border-left: 1rpx solid #e3e3e3;
    padding-left: 30rpx;
}