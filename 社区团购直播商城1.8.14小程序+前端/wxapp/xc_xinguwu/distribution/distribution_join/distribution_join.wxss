.line {
    height: 20rpx;
}

.header {
    width: 94%;
    margin: 0 auto;
    background: #fff;
    border-radius: 15rpx;
    padding-bottom: 50rpx;
}

.lead {
    margin-left: 20rpx;
    padding: 20rpx 0;
}

.lead text {
    color: #f04043;
}

.in {
    width: 90%;
    margin: 0 auto;
    padding: 10rpx 0;
    background: #f6f6f6;
    margin-bottom: 20rpx;
    border-radius: 15rpx;
}

.in input {
    width: 90%;
    margin: 0 auto;
    height: 35rpx;
}

.message {
    width: 90%;
    margin: 0 auto;
    display: flex;
    align-items: center;
}

.mess {
    width: 70%;
    background: #f6f6f6;
    padding: 10rpx 0;
    border-radius: 15rpx;
}

.mess input {
    width: 85%;
    margin: 0 auto;
}

.get {
    width: 25%;
    background: #f6f6f6;
    height: 72rpx;
    line-height: 72rpx;
    border-radius: 15rpx;
    margin-left: 5%;
    text-align: center;
}

.apply {
    width: 560rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    background: #f04043;
    margin: 0 auto;
    margin-top: 65rpx;
    border-radius: 15rpx;
}

button::after {
    border: 0;
}

.privilege {
    width: 94%;
    margin: 0 auto;
    margin-top: 20rpx;
    background: #fff;
    border-radius: 15rpx;
}

.privilege-title {
    padding: 20rpx 0;
    margin-left: 20rpx;
}

.privilege-list {
    display: flex;
    width: 90%;
    margin: 0 auto;
    align-items: center;
    padding: 15rpx 0;
}

.privilege-list image {
    width: 180rpx;
}

.content view:nth-child(1) {
    font-weight: bold;
}

.content view:nth-child(2) {
    color: #888;
}

.attention {
    padding: 15rpx;
    border-top: 1rpx solid #eaeaea;
    color: #888;
    padding-bottom: 30rpx;
}

.hint {
    position: fixed;
    left: 10%;
    top: 15vh;
    z-index: 5;
    background: #fff;
    width: 80%;
    text-align: center;
}

.close {
    width: 60rpx;
    height: 60rpx;
    position: fixed;
    right: 8%;
    top: 13vh;
}

.success {
    width: 130rpx;
    height: 130rpx;
    border-radius: 130rpx;
    margin: 0 auto;
    margin-top: 130rpx;
}

.success-title {
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    color: #f04043;
}

.congratulation {
}

.sure {
    margin: 0 auto;
    margin-top: 100rpx;
    width: 325rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    background: #f04043;
    border-radius: 15rpx;
    margin-bottom: 100rpx;
}

.deal {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 30rpx;
}

.circle {
    height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.read {
    display: flex;
    align-items: center;
    margin-left: 10rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.read text {
    color: #f04043;
}

.deal-model {
    position: fixed;
    background: #fff;
    border-radius: 10rpx;
    width: 660rpx;
    left: 45rpx;
    top: 115rpx;
    z-index: 5;
    padding: 20rpx 0;
    border: 1rpx solid #eee;
}

.model-title {
    text-align: center;
    padding: 40rpx;
    color: #f04043;
    margin-top: 20rpx;
}

.deal-list {
    margin-bottom: 35rpx;
    padding: 0 40rpx;
}

.accept {
    margin: 0 auto;
    margin-top: 10rpx;
    width: 445rpx;
    height: 80rpx;
    text-align: center;
    color: #fff;
    letter-spacing: 5rpx;
    background: #f04043;
    line-height: 80rpx;
    border-radius: 10rpx;
}

.think {
    text-align: center;
    padding: 20rpx 0;
}

.code-btn {
    width: 120rpx;
    padding: 0rpx;
    margin: 0rpx;
    float: right;
}

.in .group {
    width: 90%;
    margin: 0 auto;
    height: 55rpx;
}

.in input.phone {
    width: 330rpx;
    float: left;
    text-align: left;
    margin: 0rpx;
}

.clearboth {
    clear: both;
}