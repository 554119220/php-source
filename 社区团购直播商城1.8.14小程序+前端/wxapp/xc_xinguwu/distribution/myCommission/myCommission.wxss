.line {
    height: 25rpx;
}

.top-money .brokerage {
    border: 0;
}

.top-money {
    width: 94%;
    margin: 0 auto;
    border-radius: 15rpx;
    background: #fff;
    overflow: hidden;
}

.use-price {
    color: #f04043;
}

.canUse {
    display: flex;
    align-items: center;
    padding: 25rpx 25rpx;
    border-bottom: 1rpx solid #e8e8e8;
    justify-content: space-between;
}

.finish {
    width: 94%;
    margin: 0 auto;
    border-radius: 15rpx;
    background: #fff;
    overflow: hidden;
    margin-top: 20rpx;
}

.finish .finish-list:last-child {
    border: 0;
}

.finish-list {
    display: flex;
    align-items: center;
    background: #fff;
    padding: 25rpx;
    border-bottom: 1rpx solid #e8e8e8;
    justify-content: space-between;
}

.brokerage {
    display: flex;
    align-items: center;
    background: #fff;
    padding: 25rpx;
    border-bottom: 1rpx solid #e8e8e8;
    justify-content: space-between;
}

.bottom-price .brokerage:last-child {
    border: 0;
}

.bottom-price {
    width: 94%;
    margin: 0 auto;
    border-radius: 15rpx;
    background: #fff;
    overflow: hidden;
}

.knockdown {
    width: 94%;
    margin: 0 auto;
    padding: 30rpx 0;
    color: #888;
}

.toDeposit {
    width: 550rpx;
    height: 80rpx;
    line-height: 80rpx;
    position: fixed;
    left: 100rpx;
    background: #f04043;
    bottom: 200rpx;
    text-align: center;
    color: #fff;
    border-radius: 15rpx;
}