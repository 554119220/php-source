.line {
    height: 20rpx;
}

.head {
    background: #fff;
    width: 94%;
    margin: 0 auto;
    border-radius: 15rpx;
    overflow: hidden;
}

.detail {
    text-align: right;
    width: 90%;
    margin: 0 auto;
    padding-top: 20rpx;
}

.bag {
    width: 250rpx;
    height: 160rpx;
    margin: 0 auto;
}

.usable {
    text-align: center;
    margin-top: 10rpx;
    color: #f04043;
    height: 40rpx;
    line-height: 40rpx;
}

.info {
    text-align: center;
    height: 20rpx;
    line-height: 20rpx;
    margin-top: 10rpx;
    color: #999;
    padding-bottom: 50rpx;
}

.money {
    display: flex;
    background: #fff;
    align-items: center;
    border-top: 1rpx solid #dedede;
    height: 160rpx;
}

.total {
    width: 49.9%;
    height: 100rpx;
    border-right: 1rpx solid #e0e0e0;
    text-align: center;
}

.total view:nth-child(2) {
    margin: 0 auto;
    margin-top: 20rpx;
    color: #f04043;
}

.used {
    width: 49.9%;
    height: 100rpx;
    text-align: center;
}

.used view:nth-child(2) {
    margin: 0 auto;
    margin-top: 20rpx;
    color: #f04043;
}

.input-wechat {
    background: #fff;
    padding: 20rpx 0;
    border-radius: 15rpx;
    width: 94%;
    margin: 0 auto;
    margin-top: 60rpx;
}

.input {
    background: #fff;
    padding: 20rpx 0;
    border-radius: 15rpx;
    width: 94%;
    margin: 0 auto;
    margin-top: 20rpx;
}

.shuru {
    display: flex;
    align-items: center;
    width: 90%;
    margin: 0 auto;
}

input {
    margin-left: 40rpx;
    width: 480rpx;
}

.lint {
    width: 94%;
    margin: 0 auto;
    color: #c8c8c8;
    padding: 15rpx 0;
}

.apply {
    width: 560rpx;
    height: 80rpx;
    text-align: center;
    margin: 0 auto;
    line-height: 80rpx;
    color: #fff;
    background: #f04043;
    margin-top: 40rpx;
    margin-bottom: 40rpx;
    border-radius: 15rpx;
}

.close {
    position: fixed;
    right: 8.5%;
    top: 13.5vh;
    z-index: 6;
    width: 60rpx;
    height: 60rpx;
    border-radius: 60rpx;
}

.models {
    position: fixed;
    left: 10%;
    top: 15vh;
    width: 80%;
    background: #fff;
    border-radius: 15rpx;
    overflow: hidden;
    padding-bottom: 30rpx;
    z-index: 5;
}

.model-head {
    background: url('data:image/png;base64,');
    background-size: 100% 100%;
    text-align: center;
    color: #fff;
    height: 300rpx;
}

.success {
    margin: 0 auto;
    width: 130rpx;
    height: 130rpx;
    border-radius: 130rpx;
}

.model-line {
    height: 60rpx;
}

.head-title {
    margin-top: 30rpx;
}

.model-info {
    margin: 0 auto;
    margin-top: 40rpx;
    width: 90%;
}

.list {
    display: flex;
    padding: 15rpx 0;
    font-weight: bold;
}

.list view:nth-child(1) {
    margin-right: 40rpx;
}

.makesure {
    width: 325rpx;
    height: 80rpx;
    background: #f04043;
    color: #fff;
    border-radius: 15rpx;
    text-align: center;
    line-height: 80rpx;
    margin: 0 auto;
    margin-top: 30rpx;
}