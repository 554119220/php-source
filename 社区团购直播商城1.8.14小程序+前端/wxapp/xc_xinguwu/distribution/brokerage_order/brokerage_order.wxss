.team-tab-box-title {
    display: flex;
    height: 90rpx;
    width: 100%;
    line-height: 90rpx;
    border-top: 1rpx solid #f8f8f8;
    background: #fff;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 5;
}

.team-tab-nav {
    width: 33.3%;
    text-align: center;
}

.on {
    color: #f04043;
}

.team-tab-box-title .on view {
    width: 40%;
    height: 85rpx;
    margin: 0 auto;
    border-bottom: 2rpx solid #f04043;
}

.out {
    width: 100%;
    background: #f8f8f8;
    position: fixed;
    top: 90rpx;
    z-index: 5;
}

.weui-search-bar {
    background: #f8f8f8;
    border: 0;
}

.weui-search-bar__form {
    border: 0;
}

.weui-search-bar__input {
    border: 0;
    height: 70rpx;
    line-height: 70rpx;
    align-items: center;
}

.weui-icon-search {
    margin-top: 10rpx;
}

.weui-icon-search_in-box {
    position: absolute;
    left: 10px;
    top: 24rpx;
}

.weui-search-bar__cancel-btn {
    margin-left: 10px;
    line-height: 70rpx;
    color: #09bb07;
    white-space: nowrap;
    margin-top: 5rpx;
}

.weui-icon-clear {
    position: absolute;
    top: 8rpx;
    right: 0;
    padding: 7px 8px;
    font-size: 0;
}

.searchbar-result {
    margin-top: 0;
    font-size: 14px;
}

.searchbar-result:before {
    display: none;
}

.weui-cell {
    padding: 12px 15px 12px 35px;
}

.search {
    width: 94%;
    background: #fff;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 15rpx;
    margin-top: 20rpx;
}

.main-search {
    display: flex;
    align-items: center;
    color: #bcbcbc;
    width: 45%;
    margin: 0 auto;
}

.main-search image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 30rpx;
}

.order {
    width: 94%;
    position: absolute;
    left: 3%;
    top: 110rpx;
    z-index: 4;
    margin-bottom: 50rpx;
}

.order-list {
    margin-top: 20rpx;
    background: #fff;
    border-radius: 15rpx;
}

.order .order-list:first-child {
    margin: 0;
}

.total {
    width: 90%;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
    padding-top: 20rpx;
}

.pay {
    display: flex;
    justify-content: space-between;
    padding: 5rpx 0;
    width: 90%;
    margin: 0 auto;
    margin-top: 10rpx;
    align-items: center;
}

.time {
    width: 90%;
    margin: 0 auto;
    display: flex;
    color: #999;
}

.commission {
    display: flex;
    border: 1rpx solid #f04043;
    border-radius: 10rpx;
    color: #f04043;
    text-align: center;
    align-items: center;
}

.commission view:nth-child(1) {
    width: 76rpx;
    background: #f04043;
    color: #fff;
    text-align: center;
}

.commission view:nth-child(2) {
    text-align: center;
    padding-right: 6rpx;
}

.info {
    text-align: right;
    padding: 15rpx 30rpx;
    border-top: 1rpx solid #dbdbdb;
    margin-top: 20rpx;
}

.state {
    text-align: right;
}

.close {
    width: 55rpx;
    height: 55rpx;
    position: fixed;
    right: 55rpx;
    top: 28.5vh;
}

.model {
    position: fixed;
    left: 10%;
    top: 30vh;
    background: #fff;
    z-index: 5;
    width: 80%;
    border-radius: 15rpx;
}

.id {
    display: flex;
    margin: 0 auto;
    margin-top: 65rpx;
    align-items: center;
    width: 90%;
    justify-content: space-between;
}

.id-num {
    font-weight: bold;
}

.price {
    color: #f04043;
}

.back {
    width: 90%;
    margin: 0 auto;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 5rpx;
}

.c-time {
    color: #999;
}