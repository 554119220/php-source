}

.list input {
    margin-left: 30rpx;
}

.active {
    color: red;
}

button {
    width: 65%;
    position: absolute;
    bottom: 140rpx;
    height: 70rpx;
    line-height: 70rpx;
    left: 17.5%;
    color: #fff;
    background: red;
    letter-spacing: 5rpx;
}

.wechat {
    text-align: center;
    width: 65%;
