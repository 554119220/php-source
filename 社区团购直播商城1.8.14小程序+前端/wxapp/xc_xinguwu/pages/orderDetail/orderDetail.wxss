.order-state {
    text-align: center;
    background: #fff;
    border-top: 1rpx solid #fafafa;
    padding: 20rpx;
}

.position {
    padding: 20rpx;
    background: #fff;
    margin-top: 20rpx;
}

.position-head {
    display: flex;
    align-items: center;
}

.position-head image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.pos-content {
    margin-left: 60rpx;
}

.order {
    margin-top: 20rpx;
    background: #fff;
}

.list {
    display: flex;
}

.img {
    width: 34%;
    height: 230rpx;
    padding: 20rpx 0 20rpx 20rpx;
}

.img navigator {
    width: 100%;
    height: 100%;
}

.img navigator image {
    width: 100%;
    height: 100%;
}

.neirong {
    margin-top: 2rpx;
}

.content {
    width: 60%;
    padding: 20rpx 0 20rpx 25rpx;
}

.pro-title {
    padding: 10rpx 10rpx 0rpx 0rpx;
    flex-wrap: wrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.num-price {
    display: flex;
    margin-top: 25rpx;
}

.color {
    margin-top: 20rpx;
}

.money {
    display: flex;
}

.new {
    color: red;
    width: 140rpx;
}

.old {
    text-decoration: line-through;
    line-height: 50rpx;
    color: #999;
    width: 140rpx;
}

.totalsum {
}

.all_money {
    background: #fff;
    padding: 20rpx;
}

.price {
    padding: 10rpx 0;
}

.shuoming {
    float: left;
}

.det {
    text-align: right;
}

.order-info {
    padding: 30rpx;
    background: #fff;
    margin-top: 20rpx;
    color: #666;
}

.order-info view {
    margin-top: 10rpx;
}

.bottom-line {
    height: 90rpx;
}

.bottom {
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 80rpx;
    display: flex;
    align-items: center;
}

.bottom-list {
    width: 90%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin: 0 auto;
}

.bottom-list view {
    border: 1rpx solid #e8e8e8;
    width: 140rpx;
    text-align: center;
    border-radius: 30rpx;
    color: #888;
}

.mylogistics {
    width: 90%;
    background: #fff;
    position: fixed;
    top: 15vh;
    left: 5%;
    z-index: 5;
    border-radius: 15rpx;
    padding: 20rpx 0;
}

.mylogistics .express-name {
    width: 90%;
    margin: 0 auto;
    padding: 20rpx 0;
    position: relative;
    border-bottom: 1rpx solid #ebebeb;
}

.mylogistics .close-mylogistics {
    position: absolute;
    right: 0rpx;
    top: 10rpx;
}

.mylogistics .position {
    display: flex;
    width: 90%;
    padding: 20rpx 0;
}

.mylogistics .point {
    width: 10%;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
}

.mylogistics .pos-detail {
    width: 90%;
    text-align: left;
}

.all-logistics {
    color: #999;
}

.all-logistics .logistics-list:nth-child(1) {
    color: #ee5e7b;
}

.club {
    background: #fff;
    padding: 20rpx;
}

.club .club-top {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #eee;
}

.club .club-top .club-top-left {
    color: #444;
    font-size: 4vw;
    width: 480rpx;
}

.club .club-top .club-top-right {
    display: flex;
    color: #444;
    align-items: center;
}

.club .club-top .club-top-right image {
    width: 24rpx;
    margin-left: 20rpx;
}

.club .club-bottom {
    display: flex;
    justify-content: space-between;
    padding: 20rpx 0;
    align-items: flex-start;
}

.club .club-bottom .club-bottom-left {
    color: #444;
    width: 670rpx;
}

.club .club-bottom image {
    width: 24rpx;
}

.qrcode {
    width: 560rpx;
    height: 560rpx;
    margin: 0 auto;
    background: #fff;
    position: relative;
    top: 200rpx;
    padding: 20rpx;
    border-radius: 10rpx;
}

.qrcode image {
    width: 100%;
    height: 100%;
}