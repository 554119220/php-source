.top {
    height: 20rpx;
    text-align: right;
}

.clear-cart {
    width: 100%;
    height: 60rpx;
    background-color: #eff0f0;
}

.clear-cart text {
    color: #dadbdb;
    float: right;
    padding-right: 20rpx;
    line-height: 60rpx;
}

.all {
    background: #fff;
}

.cart-box {
    background: #fff;
    width: 94%;
    margin: 0 auto;
    margin-bottom: 120rpx;
}

.cart-list {
    display: flex;
    margin-top: 2rpx;
    padding: 20rpx 0;
}

.select {
    width: 10%;
}

.select icon {
    margin-top: 80rpx;
}

.img1 {
    width: 30%;
}

.img1 image {
    width: 100%;
    height: 200rpx;
}

.info {
    width: 55%;
    margin-left: 3%;
}

.cart-pro-name {
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: #3e3e3e;
    padding-top: 10rpx;
    height: 80rpx;
}

.set {
    margin-top: 15rpx;
    color: #b6b6b6;
}

.price {
    display: flex;
    margin-top: 15rpx;
    justify-content: space-between;
}

.cart-pro-price {
    color: #f23921;
    width: 48%;
}

.cart-count-box {
    display: flex;
    justify-content: space-between;
    width: 190rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: #fbfbfb;
}

.cart-count-down {
    width: 40rpx;
    height: 40rpx;
    border-radius: 6rpx;
    border: 1rpx solid #d1d1d1;
    text-align: center;
    color: #767676;
    line-height: 40rpx;
}

.cart-count-num {
    width: 80rpx;
    text-align: center;
    border: 1rpx solid #d1d1d1;
    border-radius: 6rpx;
    line-height: 40rpx;
    height: 40rpx;
    background: #fff;
}

.cart-count-add {
    width: 40rpx;
    height: 40rpx;
    border-radius: 6rpx;
    border: 1rpx solid #d1d1d1;
    text-align: center;
    color: #767676;
    background: #fbfbfb;
}

.cart-footer {
    position: fixed;
    bottom: 125rpx;
    left: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 0rpx 0 80rpx;
    z-index: 5;
    background: #fff;
}

.cart-menu {
    display: flex;
}

.cart-menu view:nth-child(1) {
    width: 15%;
}

.cart-menu view:nth-child(2) {
    width: 50%;
}

.cart-menu view:nth-child(3) {
    width: 25%;
    text-align: center;
    background-color: #f95015;
    color: #fff;
}

.total-select {
    position: absolute;
    left: 20rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}

.order-icon {
    position: absolute;
    right: 40rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}

.order-icon image,.order-icon navigator {
    display: block;
    width: 45rpx;
    height: 45rpx;
}

.cart-toatl-price {
    width: 120rpx;
    color: black;
}

.cart-toatl-price text {
    color: #f23921;
}

.cart-no-data {
    padding: 40rpx 0;
    text-align: center;
}

.edit {
    margin-right: 30rpx;
    margin-top: 10rpx;
}

.no-cart image {
    width: 140rpx;
    height: 200rpx;
    margin: 0 auto;
}

.no_order {
    padding-top: 160rpx;
    text-align: center;
}

.no_order image {
    width: 575rpx;
    height: 470rpx;
    margin: 0 auto;
}

.no {
    margin-top: 20rpx;
    color: #8e8e8e;
}

.go {
    width: 250rpx;
    margin: 0 auto;
    margin-top: 50rpx;
    height: 70rpx;
    line-height: 70rpx;
    color: #f23921;
    border-radius: 15rpx;
    border: 1rpx solid;
}