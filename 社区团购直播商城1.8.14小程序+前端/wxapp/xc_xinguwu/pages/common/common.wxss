.notice {
    display: flex;
    padding: 20rpx;
    background: #fff;
    color: #777;
    flex-direction: row;
    align-items: center;
}

.notive_img {
    width: 160rpx;
    height: 50rpx;
    display: block;
    padding-right: 10rpx;
    border-right: 1rpx solid #e9e9e9;
}

.notice_icon {
    width: 30rpx;
    height: 30rpx;
    display: block;
    margin-right: 10rpx;
    padding-top: 5rpx;
    margin-left: 20rpx;
}

.notice swiper {
    margin-left: 10rpx;
    width: 100%;
    height: 40rpx;
}

.notice swiper view {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    height: auto;
}

.title {
    display: flex;
    margin-left: 20rpx;
    margin-right: 20rpx;
}

.blank {
    width: 100%;
}

.line {
}

.line_cell {
    height: 0;
}

.ppt {
    width: 100%;
}

.ppt image {
    width: 100%;
    height: 100%;
}

.picturew {
    position: relative;
    width: 750rpx;
    height: 375rpx;
}

.picturew .picturew_cell {
    position: relative;
    width: 750rpx;
    height: 375rpx;
}

.picturew image {
    width: 100%;
    height: 100%;
}

.picturew .picturew-lift {
    width: 49.5%;
    position: absolute;
    height: 100%;
}

.picturew .picturew-right {
    width: 49.5%;
    position: absolute;
    height: 100%;
    left: 50.5%;
}

.picturew .picturew-right1 {
    width: 100%;
    position: absolute;
    height: 49.5%;
}

.picturew .picturew-right2 {
    width: 100%;
    position: absolute;
    height: 49.5%;
    top: 50.5%;
}

.picturew .picturew-right2-left {
    width: 49.5%;
    position: absolute;
    height: 100%;
}

.picturew .picturew-right2-right {
    width: 49.5%;
    position: absolute;
    height: 100%;
    left: 50.5%;
}

.lists {
    padding: 0 20rpx 0 20rpx;
    width: 710rpx;
    height: auto;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.lists .lists-item.block_one {
    position: relative;
    width: 100%;
}

.lists .lists-item.block_two {
    position: relative;
    width: 49%;
}

.lists .lists-item.block_three {
    position: relative;
    width: 33%;
}

.lists .image {
    width: 100%;
    height: 0;
    padding-bottom: 100%;
}

.lists .lists-item .detail {
    overflow: hidden;
    padding: 0 8rpx;
}

.lists .lists-item .detail .name {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.lists .lists-item .detail .price {
    padding-top: 10rpx;
}

.lists .lists-item .detail .price .text {
    flex: 1;
}

.lists .lists-item .detail .price .buy {
}

.lists .lists-item .detail .price .buy image {
    width: 40rpx;
    height: 40rpx;
}

.lists .lists-item.block_list {
    position: relative;
    width: 100%;
    padding: 10rpx;
    overflow: hidden;
    display: flex;
}

.lists .lists-item.block_list .image {
    width: 200rpx;
    height: 200rpx;
    padding-bottom: 0;
}

.lists .lists-item.block_list .detail {
    flex: 1;
    padding-left: 20rpx;
}

.lists .lists-item.block_list .detail .name {
    height: 120rpx;
    -webkit-line-clamp: 3;
}