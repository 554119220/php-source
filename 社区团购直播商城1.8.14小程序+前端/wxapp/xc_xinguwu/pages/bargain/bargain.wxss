.bargain {
    background: #fff;
    border-top: 1rpx solid #f8f8f8;
}

.bargain-list {
    width: 94%;
    margin: 0 auto;
    display: flex;
    padding: 25rpx 0;
    border-bottom: 1rpx solid #e2e2e2;
}

.bargain-list image {
    width: 185rpx;
    height: 165rpx;
}

.content {
    width: 71%;
    margin-left: 4%;
}

.content-name {
    letter-spacing: 2rpx;
    height: 70rpx;
    line-height: 35rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.saled {
    margin-top: 10rpx;
    color: #5c5c5c;
}

.price {
    display: flex;
    height: 50rpx;
    align-items: center;
}

.oldp {
    width: 40%;
}

.newp {
    width: 35%;
    color: #f04043;
}

.go-bargain {
    width: 25%;
    text-align: center;
    background: #f04043;
    color: #fff;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 13px;
    padding: 0;
    border-radius: 15rpx;
}

.bottom {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 90rpx;
    display: flex;
    background: #fff;
    align-items: center;
}

.bottom view {
    display: flex;
    width: 50%;
    align-items: center;
    height: 40rpx;
}

.bottom image {
    margin-left: 40rpx;
    margin-right: 40rpx;
    width: 40rpx;
    height: 40rpx;
}

.bottom view:nth-child(1) {
    border-right: 1rpx solid #e4e4e4;
}

.nav-selected {
    color: #f04043;
}

.order-tab-box-title {
    position: fixed;
    left: 0;
    top: 0;
    display: flex;
    background: #fff;
    z-index: 5;
    width: 100%;
    border-top: 1rpx solid #f9f9f9;
}

.order-tab-nav {
    display: inline-block;
    width: 20%;
    height: 90rpx;
    line-height: 90rpx;
    box-sizing: border-box;
    text-align: center;
    color: #444;
    background-color: #fff;
}

.on view {
    width: 60%;
    margin: 0 auto;
    color: #f23921;
    border-bottom: 2rpx solid #f23921;
    height: 85rpx;
}

.mybargain {
    position: absolute;
    top: 20rpx;
    left: 0;
    margin-bottom: 120rpx;
}

.mybargain-top {
    display: flex;
    padding: 25rpx 0;
    width: 94%;
    margin: 0 auto;
    border-bottom: 1rpx solid #e2e2e2;
}

.mybargain-list {
    background: #fff;
    margin-bottom: 20rpx;
}

.main {
    width: 185rpx;
    height: 165rpx;
}

.content {
    width: 71%;
    margin-left: 4%;
}

.content-name {
    letter-spacing: 2rpx;
    height: 70rpx;
    line-height: 35rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.saled {
    margin-top: 10rpx;
    color: #5c5c5c;
}

.price {
    display: flex;
    height: 50rpx;
    align-items: center;
}

.oldp {
    width: 40%;
}

.newp {
    width: 35%;
    color: #f04043;
}

.go-mybargain {
    width: 25%;
    text-align: center;
    background: #f04043;
    color: #fff;
    height: 50rpx;
    line-height: 50rpx;
    border-radius: 15rpx;
}

.mybargain-bottom {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.nopay {
    display: flex;
    padding: 20rpx 0;
    width: 94%;
    margin: 0 auto;
}

.attend {
    position: relative;
    z-index: 0;
    width: 58%;
    height: 75rpx;
}

.attend image {
    width: 65rpx;
    height: 65rpx;
    border: 4rpx solid #fff;
    border-radius: 100%;
}

.attend image:nth-child(1) {
    position: absolute;
    z-index: 4;
    left: 0;
    top: 0;
}

.attend image:nth-child(2) {
    position: absolute;
    z-index: 3;
    left: 55rpx;
    top: 0;
}

.attend image:nth-child(3) {
    position: absolute;
    z-index: 2;
    left: 110rpx;
    top: 0;
}

.attend image:nth-child(4) {
    position: absolute;
    z-index: 1;
    left: 165rpx;
    top: 0;
}

.state {
    width: 42%;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.schedule-pe {
    width: 94%;
    margin: 0 auto;
    display: flex;
    align-items: center;
    padding: 20rpx 0;
}

.schedule {
    width: 230rpx;
    height: 30rpx;
    color: #fff;
    line-height: 30rpx;
    text-align: center;
    border-radius: 30rpx;
    background: #fcb8b9;
    position: relative;
    margin-left: 30rpx;
}

.percent {
    background: #f04043;
    position: absolute;
    left: 0;
    top: 0;
    text-align: center;
    border-radius: 30rpx;
    z-index: 0;
}

.percent-num {
    position: absolute;
    left: 42%;
    top: 0;
    z-index: 1;
}

.share {
    width: 135rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    background: #f04043;
    color: #fff;
    margin-left: 50rpx;
    border-radius: 15rpx;
}

.pay {
    width: 135rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    background: #f04043;
    color: #fff;
    margin-left: 20rpx;
    border-radius: 15rpx;
}

.rank {
    width: 80%;
    border-radius: 15rpx;
    background: #fff;
    position: fixed;
    left: 10%;
    top: 12vh;
    z-index: 5;
    height: 64vh;
}

.close {
    position: absolute;
    right: -15rpx;
    top: -15rpx;
}

.rank-title {
    text-align: center;
    padding: 40rpx 0;
}

.all-rank {
    margin-top: 10rpx;
}

.rank-list {
    display: flex;
    padding: 20rpx 0;
    align-items: center;
    width: 90%;
    margin: 0 auto;
}

.rank-detail {
    width: 40rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    color: #888;
}

.rank-detail image {
    width: 40rpx;
    height: 60rpx;
}

.head {
    width: 80rpx;
    height: 80rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
}

.username {
    width: 180rpx;
}

.username view:nth-child(2) {
    color: #999;
}

.cut {
    width: 158rpx;
    text-align: center;
    margin-left: 20rpx;
}

.cut text {
    color: #f04043;
}

.content-bottom {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 20rpx;
}

.lowest {
    color: #999;
}

.surplus {
    color: #f04043;
}

.over {
}

.failed {
    color: #999;
    margin-top: 5rpx;
}

.go-on {
    width: 140rpx;
    color: #fff;
    background: #f04043;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 30rpx;
    margin-top: 20rpx;
    position: absolute;
    right: 20rpx;
}