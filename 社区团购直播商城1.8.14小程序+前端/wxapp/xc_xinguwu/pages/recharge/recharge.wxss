.top {
    height: 20rpx;
}

.all {
    background: #fff;
    width: 90%;
    margin: 0 auto;
    padding-bottom: 50rpx;
    border-radius: 15rpx;
}

.balance_head {
    text-align: center;
    padding-top: 50rpx;
}

.balance {
    font-size: 6vw;
    font-weight: 900;
}

.money {
    display: flex;
    flex-wrap: wrap;
    width: 100%;
    margin: 0 auto;
    margin-top: 40rpx;
}

.list2 {
    border: 1rpx solid #abaaaa;
    color: #888888;
}

.list1 {
    border: 1rpx solid #f13c26;
    background: url('data:image/png;base64,') right bottom no-repeat;
    background-size: 15% 40%;
    color: #f13c26;
}

.list {
    width: 40%;
    margin-top: 40rpx;
    text-align: center;
    padding: 10rpx 0;
    border-radius: 18rpx;
    margin-left: 42rpx;
}

.left {
    margin-left: 40rpx;
}

.font {
    font-size: 5vw;
}

.other {
    font-size: 5vw;
}

.other input {
    height: 100rpx;
    line-height: 100rpx;
}

.method {
    width: 90%;
    margin: 0 auto;
    padding: 15rpx 0;
    color: #abaaaa;
}

.choose {
    display: flex;
    width: 90%;
    margin: 0 auto;
    padding: 20rpx 0;
    background: #fff;
    border-radius: 15rpx;
}

.choose image {
    margin-left: 30rpx;
    margin-right: 30rpx;
}

.pay {
    color: #abaaaa;
    width: 70%;
}

button {
    width: 70%;
    margin: 0 auto;
    margin-top: 80rpx;
    background: #f13c26;
    color: #fff;
    height: 70rpx;
    line-height: 70rpx;
}

.recharge-record {
    color: #888;
    position: absolute;
    right: 40rpx;
    padding: 20rpx 25rpx 0 0;
}