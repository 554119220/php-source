.address {
    border-top: 1rpx solid #f8f8f8;
    padding: 20rpx 0;
    background: #fff;
}

.else {
    background: #fff;
    padding: 40rpx 0;
    margin-top: 2rpx;
    position: relative;
}

.toAddAddress {
    display: flex;
    padding: 10rpx 40rpx;
}

.addImg {
    margin: 0 auto;
    display: flex;
    line-height: 50rpx;
}

.addImg image {
    padding-right: 20rpx;
}

.arrows1 {
    width: 20rpx;
    height: 30rpx;
    margin-top: 40rpx;
    margin-left: 10rpx;
}

.arrows {
    width: 20rpx;
    height: 30rpx;
    position: absolute;
    right: 25rpx;
}

.address navigator {
    display: flex;
    position: relative;
}

.moren {
    padding-left: 20rpx;
    color: #a4a3a3;
}

.message {
    width: 550rpx;
}

.name {
    margin-left: 20rpx;
    display: flex;
    color: #585858;
}

.name_value {
    margin-right: 30rpx;
}

.add {
    color: #585858;
    margin-left: 20rpx;
    margin-top: 20rpx;
}

.right {
    margin-left: 25rpx;
    line-height: 120rpx;
}

.myvoucher {
    display: flex;
    padding: 20rpx 25rpx;
    background: #fff;
    margin-top: 20rpx;
    justify-content: space-between;
    align-items: center;
}

.myvoucher text {
    color: #f23921;
}

.voucher-detail {
    display: flex;
    align-items: center;
}

.voucher-kind {
    margin-right: 20rpx;
}

.voucher-kind_1 text {
    color: #FEA119;
}

.voucher-kind_2 text {
    color: #54B1FB;
}

.voucher-kind_3 text {
    color: #f2559f;
}

.myvoucher image {
    width: 20rpx;
    height: 30rpx;
}

.order {
    margin-top: 20rpx;
    background: #fff;
}

.orderid {
    border-bottom: 1rpx solid #f1f1f1;
    padding: 20rpx 0;
    padding-left: 20rpx;
    color: #a4a3a3;
}

.list {
    display: flex;
    border-bottom: 1rpx solid #f1f1f1;
}

.img {
    background-color: #fff;
    width: 34%;
    height: 230rpx;
    padding: 20rpx 0 20rpx 20rpx;
}

.img navigator {
    width: 100%;
    height: 100%;
}

.img navigator image {
    width: 100%;
    height: 100%;
}

.neirong {
    margin-top: 2rpx;
}

.content {
    background-color: #fff;
    width: 60%;
    padding: 20rpx 0 20rpx 25rpx;
}

.pro-title {
    padding: 10rpx 10rpx 0rpx 0rpx;
    flex-wrap: wrap;
    overflow: hidden;
    height: 80rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.num-price {
    display: flex;
    margin-top: 25rpx;
    width: 95%;
}

.color {
    margin-top: 20rpx;
}

.money {
    display: flex;
}

.new {
    color: #f23b24;
    width: 240rpx;
}

.totalsum {
    width: 200rpx;
    text-align: right;
}

.activity {
    width: 94%;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
    padding: 10rpx 0;
}

.activity view:nth-child(1) {
    color: #a7a7a7;
}

.total {
    display: flex;
    padding: 20rpx 0;
}

.total view:nth-child(1) {
    width: 300rpx;
    margin-left: 210rpx;
    text-align: right;
}

.total view:nth-child(2) {
    width: 200rpx;
    text-align: right;
    color: #f23921;
}

.total_m {
    color: #ff0101;
}

.all_money {
    background: #fff;
    padding: 20rpx;
}

.price {
    padding: 10rpx 0;
}

.shuoming {
    float: left;
    color: #a4a3a3;
}

.det {
    text-align: right;
    color: #565656;
}

.method {
    padding: 20rpx;
    background: #fff;
    margin-top: 20rpx;
}

.shortcut {
    padding: 15rpx 0;
    color: #a7a7a7;
    margin-top: 10rpx;
}

.want_method {
    display: flex;
}

.remark {
    display: flex;
    margin-top: 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding: 20rpx;
    background: #fff;
}

input {
    margin-left: 20rpx;
    height: 60rpx;
}

button {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    border-radius: 0;
    background: #f23921;
    color: #fff;
    z-index: 5;
}

button[class="btn"]::after {
    border: 0;
}

.vv {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
}

.nav-top {
    background: #fff;
    display: flex;
    justify-content: center;
    padding: 10rpx 30rpx;
}

.nav-top .text {
    text-align: center;
    font-size: 4vw;
    color: #999;
    height: 80rpx;
    line-height: 80rpx;
    width: 100%;
}

.nav-top .text .tip {
    color: #fff;
    background: #F93D3D;
    font-size: 16rpx;
    padding: 0 6rpx;
    border-radius: 10rpx;
    position: relative;
    bottom: 8rpx;
}

.nav-top .text.on {
    color: #444444;
    background: #f8f8f8;
}

.nav-top .text:nth-child(1) {
    border-right: 1rpx solid #eee;
}

.club {
    padding-left: 20rpx;
    padding-right: 40rpx;
    color: #999;
}

.club .header {
    display: flex;
    justify-content: space-between;
}

.club .header .header-name {
    font-size: 4vw;
    color: #333;
    font-weight: bold;
}

.club .header .tip {
    display: flex;
    align-items: center;
    border-radius: 10rpx;
    padding-left: 10rpx;
    padding-right: 10rpx;
}

.club .header .tip text {
    color: #fff;
    margin-right: 10rpx;
}

.club .header .tip image {
    width: 16rpx;
}

.club-list {
    background: #fff;
    height: 660rpx;
    position: absolute;
    bottom: 0;
    width: 100%;
}

.club-list .list-hrader {
    height: 80rpx;
    line-height: 80rpx;
    padding-left: 20rpx;
    font-size: 4.5vw;
    color: #444444;
    letter-spacing: 2rpx;
}

.club-list .list-body {
    width: auto;
    margin: 20rpx;
    height: 440rpx;
}

.club-list .list-item {
    min-height: 140rpx;
    border: 1rpx solid #eee;
    border-radius: 10rpx;
    display: flex;
    align-items: center;
    margin-bottom: 20rpx;
}

.club-list .list-item.on {
    border: 1rpx solid #f23921;
}

.club-list .list-item-avatar {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    margin-left: 40rpx;
}

.club-list .list-item-info {
    margin-left: 20rpx;
    width: 500rpx;
}

.club-list .list-item-info .top {
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
}

.club-list .list-item-info .top .top-left text:nth-child(1) {
    color: #444444;
    font-size: 4vw;
    margin-right: 20rpx;
}

.club-list .list-item-info .top .top-right {
    color: #444444;
    font-size: 4vw;
}

.club-list .list-item-info .bottom {
    color: #888888;
    font-size: 3vw;
}

.club-list .list-item .selected {
    position: relative;
    bottom: 48rpx;
    right: -6rpx;
}

.club-list .list-bottom {
    display: flex;
    height: 100rpx;
    width: 100%;
}

.club-list .list-bottom button {
    width: 50%;
    position: relative;
}

.club-list .list-bottom button::after {
    border: none;
}

.club-list .list-bottom button:nth-child(1) {
    background: #fafafa;
    color: #999;
}

.yuqibox {
    padding-left: 20rpx;
    padding-right: 40rpx;
    color: #f23921;
}

.yuqibox .yuqilabel {
    background-color: #f23921;
    display: inline-block;
    color: #fff;
    text-align: center;
    width: 140rpx;
    border-radius: 10rpx;
}

.header .tip {
    background-color: #f23921;
    color: #fff;
}

.tip text.iconsc-right {
    font-size: 35rpx;
}