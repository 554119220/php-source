.limit {
    width: 100%;
    background: #fff;
}

.limit-main {
    width: 100%;
    height: 430rpx;
}

.limit-name {
    width: 94%;
    margin: 10rpx auto;
    line-height: 35rpx;
    color: #333;
    padding: 10rpx 0;
    font-weight: bold;
    letter-spacing: 2rpx;
    font-size: 4vw;
    display: flex;
    justify-content: space-between;
}

.limit-name text {
    width: 85%;
    display: -webkit-box;
    word-wrap: break-word;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.limit-name image {
    padding: 10rpx;
}

.limit-price {
    display: flex;
    width: 94%;
    margin: 0 auto;
    justify-content: space-between;
}

.price {
    display: flex;
}

.newp {
    width: 160rpx;
    color: #f04043;
    line-height: 40rpx;
}

.oldp {
    width: 160rpx;
    color: #a7a7a7;
    height: 30rpx;
    margin-top: 5rpx;
}

.share {
    width: 40rpx;
    height: 40rpx;
}

.count-down {
    margin: 0 auto;
    margin-top: 30rpx;
    display: flex;
    width: 94%;
}

.schedule-pe {
    width: 50%;
    display: flex;
    align-items: center;
}

.schedule {
    overflow: hidden;
    width: 280rpx;
    height: 40rpx;
    color: #fff;
    line-height: 40rpx;
    text-align: center;
    border-radius: 30rpx;
    background: #fcb8b9;
    position: relative;
}

.percent {
    background: #f04043;
    position: absolute;
    left: 0;
    top: 0;
    text-align: center;
    border-radius: 30rpx;
    z-index: 0;
}

.percent-num {
    position: absolute;
    left: 42%;
    top: 0;
    z-index: 1;
}

.count-time {
    width: 42%;
    display: flex;
    margin-left: 6%;
}

.time-detail {
    margin-left: 30rpx;
    display: flex;
}

.time-detail view {
    background: #f8f8f8;
    width: 40rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    border-radius: 10rpx;
}

.service {
    display: flex;
    width: 94%;
    margin: 0 auto;
    justify-content: space-between;
    margin-top: 25rpx;
    padding-bottom: 30rpx;
    color: #999;
}

.service-list {
    display: flex;
    align-items: center;
}

.service-list image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 30rpx;
}

.goods-detail {
    width: 100%;
    background: #fff;
    margin-top: 20rpx;
}

.goods-detail view {
    margin: 0 auto;
    padding: 20rpx 0;
}

.goods-detail image {
    margin: 0 auto;
    height: 375rpx;
    margin-bottom: 150rpx;
}

.bottom {
    width: 100%;
    height: 100rpx;
    background: #fff;
    display: flex;
    align-items: center;
    position: fixed;
    z-index: 5;
    left: 0;
    bottom: 0;
}

.cho-head {
    width: 94%;
    margin: 0 auto;
    display: flex;
}

.cho-price {
    margin-left: 80rpx;
}

.cho-price view:nth-child(1) {
    margin-top: 10rpx;
    color: #f04043;
}

.cho-price view:nth-child(2) {
    margin-top: 10rpx;
    color: #666;
}

.index {
    width: 14%;
}

.index image {
    margin: 0 auto;
}

.alone {
    color: #fff;
    background: #f89696;
    height: 100rpx;
    width: 46%;
    text-align: center;
    line-height: 100rpx;
}

.group-buy {
    color: #fff;
    background: #f04043;
    height: 100rpx;
    width: 40%;
    text-align: center;
    line-height: 100rpx;
}

.toshare {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 5;
    background: #fff;
}

.share-btn {
    width: 100%;
    display: flex;
    padding: 50rpx 0 20rpx 0;
}

.share-w {
    width: 50%;
    text-align: center;
    border-right: 1rpx solid #dfdfdf;
    letter-spacing: 5rpx;
    background: #fff;
    border-radius: 0;
}

button[class="share-w"]::after {
    border: 0;
}

.share-w image {
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto;
    padding-top: 15rpx;
}

.share-i {
    width: 50%;
    text-align: center;
    letter-spacing: 5rpx;
    background: #fff;
    border-radius: 0;
}

button[class="share-i"]::after {
    border: 0;
}

.share-i image {
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto;
    padding-top: 15rpx;
}

.close-btn {
    text-align: center;
    padding: 20rpx 0;
    border-top: 1rpx solid #e3e3e3;
}

.choose {
    position: fixed;
    width: 100%;
    bottom: 0;
    left: 0;
    background: #fff;
    z-index: 5;
}

.choose image {
    width: 200rpx;
    height: 200rpx;
    margin-top: -80rpx;
    border: 4rpx solid #fff;
    border-radius: 5rpx;
}

.c_color {
    padding: 5rpx 40rpx;
    background: #f0f0f0;
    color: #737373;
    border-radius: 10rpx;
    margin-right: 40rpx;
    margin-top: 20rpx;
}

.color_selected {
    background: #f23921;
    color: #fff;
}

.c_color1 {
    margin-left: 0rpx;
}

.cho-color {
    display: flex;
}

.choose_color {
    margin: 0 auto;
    width: 88%;
    margin-top: 30rpx;
    margin-bottom: 150rpx;
}

.true {
    position: absolute;
    z-index: 6;
    width: 100%;
    height: 100rpx;
    bottom: 0;
    left: 0;
    color: #fff;
    text-align: center;
    background: #f23921;
    line-height: 100rpx;
}

.buy_num {
    display: flex;
    margin: 0 auto;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    justify-content: space-between;
}

.cart-count-box {
    display: flex;
    justify-content: space-between;
    width: 190rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: #fbfbfb;
}

.cart-count-down {
    width: 40rpx;
    height: 40rpx;
    border-radius: 6rpx;
    border: 1rpx solid #d1d1d1;
    text-align: center;
    color: #767676;
    line-height: 40rpx;
}

.cart-count-num {
    width: 80rpx;
    text-align: center;
    border: 1rpx solid #d1d1d1;
    border-radius: 6rpx;
    line-height: 40rpx;
    height: 40rpx;
    background: #fff;
}

.cart-count-add {
    width: 40rpx;
    height: 40rpx;
    border-radius: 6rpx;
    border: 1rpx solid #d1d1d1;
    text-align: center;
    color: #767676;
    background: #fbfbfb;
}

.shengcheng {
    width: 100%;
    height: 100vh;
    z-index: 6;
    position: fixed;
    left: 0;
    top: 0;
}

.shengcheng icon {
    position: absolute;
    right: 45%;
    top: 2vh;
}

.c_bg {
    width: 290px;
    height: 370px;
    margin: 0 auto;
    margin-top: 8vh;
    background: #fff;
    border-radius: 15rpx;
}

.c_bg view {
    height: 5px;
}

canvas {
    width: 280px;
    height: 360px;
    margin: 0 auto;
}

.tips {
    background: #f23921;
    height: 80rpx;
    line-height: 80rpx;
    width: 50%;
    margin: 0 auto;
    text-align: center;
    color: #fff;
    border-radius: 10rpx;
    margin-top: 20rpx;
}

.limit .tip {
    height: 80rpx;
    background-color: #f04043;
    padding: 10rpx 20rpx;
    display: flex;
    justify-content: space-between;
}

.limit .tip .left {
    color: #fff;
    font-size: 5vw;
    letter-spacing: 4rpx;
    height: 80rpx;
    line-height: 80rpx;
}

.limit .tip .right {
    display: flex;
    width: 280rpx;
    color: #fff;
    flex-direction: column;
    align-content: flex-start;
    letter-spacing: 4rpx;
}

.parameter-box {
    display: flex;
    justify-content: center;
    width: 100%;
    line-height: 80rpx;
    overflow: hidden;
    font-weight: bold;
    background: #fff;
    align-items: center;
    margin-top: 20rpx;
    padding-bottom: 6rpx;
}

.parameter-box text {
    color: #ededed;
}

.parm-nav {
    width: 49.9%;
    height: 80rpx;
    color: #444;
    text-align: center;
}

.nav-select {
    color: #f82c2c;
    height: 80rpx;
}

.nav-select view {
    height: 79rpx;
    width: 30%;
    margin: 0 auto;
    border-bottom: 1rpx solid;
}

.parameter-detail {
    background: #fff;
}

.parameter-detail-list {
    display: flex;
    width: 90%;
    margin: 0 auto;
    padding: 20rpx 0;
}

.parameter-detail-list view:nth-child(1) {
    width: 25%;
    font-weight: bold;
}

.parameter-detail-list view:nth-child(2) {
    width: 70%;
    margin-left: 2%;
    color: #999;
}