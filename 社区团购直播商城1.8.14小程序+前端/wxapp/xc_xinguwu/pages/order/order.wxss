.goods-tab-box-title {
    position: fixed;
    left: 0;
    top: 0;
    z-index: 5;
    white-space: nowrap;
    background: #fff;
}

.goods-tab-nav {
    display: inline-block;
    width: 22%;
    height: 90rpx;
    line-height: 90rpx;
    border-bottom: 1rpx solid #ededed;
    text-align: center;
    color: #505050;
    background-color: #fff;
}

.goods-tab-nav.on view {
    width: 60%;
    margin: 0 auto;
    color: #bcaa8a;
    border-bottom: 5rpx solid #bcaa8a;
}

.search {
    position: fixed;
    top: 90rpx;
    left: 0;
    width: 100%;
    padding: 20rpx 0;
    z-index: 2;
    background: #f8f8f8;
}

.search_input {
    padding: 0 20rpx;
}

.search_input_body {
    display: flex;
    background: #fff;
    height: 50rpx;
    align-items: center;
    padding: 20rpx 40rpx;
    border-radius: 70rpx;
    justify-content: space-between;
}

.search_input input {
    width: 90%;
}

.goods-content {
    position: absolute;
    top: 200rpx;
    left: 0;
    width: 100%;
    margin-bottom: 50rpx;
    z-index: 1;
}

.list {
    margin-top: 20rpx;
    height: auto;
    position: relative;
}

.head {
    display: flex;
    padding: 14rpx 20rpx 10rpx 20rpx;
    background-color: #fff;
    align-items: center;
    justify-content: space-between;
}

.limit .head {
    border-bottom: 1rpx solid #f1f1f1;
}

.bargain .head {
    border-bottom: 1rpx solid #f1f1f1;
}

.score .head {
    border-bottom: 1rpx solid #f1f1f1;
}

.list .head-left {
    display: flex;
}

.list .head-left-type {
    background: #FF720B;
    color: #fff;
    padding: 0rpx 10rpx;
    border-radius: 6rpx;
    font-size: 24rpx;
}

.list .head-left-order {
    margin-left: 10rpx;
    color: #888888;
    font-size: 26rpx;
}

.list .head-left-date {
    color: #888888;
    font-size: 26rpx;
}

.head-right {
    color: #f23921;
}

.mian {
    display: flex;
    width: 100%;
    position: relative;
    margin-top: 2rpx;
}

.img {
    width: 34%;
    height: 230rpx;
    padding: 20rpx 0 20rpx 20rpx;
}

.img navigator {
    width: 100%;
    height: 100%;
}

.img navigator image {
    width: 100%;
    height: 100%;
}

.normal .neirong {
    margin-top: 2rpx;
    background: #fff;
}

.limit .neirong {
    background: #fff;
    margin-top: 2rpx;
}

.bargain .neirong {
    background: #fff;
    margin-top: 2rpx;
}

.score .neirong {
    background: #fff;
    margin-top: 2rpx;
}

.normal .main {
    background: #f8f8f8;
}

.limit .main {
    background: #fff;
}

.content {
    width: 60%;
    padding: 20rpx 0 20rpx 25rpx;
}

.pro-title {
    padding: 10rpx 10rpx 0rpx 0rpx;
    flex-wrap: wrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.normal .num-price {
    display: flex;
    margin-top: 25rpx;
    width: 100%;
}

.limit .num-price {
    display: flex;
    margin-top: 25rpx;
    width: 100%;
    justify-content: space-between;
}

.score .num-price {
    display: flex;
    margin-top: 80rpx;
    width: 100%;
}

.color {
    margin-top: 20rpx;
}

.already-bargain {
    color: #999;
    margin-top: 20rpx;
}

.normal .money {
    display: flex;
}

.limit .money {
    width: 50%;
    display: flex;
    justify-content: space-between;
}

.bargain .money {
    display: flex;
    align-items: center;
    margin-top: 30rpx;
}

.normal .score-money {
    margin-right: 20rpx;
    color: #f23921;
}

.normal .need-score {
    display: flex;
    align-items: center;
    color: #f23921;
}

.normal .need-score image {
    width: 30rpx;
    height: 30rpx;
    border-radius: 30rpx;
    margin-right: 10rpx;
}

.lowst {
    margin-left: 20rpx;
    color: #f23921;
}

.new {
    color: #f23b24;
    width: 140rpx;
}

.old {
    color: #afaeae;
    line-height: 50rpx;
    font-size: 3vw;
    width: 140rpx;
    text-align: right;
    text-decoration: line-through;
}

.normal .totalsum {
    margin-left: 260rpx;
}

.limit .totalsum {
    margin-right: 20rpx;
}

.normal .total_m {
    color: #ff0101;
}

.normal .btn {
    padding: 10rpx 0 10rpx 0;
    margin-top: 4rpx;
    background-color: #fff;
    display: flex;
    color: #c1c1c1;
    align-items: center;
}

.cross-line {
    height: 4rpx;
    background: #f8f8f8;
}

.limit .btn {
    padding: 10rpx 0 10rpx 0;
    background-color: #fff;
    display: flex;
    color: #c1c1c1;
    align-items: center;
}

.count-down {
    color: #888;
}

count-down text {
    color: #444;
    background: #f8f8f8;
    padding: 5rpx;
    border-radius: 5rpx;
}

.todelete {
    text-align: center;
    color: #f23d26;
}

.topay {
    padding: 5rpx 20rpx;
    background: #f23b24;
    color: #fff;
    margin-left: 40rpx;
    border-radius: 10rpx;
}

.torefund {
    padding: 5rpx 15rpx;
    background: #d0cfcf;
    color: #fff;
    border-radius: 10rpx;
}

.bargain .send {
    padding: 5rpx 15rpx;
    background: #f23b24;
    color: #fff;
    margin-left: 160rpx;
    border-radius: 10rpx;
}

.send {
    padding: 5rpx 15rpx;
    background: #f23b24;
    color: #fff;
    margin-left: 20rpx;
    border-radius: 10rpx;
}

.buttn {
    background: #fff;
    padding-bottom: 15rpx;
}

.buttn1 {
    background: #fff;
    padding-bottom: 20rpx;
}

.bargain .buttn {
    padding-top: 10rpx;
}

.score .buttn {
    padding-top: 15rpx;
}

.bargain .buttn1 {
    padding-bottom: 20rpx;
}

.buttn_list {
    display: flex;
    width: 32%;
    margin-left: 66%;
}

.buttn_list1 {
    display: flex;
    width: 40%;
    margin-left: 58%;
}

.buttn_list2 {
    width: 170rpx;
    text-align: center;
    margin-left: 75%;
    clear: both;
}

.content-bottom-line {
    border-top: 2px solid #f8f8f8;
}

.sum {
    text-align: center;
    background: #fff;
    padding: 20rpx 0;
    color: #f23921;
    border-top: 2px solid #f8f8f8;
}

.total {
    width: 85%;
    text-align: right;
    margin: 0 auto;
}

.no_order {
    padding-top: 220rpx;
    text-align: center;
}

.no_order image {
    width: 25%;
    height: 22vh;
    margin: 0 auto;
}

.no {
    margin-top: 20rpx;
    color: #8e8e8e;
}

.go {
    width: 25%;
    margin: 0 auto;
    background: #f23921;
    margin-top: 30rpx;
    height: 70rpx;
    line-height: 70rpx;
    color: #fff;
    border-radius: 15rpx;
}

.hide-list-item {
    width: 100%;
    background: #fff;
    text-align: center;
    color: #f23921;
    padding: 20rpx 0;
}

.list-footer {
    border-top: 2rpx solid #f1f1f1;
    background: #fff;
    padding: 20rpx 20rpx;
}

.list-footer .footer-price {
    text-align: right;
    height: 40rpx;
    line-height: 40rpx;
    margin-bottom: 16rpx;
}

.btn-area {
    display: flex;
    flex-direction: row-reverse;
}

.btn-area .btn1 {
    border: 2rpx solid #f23921;
    color: #f23921;
    border-radius: 14rpx;
    font-size: 4vw;
    letter-spacing: 2rpx;
    padding: 6rpx 22rpx;
    margin-left: 20rpx;
}

.btn-area .btn2 {
    border: 2rpx solid #999;
    color: #999;
    border-radius: 14rpx;
    font-size: 4vw;
    letter-spacing: 2rpx;
    padding: 4rpx 20rpx;
    margin-left: 20rpx;
}

.btn-area .btn3 {
    border: 2rpx solid #f23921;
    color: #f23921;
    border-radius: 40rpx;
    font-size: 4vw;
    letter-spacing: 2rpx;
    padding: 6rpx 22rpx;
    margin-left: 20rpx;
    display: flex;
    align-items: center;
}

.qrcode {
    width: 560rpx;
    height: 560rpx;
    margin: 0 auto;
    background: #fff;
    position: relative;
    top: 200rpx;
    padding: 20rpx;
    border-radius: 10rpx;
}

.qrcode image {
    width: 100%;
    height: 100%;
}

.btn1.rebuy {
    border: none;
}