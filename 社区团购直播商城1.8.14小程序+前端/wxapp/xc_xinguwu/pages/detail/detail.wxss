.banner {
    width: 100%;
    height: 30vh;
}

.banner image {
    width: 100%;
    height: 100%;
}

.bg {
    background: #fff;
}

.detail {
    width: 94%;
    margin: 0 auto;
    padding-bottom: 20rpx;
    padding-top: 20rpx;
}

.detail-head {
    display: flex;
    align-items: center;
    width: 100%;
    justify-content: space-between;
}

.detail-name {
    width: 88%;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    font-weight: bold;
}

.detail-price {
    display: flex;
    margin-top: 10rpx;
    align-items: baseline;
}

.new {
    color: red;
    margin-right: 45rpx;
    font-weight: bold;
}

.new text {
    color: red;
}

.old {
    color: #909090;
    text-decoration: line-through;
}

.share {
    width: 8%;
    color: #999;
    text-align: center;
}

.share image {
    width: 35rpx;
    height: 35rpx;
    margin: 0 auto;
}

.position-info {
    display: flex;
    color: #a0a0a0;
}

.position-info view:nth-child(1) {
    margin-right: 50rpx;
}

.toshare {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 5;
    background: #fff;
}

.discount {
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    padding: 20rpx;
    margin-top: 20rpx;
    border-bottom: 1rpx solid #f1f1f1;
    color: #505050;
}

.discount image {
    width: 20rpx;
    height: 30rpx;
}

.chooses-tyle {
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    padding: 20rpx;
    color: #505050;
}

.chooses-tyle image {
    width: 20rpx;
    height: 30rpx;
}

.pledge {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 0;
    background: #fff;
    color: #7e7e7e;
    width: 94%;
    margin: 0 auto;
    border-top: 1rpx solid #f1f1f1;
}

.pledge view {
    display: flex;
    align-items: center;
    justify-content: center;
}

.pledge image {
    width: 30rpx;
    height: 30rpx;
    border-radius: 30rpx;
    margin-right: 20rpx;
}

.parameter-box {
    display: flex;
    justify-content: center;
    width: 100%;
    line-height: 80rpx;
    overflow: hidden;
    font-weight: bold;
    background: #fff;
    align-items: center;
    margin-top: 20rpx;
    padding-bottom: 6rpx;
}

.parameter-box text {
    color: #ededed;
}

.parm-nav {
    width: 49.9%;
    height: 80rpx;
    color: #444;
    text-align: center;
}

.nav-select {
    color: #f82c2c;
    height: 80rpx;
}

.nav-select view {
    height: 79rpx;
    width: 30%;
    margin: 0 auto;
    border-bottom: 1rpx solid;
}

.share-btn {
    width: 100%;
    display: flex;
    padding: 50rpx 0 20rpx 0;
}

.share-w {
    width: 50%;
    text-align: center;
    border-right: 1rpx solid #dfdfdf;
    letter-spacing: 5rpx;
    background: #fff;
    border-radius: 0;
}

button[class="share-w"]::after {
    border: 0;
}

.share-w image {
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto;
    padding-top: 15rpx;
}

.share-i {
    width: 50%;
    text-align: center;
    letter-spacing: 5rpx;
    background: #fff;
    border-radius: 0;
}

button[class="share-i"]::after {
    border: 0;
}

.share-i image {
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto;
    padding-top: 15rpx;
}

.close-btn {
    text-align: center;
    padding: 20rpx 0;
    border-top: 1rpx solid #bfbfbf;
}

.saled {
    background: #fff;
    margin-top: 20rpx;
}

.saled_list {
    display: flex;
    width: 94%;
    margin: 0 auto;
    padding: 20rpx 0;
}

.saled_list1 {
    border-top: 1rpx solid #e8e8e8;
    border-bottom: 1rpx solid #e8e8e8;
}

.title {
    border: 1rpx solid #f23921;
    padding: 0 10rpx;
    border-radius: 10rpx;
    color: #f23921;
}

.text {
    margin-left: 30rpx;
    width: 82%;
    color: #9d9d9d;
}

.right {
    color: #9d9d9d;
}

.g_detail {
    width: 100%;
    margin-top: 20rpx;
    background: #fff;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #f7f7f7;
    position: relative;
    z-index: 1;
}

.g_title {
    width: 94%;
    margin: 0 auto;
    font-size: 4vw;
    font-weight: 900;
}

.g_title image {
    width: 100%;
    height: 25rpx;
    padding-top: 10rpx;
}

.parameter-detail {
    background: #fff;
}

.parameter-detail-list {
    display: flex;
    width: 90%;
    margin: 0 auto;
    padding: 20rpx 0;
}

.parameter-detail-list view:nth-child(1) {
    width: 25%;
    font-weight: bold;
}

.parameter-detail-list view:nth-child(2) {
    width: 70%;
    margin-left: 2%;
    color: #999;
}

.bottom {
    display: flex;
    height: 100rpx;
    width: 100%;
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
}

.icon {
    width: 15%;
    text-align: center;
    background: #fff;
    padding: 0;
    height: 100prx;
    line-height: 50rpx;
    color: #a8a8a8;
    position: relative;
    border-radius: 0;
}

.number {
    position: absolute;
    right: 15rpx;
    top: 5rpx;
    width: 30rpx;
    height: 30rpx;
    border-radius: 20rpx;
    background: #f23921;
    line-height: 30rpx;
    text-align: center;
    color: #fff;
    font-size: 3vw;
}

.icon image {
    width: 35rpx;
    height: 35rpx;
    margin: 0 auto;
    margin-top: 15rpx;
}

.add {
    width: 30%;
    text-align: center;
    line-height: 100rpx;
    color: #fff;
    background: #f28d21;
}

.buy {
    background: #f23921;
    text-align: center;
    width: 30%;
    line-height: 100rpx;
    color: #fff;
}

.wantbuy {
    width: 100%;
    padding-top: 30rpx;
    padding-bottom: 80rpx;
    position: fixed;
    left: 0;
    bottom: 0rpx;
    z-index: 11;
    background: #fff;
}

.info {
    display: flex;
    width: 94%;
    margin: 0 auto;
    border-bottom: 1rpx solid #f0f0f0;
    padding-bottom: 20rpx;
}

.i_img {
    width: 280rpx;
    margin-top: -100rpx;
    border: 1rpx solid #fff;
}

.i_img image {
    width: 280rpx;
    height: 280rpx;
}

.i_right {
    width: 48%;
    margin-left: 30rpx;
}

.i_right-name {
    width: 90%;
    font-weight: bold;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.inventory {
    display: flex;
    align-items: baseline;
    margin-top: 40rpx;
}

.price {
    width: 160rpx;
}

.gray {
    font-size: 11pt;
}

.close {
    margin-left: 10rpx;
}

.close image {
    width: 50rpx;
    height: 50rpx;
}

.choose_color {
    width: 94%;
    margin: 0 auto;
    margin-top: 30rpx;
    padding-bottom: 30rpx;
    border-bottom: 1rpx solid #f0f0f0;
}

.color {
    display: flex;
    font-size: 11pt;
    text-align: center;
    flex-wrap: wrap;
}

.c_color {
    padding: 5rpx 60rpx;
    background: #f0f0f0;
    color: #737373;
    border-radius: 10rpx;
    margin-right: 40rpx;
    margin-top: 20rpx;
}

.color_selected {
    background: #f23921;
    color: #fff;
}

.c_color1 {
    margin-left: 0rpx;
}

.buy_num {
    display: flex;
    width: 94%;
    margin: 0 auto;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
}

.true {
    margin: 0 auto;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    position: fixed;
    left: 0;
    border: 0;
    background: #f23921;
    color: #fff;
}

.cart-count-box {
    margin-left: 370rpx;
    display: flex;
    justify-content: space-between;
    width: 190rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: #fbfbfb;
}

.cart-count-down {
    width: 40rpx;
    height: 40rpx;
    border-radius: 6rpx;
    border: 1rpx solid #d1d1d1;
    text-align: center;
    color: #767676;
    line-height: 40rpx;
}

.cart-count-num {
    width: 80rpx;
    text-align: center;
    border: 1rpx solid #d1d1d1;
    border-radius: 6rpx;
    line-height: 40rpx;
    height: 40rpx;
    background: #fff;
}

.cart-count-add {
    width: 40rpx;
    height: 40rpx;
    border-radius: 6rpx;
    border: 1rpx solid #d1d1d1;
    text-align: center;
    color: #767676;
    background: #fbfbfb;
}

.shadow {
    width: 100%;
    background: #000;
    opacity: 0.2;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 4;
}

button::after {
    border: 0;
}

.shengcheng {
    width: 100%;
    height: 100vh;
    z-index: 10;
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.6);
}

.shengcheng icon {
    position: absolute;
    right: 45%;
    top: 2vh;
    z-index: 10;
}

.c_bg {
    width: 290px;
    height: 370px;
    margin: 0 auto;
    margin-top: 8vh;
    background: #fff;
    border-radius: 15rpx;
}

.c_bg view {
    height: 5px;
}

canvas {
    width: 280px;
    height: 360px;
    margin: 0 auto;
}

.user {
    display: flex;
    line-height: 90rpx;
}

.user image {
    width: 90rpx;
    height: 90rpx;
    margin-left: 20rpx;
    border-radius: 90rpx;
}

.nickname {
    width: 120rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 0 10rpx;
    color: #f23921;
    text-align: center;
}

.s-bottom {
    display: flex;
    padding: 20rpx 20rpx 0rpx 20rpx;
}

.s-info {
    width: 65%;
}

.s-title {
    font-size: 4vw;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.s-money {
    color: #f23921;
    font-size: 5vw;
}

.s-code {
    width: 30%;
    margin-left: 2%;
}

.s-code image {
    width: 100%;
    height: 168rpx;
}

.tips {
    background: #f23921;
    height: 80rpx;
    line-height: 80rpx;
    width: 50%;
    margin: 0 auto;
    text-align: center;
    color: #fff;
    border-radius: 10rpx;
    margin-top: 20rpx;
}

.bottom-line {
    height: 120rpx;
}

.comment {
    padding: 20rpx;
    background: #fff;
    margin-top: 20rpx;
}

.comment .comment-header {
    display: flex;
    justify-content: space-between;
}

.comment .comment-header-title {
    font-size: 30rpx;
    letter-spacing: 2rpx;
    font-weight: bold;
}

.comment .comment-header-left {
    color: #f23921;
}

.comment .comment-header-left::after {
    content: '';
    display: inline-block;
    width: 16rpx;
    height: 16rpx;
    border-top: 2rpx solid #f23921;
    border-right: 2rpx solid #f23921;
    transform: rotate(45deg);
    margin-left: 10rpx;
}

.comment .comment-list-group {
    padding-top: 10rpx;
    padding-bottom: 10rpx;
}

.comment .comment-list-item {
    padding-bottom: 10rpx;
}

.comment .comment-list-item-info {
    display: flex;
    height: 70rpx;
    align-items: center;
}

.comment .comment-list-item-info image {
    border-radius: 50%;
}

.comment .comment-list-item-info view {
    margin-left: 20rpx;
    color: #bbb;
    font-size: 28rpx;
}

.comment .comment-list-item-info text {
    color: #666;
    font-size: 28rpx;
}