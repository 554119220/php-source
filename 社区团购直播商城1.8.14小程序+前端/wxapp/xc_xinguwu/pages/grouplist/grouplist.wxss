::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.search {
    width: 100%;
    background: #f9f9f9;
    padding: 10rpx 0;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 10;
}

.weui-search-bar {
    background: #f8f8f8;
    border: 0;
}

.weui-search-bar__form {
    border: 0;
}

.weui-search-bar__input {
    border: 0;
    height: 70rpx;
    line-height: 70rpx;
    align-items: center;
}

.weui-icon-search {
    margin-top: 10rpx;
}

.weui-icon-search_in-box {
    position: absolute;
    left: 10px;
    top: 24rpx;
}

.weui-search-bar__cancel-btn {
    margin-left: 10px;
    line-height: 70rpx;
    color: #09bb07;
    white-space: nowrap;
    margin-top: 5rpx;
}

.weui-icon-clear {
    position: absolute;
    top: 8rpx;
    right: 0;
    padding: 7px 8px;
    font-size: 0;
}

.searchbar-result {
    margin-top: 0;
    font-size: 14px;
}

.searchbar-result:before {
    display: none;
}

.weui-cell {
    padding: 12px 15px 12px 35px;
}

.goods-tab-box-title {
    position: fixed;
    left: 0;
    top: 114rpx;
    z-index: 5;
    width: 100%;
    border-top: 1rpx solid #f9f9f9;
    display: flex;
    align-items: center;
}

.menu {
    width: 90%;
    display: flex;
    align-items: center;
    height: 90rpx;
    background: #fff;
    white-space: nowrap;
}

.goods-tab-nav {
    display: inline-block;
    width: 22%;
    height: 90rpx;
    line-height: 90rpx;
    box-sizing: border-box;
    text-align: center;
    color: #444;
    background-color: #fff;
}

.goods-tab-nav.on view {
    width: 80%;
    margin: 0 auto;
    color: #f23921;
    border-bottom: 2rpx solid #f23921;
    height: 85rpx;
}

.kind-tab-box-title {
    position: fixed;
    left: 0;
    top: 194rpx;
    z-index: 5;
    width: 100%;
    padding: 20rpx 0;
    display: flex;
    background: #fff;
    border-top: 1rpx solid #e5e5e5;
    align-items: center;
}

.tab-box-title {
    width: 90%;
}

.tab-nav {
    display: inline-block;
    width: 33%;
    height: 30rpx;
    line-height: 30rpx;
    box-sizing: border-box;
    text-align: center;
    color: #444;
}

.tab-nav.on view {
    margin: 0 auto;
    color: #569bb2;
}

.tab-nav2 {
    border-left: 1rpx solid #e9e9e9;
    border-right: 1rpx solid #e9e9e9;
}

.change {
    background: #fff;
    width: 10%;
    height: 90rpx;
    display: flex;
    align-items: center;
}

.change image {
    width: 35rpx;
    height: 35rpx;
    margin: 0 auto;
}

.totallist {
    z-index: 6;
    position: fixed;
    top: 194rpx;
    left: 0;
    width: 100%;
    height: 72vh;
}

.all-list {
}

.all-list .main-list {
    display: flex;
    align-items: center;
}

.double-list {
    z-index: 10;
    position: fixed;
    left: 3%;
    width: 94%;
}

.double-list .flex {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.all-list .list {
    background: #fff;
    margin-top: 20rpx;
    padding: 20rpx 0;
}

.double-list .list {
    width: 48.5%;
    background: #fff;
    margin-top: 20rpx;
}

.all-list .main-list {
    width: 94%;
    margin: 0 auto;
    display: flex;
}

.all-list .main-list image {
    width: 206rpx;
    height: 200rpx;
}

.double-list .main-list image {
    width: 100%;
    height: 340rpx;
}

.all-list .content {
    width: 70%;
    margin-left: 20rpx;
}

.all-list .content-name {
    position: relative;
}

.double-list .content-name {
    position: relative;
    margin-top: 10rpx;
}

.all-list .group-kind {
    width: 85rpx;
    height: 35rpx;
    line-height: 35rpx;
    position: absolute;
    left: 0;
    top: 0;
    color: #f04043;
    border: 1rpx solid #f04043;
    text-align: center;
    border-radius: 8rpx;
}

.double-list .group-kind {
    width: 85rpx;
    height: 35rpx;
    line-height: 35rpx;
    position: absolute;
    left: 10rpx;
    top: 0;
    color: #f04043;
    border: 1rpx solid #f04043;
    text-align: center;
    border-radius: 8rpx;
}

.all-list .content-name text {
    text-indent: 100rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    word-break: break-all;
    font-weight: bold;
}

.double-list .content-name text {
    text-indent: 110rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    word-break: break-all;
    font-weight: bold;
}

.all-list .content-price {
    display: flex;
    margin-top: 30rpx;
    height: 60rpx;
    line-height: 60rpx;
}

.double-list .content-price {
    display: flex;
    height: 60rpx;
    line-height: 60rpx;
    justify-content: space-between;
    width: 96%;
}

.all-list .content-money {
    width: 120rpx;
    color: #f23921;
}

.double-list .content-money {
    width: 50%;
    color: #f23921;
}

.all-list .content-saled {
    width: 160rpx;
    color: #d4d4d4;
}

.double-list .content-saled {
    width: 50%;
    color: #d4d4d4;
    text-align: right;
}

.all-list .content-btn {
    width: 120rpx;
    height: 60rpx;
    text-align: center;
    color: #f04043;
    line-height: 60rpx;
    border: 1rpx solid;
    border-radius: 15rpx;
    margin-left: 60rpx;
}

.bottom {
    margin-top: 10rpx;
    text-align: center;
    color: #d4d4d4;
}