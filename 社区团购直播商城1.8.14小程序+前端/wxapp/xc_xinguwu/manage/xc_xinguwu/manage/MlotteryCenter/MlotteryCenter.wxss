.search-box {
    height: 80rpx;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
}

.search-main {
    width: 640rpx;
    height: 75rpx;
    border-radius: 38rpx;
    display: flex;
    padding: 0 35rpx;
    align-items: center;
    justify-content: space-between;
    border: 1rpx solid #e5e5e5;
}

.search-main input {
    width: 540rpx;
}

.search-main image {
    width: 38rpx;
    height: 38rpx;
}

.l-nav {
    display: flex;
    align-items: center;
    height: 90rpx;
    line-height: 90rpx;
    width: 100%;
    background: #fff;
    border-bottom: 1rpx solid #f8f8f8;
}

.l-nav-list {
    width: 33.33%;
    text-align: center;
}

.on {
    color: #f93d3d;
}

.line {
    height: 80rpx;
}

.ver-box {
    background: #fff;
    padding: 0 20rpx;
}

.ver-list {
    padding: 30rpx 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #e3e3e3;
}

.ver-list image {
    width: 155rpx;
    height: 140rpx;
    border-radius: 10rpx;
}

.ver-content {
    width: 520rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.content-name {
    font-size: 28rpx;
}

.content-date {
    color: #888;
    font-size: 3vw;
    margin-top: 20rpx;
}

.ver-code {
    width: 115rpx;
    height: 50rpx;
    border-radius: 25rpx;
    line-height: 50rpx;
    text-align: center;
    color: #f93d3d;
    border: 1rpx solid #f93d3d;
}

.change {
    color: #f93d3d;
    margin-top: -60rpx;
}

.model {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.4);
    z-index: 5;
}

.model-box {
    width: 625rpx;
    height: 605rpx;
    background: #fff;
    border-radius: 20rpx;
    margin: 0 auto;
    margin-top: 205rpx;
}

.model-head {
    text-align: center;
    position: relative;
    height: 125rpx;
    line-height: 125rpx;
    border-bottom: 1rpx solid #e7e7e7;
    font-size: 36rpx;
    letter-spacing: 5rpx;
}

.model-box .model-close {
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    right: 38rpx;
    top: 24rpx;
}

.model-content {
    margin-top: 45rpx;
}

.model-list {
    display: flex;
    align-items: center;
    padding: 0 75rpx;
    margin-bottom: 20rpx;
    font-size: 28rpx;
}

.list-left {
    color: #888;
    margin-right: 35rpx;
}

.ipt {
    width: 400rpx;
    height: 80rpx;
    background: #f9f9f9;
    margin: 0 auto;
    margin-top: 10rpx;
    padding: 0 35rpx;
}

.model-bottom {
    display: flex;
    padding: 0 60rpx;
    margin-top: 40rpx;
    justify-content: space-between;
}

.model-bottom view {
    width: 235rpx;
    height: 85rpx;
    border-radius: 43rpx;
    border: 1rpx solid #f93d3d;
    text-align: center;
    line-height: 85rpx;
}

.cancel {
    color: #f93d3d;
}

.make-sure {
    background: #f93d3d;
    color: #fff;
}

.method-model {
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.4);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 5;
}

.method-box {
    background: #fff;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 6;
    width: 100%;
}

.method-list {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #e4e4e4;
}

.method-list image {
    width: 40rpx;
    height: 25rpx;
    margin-right: 15rpx;
}

.method-cancel {
    padding: 35rpx 0;
    text-align: center;
    color: #888;
}

.no-add-model {
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.4);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 5;
}

.toAdd-box {
    width: 630rpx;
    background: #fff;
    border-radius: 30rpx;
    margin: 0 auto;
    margin-top: 300rpx;
    text-align: center;
    padding-bottom: 35rpx;
}

.toAdd-box .model-close {
    width: 27rpx;
    height: 27rpx;
    float: right;
    margin-right: 35rpx;
    margin-top: 35rpx;
}

.clear {
    clear: both;
}

.no-address-text {
    margin-top: 45rpx;
    font-size: 32rpx;
}

.add-address {
    text-align: center;
    width: 480rpx;
    line-height: 90rpx;
    height: 90rpx;
    border-radius: 45rpx;
    background: #f93d3d;
    color: #fff;
    margin: 0 auto;
    margin-top: 80rpx;
}

.ask-for-model {
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.4);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 5;
}

.ask-for-box {
    background: #fff;
    width: 620rpx;
    margin: 0 auto;
    margin-top: 180rpx;
    border-radius: 10rpx;
}

.ask-for-head {
    display: flex;
    align-items: center;
    padding: 30rpx 35rpx;
    border-bottom: 1rpx solid #ecebeb;
    justify-content: space-between;
}

.ask-for-head image {
    width: 27rpx;
    height: 27rpx;
}

.ask-for-main {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 30rpx 35rpx;
}

.ask-for-main image {
    width: 145rpx;
    height: 145rpx;
    border-radius: 20rpx;
}

.ask-for-content {
    width: 370rpx;
}

.ask-for-name {
    font-weight: bold;
}

.ask-for-content-list {
    display: flex;
    align-items: center;
    color: #888;
    margin-top: 5rpx;
}

.ask-for-user {
    display: flex;
    align-items: center;
}

.ask-for-user view {
    width: 14rpx;
}

.ask-for-address {
    padding: 25rpx 35rpx;
    border-top: 1rpx solid #e4e4e4;
}

.ask-for-address-head {
    display: flex;
    align-items: center;
    font-weight: bold;
}

.ask-for-address-head1 {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.ask-for-address-head1 image {
    width: 15rpx;
    height: 25rpx;
}

.ask-for-address-head-left {
    display: flex;
    align-items: center;
}

.ask-for-tell {
    margin-left: 50rpx;
}

.ask-for-mAddress {
    color: #888;
    margin-top: 10rpx;
}

.change-code {
    padding: 30rpx 35rpx;
    border-top: 1rpx solid #e4e4e4;
}

.change-code image {
    width: 220rpx;
    height: 220rpx;
    margin: 20rpx auto;
}

.code-num {
    text-align: center;
}

.odd-numbers {
    padding: 30rpx 35rpx 50rpx 35rpx;
    border-top: 1rpx solid #e4e4e4;
}

.number-list {
    margin-bottom: 5rpx;
    color: #888;
}