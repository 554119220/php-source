page {
    background: #f9f9f9;
    color: #444;
}

.invite {
    display: flex;
    align-items: center;
    padding: 20rpx 0;
    border-top: 4rpx solid #f9f9f9;
    background: #fff;
    font-weight: bold;
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 5;
}

.invite .tab-nav {
    width: 49.9%;
    text-align: center;
}

.invite .tab-nav:nth-child(1) {
    border-right: 1rpx solid #ebebeb;
}

.on {
    color: #f93d3d;
}

.team {
    background: #fff;
    margin-top: 110rpx;
}

.team .team-member {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 90%;
    margin: 0 auto;
    padding: 20rpx;
    border-bottom: 1rpx solid #ebebeb;
}

.team .team-member:last-child {
    border: 0;
    margin-bottom: 120rpx;
}

.head {
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
}

.content {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 520rpx;
}

.content image {
    width: 30rpx;
    height: 30rpx;
}

.content-main view:nth-child(2) {
    margin-top: 10rpx;
}

.agree {
    width: 110rpx;
    height: 55rpx;
    border-radius: 30rpx;
    background: #f93d3d;
    color: #fff;
    line-height: 55rpx;
    text-align: center;
}

.team-code {
    width: 480rpx;
    height: 670rpx;
    position: fixed;
    left: 135rpx;
    top: 280rpx;
    z-index: 6;
}

.code-main {
    width: 100%;
    height: 100%;
    position: relative;
}

.code-bg {
    width: 100%;
    height: 100%;
}

.team-content {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

.code-head {
    width: 135rpx;
    height: 135rpx;
    border-radius: 50%;
    margin: 0 auto;
    margin-top: -65rpx;
    border: 8rpx solid #fff;
}

.code-name {
    text-align: center;
    margin-top: 20rpx;
}

.code-detail {
    width: 350rpx;
    height: 325rpx;
    margin: 0 auto;
    margin-top: 40rpx;
}

.hint {
    text-align: center;
    height: 70rpx;
    line-height: 70rpx;
}

.save {
    width: 120rpx;
    margin: 0 auto;
    margin-left: 160rpx;
    text-align: center;
    color: #f93d3d;
    margin-top: 20rpx;
}

.bottom-line {
    height: 180rpx;
}

.invite-team {
    padding: 0;
    margin: 0;
    height: 90rpx;
    line-height: 90rpx;
    color: #fff;
    background: #f93d3d;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    z-index: 4;
    border-radius: 0;
}

.toshare {
    position: fixed;
    left: 0;
    bottom: 0;
    background: #fff;
    width: 100%;
    z-index: 5;
}

.share-btn {
    width: 100%;
    display: flex;
    padding: 50rpx 0 20rpx 0;
    justify-content: center;
}

.share-w {
    text-align: center;
    letter-spacing: 5rpx;
    background: #fff;
    border-radius: 0;
    width: 140rpx;
    padding: 0;
    color: #666;
    margin: 0;
}

.wid {
    width: 160rpx;
}

button::after {
    border: 0;
}

.share-w image {
    width: 140rpx;
    height: 140rpx;
    margin: 0 auto;
    border-radius: 50%;
    padding-top: 15rpx;
}

.share-i {
    text-align: center;
    letter-spacing: 5rpx;
    background: #fff;
    border-radius: 0;
    width: 140rpx;
    padding: 0;
    margin: 0;
    color: #666;
}

button[class="share-i"]::after {
    border: 0;
}

.share-i image {
    width: 140rpx;
    height: 140rpx;
    margin: 0 auto;
    border-radius: 50%;
    padding-top: 15rpx;
}

.close-btn {
    text-align: center;
    padding: 20rpx 0;
    border-top: 1rpx solid #e7e7e7;
}