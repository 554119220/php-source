.float {
    position: fixed;
    bottom: 120rpx;
    right: 30rpx;
    width: 100rpx;
}

.float-group {
    display: flex;
    flex-direction: column;
}

.float-list {
    margin-bottom: 10rpx;
    border-radius: 50%;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0;
    background: #fff;
    box-shadow: 2rpx 4rpx 2rpx #999;
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: center;
}

.float-list::after {
    border: none;
}

.float-list .ximg {
    width: 72rpx;
    height: 72rpx;
}