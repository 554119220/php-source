<template name="goHome">
    <navigator catchtouchmove="moveGoHome" class="goHome2" hoverClass="active" openType="redirect" style="left:{{left}}rpx;top:{{top}}rpx" url="/xc_xinguwu/pages/index/index">
        <image mode="widthFix" src="{{blackhomeimg?blackhomeimg:'/we7/resource/images/blackhome.png'}}"></image>
    </navigator>
</template>
<template name="footerWx">
    <view class="we7-bottom" style="background-color:{{backgroundColor}};border-color:{{borderStyle}}" wx:if="{{list}}">
        <block wx:for="{{list}}" wx:key="{{index}}">
            <view class="we7-bottom-item" hoverClass="active" wx:if="{{item.pageUrl=='/'+thisurl||'xc_xinguwu/minor/index/index'==thisurl&&index==0}}">
                <navigator hoverClass="active" openType="redirect" url="{{item.pagePath}}">
                    <image class="item-img" mode="aspectFill" src="{{item.selectedIconPath}}"></image>
                    <view class="item-text" style="color: {{selectedColor}};">{{item.text}} </view>
                </navigator>
            </view>
            <view class="we7-bottom-item" hoverClass="active" wx:else>
                <navigator hoverClass="active" hoverStayTime="1000" openType="redirect" url="{{item.pagePath}}">
                    <image class="item-img" mode="aspectFill" src="{{item.iconPath}}"></image>
                    <view class="item-text" style="color: {{color}}">{{item.text}}</view>
                </navigator>
                <view class="car_top" wx:if="{{item.pageUrl=='/xc_xinguwu/pages/cart/cart'&&number!=0}}">
                    <text>{{number}}</text>
                </view>
            </view>
        </block>
    </view>
    <view class="we7-bottom-placeholder"></view>
</template>
