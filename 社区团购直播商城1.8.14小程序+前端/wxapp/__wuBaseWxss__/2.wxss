.dis-tab-box {
    background: #fff;
    height: 90rpx;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    display: flex;
    border-top: 1rpx solid #f8f8f8;
    align-items: center;
    z-index: 5;
}

.dis-tab-box text {
    color: #eaeaea;
}

.dis-tab-box .tab-nav {
    width: 49.9%;
    text-align: center;
    height: 50rpx;
    border-right: 1rpx solid #eaeaea;
    color: #666;
}

.dis-tab-box .tab-nav:last-child {
    border: 0;
}

.on view {
    width: 23%;
    margin: 0 auto;
    border-bottom: 1rpx solid #f23921;
    color: #f23921;
    height: 68rpx;
}

.contnet-line {
    height: 112rpx;
}

.dis-list {
    padding: 0 20rpx;
}

.dis-list .xc-dis-kind1 {
    height: 200rpx;
    position: relative;
    margin-bottom: 30rpx;
}

.dis-bg {
    width: 100%;
    height: 100%;
}

.xc-dis-kind1 .dis-content {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    display: flex;
    align-items: center;
}

.dis-2 .dis-left {
    color: #ff7e19;
}

.dis-2 .dis-left text {
    color: #ff7e19;
}

.dis-2 .content-head-left {
    color: #ff7e19;
}

.dis-2 .content-head-left text {
    color: #ff7e19;
}

.dis-2 .right-bottom-right {
    background: #ff7e19;
}

.dis-2 .dis-state {
    color: #ff7e19;
}

.dis-1 .dis-left {
    color: #fb5750;
}

.dis-1 .dis-left text {
    color: #fb5750;
}

.dis-1 .content-head-left {
    color: #fb5750;
}

.dis-1 .content-head-left text {
    color: #fb5750;
}

.dis-1 .right-bottom-right {
    background: #fb5750;
}

.dis-1 .dis-state {
    color: #fb5750;
}

.dis-3 .dis-left {
    color: #fdc01e;
}

.dis-3 .dis-left text {
    color: #fdc01e;
}

.dis-3 .content-head-left {
    color: #fdc01e;
}

.dis-3 .content-head-left text {
    color: #fdc01e;
}

.dis-3 .right-bottom-right {
    background: #fdc01e;
}

.dis-3 .dis-state {
    color: #fdc01e;
}

.xc-dis-kind1 .dis-left {
    width: 240rpx;
    text-align: center;
}

.xc-dis-kind1 .dis-money {
    height: 60rpx;
    line-height: 60rpx;
    font-weight: bold;
}

.xc-dis-kind1 .dis-center {
    width: 400rpx;
    height: 200rpx;
}

.xc-dis-kind1 .dis-center .dis-cneter-content {
    width: 80%;
    margin: 0 auto;
    height: 200rpx;
}

.xc-dis-kind1 .dis-info {
    margin-top: 10rpx;
    font-weight: bold;
}

.xc-dis-kind1 .swiper-box {
    height: 110rpx;
    margin-top: 30rpx;
}

.xc-dis-kind1 .swiper-box swiper-item {
    height: 110rpx;
}

.xc-dis-kind1 .swiper-box swiper-item:nth-child(1) .text {
    line-height: 30rpx;
    font-size: 3vw;
    color: #888;
    overflow: hidden;
    height: 30rpx;
}

.xc-dis-kind1 .swiper-box swiper-item:nth-child(2) .text {
    line-height: 30rpx;
    font-size: 3vw;
    color: #888;
    overflow: hidden;
    height: 60rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.xc-dis-kind1 .dis-change {
    display: flex;
    align-items: center;
    margin-top: 20rpx;
}

.xc-dis-kind1 .dis-change image {
    width: 10rpx;
    height: 20rpx;
    margin-right: 30rpx;
}

.xc-dis-kind1 .dis-right {
    width: 75rpx;
}

.xc-dis-kind1 .dis-right view {
    width: 50rpx;
    text-align: center;
    margin: 0 auto;
    line-height: 30rpx;
    color: #fff;
}

.xc-dis-kind2 {
    height: 270rpx;
    position: relative;
    margin-bottom: 30rpx;
}

.xc-dis-kind2 .dis-content {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.xc-dis-kind2 .dis-content-head {
    display: flex;
    align-items: center;
}

.xc-dis-kind2 .content-head-left {
    width: 230rpx;
    display: flex;
    align-items: center;
    height: 170rpx;
}

.xc-dis-kind2 .content-head-left-main {
    display: flex;
    align-items: baseline;
    justify-content: center;
    width: 230rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.xc-dis-kind2 .content-head-left-main text:nth-child(1) {
    width: 20rpx;
}

.xc-dis-kind2 .content-head-left view {
    font-weight: bold;
}

.xc-dis-kind2 .content-head-right {
    width: 420rpx;
    margin-left: 40rpx;
}

.xc-dis-kind2 .content-head-right .right-bottom {
    display: flex;
    justify-content: space-between;
}

.xc-dis-kind2 .right-bottom-left {
    color: #888;
    overflow: hidden;
    line-height: 30rpx;
    margin-top: 5rpx;
}

.xc-dis-kind2 .dis-kind {
    font-weight: bold;
}

.xc-dis-kind2 .right-bottom-right {
    width: 120rpx;
    border-radius: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-top: 15rpx;
    text-align: center;
    color: #fff;
}

.xc-dis-kind2 .dis-content-bottom {
    width: 90%;
    margin: 0 auto;
    height: 95rpx;
    display: flex;
    align-items: center;
}

.xc-dis-kind2 .dis-content-bottom view {
    color: #888;
    height: 60rpx;
    line-height: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.xc-dis-kind3 {
    border-radius: 15rpx;
    overflow: hidden;
    box-shadow: 0 0 8rpx #e3e3e3;
    margin-bottom: 30rpx;
    z-index: 5;
}

.xc-dis-kind3 .dis-bg {
    width: 100%;
    height: 100%;
}

.xc-dis-kind3 .xc-dis-kind3-head {
    width: 100%;
    height: 175rpx;
    position: relative;
}

.xc-dis-kind3 .xc-dis-kind3-info {
    min-height: 50rpx;
}

.xc-dis-kind3 .kind3-info-head {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 30rpx;
    height: 60rpx;
}

.xc-dis-kind3 .xc-dis-kind3-content {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    display: flex;
}

.xc-dis-kind3 .kind3-content-left {
    width: 220rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 175rpx;
    color: #fff;
}

.xc-dis-kind3 .kind3-content-left text {
    color: #fff;
}

.kind3-money {
    display: flex;
    align-items: baseline;
}

.xc-dis-kind3 .dis-kind {
    margin-left: 10rpx;
}

.xc-dis-kind3 .kind3-content-right {
    width: 465rpx;
    height: 170rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.xc-dis-kind3 .dis-info {
    color: #fff;
}

.xc-dis-kind3 .dis-state {
    background: #fff;
    text-align: center;
    border-radius: 30rpx;
    height: 60rpx;
    width: 160rpx;
    line-height: 60rpx;
}

.xc-dis-kind3 .kind3-info-head image {
    width: 30rpx;
    height: 20rpx;
}

.xc-dis-kind3 .kind3-info-bottom {
    padding: 0 30rpx 10rpx 30rpx;
    color: #888;
}

.dis-used {
    width: 130rpx;
    border-radius: 50%;
    height: 130rpx;
    position: absolute;
    right: 30rpx;
    top: 30rpx;
    z-index: 5;
    transform: rotateZ(-30deg);
}