.kuaidiAddress {
    padding: 30rpx 30rpx 0rpx;
    display: flex;
    flex-direction: column;
    width: 690rpx;
    font-size: 28rpx;
    color: #000;
    margin-top: 20rpx;
    background-color: #fff;
    position: relative;
}

.positionIcon {
    width: 30rpx;
    height: 40rpx;
    position: absolute;
    left: 30rpx;
    top: 40rpx;
}

.name-number {
    display: flex;
    width: 640rpx;
    justify-content: space-between;
    flex-wrap: wrap;
    margin-left: 50rpx;
}

.shipping-address {
    margin: 10rpx 0rpx 30rpx 50rpx;
}

.bottomBorder {
    width: 750rpx;
    height: 10rpx;
    position: absolute;
    left: 0rpx;
    bottom: 70rpx;
}

.addNew {
    width: 600rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    margin: 30rpx auto;
    border-radius: 10rpx;
    box-sizing: border-box;
    font-size: 32rpx;
    background-color: #ffb825;
    color: #fff;
}

.morenBox {
    padding: 20rpx 0rpx;
    justify-content: flex-end;
}

.gou_icon_box {
    width: 30rpx;
    height: 30rpx;
}

.gou_icon {
    width: 30rpx;
    height: 30rpx;
    display: flex;
}

.morenWord {
    font-size: 28rpx;
    color: #999;
    margin-left: 20rpx;
}