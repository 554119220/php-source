.form-box {
    margin-top: 30rpx;
}

.label_Row {
    height: 90rpx;
    display: flex;
    flex: 1;
    background-color: #fff;
    font-size: 30rpx;
    color: #000;
    margin: 0rpx 30rpx 20rpx;
    border-radius: 20rpx;
    padding: 0rpx 20rpx;
    line-height: 90rpx;
}

.big_Row {
    background-color: #fff;
    margin: 0rpx 30rpx 20rpx;
    border-radius: 20rpx;
    overflow: hidden;
}

.big_label_Row {
    height: 90rpx;
    display: flex;
    flex: 1;
    background-color: #fff;
    font-size: 30rpx;
    color: #000;
    padding: 0rpx 20rpx;
}

.input {
    color: #999;
    width: 500rpx;
    text-align: left;
}

.textarea {
    width: 100%;
    height: 100rpx;
}

.checkDataBox {
    font-size: 26rpx;
    color: #999;
    padding: 25rpx;
}

.checkbox {
    line-height: 40rpx;
    margin-left: 60rpx;
}

.checkbox.active {
    color: #ffb62b;
}

.checkbox text {
    margin-left: 10rpx;
}

.picker {
    width: 200rpx;
    text-align: center;
}

.role {
    margin: 0rpx 20rpx;
}

.banner_Row {
    background-color: #fff;
    margin: 0rpx 30rpx;
    border-radius: 20rpx;
    font-size: 30rpx;
    color: #000;
    margin-bottom: 20rpx;
}

.advice_wid {
    padding: 20rpx 20rpx 40rpx 20rpx;
}

.intro-title {
    margin: 0rpx 20rpx;
    padding: 20rpx 0rpx 0rpx 0rpx;
    border-bottom: 1rpx solid #eee;
}

.intro textarea {
    width: 100%;
}

.adv_width {
    padding: 10rpx 20rpx 60rpx 20rpx;
}

button::after {
    border: none;
}

.margin {
    margin: 40rpx auto 20rpx;
}

.content_add {
    width: 150rpx;
    height: 150rpx;
}

.add_img {
    width: 150rpx;
    height: 150rpx;
    border-radius: 10rpx;
    display: flex;
    border: 1rpx solid #eee;
    margin-left: 5%;
}

.noBorder {
    border: none;
}

.upload {
    height: auto;
    background: #fff;
    padding: 0rpx 10rpx 10rpx;
}

.content_add {
    width: 25%;
    height: 100%;
    display: inline-block;
}

.banner {
    font-size: 26rpx;
    color: #000;
}

.border {
    border-bottom: 1rpx solid #eee;
}

.banner_list {
    height: 90rpx;
    padding: 0rpx 30rpx;
}

.confirm {
    width: 600rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #ffb825;
    color: #fff;
    line-height: 90rpx;
    text-align: center;
    margin-top: 40rpx;
    margin-bottom: 30rpx;
}

button::after {
    border: none;
}

.delete {
    width: 40rpx;
    height: 40rpx;
    right: 10rpx;
    top: 0rpx;
}

.ab {
    width: 40rpx;
    display: flex;
}

checkbox {
    width: 30rpx;
    height: 30rpx;
}

checkbox .wx-checkbox-input {
    width: 30rpx;
    height: 30rpx;
    margin-top: -10rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    background: #ffb62b;
    border: none;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}

