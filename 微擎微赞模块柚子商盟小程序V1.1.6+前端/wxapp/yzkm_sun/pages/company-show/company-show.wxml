<view class="container">
    <image class="companyImg" mode="widthFix" src="{{url}}{{aboutUs.us_img}}"></image>
    <view class="bottomWord">
        <view class="companyTitle">关于我们</view>
        <text class="Contents">{{aboutUs.us_details}}</text>
        <view class="connectUs">联系我们</view>
        <view class="Contents">
            <view>联系电话：{{aboutUs.us_tel}}</view>
            <view>公司地址：{{aboutUs.us_addr}}</view>
        </view>
    </view>
    <view bindtap="callMe" class="callMe" data-tel="{{aboutUs.us_tel}}">联系我们</view>
</view>
