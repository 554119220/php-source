<view>
    <view class="userinfo">
        <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" wx:if="{{userInfo&&userInfo.avatarUrl}}"></image>
        <text class="userinfo-nickname">{{userInfo.nickName}}</text>
        <view bindtap="saomaCode" class="code_box">
            <image class="code_icon" src="../../resource/images/entr/saoma.png"></image>
        </view>
    </view>
    <view class="todayRow">
        <view class="item_today">
            <view class="item_num">0</view>
            <view>今日订单</view>
        </view>
        <view class="item_today">
            <view class="item_num">0</view>
            <view>今日收入</view>
        </view>
        <view class="item_today">
            <view class="item_num">0</view>
            <view>日访问量</view>
        </view>
    </view>
    <view class="serverRow">
        <view class="server_title">我的财务</view>
        <view class="list_caiwu">
            <view class="left_Box">
                <view class="caiwu_item">
                    <view class="caiwu_num">0</view>
                    <view>今日收益</view>
                </view>
                <view class="caiwu_item">
                    <view class="caiwu_num">0</view>
                    <view>昨日收益</view>
                </view>
                <view class="caiwu_item">
                    <view class="caiwu_num">0</view>
                    <view>累计收益</view>
                </view>
                <view class="caiwu_item">
                    <view class="caiwu_num">0</view>
                    <view>￥{{balance_sj}}</view>
                </view>
            </view>
            <view bindtap="withDrawalTap" class="right-btn" data-balance_sj="{{balance_sj}}">提现</view>
        </view>
    </view>
    <view class="serverRow">
        <view class="server_title">订单核销</view>
        <view class="shuruBox">
            <input bindinput="checkOrderNum" class="inputBox" placeholder="请输入订单编号" placeholderClass="diyHolder" value="{{orderNum}}"></input>
            <view bindtap="deterBtn" class="deterBtn">确定提交</view>
        </view>
    </view>
    <button bindtap="backIndex" class="backFir-btn">回到首页</button>
    <view class="show-popup" hidden="{{hideRuzhu}}">
        <view bindtap="closePopupTap" class="popup-mask"></view>
        <view class="rulesBox">
            <view class="paySuccess">订单详情</view>
            <view class="a-order">
                <view class="goodsList">
                    <image class="goodsImg" src="{{url}}{{info.img}}"></image>
                    <view class="goodsMes">
                        <view class="goodsName">{{info.gname}}</view>
                        <view class="price-guige">
                            <view class="price">¥{{info.money}}</view>
                            <view class="num">
                                <text hidden="{{!info.guige}}">规格：{{info.guige}}，</text>
                                <text hidden="{{!info.buynum}}">数量：{{info.buynum}}</text>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
            <view class="kuaidiAddress">
                <image class="positionIcon" src="../../../resource/images/payOrder/dizhi.png"></image>
                <view class="name-number">
                    <view>收货人：{{info.userName}}</view>
                </view>
                <view class="shipping-address">
          联系电话：{{info.phone}}
        </view>
                <image class="bottomBorder" src="../../../resource/images/payOrder/biankuang.jpg"></image>
            </view>
            <view bindtap="applyFor" class="push-btn" data-id="{{info.id}}">确定核销</view>
        </view>
    </view>
</view>
