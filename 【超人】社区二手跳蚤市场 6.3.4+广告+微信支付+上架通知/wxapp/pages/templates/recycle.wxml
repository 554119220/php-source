<template name="recycleTemplate">
    <block wx:if="{{open}}">
        <navigator appId="{{style.appid}}" hoverClass="none" path="{{style.url}}" target="miniProgram" wx:if="{{style.appid!=''}}">
            <view class="recycle-wrapper" style="{{style.bgcolor}};bottom: {{bottom}}">
                <view class="iconfont icon-recycle" wx:if="{{!style.icon}}"></view>
                <view class="recycle-icon" wx:else>
                    <image src="{{style.icon}}"></image>
                </view>
                <text class="text">{{style.title}}</text>
            </view>
        </navigator>
        <view bindtap="jumpToPage" data-url="{{style.url}}" wx:else>
            <view class="recycle-wrapper" style="{{style.bgcolor}};bottom: {{bottom}}">
                <view class="iconfont icon-recycle" wx:if="{{!style.icon}}"></view>
                <view class="recycle-icon" wx:else>
                    <image src="{{style.icon}}"></image>
                </view>
                <text class="text">{{style.title}}</text>
            </view>
        </view>
    </block>
</template>
