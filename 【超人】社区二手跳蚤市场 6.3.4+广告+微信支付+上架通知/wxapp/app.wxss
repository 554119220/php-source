.cate-item {
    display: inline-block;
    width: 25%;
    text-align: center;
    margin-bottom: 15px;
}

.cate-img {
    width: 50px;
    height: 50px;
}

.move-panel {
    width: 100%;
    height: 90vh;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99;
}

.move-block {
    width: 100px;
    height: 22px;
    border-radius: 10px;
    padding: 5px 0;
    text-align: center;
    background: #f40;
    color: #fff;
    font-size: 14px;
}

.move-block .iconfont {
    font-size: 20px;
    margin-right: 5px;
}

.clearfix:after {
    content: '';
    width: 0;
    height: 0;
    display: block;
    clear: both;
    visibility: visible;
}

.text-white {
    color: #fff;
}

.text-more {
    overflow-x: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}