<view>
    <view class="selBox">
        <view class="selRow flex-center-spa border_b">
            <picker bindchange="bindMultiPickerChange" bindcolumnchange="bindMultiPickerColumnChange" class="mypicker" mode="multiSelector" range="{{multiArray}}" rangeKey="name" value="{{multiIndex}}">
                <view>选择地区</view>
                <view class="picker">
          {{multiArray[0][ multiIndex[0] ].name}}，{{multiArray[1][ multiIndex[1] ].name}}，{{multiArray[2][ multiIndex[2] ].name}}
        </view>
                <image class="arrow" src="../../resource/images/first/jiantou.png"></image>
            </picker>
        </view>
        <view bindtap="backIndex" class="checkCounty">点击切换</view>
    </view>
</view>
