page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    width: 750rpx;
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 750rpx;
    height: 265rpx;
    font-size: 32rpx;
}

.userinfo-avatar {
    width: 140rpx;
    height: 140rpx;
    margin-top: 40rpx;
    border-radius: 50%;
    border: 5rpx solid rgba(255,255,255,0.5);
}

.userinfo-nickname {
    margin-top: 10rpx;
    font-size: 28rpx;
    color: #fff;
}

.shopMes {
    height: 110rpx;
    background-color: #100d1d;
    color: #fff;
    font-size: 26rpx;
    display: flex;
    justify-content: space-around;
}

.itemMes {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.itemMes .num {
    color: #ffb825;
    font-size: 34rpx;
    margin-top: 5rpx;
}

.itemMes .word {
    margin-top: 10rpx;
}

.dingdan-manager {
    margin-top: 20rpx;
    background-color: #fff;
}

.dingdan-title {
    font-size: 34rpx;
    color: #333;
    width: 750rpx;
    height: 105rpx;
    border-bottom: 1rpx solid #eee;
    display: flex;
}

.dingdan-list {
    display: flex;
}

.dingdan-item {
    width: 250rpx;
    height: 213rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 30rpx;
}

.role {
    width: 1rpx;
    height: 110rpx;
    border-right: 1rpx solid #eee;
    margin-top: 30rpx;
    box-sizing: border-box;
}

.dingdan-icon {
    width: 40rpx;
    height: 45rpx;
    margin-top: 40rpx;
    margin-left: 30rpx;
}

.dingdan-word {
    margin-top: 40rpx;
    margin-left: 20rpx;
}

.dingdanImg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 20rpx;
}

.dingdanImgs {
    width: 80rpx;
    height: 60rpx;
    margin-top: 20rpx;
}

.dingdanImgss {
    width: 64rpx;
    height: 60rpx;
    margin-top: 20rpx;
}

.dingdanWord {
    margin-top: 10rpx;
    color: #999;
}

.dingdanNum {
    color: #ce0000;
}

.roles {
    width: 1rpx;
    height: 110rpx;
    border-right: 1rpx solid #eee;
    margin-bottom: 30rpx;
    box-sizing: border-box;
}

.caiwu-title {
    font-size: 34rpx;
    color: #333;
    width: 750rpx;
    height: 105rpx;
    display: flex;
}

.caiwu-list {
    display: flex;
    flex-wrap: wrap;
}

.caiwu-item {
    width: 375rpx;
    height: 125rpx;
    display: flex;
    border-top: 1rpx solid #eee;
    box-sizing: border-box;
}

.bottom-icon {
    width: 60rpx;
    height: 60rpx;
    margin-left: 85rpx;
    margin-top: 30rpx;
}

.rightMes {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 17rpx;
    margin-left: 30rpx;
    font-size: 30rpx;
}

.caiwuNum {
    color: #ce0000;
}

.caiwuWord {
    color: #999;
}

.footer-box {
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    width: 100%;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    border-top: 1rpx solid #eee;
    background-color: #fff;
}

.diyItem {
    width: 80rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
}

.diyTab {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/gongzuotai.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTab.active {
    background: url(http://oydnzfrbv.bkt.clouddn.com/gongzuotai-h.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTabs {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/dingdan.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTabs.active {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/dingdan-h.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTabss {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/shezhi.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTabss.active {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/shezhi-h.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyWord {
    font-size: 20rpx;
    margin-top: 5rpx;
}

.diyWord.active {
    font-size: 20rpx;
    color: #ce0000;
}