page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    width: 750rpx;
}

.switchRow {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 690rpx;
    height: 100rpx;
    padding: 0 30rpx;
    background-color: #fff;
    margin-top: 20rpx;
}

.leftWord {
    display: flex;
    align-items: center;
}

.title {
    font-size: 30rpx;
    color: #000;
}

.prompt {
    color: #999;
    font-size: 26rpx;
    margin-left: 20rpx;
}

.logOut {
    width: 600rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    background-color: #ffb825;
    color: #fff;
    border-radius: 10rpx;
    font-size: 32rpx;
    margin-top: 40rpx;
}

.footer-box {
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    width: 100%;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    border-top: 1rpx solid #eee;
    background-color: #fff;
}

.diyItem {
    width: 80rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
}

.diyTab {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/gongzuotai.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTab.active {
    background: url(http://oydnzfrbv.bkt.clouddn.com/gongzuotai-h.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTabs {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/dingdan.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTabs.active {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/dingdan-h.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTabss {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/shezhi.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyTabss.active {
    width: 60rpx;
    height: 54rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/shezhi-h.png) no-repeat 0rpx 5rpx;
    background-size: contain;
}

.diyWord {
    font-size: 26rpx;
}

.diyWord.active {
    font-size: 26rpx;
    color: #ce0000;
}