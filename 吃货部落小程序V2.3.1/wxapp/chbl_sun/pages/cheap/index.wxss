.vipTag {
    display: inline-block;
    width: 60rpx;
    height: 30rpx;
    border-radius: 5rpx;
    background-color: #ffb825;
    margin: 0 0 0 20rpx;
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    line-height: 30rpx;
    font-weight: normal;
}

page {
    background-color: #f2f2f2;
}

.nowP {
    color: #ce0000;
}

.scrollView {
    width: 100%;
    height: 90rpx;
    background-color: #fff;
    white-space: nowrap;
    border-top: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
}

.scrollBox {
    min-width: 100%;
    display: inline-block;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.type-navbar {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    height: 90rpx;
}

.type-box {
    line-height: 84rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
    box-sizing: border-box;
}

.type-navbar-item {
    margin: 0 auto;
    text-align: center;
    font-size: 30rpx;
}

.type-navbar-item.type-item-on {
    border-bottom: 4rpx solid #ce0000;
}

.zhaopai {
    margin-top: 20rpx;
    width: 100%;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.shops_item {
    padding: 30rpx 30rpx 20rpx;
    background-color: #fff;
    font-size: 26rpx;
    color: #999;
}

.goodsImg_box {
    width: 210rpx;
    height: 200rpx;
}

.goodsImg {
    width: 210rpx;
    height: 200rpx;
    display: flex;
}

.right_box {
    width: 450rpx;
    margin-left: 30rpx;
}

.one_row {
    font-size: 30rpx;
    color: #000;
    justify-content: space-between;
}

.two_row {
    margin: 10rpx 0rpx;
}

.dw_dis {
    color: #ffb826;
    margin-left: 20rpx;
    position: absolute;
    right: 30rpx;
}

.dw_word {
    width: 260rpx;
}

.four_row {
    margin: 5rpx 0rpx;
}

.dw_box {
    width: 26rpx;
    height: 26rpx;
    margin: 0rpx 10rpx 0rpx 0rpx;
}

.dw-icon {
    width: 26rpx;
    height: 26rpx;
}

.swiper {
    width: 100%;
    background-color: transparent;
}

.footer-box {
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    width: 100%;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    border-top: 1rpx solid #eee;
    background-color: #fff;
}

.diyItem {
    width: 80rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
}

.diyTab {
    width: 54rpx;
    height: 54rpx;
    margin-top: 10rpx;
}

.diyWord {
    font-size: 20rpx;
    margin-top: 5rpx;
}

.diyWord.active {
    font-size: 20rpx;
    color: #ce0000;
}

.grousp_num {
    margin-left: 20rpx;
}