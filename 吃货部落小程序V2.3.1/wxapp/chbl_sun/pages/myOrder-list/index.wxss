.vipTag {
    display: inline-block;
    width: 60rpx;
    height: 30rpx;
    border-radius: 5rpx;
    background-color: #ffb825;
    margin: 5rpx 0 0 20rpx;
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    line-height: 30rpx;
    font-weight: normal;
}

page {
    min-height: 100%;
    background-color: #efefef;
}

.container {
    width: 750rpx;
}

.border_bottom {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    margin-top: 20rpx;
    background-color: #fff;
    border-bottom: 1rpx solid #eeeeee;
}

.status-box {
    width: 690rpx;
    height: 90rpx;
    line-height: 90rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
}

.status_label {
    height: 100%;
    text-align: center;
    font-size: 30rpx;
    color: #000000;
    box-sizing: border-box;
    position: relative;
}

.status_label.active {
    border-bottom: 4rpx solid #ce0000;
}

.flag {
    width: 30rpx;
    height: 30rpx;
    background-color: #ce0000;
    color: #fff;
    text-align: center;
    line-height: 30rpx;
    margin-left: 10rpx;
}

.order-list {
    width: 100%;
}

.order-list .a-order {
    width: 100%;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 20rpx;
}

.orderTitle {
    width: 690rpx;
    height: 70rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    color: #000;
}

.order-number {
    color: #666;
    font-size: 26rpx;
}

.statusWord {
    color: #ce0000;
    font-size: 28rpx;
}

.goodsList {
    width: 690rpx;
    height: 170rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 0rpx 30rpx;
    position: relative;
    border-bottom: 1rpx solid #eee;
    border-top: 1rpx solid #eee;
}

.goodsImg {
    width: 130rpx;
    height: 130rpx;
}

.goodsBoxs {
    display: flex;
}

.goodsMes {
    display: flex;
    flex: 1;
    flex-direction: column;
    margin-left: 30rpx;
}

.goodsName {
    font-size: 30rpx;
    color: #000;
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.price-guige {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.price {
    font-size: 32rpx;
    color: #ce0000;
}

.num {
    color: #999;
    font-size: 26rpx;
    position: absolute;
    right: 30rpx;
}

.statusButton {
    height: 70rpx;
    width: 690rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    font-size: 28rpx;
}

.btn {
    width: 180rpx;
    height: 50rpx;
    box-sizing: border-box;
    text-align: center;
    line-height: 50rpx;
    border-radius: 6rpx;
    margin-left: 20rpx;
    font-size: 30rpx;
}

.cancel-btn {
    color: #333;
    border: 1rpx solid #333;
}

.topay-btn {
    color: #ce0000;
    border: 1rpx solid #ce0000;
}

.backHome {
    width: 100rpx;
    height: 100rpx;
    overflow: hidden;
    position: fixed;
    right: 30rpx;
    bottom: 250rpx;
}

.homeImg {
    width: 100rpx;
    height: 100rpx;
    display: flex;
}