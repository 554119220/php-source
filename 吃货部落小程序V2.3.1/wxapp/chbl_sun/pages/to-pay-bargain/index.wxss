page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    width: 750rpx;
}

.goodsList {
    width: 690rpx;
    height: 170rpx;
    margin-top: 20rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 0rpx 30rpx;
    position: relative;
}

.goodsImg {
    width: 130rpx;
    height: 130rpx;
}

.goodsMes {
    display: flex;
    flex-direction: column;
    margin-left: 30rpx;
}

.goodsName {
    font-size: 30rpx;
    color: #000;
}

.price-guige {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.price {
    font-size: 32rpx;
    color: #ce0000;
}

.num {
    color: #999;
    font-size: 26rpx;
    position: absolute;
    right: 30rpx;
}

.post-get {
    margin-top: 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.connecter {
    margin-top: 20rpx;
}

.post {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #000;
    font-size: 28rpx;
    background-color: #fff;
    width: 690rpx;
    height: 100rpx;
    padding: 0rpx 30rpx;
    border-bottom: 1rpx solid #eee;
}

.spost {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #000;
    font-size: 28rpx;
    background-color: #fff;
    width: 690rpx;
    height: 100rpx;
    padding: 0rpx 30rpx;
    margin-top: 20rpx;
}

.kuaidi {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #000;
    font-size: 28rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/noselect.png) no-repeat 690rpx center;
    background-size: 30rpx auto;
    background-color: #fff;
    width: 690rpx;
    height: 100rpx;
    padding: 0rpx 30rpx;
    border-bottom: 1rpx solid #eee;
}

.kuaidi.active {
    background: url(http://oydnzfrbv.bkt.clouddn.com/select.png) no-repeat 690rpx center;
    background-size: 30rpx auto;
    background-color: #fff;
}

.cheaper {
    color: #ce0000;
}

input {
    color: #999;
    font-size: 26rpx;
}

.kuaidiAddress {
    padding: 30rpx 30rpx;
    display: flex;
    flex-direction: column;
    width: 690rpx;
    font-size: 28rpx;
    color: #000;
    margin-top: 20rpx;
    background-color: #fff;
    position: relative;
}

.positionIcon {
    width: 30rpx;
    height: 40rpx;
    position: absolute;
    left: 30rpx;
    top: 40rpx;
}

.name-number {
    display: flex;
    width: 640rpx;
    justify-content: space-between;
    flex-wrap: wrap;
    margin-left: 50rpx;
}

.shipping-address {
    margin-left: 50rpx;
    margin-top: 10rpx;
}

.bottomBorder {
    width: 750rpx;
    height: 10rpx;
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
}

.footer-box {
    width: 750rpx;
    height: 100rpx;
    display: flex;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    background-color: #fff;
    z-index: 10;
}

.leftMes {
    width: 490rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.totalPrice {
    color: #000;
    font-size: 26rpx;
}

.totalNum {
    color: #666;
    font-size: 26rpx;
}

.jiesuan {
    width: 260rpx;
    height: 100rpx;
    background-color: #ce0000;
    color: #fff;
    font-size: 32rpx;
    line-height: 100rpx;
    text-align: center;
}