.vipTag {
    display: inline-block;
    width: 60rpx;
    height: 30rpx;
    border-radius: 5rpx;
    background-color: #ffb825;
    margin: 0 0 0 20rpx;
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    line-height: 30rpx;
    font-weight: normal;
}

page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    width: 750rpx;
}

.border_bottom {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    background-color: #fff;
    border-bottom: 1rpx solid #eeeeee;
}

.status-box {
    width: 490rpx;
    height: 90rpx;
    line-height: 90rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
}

.status_label {
    height: 100%;
    text-align: center;
    font-size: 30rpx;
    color: #000000;
    box-sizing: border-box;
    position: relative;
}

.status_label.active {
    border-bottom: 4rpx solid #ce0000;
}

.goodItem {
    margin: 30rpx;
    display: flex;
    flex-direction: column;
    background-color: #fff;
    border-radius: 10rpx;
    box-shadow: 0.629px 0.777px 10px 0px rgba(144,147,147,0.1);
}

.goodsImg {
    width: 100%;
    height: 420rpx;
    border-top-left-radius: 15rpx;
    border-top-right-radius: 15rpx;
    display: flex;
}

.goodsName {
    font-size: 26rpx;
    color: #333;
    margin: 15rpx 30rpx 5rpx;
}

.goodsDes {
    display: flex;
    margin: 0 30rpx 10rpx;
    font-size: 26rpx;
    color: #999;
}

.starImgs {
    width: 30rpx;
    height: 30rpx;
    margin: 0 10rpx 0 0;
}

.renjun {
    margin: 0 20rpx 0 0;
}

.goodsTag {
    width: 60rpx;
    height: 40rpx;
    background-color: yellow;
    font-size: 26rpx;
    color: #333;
    text-align: center;
    line-height: 40rpx;
    margin: 0 20rpx 0 0;
}

.goodsYh {
    font-size: 30rpx;
    color: #333;
    flex: 1;
}

.bottomBox {
    padding: 5rpx 0;
    margin: 0 30rpx 0;
}

.bottomBox:last-child {
    padding: 5rpx 0 20rpx;
    margin: 0 30rpx 0;
}

.dw_icon {
    width: 25rpx;
    height: 25rpx;
    margin: 0 15rpx 0 0;
}

.address {
    color: #999;
    font-size: 26rpx;
}

.distance {
    font-size: 26rpx;
    color: #ff9d55;
}

.join_row {
    position: relative;
    font-size: 26rpx;
    color: #000;
    padding: 0 30rpx 10rpx;
}

.active-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #fff;
    width: 750rpx;
    margin-bottom: 20rpx;
    position: relative;
}

.goodsPic {
    width: 230rpx;
    height: 200rpx;
    margin: 20rpx 30rpx;
}

.goodsMes {
    display: flex;
    flex-direction: column;
    margin-right: 30rpx;
}

.goodsName {
    color: #ce0000;
    font-size: 30rpx;
    margin-top: 20rpx;
}

.shutgoodsName {
    color: #000;
    font-size: 30rpx;
    margin-top: 20rpx;
}

.goodsName.active {
    color: #ce0000;
    font-size: 30rpx;
    margin-top: 20rpx;
}

.pintuanNum {
    color: #000;
    font-size: 26rpx;
    margin-top: 10rpx;
}

.activeShut {
    color: #999;
    font-size: 26rpx;
    margin-top: 10rpx;
}

.joinStatus {
    width: 140rpx;
    height: 50rpx;
    border-radius: 25rpx;
    font-size: 30rpx;
    color: #fff;
    background-color: #ce0000;
    line-height: 50rpx;
    text-align: center;
    position: absolute;
    right: 30rpx;
    bottom: 10rpx;
}

.shutStatus {
    width: 140rpx;
    height: 50rpx;
    border-radius: 25rpx;
    font-size: 30rpx;
    color: #fff;
    background-color: #999;
    line-height: 50rpx;
    text-align: center;
    position: absolute;
    bottom: 20rpx;
    right: 30rpx;
}