<view class="container">
    <view class="topMask">
        <view class="shengyu">距离结束仅剩</view>
        <view class="activeTime">
            <view class="time">{{bargainList[0].clock}}</view>
        </view>
    </view>
    <image class="activeImg" src="{{url}}{{active.thumb}}"></image>
    <view class="centerImg" style="background:url(data:image/png;base64,) no-repeat 0rpx center;background-size: cover;">
        <view class="topWord">{{active.title}}</view>
        <view class="activeWord">{{active.subtitle}}</view>
        <text class="vipTag" wx:if="{{active.is_vip==1}}">VIP</text>
    </view>
    <view class="bottom-wrap">
        <view class="drawModule">
            <view class="imgsWrap">
                <view class="mask" wx:for="{{giftData}}">
                    <view class="maskimg" hidden="{{item.num}}"></view>
                    <image class="imgs" src="{{url}}{{!item.num?item.thumb2:item.thumb}}"></image>
                    <view class="sum" hidden="{{!item.num}}">
                        <text>{{item.num}}</text>
                    </view>
                </view>
            </view>
            <view class="luckyButton" hidden="{{hidden!='1'}}">
                <form bindreset="formReset" bindsubmit="goLucky" reportSubmit="true">
                    <button class="luckyDraw" data-clock="{{bargainList[0].clock}}" data-id="{{active.id}}" formType="submit" hidden="{{giftData.length==Luck.length}}">我要抽奖（剩余{{jikaNum}}次）</button>
                </form>
                <view bindtap="goReceive" class="luckyDraw" data-id="{{active.id}}" data-title="已集齐" hidden="{{giftData.length!=Luck.length}}">点击领取</view>
                <view bindtap="bindShareTap" class="forHelp" data-statu="open">
                    <view data-num="{{active.num}}" data-tid="{{active.id}}" data-title="{{active.title}}"></view>求助好友</view>
            </view>
        </view>
    </view>
    <view class="ruleWrap">
        <text class="ruleTitle">活动规则</text>
        <view>
            <text class="ruleContents">{{active.content}}</text>
        </view>
    </view>
    <view class="wall"></view>
    <view bindtap="joinGroup" class="welfareRow flex-center" wx:if="{{system.showgw==1}}">
        <image class="groupIcon" src="{{url}}{{system.wg_img}}"></image>
        <view class="rightWelf flex1">
            <view class="groupText">{{system.wg_title}}</view>
            <view class="prompt">发送{{system.wg_keyword}}获取福利群详情</view>
        </view>
    </view>
    <view class="wall"></view>
    <view class="address-time border_t">
        <view class="three_row flex-center">
            <view class="flex1 flex-center">
                <view class="dw_box">
                    <image class="dw-icon" src="../../resource/images/first/dw.png"></image>
                </view>
                <text class="dw_word dot flex1">{{active.address}}</text>
            </view>
            <text class="dw_dis" hidden="{{!active.distance}}">{{active.distance}}km</text>
        </view>
        <view class="four_row flex">
            <view class="dw_box">
                <image class="dw-icon" src="../../resource/images/first/dh.png"></image>
            </view>
            <view>{{active.tel}}</view>
        </view>
        <view class="five_row">
            <view class="flex-center">
                <view class="dw_box">
                    <image class="dw-icon" src="../../resource/images/first/dp.png"></image>
                </view>
                <view class="dot">{{active.store_name}}</view>
            </view>
            <view catchtap="makeCall" class="useNow-btn" data-tel="{{active.tel}}">拨打电话</view>
        </view>
    </view>
    <view class="wall"></view>
    <view class="details-wrap">
        <view class="kanjia-title">商品详情</view>
        <view class="contents">
            <text>{{active.font_details}}</text>
            <image mode="widthFix" src="{{url}}{{item}}" wx:for="{{active.details}}"></image>
        </view>
    </view>
    <view class="show-popup" hidden="{{hideShopPopup}}">
        <view class="popup-mask"></view>
        <view class="popup-contents">
            <image class="popupBackImg" src="../../resource/images/jika/tanchuanbj.jpg"></image>
            <view class="drawWord">恭喜你获得一张卡片</view>
            <image class="smallImg" src="{{url}}{{ckapian.thumb}}"></image>
            <view bindtap="goLucky" class="remainTimes" data-clock="{{bargainList[0].clock}}" data-id="{{active.id}}" hidden="{{!jikaNum}}">我要抽奖（剩余{{jikaNum}}次）</view>
        </view>
        <view bindtap="closePopupTap" class="close">
            <image class="closeImg" src="../../resource/images/jika/guanbi.png"></image>
        </view>
    </view>
    <view bindtap="goBackHome" class="backHome" wx:if="{{comein==1}}">
        <image class="homeImg" src="../../resource/images/kanjia/home.png"></image>
    </view>
    <view bindtap="close" class="drawer_screen" data-statu="close" wx:if="{{showModalStatus}}"></view>
    <view animation="{{animationData}}" class="drawer_box" wx:if="{{showModalStatus}}">
        <button class="shareFriend" data-num="{{active.num}}" data-tid="{{active.id}}" data-title="{{active.title}}" openType="share">发给好友</button>
        <view bindtap="createPoster" class="actionList border_t">生成卡片</view>
        <view bindtap="close" class="actionList" data-statu="close">取消</view>
    </view>
</view>
<login bindtogetuserinfo="updateUserInfo" modalHidden="{{is_modal_Hidden}}"></login>
<view class="show-popup" hidden="{{joinGroup}}">
    <view bindtap="closeWelfare" class="popup-mask"></view>
    <view class="welfareBox">
        <view class="joinGroup">加入群聊</view>
        <view class="inputText">请在客服窗口输入"{{system.wg_keyword}}"</view>
        <view class="contactbtn rel">
            <button class="contact" openType="contact" sessionFrom="weapp"></button>
            <view class="deter">知道了</view>
        </view>
    </view>
</view>
