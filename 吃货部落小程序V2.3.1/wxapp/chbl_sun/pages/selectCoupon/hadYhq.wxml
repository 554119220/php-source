<view class="wrap">
    <view bindtap="useNow" class="yhq-item flex-center" data-cid="{{item.id}}" data-downprice="{{item.val.c}}" data-sid="{{item.store_id}}" wx:for="{{myStoreCoupon}}">
        <view class="yhq_box">
            <image class="yhq_icon" src="../../resource/images/first/byhq.png"></image>
        </view>
        <view class="details_box">
            <view class="shopsName">{{item.store_name}}</view>
            <view class="edu">{{item.val.c}}元优惠券</view>
            <view class="jian">满{{item.val.b}}减{{item.val.c}}</view>
        </view>
        <view class="btn-box">
            <view class="useNow-btn">立即使用</view>
        </view>
    </view>
</view>
