.contacts {
    position: absolute;
    opacity: 0;
    width: 150rpx;
    height: 60rpx;
    z-index: 8;
}

.vipTag {
    display: inline-block;
    width: 60rpx;
    height: 30rpx;
    border-radius: 5rpx;
    background-color: #ffb825;
    margin: 0 0 0 20rpx;
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    line-height: 30rpx;
    font-weight: normal;
}

page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    width: 750rpx;
}

.swiper-container {
    width: 750rpx;
    position: relative;
}

.swiper_box {
    width: 100%;
    height: 550rpx;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
    height: 550rpx;
}

.goodsMesWrap {
    width: 100%;
    display: flex;
    flex-direction: column;
    background-color: #fff;
}

.goodsName {
    font-size: 30rpx;
    color: #000;
    font-weight: bold;
    margin-top: 20rpx;
    padding-left: 30rpx;
}

.goodsNumPrice {
    display: flex;
    font-size: 26rpx;
    color: #999;
    padding-left: 30rpx;
    margin-top: 15rpx;
    justify-content: space-between;
    margin-bottom: 35rpx;
}

.prices {
    display: flex;
}

.eaterPrice {
    color: #ce0000;
}

.originPrice {
    margin-left: 30rpx;
    text-decoration: line-through;
}

.salesNum {
    margin-right: 32rpx;
}

.address-time {
    width: 100%;
    font-size: 26rpx;
    color: #999;
    padding: 20rpx 0rpx;
    background-color: #fff;
}

.right_box {
    width: 450rpx;
    margin-left: 30rpx;
}

.one_row {
    font-size: 30rpx;
    color: #000;
    justify-content: space-between;
}

.two_row {
    margin: 10rpx 0rpx;
}

.three_row {
    display: flex;
    justify-content: space-between;
    margin: 0rpx 30rpx;
    align-items: flex-start;
}

.dw_word {
    width: 500rpx;
}

.dw_dis {
    color: #ffb826;
    margin-left: 20rpx;
}

.four_row {
    margin: 5rpx 30rpx;
}

.five_row {
    margin: 0rpx 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
}

.dw_box {
    width: 26rpx;
    height: 26rpx;
    margin: 0rpx 10rpx 0rpx 0rpx;
}

.dw-icon {
    width: 26rpx;
    height: 26rpx;
}

.useNow-btn {
    width: 140rpx;
    height: 50rpx;
    border-radius: 10rpx;
    color: #fff;
    background-color: #ce0000;
    line-height: 50rpx;
    text-align: center;
}

.details-comments {
    margin: 0rpx 0rpx 20rpx;
    background-color: #fff;
}

.comments-details {
    width: 750rpx;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    border-bottom: 1rpx solid #eee;
}

.de-com {
    color: #000;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 30rpx;
    margin: 0 30rpx;
}

.de-com.yes {
    border-bottom: 4rpx solid #ce0000;
}

.goodsImg-wrap {
    width: 750rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin: 30rpx 0rpx;
}

.introduce-img {
    width: 690rpx;
    display: flex;
    margin-bottom: 20rpx;
}

.footerWrap {
    height: 100rpx;
    width: 100%;
}

.footer-box {
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    align-items: center;
}

.contact2 {
    position: relative;
    width: 50rpx;
    height: 50rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 20rpx;
    color: #008000;
    box-sizing: border-box;
    background-size: 55rpx auto;
}

contact-button {
    position: absolute;
    left: 80rpx;
    top: 0rpx;
}

.icon-kf {
    width: 40rpx;
    height: 40rpx;
    margin-left: 60rpx;
    position: absolute;
    top: 8rpx;
}

.icon-cart {
    width: 45rpx;
    height: 40rpx;
    margin-left: 50rpx;
}

.footer-box .shop-cart-btn {
    position: relative;
    line-height: 100rpx;
    text-align: center;
    padding-top: 26rpx;
    font-size: 20rpx;
    color: #acacb7;
    box-sizing: border-box;
    background-size: 44rpx auto;
    margin-left: 80rpx;
}

.footer-box .shop-cart-btn .shop-num {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 30rpx;
    height: 30rpx;
    border-radius: 100%;
    background-color: #ffb825;
    position: absolute;
    color: #fff;
    left: 75rpx;
    top: 30rpx;
}

.footer-box .join-shop-cart {
    text-align: center;
    width: 220rpx;
    height: 100%;
    line-height: 100rpx;
    background-color: #ffb825;
    color: #fff;
    font-size: 32rpx;
    margin-left: 90rpx;
}

.footer-box .now-buy {
    text-align: center;
    height: 100%;
    width: 220rpx;
    line-height: 100rpx;
    background-color: #ce0000;
    color: #fff;
    font-size: 32rpx;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 5;
}

rich-text {
    word-break: break-all;
    padding: 0 30rpx;
}

.popup-contents {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 6;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.pop-goods-info {
    width: 710rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-bottom: 1px solid #eee;
}

.pop-img-box {
    width: 230rpx;
    height: 230rpx;
    margin: 30rpx 30rpx 30rpx 0rpx;
}

.pop-img-box .goods-thumbnail {
    width: 230rpx;
    height: 230rpx;
}

.pop-goods-title {
    width: 410rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 30rpx;
    color: #000;
}

.pop-goods-price {
    font-size: 30rpx;
    color: #ce0000;
    margin-top: 5rpx;
}

.pop-goods-num {
    color: #999;
    font-size: 26rpx;
    margin-top: 5rpx;
}

.pop-goods-close {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}

.closeImg {
    width: 30rpx;
    height: 30rpx;
}

.size-label-box {
    width: 710rpx;
    color: #000;
    font-size: 28rpx;
}

.size-label-box .label {
    font-size: 26rpx;
    color: #000;
    margin: 20rpx 0rpx;
}

.size-label-box .label-item-box {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.size-label-box .label-item {
    color: #666;
    background-color: #f3f3f3;
    padding: 14rpx 20rpx;
    height: 40rpx;
    margin-bottom: 10rpx;
    border-radius: 20rpx;
    text-align: center;
    line-height: 40rpx;
    margin-right: 20rpx;
}

.size-label-box .label-item.active {
    color: #fff;
    background-color: #ce0000;
}

.buy-num-box {
    width: 710rpx;
    display: flex;
    justify-content: space-between;
    padding: 30rpx 0rpx 40rpx 0;
    border-top: 1px solid #eee;
    margin-top: 20rpx;
    align-items: center;
}

.num-label {
    font-size: 28rpx;
    color: #000;
}

.buy-num-box .num-box {
    display: flex;
}

.buy-num-box .num-box .num-jian,.buy-num-box .num-box .num-input,.buy-num-box .num-box .num-jia {
    width: 80rpx;
    height: 64rpx;
    line-height: 62rpx;
    text-align: center;
    border: 1px solid #eee;
}

.buy-num-box .num-box .num-input {
    font-size: 28rpx;
    margin: 0rpx 5rpx;
}

.buy-num-box .num-box .num-input input {
    height: 100%;
}

.popup-join-btn {
    width: 600rpx;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    font-size: 32rpx;
    color: #fff;
    background-color: #ffb825;
    border-radius: 10rpx;
    margin-bottom: 30rpx;
}

.buy-join-btn {
    width: 600rpx;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    font-size: 32rpx;
    color: #fff;
    background-color: #ce0000;
    border-radius: 10rpx;
    margin-bottom: 30rpx;
}

.buy-num-box .num-box .hui {
    background-color: #f5f5f9;
}