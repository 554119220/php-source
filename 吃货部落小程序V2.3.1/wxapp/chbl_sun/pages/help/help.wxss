.header {
    width: 100%;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.goodsImg {
    width: 100%;
    height: 550rpx;
    display: flex;
}

.goodsName {
    font-size: 30rpx;
    color: #000;
    font-weight: bold;
    width: 690rpx;
    display: flex;
    margin-top: 20rpx;
}

.kanjia-mes {
    width: 690rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 20rpx;
}

.floorPrice {
    font-size: 28rpx;
    color: #999;
}

.fprice {
    font-size: 30rpx;
    color: #ce0000;
}

.originPrice {
    color: #999;
    font-size: 26rpx;
    text-decoration: line-through;
    margin-left: 10rpx;
}

.rightMes {
    color: #999;
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    align-items: flex-end;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 5;
}

.popup-contents {
    position: fixed;
    top: 130rpx;
    left: 85rpx;
    width: 580rpx;
    height: 695rpx;
    border-radius: 20rpx;
    background-color: #fff;
    z-index: 6;
}

.popupBackImg {
    width: 580rpx;
    height: 695rpx;
    border-radius: 20rpx;
}

.drawWord {
    width: 580rpx;
    text-align: center;
    font-weight: bold;
    font-size: 34rpx;
    color: #c88d13;
    position: absolute;
    top: 40rpx;
}

.kanjia-text {
    font-size: 30rpx;
    color: #ce0000;
    text-align: center;
    margin-top: 20rpx;
}

.moneyNum {
    color: #ce0000;
}

.shareBtn {
    width: 400rpx;
    height: 80rpx;
}

.remainTimes {
    width: 400rpx;
    height: 80rpx;
    color: #fff;
    border-radius: 40rpx;
    background-color: #ffb825;
    line-height: 80rpx;
    text-align: center;
    font-size: 30rpx;
    position: absolute;
    bottom: 50rpx;
    left: 90rpx;
}

.close {
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    top: 850rpx;
    left: 335rpx;
    z-index: 5;
}

.closeImg {
    width: 80rpx;
    height: 80rpx;
    position: relative;
    z-index: 100;
}

button {
    width: 170rpx;
    height: 80rpx;
    position: absolute;
    z-index: 6;
}

.top-up {
    position: relative;
    height: 450rpx;
    width: 750rpx;
}

.top-up image {
    height: 450rpx;
    width: 750rpx;
}

.top-up text {
    display: block;
    height: 66rpx;
    width: 730rpx;
    background: rgba(65,194,252,0.55);
    font-size: 28rpx;
    color: #fff;
    font-weight: 700;
    position: absolute;
    bottom: 0rpx;
    line-height: 66rpx;
    padding-left: 22rpx;
}

.top-down {
    padding: 0rpx 20rpx;
    background: #fff;
    width: 710rpx;
}

.title {
    color: #222;
    font-size: 32rpx;
    padding-top: 40rpx;
}

.price {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 26rpx;
    color: #999;
    margin-top: 30rpx;
    padding-bottom: 24rpx;
}

.price-detail {
    color: #f33030;
}

.price-detail:nth-child(2) {
    font-weight: 700;
    font-size: 34rpx;
}

.bargain {
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 32rpx;
    color: #222;
}

.username {
    margin: 0rpx 24rpx 0rpx 0rpx;
    color: #000;
}

.succmsg {
    width: 580rpx;
    color: #ce0000;
    line-height: 40rpx;
    padding-left: 20rpx;
}

.friend {
    height: 90rpx;
    width: 90rpx;
    margin-left: 30rpx;
    border-radius: 100%;
}

.friends {
    width: 68rpx;
    height: 68rpx;
    border-radius: 100%;
    margin-right: 30rpx;
}

._bargain {
    justify-content: space-between;
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 10rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.barprice {
    color: #999;
    font-size: 28rpx;
}

.barprice text {
    color: #f33030;
}

.bargain-pro {
    width: 660rpx;
    height: 80rpx;
    margin: 0px auto 30rpx;
    border-radius: 14rpx;
