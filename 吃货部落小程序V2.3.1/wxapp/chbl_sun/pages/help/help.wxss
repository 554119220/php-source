    align-items: center;
    font-size: 32rpx;
    color: #41c2fc;
    border-bottom: 1rpx solid #ececec;
}

.icons {
    width: 36rpx;
    height: 36rpx;
    display: flex;
    margin-right: 20rpx;
}

.process {
    display: flex;
    flex-direction: row;
    padding: 0rpx 20rpx;
    margin-top: 22rpx;
    align-items: cneter;
}

.pro {
    width: 194rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    color: #999;
    padding-bottom: 24rpx;
}

.proimg {
    width: 66rpx;
    height: 66rpx;
    margin-bottom: 16rpx;
}

.arrow {
