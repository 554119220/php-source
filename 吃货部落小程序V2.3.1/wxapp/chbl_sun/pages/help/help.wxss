    margin-top: 32rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.bargain-pro .pro-btn {
    width: 660rpx;
    height: 80rpx;
    color: #fff;
    background: #ce0000;
    text-align: center;
    line-height: 80rpx;
    border-radius: 14rpx;
}

.bargain-pro text {
    display: inline-block;
    color: rgb(243,48,48);
    margin-bottom: 32rpx;
}

.bargain-pro progress {
    border-radius: 14rpx;
    height: 28rpx;
}

.message {
    display: flex;
    align-items: center;
    font-size: 28rpx;
}

.block {
    margin-top: 10rpx;
    background: #fff;
}

.session {
    padding: 0rpx 20rpx;
    width: 710rpx;
    height: 90rpx;
