page {
    background-color: #f2f2f2;
}

.wrap {
    margin: 30rpx 0rpx;
}

.yhq-item {
    margin: 0rpx 30rpx 20rpx 30rpx;
    border-radius: 10rpx;
    background-color: #fff;
    padding: 20rpx;
}

.yhq_box {
    width: 60rpx;
    height: 60rpx;
}

.yhq_icon {
    width: 60rpx;
    height: 60rpx;
    display: flex;
}

.details_box {
    padding: 0rpx 30rpx 0rpx 30rpx;
    width: 50%;
    height: 100%;
}

.shopsName {
    font-size: 30rpx;
    color: #000;
}

.edu {
    color: #ce0000;
    font-size: 40rpx;
    margin: 5rpx 0rpx;
}

.jian {
    font-size: 26rpx;
    color: #999;
}

.btn-box {
    width: 30%;
    height: 100%;
    display: -webkit-flex;
    justify-content: flex-end;
    border-left: 1rpx solid #eee;
}

.useNow-btn {
    width: 140rpx;
    height: 50rpx;
    border-radius: 10rpx;
    background-color: #ce0000;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 50rpx;
}