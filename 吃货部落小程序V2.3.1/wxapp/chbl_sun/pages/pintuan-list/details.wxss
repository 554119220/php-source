.vipTag {
    display: inline-block;
    width: 60rpx;
    height: 30rpx;
    border-radius: 5rpx;
    background-color: #ffb825;
    margin: 0 0 0 20rpx;
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    line-height: 30rpx;
    font-weight: normal;
}

page {
    background-color: #f2f2f2;
}

.container {
    width: 750rpx;
    font-size: 28rpx;
    color: #000;
}

.header {
    width: 100%;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.swiper-container {
    width: 750rpx;
    position: relative;
}

.swiper_box {
    width: 100%;
    height: 550rpx;
}

.goodsImg {
    width: 100%;
    height: 550rpx;
    display: inline-block;
    overflow: hidden;
}

.goodsName {
    font-size: 30rpx;
    color: #000;
    font-weight: bold;
    width: 690rpx;
    display: flex;
    margin-top: 20rpx;
}

.kanjia-mes {
    width: 690rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 20rpx;
}

.floorPrice {
    font-size: 28rpx;
    color: #999;
    max-width: 420rpx;
}

.pintuan {
    display: inline-block;
    min-width: 140rpx;
    text-align: center;
    border-radius: 6rpx;
    font-size: 24rpx;
    color: #999;
    margin-left: 20rpx;
}

.fprice {
    font-size: 30rpx;
    color: #ce0000;
}

.originPrice {
    color: #999;
    font-size: 26rpx;
    text-decoration: line-through;
    margin-left: 10rpx;
}

.rightMes {
    color: #999;
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    width: 300rpx;
}

.shengyuTime {
    word-break: break-all;
    color: #ce0000;
}

.num_total {
    display: flex;
}

.pintuan-wrap {
    margin-top: 20rpx;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.pintuan-title {
    width: 690rpx;
    height: 70rpx;
    line-height: 70rpx;
    color: #999;
    font-size: 28rpx;
}

.pintuan-item {
    width: 690rpx;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    padding-left: 30rpx;
    padding-right: 30rpx;
    justify-content: space-between;
    border-top: 1rpx solid #eee;
}

.friend-mes {
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #000;
}

.headerImg {
    width: 70rpx;
    height: 70rpx;
    border-radius: 100%;
}

.nickname {
    margin-left: 20rpx;
    max-width: 280rpx;
}

.pintuan-status {
    display: flex;
    align-items: center;
}

.times-poor {
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    font-size: 26rpx;
}

.poor-num {
    color: #ce0000;
}

.times {
    color: #999;
}

.cantuan-btn {
    width: 140rpx;
    height: 50rpx;
    border-radius: 25rpx;
    background-color: #ce0000;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 50rpx;
    margin-left: 20rpx;
}

.liucheng-wrap {
    width: 100%;
    background-color: #fff;
    margin-top: 20rpx;
    display: flex;
    flex-direction: column;
}

.kanjia-title {
    width: 120rpx;
    margin-left: 30rpx;
    margin-top: 10rpx;
    color: #ce0000;
    font-size: 30rpx;
    border-bottom: 1rpx solid #ce0000;
}

.detail_contents {
    width: 92%;
    margin-left: 30rpx;
    margin-top: 10rpx;
}

.detail_contents rich-text {
    width: 100%;
}

.liucheng {
    display: flex;
    align-items: center;
    width: 680rpx;
    justify-content: space-between;
    margin-left: 35rpx;
    margin-top: 23rpx;
}

.item-liu {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 140rpx;
    text-align: center;
}

.liucheng-text {
    color: #666;
    font-size: 26rpx;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.kanjiaImgs {
    width: 90rpx;
    height: 90rpx;
    display: flex;
}

.arrow {
    width: 40rpx;
    height: 10rpx;
}

.details-wrap {
    margin-top: 20rpx;
    width: 100%;
    background-color: #fff;
    margin-bottom: 120rpx;
}

.detailsImgs {
    width: 690rpx;
    margin-left: 30rpx;
    margin-top: 23rpx;
    display: flex;
}

.newFooter {
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    color: #fff;
    font-size: 32rpx;
    display: flex;
}

.helperBar {
    width: 50%;
    height: 100%;
    background-color: #ce0000;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.buyNow {
    width: 50%;
    height: 100%;
    background-color: #ffb825;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.helper-price {
    line-height: 40rpx;
}

.address-time {
    width: 100%;
    font-size: 26rpx;
    color: #999;
    padding: 20rpx 0rpx;
    background-color: #fff;
}

.right_box {
    width: 450rpx;
    margin-left: 30rpx;
}

.one_row {
    font-size: 30rpx;
    color: #000;
    justify-content: space-between;
}

.two_row {
    margin: 10rpx 0rpx;
}

.three_row {
    justify-content: space-between;
    margin: 0rpx 30rpx;
}

.dw_dis {
    color: #ffb826;
    margin-left: 20rpx;
}

.four_row {
    margin: 5rpx 30rpx;
}

.five_row {
    margin: 0rpx 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
}

.dw_box {
    width: 26rpx;
    height: 26rpx;
    margin: 0rpx 10rpx 0rpx 0rpx;
}

.dw-icon {
    width: 26rpx;
    height: 26rpx;
}

.useNow-btn {
    width: 140rpx;
    height: 50rpx;
    border-radius: 10rpx;
    color: #fff;
    background-color: #ce0000;
    line-height: 50rpx;
    text-align: center;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 5;
}

.popup-contents {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 6;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.pop-goods-info {
    width: 710rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-bottom: 1px solid #eee;
}

.pop-img-box {
    width: 230rpx;
    height: 230rpx;
    margin: 30rpx 30rpx 30rpx 0rpx;
}

.pop-img-box .goods-thumbnail {
    width: 230rpx;
    height: 230rpx;
}

.pop-goods-title {
    width: 410rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 30rpx;
    color: #000;
}

.pop-goods-price {
    font-size: 30rpx;
    color: #ce0000;
    margin-top: 5rpx;
}

.pop-goods-num {
    color: #999;
    font-size: 26rpx;
    margin-top: 5rpx;
}

.pop-goods-close {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}

.closeImg {
    width: 30rpx;
    height: 30rpx;
}

.size-label-box {
    width: 710rpx;
    color: #000;
    font-size: 28rpx;
}

.size-label-box .label {
    font-size: 26rpx;
    color: #000;
    margin: 20rpx 0rpx;
}

.size-label-box .label-item-box {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.size-label-box .label-item {
    color: #666;
    background-color: #f3f3f3;
    padding: 14rpx 20rpx;
    height: 40rpx;
    margin-bottom: 10rpx;
    border-radius: 20rpx;
    text-align: center;
    line-height: 40rpx;
    margin-right: 20rpx;
}

.size-label-box .label-item.active {
    color: #fff;
    background-color: #ce0000;
}

.buy-num-box {
    width: 710rpx;
    display: flex;
    justify-content: space-between;
    padding: 30rpx 0rpx 40rpx 0;
    border-top: 1px solid #eee;
    margin-top: 20rpx;
    align-items: center;
}

.num-label {
    font-size: 28rpx;
    color: #000;
}

.buy-num-box .num-box {
    display: flex;
}

.buy-num-box .num-box .num-jian,.buy-num-box .num-box .num-input,.buy-num-box .num-box .num-jia {
    width: 80rpx;
    height: 64rpx;
    line-height: 62rpx;
    text-align: center;
    border: 1px solid #eee;
}

.buy-num-box .num-box .num-input {
    font-size: 28rpx;
    margin: 0rpx 5rpx;
}

.buy-num-box .num-box .num-input input {
    height: 100%;
}

.popup-join-btn {
    width: 600rpx;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    font-size: 32rpx;
    color: #fff;
    background-color: #ffb825;
    border-radius: 10rpx;
    margin-bottom: 30rpx;
}

.buy-join-btn {
    width: 600rpx;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    font-size: 32rpx;
    color: #fff;
    background-color: #ce0000;
    border-radius: 10rpx;
    margin-bottom: 30rpx;
}

.buy-num-box .num-box .hui {
    background-color: #f5f5f9;
}