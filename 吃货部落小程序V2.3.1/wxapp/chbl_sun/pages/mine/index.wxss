page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    width: 750rpx;
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 750rpx;
    font-size: 32rpx;
}

.userinfo-avatar {
    width: 150rpx;
    height: 150rpx;
    margin-top: 35rpx;
    border-radius: 50%;
    border: 5rpx solid #fff;
}

.userinfo-nickname {
    margin-top: 10rpx;
    margin-bottom: 40rpx;
    font-size: 28rpx;
    color: #fff;
}

.smsj {
    width: 100%;
    display: flex;
    justify-content: space-around;
    font-size: 30rpx;
    color: #000;
    background-color: #fff;
}

.smsj_item {
    display: flex;
    align-items: center;
    margin: 30rpx 0;
}

.smsj_imgBox {
    width: 50rpx;
    height: 50rpx;
}

.smsj_imgBox_imgs {
    width: 50rpx;
    height: 50rpx;
    display: flex;
}

.smsj_collect {
    margin-left: 20rpx;
}

.myList {
    width: 100%;
    margin-top: 20rpx;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.myItem {
    width: 690rpx;
    height: 100rpx;
    border-bottom: 1rpx solid #eee;
    display: flex;
    align-items: center;
}

.sellEntr {
    width: 690rpx;
    height: 100rpx;
    border-bottom: 1rpx solid #eee;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.itemIcon {
    width: 35rpx;
    height: 30rpx;
}

.itemWord {
    color: #000;
    font-size: 30rpx;
    margin-left: 20rpx;
}

.itemWord_b {
    color: #666;
    font-size: 30rpx;
    margin-left: 20rpx;
}

.leftMes {
    display: flex;
    align-items: center;
}

.arrow {
    width: 12rpx;
    height: 25rpx;
    margin-left: 22rpx;
}

.geren {
    position: absolute;
    top: -100rpx;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 5;
}

.sellerMes {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 6;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.inputWrap {
    width: 690rpx;
    height: 100rpx;
    border: 1rpx solid #eee;
    box-sizing: border-box;
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
    color: #000;
    font-size: 30rpx;
}

.inputWrap:first-child {
    margin-top: 30rpx;
}

picker {
    width: 100%;
}

.picker {
    width: 100%;
}

.inputBox {
    margin-left: 30rpx;
}

.inputBox .diyHolder {
    font-size: 26rpx;
    color: #999;
}

.payRow {
    font-size: 30rpx;
    color: #000;
    display: flex;
    justify-content: flex-end;
}

.payRow .payMoney {
    color: #ce0000;
}

.push-btn {
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    width: 600rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #ffb825;
    font-size: 32rpx;
    color: #fff;
    text-align: center;
    line-height: 90rpx;
}

button:after {
    border: none;
}

.succContents {
    width: 690rpx;
    height: 300rpx;
    background-color: #fff;
    position: fixed;
    top: 430rpx;
    left: 30rpx;
    z-index: 6;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.paySuccess {
    color: #ce0000;
    font-size: 36rpx;
    font-weight: bold;
    margin-top: 30rpx;
}

.payText {
    color: #999;
    font-size: 26rpx;
    margin: 20rpx 0rpx 10rpx 0rpx;
}

.rulesBox {
    width: 92%;
    background-color: #fff;
    position: fixed;
    top: 310rpx;
    left: 30rpx;
    z-index: 6;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-bottom: 30rpx;
}

.scrollView {
    height: 210rpx;
    padding-left: 60rpx;
    padding-right: 60rpx;
    word-break: break-word;
}

.contentsText {
    font-size: 26rpx;
    color: #999;
    margin: 20rpx;
    padding: 0rpx 40rpx;
    display: flex;
    flex-wrap: wrap;
}

.footer-box {
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    width: 100%;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    border-top: 1rpx solid #eee;
    background-color: #fff;
}

.diyItem {
    width: 80rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
}

.diyTab {
    width: 54rpx;
    height: 54rpx;
    margin-top: 10rpx;
}

.diyTab.active {
}

.diyTabs {
    width: 54rpx;
    height: 54rpx;
}

.diyTabs.active {
    width: 54rpx;
    height: 54rpx;
}

.diyTabss {
    width: 54rpx;
    height: 54rpx;
}

.diyTabss.active {
    width: 54rpx;
    height: 54rpx;
}

.diyWord {
    font-size: 20rpx;
    margin-top: 5rpx;
}

.diyWord.active {
    font-size: 20rpx;
    color: #ce0000;
}