page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    width: 750rpx;
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 750rpx;
    font-size: 32rpx;
}

.userinfo-avatar {
    width: 150rpx;
    height: 150rpx;
    margin-top: 35rpx;
    border-radius: 50%;
    border: 5rpx solid #fff;
}

.userinfo-nickname {
    margin-top: 20rpx;
    margin-bottom: 50rpx;
    font-size: 28rpx;
    color: #fff;
}

.myList {
    width: 100%;
    margin-top: 20rpx;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.myItem {
    width: 100%;
    height: 100rpx;
    border-bottom: 1rpx solid #eee;
    display: flex;
    align-items: center;
}

.sellEntr {
    width: 92%;
    height: 100rpx;
    border-bottom: 1rpx solid #eee;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.itemIcon {
    width: 35rpx;
    height: 30rpx;
    margin-left: 30rpx;
}

.itemWord {
    color: #000;
    font-size: 30rpx;
    margin-left: 20rpx;
}

.usWord {
    color: #000;
    font-size: 30rpx;
}

.leftMes {
    display: flex;
    align-items: center;
}

.arrow {
    width: 12rpx;
    height: 25rpx;
    margin-left: 52rpx;
}

.geren {
    position: absolute;
    top: -100rpx;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 5;
}

.sellerMes {
    height: 705rpx;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 6;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.inputWrap {
    width: 690rpx;
    height: 100rpx;
    border: 1rpx solid #eee;
    box-sizing: border-box;
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
    color: #000;
    font-size: 30rpx;
}

.inputWrap:first-child {
    margin-top: 30rpx;
}

.inputBox {
    margin-left: 30rpx;
}

.inputBox .diyHolder {
    font-size: 26rpx;
    color: #999;
}

.payRow {
    font-size: 30rpx;
    color: #000;
    display: flex;
    justify-content: flex-end;
}

.payRow .payMoney {
    color: #ce0000;
}

.push-btn {
    margin-top: 30rpx;
    width: 600rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #ffb825;
    font-size: 32rpx;
    color: #fff;
    text-align: center;
    line-height: 90rpx;
}

.back-btn {
    width: 600rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #efefef;
    border: 1rpx solid #ffb825;
    font-size: 32rpx;
    color: #ffb825;
    text-align: center;
    line-height: 90rpx;
    margin-top: 20rpx;
    box-sizing: border-box;
}

button:after {
    border: none;
}