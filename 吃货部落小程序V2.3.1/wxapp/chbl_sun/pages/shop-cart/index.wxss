page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    width: 750rpx;
}

.selectAll {
    width: 690rpx;
    display: flex;
    justify-content: space-between;
    padding: 20rpx 30rpx;
    margin-top: 20rpx;
    align-items: center;
    background: url(http://oydnzfrbv.bkt.clouddn.com/noselect.png) no-repeat 30rpx center;
    background-size: 30rpx auto;
    background-color: #fff;
    color: #000;
    font-size: 30rpx;
    border-bottom: 1rpx solid #eee;
}

.selectAll.active {
    background: url(http://oydnzfrbv.bkt.clouddn.com/select.png) no-repeat 30rpx center;
    background-size: 30rpx auto;
    background-color: #fff;
}

.allWord {
    margin-left: 50rpx;
}

.deleteIcon {
    width: 30rpx;
    height: 30rpx;
}

.cart_shopName {
    font-size: 30rpx;
    color: #000;
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 30rpx;
    background-color: #fff;
}

.cartList {
    margin-bottom: 20rpx;
}

.cartItem {
    display: flex;
    width: 690rpx;
    position: relative;
    padding: 20rpx 30rpx;
    background: url(http://oydnzfrbv.bkt.clouddn.com/noselect.png) no-repeat 30rpx 75rpx;
    background-size: 30rpx 30rpx;
    background-color: #fff;
    border-bottom: 1rpx solid #eee;
}

.cartItem.active {
    background: url(http://oydnzfrbv.bkt.clouddn.com/select.png) no-repeat 30rpx 75rpx;
    background-size: 30rpx 30rpx;
    background-color: #fff;
}

.goodsImg {
    width: 130rpx;
    height: 130rpx;
    margin-bottom: 60rpx;
    margin-left: 50rpx;
}

.rightWord {
    margin-left: 30rpx;
    margin-top: 25rpx;
    flex: 1;
}

.goodsName {
    color: #000;
    font-size: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.guige-price {
    width: 480rpx;
    display: flex;
    justify-content: space-between;
}

.guige {
    font-size: 26rpx;
    color: #999;
}

.price {
    color: #ce0000;
    font-size: 32rpx;
}

.num-box {
    bottom: 20rpx;
    right: 30rpx;
    position: absolute;
    display: flex;
    z-index: 9;
}

.num-box .num-jian,.num-box .num-input,.num-box .num-jia {
    width: 66rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    border: 1px solid #eee;
    font-size: 40rpx;
    margin: 3rpx;
}

.num-box .num-jian,.num-box .num-jia {
    background-color: #fff;
}

.num-box .num-jian.disabled,.num-box .num-jia.disabled {
    background-color: #f5f5f9;
}

.num-box .num-input {
    font-size: 28rpx;
}

.num-box .num-input input {
    height: 100%;
}

.footer-box {
    width: 750rpx;
    height: 100rpx;
    display: flex;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    background-color: #fff;
    z-index: 10;
}

.leftMes {
    width: 490rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.totalPrice {
    color: #000;
    font-size: 26rpx;
}

.totalNum {
    color: #666;
    font-size: 26rpx;
}

.jiesuan {
    width: 260rpx;
    height: 100rpx;
    background-color: #ce0000;
    color: #fff;
    font-size: 32rpx;
    line-height: 100rpx;
    text-align: center;
}