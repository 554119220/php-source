.ad {
    width: 100%;
}

.horn-icon {
    width: 30rpx;
    height: 30rpx;
    z-index: 12;
    position: absolute;
}

.newest_content {
    width: 80%;
    height: 90rpx;
    line-height: 90rpx;
    color: #000;
    font-size: 26rpx;
    margin: 0 0 0 50rpx;
}

page {
    background-color: #efefef;
    min-height: 100%;
}

.container {
    background-color: #fff;
    margin-bottom: 20rpx;
}

.card-wrap {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
    margin-top: -5rpx;
    position: relative;
}

.cardBj {
    width: 100%;
    height: 500rpx;
    position: absolute;
}

.cardImgView {
    width: 630rpx;
    height: 360rpx;
    border-radius: 20rpx;
    margin-top: 40rpx;
    z-index: 10;
    background-color: #dadada;
}

.cardImg {
    width: 630rpx;
    height: 360rpx;
    border-radius: 20rpx;
    z-index: 10;
}

.cardNumber {
    font-size: 26rpx;
    color: #333;
    position: absolute;
    left: 90rpx;
    top: 60rpx;
    z-index: 11;
}

.buyRow {
    margin-top: 50rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    width: 690rpx;
    border-bottom: 1rpx solid #eee;
    padding-bottom: 20rpx;
    z-index: 5;
}

.classic_item {
    width: 200rpx;
    height: 100rpx;
    border-radius: 10rpx;
    text-align: center;
    background-color: #faeedb;
    font-size: 30rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.classic_item.active {
    border: 1px solid #ffe4ba;
}

.classic_text {
    font-size: 26rpx;
    color: #000;
}

.classic_money {
    color: #ce0000;
    font-size: 36rpx;
}

.jihuoRow {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    width: 690rpx;
    border-bottom: 1rpx solid #eee;
    padding-bottom: 20rpx;
    margin-top: 15rpx;
    z-index: 5;
}

.topRow {
    width: 100%;
}

.oneWord {
    font-size: 30rpx;
    color: #000;
    display: flex;
}

.twoWord {
    color: #999;
    font-size: 26rpx;
    margin-top: 10rpx;
}

.price {
    color: #ce0000;
}

.buyButton {
    width: 140rpx;
    height: 50rpx;
    border-radius: 25rpx;
    background-color: #ce0000;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 50rpx;
}

.card-rules {
    color: #666;
    font-size: 26rpx;
    min-height: 370rpx;
    padding-bottom: 50rpx;
}

.ruleTitle {
    display: inline-block;
    margin-top: 45rpx;
    margin-bottom: 20rpx;
    color: #ce0000;
    font-size: 30rpx;
    padding-bottom: 10rpx;
    border-bottom: 1rpx solid #ce0000;
}

.rules {
    width: 690rpx;
}

.footer-box {
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    width: 100%;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    border-top: 1rpx solid #eee;
    background-color: #fff;
}

.diyItem {
    width: 80rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
