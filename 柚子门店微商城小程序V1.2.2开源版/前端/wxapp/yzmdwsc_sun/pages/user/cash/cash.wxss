.container {
    background: #fff;
}

.header {
    height: 354rpx;
    width: 750rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(255,136,92) 0%,rgb(252,77,73) 100%);
    position: relative;
}

.cash {
    text-align: center;
    padding-top: 60rpx;
    color: #fff;
    font-size: 32rpx;
}

.cash>view:first-child {
    font-size: 78rpx;
    color: #fff6cb;
    font-weight: 700;
    line-height: 78rpx;
    margin-bottom: 20rpx;
}

.head {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 710rpx;
    margin: 0rpx 20rpx;
    height: 114rpx;
    color: #fff;
    position: absolute;
    bottom: 0rpx;
    border-top: 2rpx solid rgba(255,255,255,0.6);
}

.head>view {
    height: 88rpx;
    width: 355rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.head .account {
    border-right: 1rpx solid #fff;
}

.account>view:first-child {
    font-weight: 700;
    font-size: 48rpx;
    line-height: 48rpx;
    margin-bottom: 10rpx;
}

.session {
    padding: 0rpx 20rpx;
}

.sessionTitle {
    padding: 30rpx 0rpx;
    color: #666;
    text-align: center;
}

.rule {
    font-size: 26rpx;
    color: #666;
    line-height: 39rpx;
}