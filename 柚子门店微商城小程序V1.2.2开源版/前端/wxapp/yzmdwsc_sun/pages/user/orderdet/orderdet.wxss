.header {
    width: 750rpx;
    height: 120rpx;
    background-image: -webkit-linear-gradient(180deg,rgb(255,120,0)0%,rgb(255,155,66)100%);
    display: flex;
    justify-content: space-between;
}

.header>view {
    padding: 0rpx 20rpx;
    color: #fff;
    font-size: 32rpx;
    display: flex;
    align-items: center;
}

.btnGroup {
    display: flex;
}

.btnGroup>button {
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    line-height: 52rpx;
    background: transparent;
    color: #fff;
    box-sizing: content-box;
}

.btnGroup>view,.btnGroup>button {
    width: 144rpx;
    height: 52rpx;
    text-align: center;
    line-height: 52rpx;
    border: 1rpx solid #fff;
    border-radius: 6rpx;
    font-size: 30rpx;
    margin-right: 10rpx;
}

.address {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 20rpx;
    line-height: 46rpx;
    font-size: 26rpx;
    color: #666;
    background: #fff;
    margin-top: 10rpx;
    margin-bottom: 20rpx;
}

.addrIcon {
    height: 32rpx;
    width: 24rpx;
    margin-right: 20rpx;
}

.addressLeft {
    display: flex;
    align-items: center;
}

.addressLeft>view {
    width: 660rpx;
}

.right {
    width: 12rpx;
    height: 21rpx;
    margin-left: 20rpx;
}

.session {
    margin-top: 20rpx;
    background: #fff;
}

.shop {
    height: 82rpx;
    width: 710rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
    font-size: 30rpx;
}

.contact {
    display: flex;
    align-items: center;
    margin-left: 0rpx;
    margin-right: 0rpx;
    background: #fff;
    padding-right: 0rpx;
    padding-left: 0rpx;
    font-size: 28rpx;
}

.contact>image {
    width: 38rpx;
    height: 34rpx;
    margin-right: 16rpx;
}

.sesList {
    height: 180rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-left: 20rpx;
    padding: 20rpx 20rpx 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
}

.sesList:last-child {
    border-bottom: 0rpx solid #ececec;
}

.sLeft {
    display: flex;
}

.goodImg {
    margin-right: 30rpx;
    height: 180rpx;
    width: 180rpx;
    border: 1rpx solid #dcdcdc;
    border-radius: 6rpx;
}

.sLeft image {
    height: 180rpx;
    width: 180rpx;
    border-radius: 6rpx;
}

.sRight {
    font-weight: 700;
    font-size: 30rpx;
}

.det {
    margin-top: 10rpx;
    position: relative;
    width: 428rpx;
}

.title {
    font-weight: 700;
    margin-bottom: 6rpx;
    line-height: 36rpx;
}

.specConn {
    font-size: 24rpx;
    color: #666;
    margin-top: 10rpx;
}

.goodsPrice {
    color: #ff7800;
    font-weight: 700;
    font-size: 34rpx;
    position: absolute;
    bottom: 10rpx;
}

.count {
    margin-left: 20rpx;
    padding: 20rpx 20rpx 0rpx 0rpx;
}

.count>view {
    margin-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
    font-size: 24rpx;
}

.countpri {
    color: #666;
}

.ordernum {
    padding: 20rpx;
    background: #fff;
    font-size: 24rpx;
    line-height: 42rpx;
}

.totalPrice {
    font-weight: 700;
    font-size: 26rpx;
    height: 82rpx;
    line-height: 82rpx;
    border-top: 1rpx solid #ececec;
    padding: 0rpx 20rpx 0rpx 0rpx;
}

.sum {
    margin-bottom: 100rpx;
    height: 92rpx;
    line-height: 92rpx;
    padding-left: 20rpx;
}

.sum text {
    font-weight: 700;
    font-size: 34rpx;
    color: #ff7800;
}

.qrcode {
    padding-left: 20rpx;
    width: 730rpx;
    margin: 0rpx auto;
    padding: 0rpx 0rpx 30rpx 20rpx;
    background: #fff;
}

.qrcode>view {
    padding-right: 20rpx;
    border-top: 1rpx solid #ececec;
    width: 710rpx;
    display: flex;
    justify-content: center;
    padding-top: 20rpx;
}

.qrcode>view>image {
    width: 320rpx;
    height: 320rpx;
}