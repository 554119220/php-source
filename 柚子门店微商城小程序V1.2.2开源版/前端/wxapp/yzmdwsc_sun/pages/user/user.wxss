.head {
    height: 346rpx;
    width: 750rpx;
    display: flex;
    justify-content: center;
    background: #FFEEDF;
    position: relative;
}

.privilege {
    position: absolute;
    right: 0rpx;
    top: 60rpx;
    width: 154rpx;
    height: 62rpx;
    background: #ff7800;
    color: #fff;
    text-align: center;
    line-height: 62rpx;
    font-weight: 700;
    border-radius: 31rpx 0rpx 0rpx 31rpx;
}

.uthumb {
    border: 2rpx solid #fff;
}

.uthumb,.uthumb image {
    width: 156rpx;
    height: 156rpx;
    border-radius: 50%;
}

.uthumb {
    margin-bottom: 36rpx;
}

.head-thumb {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 40rpx;
    letter-spacing: 2rpx;
    z-index: 999;
    line-height: 42rpx;
}

.user-count {
    display: flex;
    padding: 28rpx 0rpx 24rpx;
    background: #fff;
    justify-content: space-around;
}

.user-money {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.icon1 {
    width: 56rpx;
    height: 44rpx;
}

.m_up {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 26rpx;
    justify-content: center;
    font-weight: 700;
}

.m_up>text {
    display: inline-block;
    margin-top: 30rpx;
}

.block {
    background: #fff;
    margin-top: 20rpx;
}

.block-up {
    padding: 0rpx 30rpx;
    display: flex;
    height: 90rpx;
    justify-content: space-between;
    align-items: center;
}

.rightIcon {
    margin-left: 20rpx;
    width: 16rpx;
    height: 24rpx;
}

.block-up view {
    display: flex;
    align-items: center;
    color: #666;
}

.block-down {
    display: flex;
    justify-content: space-around;
    align-items: center;
    height: 154rpx;
    border-top: 1rpx solid #ececec;
    font-size: 22rpx;
    padding-right: 20rpx;
    margin-left: 20rpx;
}

.block-down view {
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
}

.icn {
    width: 52rpx;
    height: 52rpx;
    margin-bottom: 20rpx;
}

.icon {
    width: 32rpx;
    height: 34rpx;
    margin-right: 34rpx;
}

.session {
    background: #fff;
    padding-left: 30rpx;
    margin-top: 20rpx;
}

.session .list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 30rpx;
    border-bottom: 1rpx solid #ececec;
    height: 92rpx;
}

.session .list:last-child {
    border-bottom: 1rpx solid #fff;
}

.list view {
    display: flex;
    align-items: center;
}

.ses-last {
    margin-bottom: 100rpx;
}

.concat {
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    font-size: 28rpx;
    background: #fff;
    color: #222;
    display: flex;
    align-items: center;
}

.concat::after {
    border: none;
}

.d_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.d_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
    height: 0rpx;
    transition: height 0.5s ease;
}

.payBox {
    height: 550rpx;
}

.d_title {
    height: 130rpx;
    font-size: 34rpx;
    display: flex;
    padding: 0rpx 20rpx;
    align-items: center;
    justify-content: space-between;
    border-bottom: 12rpx solid #cbcbcb;
}

.dTitle {
    width: 676rpx;
    text-align: center;
    color: #222;
}

.cancel {
    width: 34rpx;
    height: 34rpx;
}

.d_content {
    height: 420rpx;
}

.choose-pay {
    background: #fff;
}

.section {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
}

.radio {
    width: 730rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
    font-size: 32rpx;
    color: #999;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
    padding-right: 20rpx;
}

.radio radio {
    width: 48rpx;
    height: 48rpx;
}

.radio:last-child {
    border-bottom: 0rpx solid #ececec;
}

.deta {
    display: flex;
    align-items: center;
}

.wxicon {
    width: 34rpx;
    height: 34rpx;
    margin-left: 24rpx;
    margin-right: 20rpx;
}

.d-foot {
    border-top: 1rpx solid #acacac;
    height: 130rpx;
    padding: 20rpx 20rpx;
}

.d-foot button {
    height: 90rpx;
    background: #ff7800;
    font-size: 34rpx;
    color: #fff;
}