<view class="container">
    <view>
        <view bindtap="goodsChoose" class="list" data-gid="{{item.id}}" data-gname="{{item.goods_name}}" wx:for="{{goodslist}}">
            <view>
                <image class="goodsImg" src="{{url}}{{item.lb_img}}"></image>
            </view>
            <view class="goodsTitle">{{item.goods_name}}</view>
        </view>
    </view>
</view>
