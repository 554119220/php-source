.searchIcon {
    width: 28rpx;
    height: 28rpx;
}

.search {
    background: #fff;
    height: 98rpx;
    display: flex;
    align-items: center;
    padding: 0rpx 20rpx;
}

.search input {
    height: 60rpx;
    width: 556rpx;
    border: 2rpx solid #ececec;
    background: #f8f8f8;
    border-radius: 10rpx;
    font-size: 30rpx;
    padding-left: 20rpx;
    margin-right: 20rpx;
}

.search button {
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-right: 0rpx;
    padding-left: 0rpx;
    width: 110rpx;
    height: 64rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #ff7800;
    border-radius: 10rpx;
}

.session {
    background: #FFF;
    width: 730rpx;
    padding-left: 20rpx;
    margin-top: 20rpx;
}

.goodsList {
    display: flex;
    width: 700rpx;
    padding: 20rpx 30rpx 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
    position: relative;
}

.goodsList:last-child {
    border-bottom: 0rpx solid #ececec;
}

.goodsImg {
    height: 200rpx;
    width: 200rpx;
    border-radius: 12rpx;
    margin-right: 28rpx;
    position: relative;
    overflow: hidden;
}

.goodsImg .goods-img {
    height: 200rpx;
    width: 200rpx;
    border-radius: 12rpx;
    position: absolute;
    top: 0rpx;
    left: 0rpx;
}

.goodsImg .remind {
    width: 240rpx;
    height: 62rpx;
    position: absolute;
    bottom: 0rpx;
    left: 0rpx;
    background: #f21e87;
    color: #fff;
    text-align: center;
    line-height: 62rpx;
    font-size: 26rpx;
}

.goodsRight {
    width: 432rpx;
}

.goodsTitle {
    font-weight: 700;
    font-size: 28rpx;
    line-height: 36rpx;
}

.goodsDet {
    width: 432rpx;
    position: absolute;
    bottom: 20rpx;
    display: flex;
    justify-content: space-between;
}

.toBook {
    height: 68rpx;
    width: 180rpx;
    font-size: 26rpx;
    color: #fff;
    border-radius: 34rpx;
    font-weight: 700;
    line-height: 68rpx;
    text-align: center;
    background: #ff7800;
    position: absolute;
    bottom: 0rpx;
    right: 0rpx;
}

.goodsDet text {
    font-size: 26rpx;
    font-weight: 500;
}

.oldprice {
    font-size: 34rpx;
    color: #999;
}

.price {
    font-size: 34rpx;
    color: #ff7800;
    font-weight: 700;
    margin-top: 24rpx;
}

.bookNum {
    height: 36rpx;
    width: 138rpx;
    background: #ffc999;
    color: #ff7800;
    font-size: 22rpx;
    border-radius: 2rpx;
    margin-top: 20rpx;
    text-align: center;
    line-height: 36rpx;
}