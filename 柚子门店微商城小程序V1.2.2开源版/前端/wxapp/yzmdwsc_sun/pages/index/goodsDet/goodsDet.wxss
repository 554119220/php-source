swiper,.slide-image {
    height: 750rpx;
    width: 750rpx;
}

swiper {
    overflow: hidden;
    z-index: 9;
}

.head {
    position: relative;
    height: 750rpx;
    width: 750rpx;
}

.oldprice {
    font-size: 24rpx;
    margin-left: 30rpx;
}

.oldprice view {
    margin: 6rpx 0rpx;
}

.countDown {
    font-weight: 700;
}

.session {
    padding: 20rpx 20rpx 0rpx 20rpx;
    background: #fff;
}

.secUp {
    display: flex;
    justify-content: space-between;
    padding-bottom: 26rpx;
}

.title {
    font-size: 32rpx;
    line-height: 48rpx;
    font-weight: 700;
    margin-top: 6rpx;
}

.price {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 80rpx;
    padding-bottom: 4rpx;
    border-bottom: 1rpx solid #ececec;
    color: #999;
}

.share {
    height: 60rpx;
    width: 60rpx;
    border-radius: 50%;
    padding-left: 0rpx;
    padding-right: 0rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
}

.shareGoods {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.share image {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
}

.freight {
    white-space: nowrap;
    margin-top: 20rpx;
    color: #999;
}

.goodsPrice {
    font-size: 34rpx;
    font-weight: 700;
    color: #ff7800;
    margin-top: 10rpx;
}

.icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 12rpx;
}

.guarantee {
    height: 90rpx;
    padding-bottom: 0rpx;
    font-size: 23rpx;
    border-bottom: 0rpx solid #fff;
    border-top: 1rpx solid #ececec;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.guarantee view {
    display: flex;
    align-items: center;
    width: 236rpx;
}

.block {
    padding-bottom: 100rpx;
}

.nav-group {
    display: flex;
    align-items: center;
    justify-content: space-around;
    background: #fff;
    border-bottom: 1rpx solid #cbcbcb;
    height: 89rpx;
    margin-top: 20rpx;
    padding: 0rpx 20rpx;
}

.nav {
    display: inline-block;
    width: 355rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 32rpx;
    font-weight: 700;
    color: #666;
    box-sizing: border-box;
    border-bottom: 4rpx solid transparent;
}

.on {
    color: #ff7800;
    border-bottom: 4rpx solid #ff7800;
}

.detail {
    margin-top: 4rpx;
    word-break: break-all;
    line-height: 36rpx;
}

.detail image {
    display: block;
    width: 750rpx;
}

.icn {
    width: 60rpx;
    height: 60rpx;
}

.fixNav {
    display: flex;
    height: 98rpx;
    align-items: center;
    position: fixed;
    bottom: 0rpx;
    background: #fff;
    box-shadow: 0rpx -3rpx 3rpx 0rpx rgba(180,180,180,.21);
    z-index: 99;
}

.fixLeft {
    width: 278rpx;
    display: flex;
    justify-content: space-around;
    font-size: 20rpx;
    color: #8c8c8c;
}

.nav-list {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    line-height: 28rpx;
    background-color: #fff;
    font-size: 20rpx;
    color: #8c8c8c;
    padding-left: 0rpx;
    padding-right: 0rpx;
    border-radius: 0rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
}

.nav-list::after {
    border: none;
}

.fixRight {
    display: flex;
    width: 472rpx;
    font-weight: 700;
}

.fixRight>view {
    height: 98rpx;
    background: #ff7800;
    color: #FFF;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    line-height: 38rpx;
}

.fixRight .singleBuy {
    width: 190rpx;
    background: #ffad37;
}

.botPrice {
    font-size: 32rpx;
}

.toGroup {
    width: 282rpx;
}

.comment {
    min-height: 500rpx;
    background: #fff;
}

.session-comment {
    padding-bottom: 100rpx;
}

.uface {
    height: 80rpx;
    width: 80rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.listComm {
    padding: 32rpx 20rpx 50rpx;
    display: flex;
    border-bottom: 1rpx solid #ececec;
}

.content {
    margin-top: 10rpx;
    display: flex;
    justify-content: space-between;
    font-size: 24rpx;
    color: #999;
    width: 610rpx;
    line-height: 26rpx;
}

.ucomm {
    width: 610rpx;
    margin-top: 20rpx;
    color: #222;
    font-size: 24rpx;
    line-height: 32rpx;
}

.rightIcon {
    width: 11rpx;
    height: 21rpx;
    margin-left: 22rpx;
}

.description {
    display: flex;
    justify-content: space-between;
    width: 710rpx;
    padding: 0rpx 20rpx;
    height: 118rpx;
    align-items: center;
    background: #FFF;
    margin-top: 20rpx;
}

.descTit {
    font-size: 26rpx;
}

.descTit text {
    color: #999;
}

.process {
    font-size: 20rpx;
    color: #666;
    font-weight: 700;
    display: flex;
}

.process>view {
    margin-right: 20rpx;
    margin-top: 22rpx;
}

.process text {
    display: inline-block;
    height: 22rpx;
    width: 22rpx;
    background: #999;
    color: #fff;
    text-align: center;
    line-height: 22rpx;
    margin-right: 6rpx;
    border-radius: 50%;
}

.descRight {
    font-size: 26rpx;
    font-weight: 700;
}

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.drawer_box {
    width: 750rpx;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

.drawer_title {
    display: flex;
    padding: 0rpx 20rpx;
    justify-content: space-between;
    border-bottom: 1rpx solid #ececec;
}

.cancel {
    width: 34rpx;
    height: 34rpx;
    margin-top: 20rpx;
}

.drawer_content {
    height: 460rpx;
    overflow-y: scroll;
    padding-bottom: 100rpx;
}

.goods-thumb {
    width: 202rpx;
    height: 202rpx;
    background: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 8rpx;
    margin-top: -60rpx;
    z-index: 999;
    box-shadow: 0rpx 0rpx 8rpx 0rpx rgba(3,3,3,0.11);
}

.thumb {
    width: 192rpx;
    height: 192rpx;
    border-radius: 8rpx;
}

.goods-detail {
    width: 450rpx;
    margin-left: 24rpx;
    height: 166rpx;
}

.goods-title {
    font-size: 32rpx;
    line-height: 36rpx;
}

.goods-price {
    margin-top: 50rpx;
    font-size: 30rpx;
    color: #ff7800;
    font-weight: 700;
}

.goods-gg {
    font-size: 24rpx;
    color: #666;
    margin-top: 24rpx;
}

.spec {
    margin: 0rpx 20rpx;
    padding: 36rpx 0rpx 18rpx;
    border-bottom: 1rpx solid #ececec;
}

.specName {
    width: 180rpx;
}

.specValue {
    margin-top: 18rpx;
}

.specValue text {
    display: inline-block;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    background: #f8f8f8;
    padding: 0rpx 35rpx;
    margin-right: 20rpx;
    margin-bottom: 10rpx;
}

.specValue .active {
    background: #ff7800;
    color: #fff;
}

.project-list {
    padding: 36rpx 20rpx 82rpx;
}

.icna {
    width: 29rpx;
    height: 30rpx;
}

.icnr {
    width: 29rpx;
    height: 3rpx;
}

.choose {
    display: flex;
    margin-top: 16rpx;
}

