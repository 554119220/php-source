    display: inline-block;
    width: 62rpx;
    height: 50rpx;
    border-top: 1rpx solid #ececec;
    border-bottom: 1rpx solid #ececec;
    text-align: center;
    min-height: 0rpx;
    color: #222;
    font-weight: 700;
    font-size: 32rpx;
}

.choose view {
    width: 58rpx;
    height: 50rpx;
    border: 1rpx solid #ececec;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.drawer-foot {
    width: 750rpx;
    border-top: 1rpx solid #ececec;
    height: 98rpx;
    z-index: 9999;
    background: #fff;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
}

.drawer-foot button {
    width: 330rpx;
    height: 72rpx;
    line-height: 72rpx;
    color: #fff;
    font-size: 30rpx;
    background: #ff7800;
    margin-top: 13rpx;
    border-radius: 4rpx;
}

.drawer-foot button::after {
    border: none;
}