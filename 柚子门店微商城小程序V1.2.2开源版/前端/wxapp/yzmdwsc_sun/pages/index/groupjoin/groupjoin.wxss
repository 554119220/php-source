.goods {
    display: flex;
    justify-content: space-between;
    padding: 20rpx 20rpx 30rpx 20rpx;
    background: #fff;
    border-bottom: 1rpx solid #dcdcdc;
}

.goodsImg {
    height: 270rpx;
    width: 270rpx;
}

.goodsRight {
    height: 270rpx;
    margin-left: 20rpx;
    width: 420rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.title {
    font-size: 32rpx;
    font-weight: 700;
    line-height: 38rpx;
}

.price {
    color: #666;
}

.price>view:last-child {
    color: #ff7800;
    font-size: 34rpx;
    margin-top: 30rpx;
    font-weight: 700;
}

.icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 12rpx;
}

.guarantee {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #999;
    height: 90rpx;
    padding-bottom: 0rpx;
    font-size: 23rpx;
    border-bottom: 0rpx solid #fff;
    border-top: 1rpx solid #ececec;
    background: #fff;
    padding: 0rpx 20rpx;
}

.guarantee view {
    display: flex;
    align-items: center;
}

.description {
    display: flex;
    justify-content: space-between;
    width: 710rpx;
    padding: 0rpx 20rpx;
    height: 118rpx;
    align-items: center;
    background: #FFF;
    margin-top: 20rpx;
}

.descTit {
    font-size: 26rpx;
}

.descTit text {
    color: #999;
}

.process {
    font-size: 20rpx;
    color: #666;
    font-weight: 700;
    display: flex;
}

.process>view {
    margin-right: 20rpx;
    margin-top: 22rpx;
}

.process text {
    display: inline-block;
    height: 22rpx;
    width: 22rpx;
    background: #999;
    color: #fff;
    text-align: center;
    line-height: 22rpx;
    margin-right: 6rpx;
    border-radius: 50%;
}

.descRight {
    font-size: 26rpx;
    font-weight: 700;
}

.rightIcon {
    width: 11rpx;
    height: 21rpx;
    margin-left: 22rpx;
}

.member {
    background: #fff;
    padding: 20rpx 45rpx 100rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 10rpx;
}

.joinuser {
    display: flex;
}

.uthumb {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    margin: 30rpx 25rpx;
    position: relative;
    border: 1rpx dashed #cbcbcb;
}

.thumb {
    border: 1rpx solid #ff7800;
}

.uthumb>image {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
}

.uthumb:first-child::after {
    content: "团长";
    display: inline-block;
    width: 50rpx;
    height: 28rpx;
    color: #fff;
    background: #ff7800;
    font-size: 20rpx;
    position: absolute;
    left: -18rpx;
    text-align: center;
    line-height: 28rpx;
    border-radius: 8rpx;
    border: 1rpx solid #fff;
}

.btn {
    width: 660rpx;
    height: 90rpx;
    font-size: 34rpx;
    color: #fff;
    background: #ff7800;
    border-radius: 10rpx;
}

.over {
    background: #cecece;
}

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.drawer_box {
    width: 750rpx;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

.drawer_title {
    display: flex;
    padding: 0rpx 20rpx;
    justify-content: space-between;
    border-bottom: 1rpx solid #ececec;
}

.cancel {
    width: 34rpx;
    height: 34rpx;
    margin-top: 20rpx;
}

.drawer_content {
    height: 460rpx;
    overflow-y: scroll;
    padding-bottom: 100rpx;
}

.goods-thumb {
    width: 202rpx;
    height: 202rpx;
    background: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 8rpx;
    margin-top: -60rpx;
    z-index: 999;
    box-shadow: 0rpx 0rpx 8rpx 0rpx rgba(3,3,3,0.11);
}

.athumb {
    width: 192rpx;
    height: 192rpx;
    border-radius: 8rpx;
}

.goods-detail {
    width: 450rpx;
    margin-left: 24rpx;
    height: 166rpx;
}

.goods-title {
    font-size: 32rpx;
    line-height: 36rpx;
}

.goods-price {
    margin-top: 50rpx;
    font-size: 30rpx;
    color: #ff7800;
    font-weight: 700;
}

.goods-gg {
    font-size: 24rpx;
    color: #666;
    margin-top: 24rpx;
}

.spec {
    margin: 0rpx 20rpx;
    padding: 36rpx 0rpx 18rpx;
    border-bottom: 1rpx solid #ececec;
}

.specName {
    width: 180rpx;
}

.specValue {
    margin-top: 18rpx;
}

.specValue text {
    display: inline-block;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    background: #f8f8f8;
    padding: 0rpx 35rpx;
    margin-right: 20rpx;
    margin-bottom: 10rpx;
}

.specValue .active {
    background: #ff7800;
    color: #fff;
}

.project-list {
    padding: 36rpx 20rpx 82rpx;
}

.icna {
    width: 29rpx;
    height: 30rpx;
}

.icnr {
    width: 29rpx;
    height: 3rpx;
}

.choose {
    display: flex;
    margin-top: 16rpx;
}

#chooseNum {
    display: inline-block;
    width: 62rpx;
    height: 50rpx;
    border-top: 1rpx solid #ececec;
    border-bottom: 1rpx solid #ececec;
    text-align: center;
    min-height: 0rpx;
    color: #222;
    font-weight: 700;
    font-size: 32rpx;
}

.choose view {
    width: 58rpx;
    height: 50rpx;
    border: 1rpx solid #ececec;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.drawer-foot {
    width: 750rpx;
    border-top: 1rpx solid #ececec;
    height: 98rpx;
    z-index: 9999;
    background: #fff;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
}

.drawer-foot button {
    width: 330rpx;
    height: 72rpx;
    line-height: 72rpx;
    color: #fff;
    font-size: 30rpx;
    background: #ff7800;
    margin-top: 13rpx;
    border-radius: 4rpx;
}

.drawer-foot button::after {
    border: none;
}

.isMask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
}

#isLogin {
    position: fixed;
    width: 480rpx;
    height: 320rpx;
    background: #fff;
    top: 30%;
    left: 85rpx;
    z-index: 1001;
    padding: 50rpx 50rpx;
}

.isLoginBtn {
    display: flex;
    justify-content: space-around;
}

.isLoginBtn button {
    background: #fff;
    width: 100%;
    height: 68repx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    margin-right: 0rpx;
    margin-left: 0rpx;
    font-size: 28rpx;
    margin-top: 50rpx;
    color: #4bad37;
}

.loginTitle {
    font-size: 34rpx;
}

.shopImg {
    height: 80rpx;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
}

.shopImg>image {
    width: 68rpx;
    height: 68rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.isAgary {
    display: flex;
    align-items: center;
    font-size: 24rpx;
    color: #666;
    margin-top: 30rpx;
}

.isAgary>text {
    margin-right: 20rpx;
}