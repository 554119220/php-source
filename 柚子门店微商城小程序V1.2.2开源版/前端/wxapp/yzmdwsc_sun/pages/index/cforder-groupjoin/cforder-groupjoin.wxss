.cards {
    margin: 0rpx 20rpx 30rpx;
    font-size: 24rpx;
    box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(255,100,15,0.11);
    background: rgb(255,241,233);
}

.card-prices {
    padding: 6rpx 20rpx 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border: 1px solid #ff7800;
    color: #ff7800;
    border-bottom: 1px dashed #ff7800;
    border-radius: 12rpx 12rpx 0rpx 0rpx;
    position: relative;
}

.card-prices:before,.card-prices:after {
    content: " ";
    display: block;
    width: 16rpx;
    height: 16rpx;
    background: #f8f8f8;
    border-radius: 50%;
    position: absolute;
    bottom: -12rpx;
    border: 1px solid #ff7800;
    z-index: 99;
    border-left: 1rpx solid transparent;
    border-bottom: 1rpx solid transparent;
}

.card-prices:before {
    left: -10rpx;
    transform: rotate(45deg);
}

.card-prices:after {
    transform: rotate(-135deg);
    right: -10rpx;
}

.card-time {
    border: 1px solid #ff7800;
    border-top: 0px solid #ff7800;
    border-radius: 0rpx 0rpx 12rpx 12rpx;
    height: 118rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding-left: 20rpx;
    line-height: 40rpx;
    color: #666;
}

.price {
    display: flex;
    line-height: 92rpx;
    margin-top: 16rpx;
}

.price text:nth-child(1) {
    font-size: 42rpx;
