.banner,.banner image {
    height: 374rpx;
    width: 750rpx;
}

.session {
    background: #fff;
    padding-left: 30rpx;
}

.session .list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 30rpx;
    border-bottom: 1rpx solid #ececec;
    height: 90rpx;
    line-height: 32rpx;
}

.session .list:last-child {
    border-bottom: 1rpx solid #fff;
}

.list view {
    display: flex;
    align-items: center;
}

.ses-last {
    margin-bottom: 100rpx;
}

.icon {
    width: 26rpx;
    height: 32rpx;
    margin-right: 20rpx;
}

.provide {
    margin-top: 20rpx;
    background: #fff;
    padding: 26rpx 20rpx 0rpx;
}

.provide>view {
    display: inline-block;
    height: 34rpx;
    padding: 0rpx 30rpx;
    line-height: 34rpx;
    border: 2rpx solid #ff7800;
    border-radius: 4rpx;
    font-size: 24rpx;
    color: #ff7800;
    margin-right: 30rpx;
    margin-bottom: 26rpx;
}

.cont {
    padding-bottom: 100rpx;
}

.contTtile {
    width: 750rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 32rpx;
    color: #ff7800;
    font-weight: 700;
    background: #fff;
    margin-top: 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.wrap {
    margin-top: 20rpx;
    background: #fff;
}

.shopDes {
    padding: 20rpx 20rpx 30rpx;
    font-size: 26rpx;
    color: #666;
    line-height: 39rpx;
}

.shopDet image {
    width: 750rpx;
}