<!--yzmdwsc_sun/pages/user/cash/cash.wxml-->
<view class="container">
  <view>
    <view class="header">
      <view class="cash">
        <view>￥{{user.money}}</view>
        <view>账户余额</view>
      </view>
      <view class="head">
        <view class="account">
          <view>{{user.total_money}}</view>
          <view>累计赚取 （元）</view>
        </view>
        <!--<view>立即提现</view>-->
      </view>
    </view>
    <view class="session" wx:if="{{rule}}">
      <view class="sessionTitle">详细规则</view>
      <view class="rule">
        <rich-text nodes="这里是规则"></rich-text>
      </view>
    </view>
  </view>
</view>

