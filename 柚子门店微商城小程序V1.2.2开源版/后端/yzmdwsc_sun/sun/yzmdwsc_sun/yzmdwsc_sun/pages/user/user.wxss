/* yzmdwsc_sun/pages/user/user.wxss */
.head{
  height: 346rpx;
  width:750rpx;
  display: flex;
  justify-content:center;
  background: #FFEEDF;
}
.uthumb{
  border:2rpx solid #fff;
}
.uthumb,.uthumb image{
  width:156rpx;
  height:156rpx;
  border-radius: 50%;
}
.uthumb{
  margin-bottom: 36rpx;
}
.head-thumb{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top:60rpx;
  letter-spacing: 2rpx;
  z-index: 999
}
/**/
.user-count{
  display: flex;
  padding:19rpx 0rpx;
  background:#fff;
}
.user-money{
  display: flex;
  flex-direction: column;
  align-items: center;
  width:375rpx;
  height:90rpx;
}
.user-money:first-child{
  width:374rpx;
  border-right:1rpx solid #cbcbcb;
}
.icon2{
  width:32rpx;
  height:35rpx;
  margin-right: 24rpx;
}
.icon1{
  width:42rpx;
  height: 28rpx;
  margin-right: 24rpx;
}
.m_up{
  height: 90rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
/**/
.block{
  background: #fff;
  margin-top:20rpx;
}
.block-up{
  padding:0rpx 30rpx;
  display: flex;
  height:90rpx;
  justify-content: space-between;
  align-items: center;
}
.rightIcon{
  margin-left:20rpx;
  width: 16rpx;
  height: 24rpx;
}
.block-up view{
  display: flex;
  align-items: center;
  color:#666;
}
.block-down{
  display: flex;
  justify-content: space-around;
  align-items: center;
  height:154rpx;
  border-top:1rpx solid #ececec;
  font-size:22rpx;
  padding-right:20rpx;
  margin-left:20rpx;
}
.block-down view{
  display: flex;
  flex-direction: column;
  align-items: center;
  color:#000;
}
.icn{
  width:52rpx;
  height:52rpx;
  margin-bottom:20rpx;
}
.icon{
  width:34rpx;
  height:34rpx;
  margin-right:34rpx;
}
.session{
  background: #fff;
  padding-left:30rpx;
  margin-top:20rpx;
}
.session .list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right:30rpx;
  border-bottom:1rpx solid #ececec;
  height:92rpx;
}
.session .list:last-child{
  border-bottom:1rpx solid #fff;
}
.list view{
  display: flex;
  align-items: center;
}
.ses-last{
  margin-bottom: 100rpx;
}
.concat{
  margin-left:0rpx;
  margin-right:0rpx;
  padding-left:0rpx;
  padding-right:0rpx;
  font-size: 28rpx;
  background: #fff;
  color:#222;
  display: flex;
  align-items: center;
}
.concat::after{
  border: none;
}