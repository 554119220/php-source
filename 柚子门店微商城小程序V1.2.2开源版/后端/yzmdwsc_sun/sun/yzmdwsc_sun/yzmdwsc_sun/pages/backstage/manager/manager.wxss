/* yzmdwsc_sun/pages/backstage/manager/manager.wxss */
.searchIcon{
  height: 28rpx;
  width: 28rpx;
  padding:0rpx 20rpx;
}
.searchForm{
  display: inline-block;
  background: #fff;
  width: 672rpx;
  height: 64rpx;
  padding: 17rpx 39rpx;
}
.search{
  display: flex;
  width: 672rpx;
  height: 64rpx;
  border-radius: 10rpx;
  align-items: center;
  background: #f8f8f8;
}
.search input{
  width: 604rpx;
  font-size: 30rpx;
}
.search button{
  padding-left:0rpx;
  padding-right:0rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  background: transparent;
}
.search button::after{
  border: none;
}
/**/
.session{
  background: #fff;
  padding-left: 20rpx;
  border-top: 1rpx solid #dcdcdc;
}
.sessionTtile{
  height: 82rpx;
  text-align: center;
  line-height: 82rpx;
  font-size: 30rpx;
  font-weight: 700;
}
.list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 98rpx;
  width: 710rpx;
  padding:20rpx 20rpx 20rpx 0rpx;
  border-bottom: 1rpx solid #dcdcdc;
}
.list:last-child{
  border-bottom: 0rpx solid #fff;
}
.listLeft{
  display: flex;
}
.listRight{
  width: 128rpx;
  height: 62rpx;
  text-align: center;
  line-height: 62rpx;
  background: #ff7800;
  font-size: 30rpx;
  color: #fff;
  font-weight: 700;
  border-radius: 10rpx;
}
.uthumb{
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
  margin-right: 24rpx;
}
.listCont{
  width: 450rpx;
  height: 78rpx;
  padding:10rpx 0rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-weight: 700;
}