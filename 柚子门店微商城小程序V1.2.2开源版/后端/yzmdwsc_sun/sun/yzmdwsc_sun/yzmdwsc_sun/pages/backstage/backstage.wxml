    <view class="wrap">
      <!-- <image src="../../../style/images/logobs.png" class="logo"/> -->
      <view class="logo pretend">假装有logo</view>
      <view>
        <form bindsubmit="formSubmit" >
          <view class="session">
            <input type="text" placeholder="" decode="{{true}}"/>
