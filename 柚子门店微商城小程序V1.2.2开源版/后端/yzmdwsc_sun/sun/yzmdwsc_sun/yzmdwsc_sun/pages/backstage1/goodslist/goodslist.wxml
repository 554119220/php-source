<!--yzmdwsc_sun/pages/backstage/goodslist/goodslist.wxml-->
<view class="container">
  <view>

    <view wx:for="{{goodslist}}" wx:key="" data-gid='{{item.id}}' data-gname='{{item.goods_name}}' class="list" bindtap='goodsChoose'>
      <view>
        <image src="{{url}}{{item.lb_img}}" class="goodsImg"/>
      </view>
      <view class="goodsTitle">{{item.goods_name}}</view>
    </view>

  </view>
</view>