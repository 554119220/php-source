page {
    background: #fff;
}

.logList {
    padding: 20rpx;
}

.logList .logItem {
    padding: 20rpx;
    border-bottom: 2rpx dashed #eee;
}

.logList .logItem .logInfoItem {
    color: #585858;
    font-size: 14px;
    margin-bottom: 6rpx;
}

.logList .logItem .logInfoActivity {
    padding: 20rpx;
    border: 1px solid #eee;
    border-radius: 20rpx;
    margin-top: 20rpx;
}

.logList .logItem .logInfoActivityLogo {
    display: inline-block;
    vertical-align: middle;
    width: 100rpx;
    height: 100rpx;
}

.logList .logItem .logInfoActivityTitle {
    display: inline-block;
    vertical-align: middle;
    margin-left: 20rpx;
    font-size: 13px;
    width: 500rpx;
}