page {
    background: #fff;
}

.userList {
    padding: 20rpx;
}

.userList .userItem {
    padding: 20rpx;
    border-bottom: 2rpx dashed #eee;
}

.userList .userItem .userInfoItem {
    color: #585858;
    font-size: 14px;
    margin-bottom: 6rpx;
}

.userList .userItem .userTopInfo {
    margin-bottom: 16rpx;
}

.userList .userItem .userTopInfo .userTopInfoAvatar {
    width: 70rpx;
    height: 70rpx;
    display: inline-block;
    vertical-align: middle;
    border-radius: 50%;
}

.userList .userItem .userTopInfo .userTopInfoName {
    display: inline-block;
    vertical-align: middle;
    font-size: 15px;
    margin-left: 20rpx;
}