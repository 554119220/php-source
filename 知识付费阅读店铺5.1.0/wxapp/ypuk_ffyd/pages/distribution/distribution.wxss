.distributionText {
    background: #fff;
    padding: 30rpx;
    font-size: 14px;
}

.distributionCode {
    background: #fff;
    padding: 30rpx 30rpx 60rpx;
    font-size: 14px;
}

.distributionCodeIpt {
    border: 1px solid #eee;
    padding: 20rpx 30rpx;
    border-radius: 20rpx;
    text-align: center;
}

.distributionCodeLabel {
    text-align: center;
    color: #999;
    margin-right: 20rpx;
    margin-bottom: 30rpx;
    font-weight: 600;
    border-top: 1px dashed #eee;
    padding-top: 30rpx;
}

.distributionCodeUid {
    color: #fede34;
    font-size: 20px;
    font-weight: 600;
}

.distributionCodeCopy {
    color: #aaa;
    font-size: 12px;
}

.distributionDate {
    background: #fff;
    padding: 30rpx;
    border-bottom: 1px solid #eee;
}

.distributionDateItem {
    display: inline-block;
    text-align: center;
    width: 25%;
    font-size: 14px;
}

.distributionDateNum {
    font-size: 16px;
}

.Withdraw {
    width: 600rpx;
    margin: 50rpx auto 0;
    text-align: center;
    background: #ee4f4f;
    padding: 30rpx 0;
    font-size: 15px;
    border-radius: 20rpx;
    color: #fff;
}

.red {
    color: #f60;
}