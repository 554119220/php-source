.containers {
    display: flex;
    flex-direction: column;
    width: 100%;
}

.container>view {
    flex: 1;
}

.title {
    background: #f8f8f8;
    height: 50rpx;
    line-height: 50rpx;
    padding-left: 20rpx;
    color: #000;
    font-size: 24rpx;
}

.location {
    height: 80rpx;
    display: flex;
    align-items: center;
    font-weight: bold;
    font-size: 26rpx;
    padding-left: 20rpx;
    background: #ffffff;
}

.ad_icon {
    width: 40rpx;
    height: 40rpx;
}

.label_view {
    padding: 10rpx 25rpx;
    background: #ffffff;
    display: flex;
    flex-wrap: wrap;
}

.label_city {
    width: 190rpx;
    height: 60rpx;
    line-height: 60rpx;
    border: 1rpx solid #000000;
    border-radius: 10rpx;
    text-align: center;
    margin: 10rpx 0;
    margin-right: 40rpx;
    font-size: 24rpx;
    font-weight: bold;
}

.info {
    display: flex;
    flex-direction: column;
    flex: 1;
}

.column_city {
    padding: 0 25rpx;
    height: 60rpx;
    background: #ffffff;
    flex: 1;
    font-size: 26rpx;
    line-height: 60rpx;
}

.src {
    position: relative;
    height: 100%;
}

.nav {
    position: fixed;
    right: 30rpx;
    z-index: 2;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    color: #000;
    height: 1040rpx;
    font-size: 26rpx;
}

.nav>view {
    color: #6c6b73;
    text-align: center;
    height: 40rpx;
    width: 40rpx;
    line-height: 40rpx;
}