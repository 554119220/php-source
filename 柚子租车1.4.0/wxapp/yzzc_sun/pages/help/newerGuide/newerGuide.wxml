<import src="../../wxParse/wxParse.wxml"></import>
<view class="containers">
    <image class="topImg" mode="widthFix" src="{{imgLink+msg.pic}}" wx:if="{{msg.pic}}"></image>
    <view class="bianji">
        <view class="title">{{msg.title}}</view>
        <view class="fangfa">
            <view class="fangfa-title">{{msg.title}}</view>
            <view class="contents">
                <template is="wxParse" data="{{wxParseData:detail.nodes}}"></template>
            </view>
        </view>
    </view>
</view>
