page {
    background-color: #f2f2f2;
}

.list_dhf {
    padding: 30rpx;
}

.item_dhf {
    height: 200rpx;
    padding: 12rpx 10rpx 30rpx 20rpx;
    border-radius: 10rpx;
    background-color: #fff;
    font-size: 30rpx;
    color: #000;
    margin-bottom: 20rpx;
}

.item_title {
    font-size: 28rpx;
    color: #999;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
}

.iAnswer {
    width: 120rpx;
    height: 50rpx;
    background-color: #618dfb;
    color: #fff;
    border-radius: 15rpx;
    text-align: center;
    line-height: 50rpx;
    font-size: 26rpx;
}

.lvshi {
    font-size: 28rpx;
    color: #999;
}

.lv_name {
    color: #618dfb;
    margin-left: 20rpx;
}

.contents {
    font-size: 30rpx;
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}