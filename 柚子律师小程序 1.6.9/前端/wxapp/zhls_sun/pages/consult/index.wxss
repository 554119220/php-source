page {
    background-color: #f2f2f2;
}

.container {
    width: 750rpx;
}

.header {
    width: 100%;
    height: 110rpx;
    background-color: #618dfa;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.searchBox {
    width: 690rpx;
    height: 70rpx;
    border-radius: 30rpx;
    font-size: 28rpx;
    color: #000;
    background-color: #fff;
    display: flex;
    align-items: center;
    margin-top: 20rpx;
}

.searchIcon {
    margin-left: 25rpx;
}

.input {
    margin-left: 20rpx;
}

.diyHolder {
    color: #000;
    font-size: 28rpx;
}

.bodys {
    width: 100%;
    min-height: 440rpx;
    font-size: 30rpx;
    color: #000;
    margin-top: 20rpx;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.anli-title {
    margin: 40rpx 0rpx 60rpx 0rpx;
}

.anli-list {
    width: 690rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.anli-item {
    width: 160rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-left: 10rpx;
    margin-bottom: 30rpx;
}

.anli-list:after {
    flex: 1;
}

.anli-Icon {
    width: 70rpx;
    height: 70rpx;
    display: flex;
}

.anli-name {
    margin-top: 20rpx;
}

.footer {
    margin-top: 30rpx;
    width: 100%;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.youzhi-title {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
    font-size: 30rpx;
    padding: 40rpx 0rpx 20rpx 0rpx;
    border-bottom: 1rpx solid #eee;
}

.youzhi-item {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #eee;
}

.ques {
    display: flex;
    line-height: 28rpx;
    margin: 20rpx 30rpx 0rpx 30rpx;
}

.answ {
    display: flex;
    margin: 10rpx 30rpx 0rpx 30rpx;
}

.ques-word {
    width: 630rpx;
    font-size: 28rpx;
    color: #000;
    margin-left: 20rpx;
    display: -webkit-box!important;
    line-height: 44rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.ques-Icon {
    width: 45rpx;
    height: 45rpx;
}

.answ-word {
    width: 630rpx;
    color: #666;
    font-size: 28rpx;
    margin-left: 20rpx;
    display: -webkit-box!important;
    line-height: 44rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.leix-Date {
    width: 630rpx;
    margin-left: 65rpx;
    margin-top: 20rpx;
    color: #999;
    font-size: 26rpx;
    display: flex;
    justify-content: space-between;
}

.leix-status {
    width: 120rpx;
    height: 40rpx;
    border-radius: 10rpx;
    border: 1rpx solid #f58144;
    color: #f58144;
    font-size: 26rpx;
    text-align: center;
    line-height: 40rpx;
}