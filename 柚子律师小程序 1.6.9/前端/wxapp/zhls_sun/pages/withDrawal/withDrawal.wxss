.drawIcon {
    width: 50rpx;
    margin: 0 20rpx 0 0;
}

.mesInput {
    margin: 0 0 0 30rpx;
    flex: 1;
}

.checkBox {
}

.noticeWrap {
    padding: 30rpx 30rpx 50rpx;
    color: #666;
}

.notice {
    color: #ff5255;
}

.withDrawBtn {
    width: 600rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #d2d2d2;
    color: #fff;
    font-size: 30rpx;
    line-height: 90rpx;
}

.withDrawBtn.active {
    background-color: #ff5255;
}

button::after {
    border: none;
}

.introWord {
    color: #666;
    padding: 50rpx 30rpx;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 5;
}

.succContents {
    width: 500rpx;
    height: 700rpx;
    background-color: #fff;
    position: fixed;
    top: 200rpx;
    left: 125rpx;
    z-index: 6;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 10rpx;
}

.title {
    width: 100%;
    height: 70rpx;
    text-align: center;
    line-height: 70rpx;
    background-color: #ff5255;
    color: #fff;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.noticeRuleBox {
    padding: 30rpx;
    overflow-y: scroll;
    word-break: break-all;
}

rich-text {
    word-break: break-all;
}

.deterBtn {
    width: 150rpx;
    height: 60rpx;
    border-radius: 10rpx;
    background-color: #ff5255;
    color: #fff;
    text-align: center;
    line-height: 60rpx;
    position: absolute;
    bottom: 20rpx;
}

.drawText1 {
    display: inline-block;
    margin-top: 20rpx;
    margin-right: 10rpx;
}

.drawText11 {
    margin-top: 20rpx;
    display: inline-block;
    width: 200rpx;
}