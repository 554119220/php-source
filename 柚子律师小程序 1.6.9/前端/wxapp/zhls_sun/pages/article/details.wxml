<view class="container">
    <view class="article-wrap">
        <view class="header">
            <view class="titleWord">{{dynamics.team_name}}</view>
            <view class="dateWord">{{dynamics.selftime}}</view>
        </view>
        <view class="contents">
            <view class="imgWrap">
                <image class="contentsImg" src="{{url}}{{dynamics.imgs}}"></image>
            </view>
            <rich-text class="contentsWord" nodes="{{dynamics.content}}"></rich-text>
        </view>
    </view>
</view>
