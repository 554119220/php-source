page {
    background-color: #f2f2f2;
}

.container {
    width: 750rpx;
}

.scrollView {
    width: 100%;
    height: 90rpx;
    white-space: nowrap;
}

.border_bottom {
    display: inline-block;
}

.status-box {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
    box-sizing: border-box;
}

.status_label {
    height: 100%;
    text-align: center;
    font-size: 30rpx;
    color: #000000;
    box-sizing: border-box;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
}

.borderS.active {
    border-bottom: 7rpx solid #618dfb;
}

.detailsList {
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.anli-item {
    width: 690rpx;
    height: 170rpx;
    display: flex;
    margin-top: 20rpx;
    background-color: #fff;
    padding: 20rpx 30rpx;
    position: relative;
}

.anliImg {
    width: 220rpx;
    height: 170rpx;
}

.anli-Title {
    width: 450rpx;
    height: 85rpx;
    color: #000;
    font-size: 30rpx;
    margin-left: 20rpx;
    display: -webkit-box!important;
    line-height: 44rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.anli-Date {
    color: #999;
    font-size: 26rpx;
    position: absolute;
    bottom: 20rpx;
    right: 30rpx;
}

.footer-box {
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    width: 100%;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    border-top: 1rpx solid #eee;
    background-color: #fff;
}

.diyItem {
    width: 80rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
}

.diyTab {
    width: 54rpx;
    height: 54rpx;
    margin-top: 10rpx;
}

.diyTab.active {
}

.diyTabs {
    width: 54rpx;
    height: 54rpx;
}

.diyTabs.active {
    width: 54rpx;
    height: 54rpx;
}

.diyTabss {
    width: 54rpx;
    height: 54rpx;
}

.diyTabss.active {
    width: 54rpx;
