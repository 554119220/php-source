<template name="productList">
    <view catchtap="productDetails" class="product-List-left" wx:for="{{listArray.data}}">
        <view class="picture-area">
            <image class="productlist1" src="{{listArray.other.img_root+item.icon}}"></image>
            <image class="underway" src="/zhy/resource/image/integrationMall/tupian2.png"></image>
        </view>
        <view class="product-introduction">
            <text class="product-List1-text">仓鼠坐垫一体椅垫</text>
            <view class="product-number">
                <text class="product-price">
                    <text class="price">价格</text>
                    <text class="price-count">￥76.8</text>
                </text>
                <text class="product-price2">
                    <text>剩余</text>
                    <text class="count">45</text>
                    <text>份</text>
                </text>
            </view>
            <view class="lowest-integral">
                <image class="tupian4" src="/zhy/resource/image/integrationMall/tupian4.jpg"></image>
                <text class="product-price3">
                    <text class="price-text">最低</text>
                    <text class=" integra-number">1920</text>
                    <text>点劵申请</text>
                </text>
            </view>
        </view>
    </view>
</template>
