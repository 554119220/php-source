.isMask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
}

.isLogin {
    position: fixed;
    padding: 50rpx 50rpx;
    width: 480rpx;
    height: 670rpx;
    background: #fff;
    top: 50%;
    left: 50%;
    margin-left: -280rpx;
    margin-top: -422rpx;
    z-index: 1001;
    border-radius: 10px;
}

.loginimg {
    text-align: center;
}

.loginimg image {
    margin: 0 auto;
    width: 330rpx;
    height: 330rpx;
}

.isLoginBtn {
    display: flex;
    justify-content: space-around;
}

.isLoginBtn button {
    background: #8db4ff;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 34rpx;
    margin-top: 50rpx;
    color: #ffffff;
}

.loginTitle {
    font-size: 34rpx;
}

.loginshop {
    padding-top: 20rpx;
    font-size: 34rpx;
    line-height: 60rpx;
    text-align: center;
    color: #333;
}

.isAgary {
    text-align: center;
    align-items: center;
    font-size: 28rpx;
    color: #999;
    margin-top: 10rpx;
}

.isAgary>text {
    margin-right: 20rpx;
}