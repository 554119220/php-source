.tab {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    background: #fff;
    border-top: 2rpx solid #acacac;
    height: 92rpx;
    padding-bottom: 4rpx;
}

.ipx {
    padding-bottom: 68rpx;
}

.tabnav {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.tablist {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #8c8c8c;
}

.active {
    color: #ff640f;
}

.tabimg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 8rpx;
    margin-bottom: 0rpx;
}

.session {
    font-size: 30rpx;
    color: #222;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0rpx 30rpx 0rpx 20rpx;
    height: 90rpx;
    background: #fff;
    margin-top: 30rpx;
}

.shop {
    margin-top: 20rpx;
}

.status {
    margin-left: 50rpx;
    font-size: 28rpx;
    color: #999;
}

.order-nav {
    width: 750rpx;
    height: 80rpx;
    display: flex;
    justify-content: space-around;
    background: #fff;
    line-height: 80rpx;
    border-top: 1rpx solid #cbcbcb;
}

.nav {
    display: inline-block;
    height: 80rpx;
    font-size: 30rpx;
    color: #666;
    font-weight: 500;
}

.on {
    box-sizing: border-box;
    color: #ff640f;
    border-bottom: 4rpx solid #ff640f;
}

.no {
    margin-top: 200rpx;
    height: 270rpx;
    width: 750rpx;
}

.order-cont {
    padding-bottom: 100rpx;
}

.list {
    background: #fff;
    margin-top: 20rpx;
    font-size: 28rpx;
}

.order-num {
    display: flex;
    font-size: 28rpx;
    justify-content: space-between;
    align-items: center;
    height: 82rpx;
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.order-num view:first-child {
    font-weight: 700;
}

.list-up {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
    font-weight: 700;
}

.list-left {
    display: flex;
    padding-left: 20rpx;
}

.list-right {
    padding-right: 20rpx;
    white-space: nowrap;
}

.left-det {
    display: flex;
    flex-direction: column;
    margin-left: 20rpx;
    font-size: 24rpx;
    position: relative;
}

.style {
    display: inline-block;
    margin-right: 20rpx;
    font-weight: 500;
}

.left-title {
    color: #222;
    font-size: 28rpx;
    line-height: 36rpx;
    margin-top: 14rpx;
}

.price {
    font-size: 30rpx;
    position: absolute;
    bottom: 0rpx;
}

.goodsImg {
    border: 1rpx solid #ececec;
}

.img {
    height: 180rpx;
    width: 180rpx;
}

.list-down {
    height: 80rpx;
    background-color: #fff;
    display: flex;
    justify-content: flex-end;
    padding: 0rpx 20rpx;
    align-items: center;
}

.orderDet {
    justify-content: space-between;
    border-bottom: 1rpx solid #ececec;
    font-size: 26rpx;
    line-height: 80rpx;
}

.sum {
    margin-right: 16rpx;
}

.actualPri {
    font-weight: 700;
    font-size: 30rpx;
    color: #f33030;
}

.more {
    text-align: center;
    padding: 10rpx;
    color: #999;
    padding-top: 30rpx;
}