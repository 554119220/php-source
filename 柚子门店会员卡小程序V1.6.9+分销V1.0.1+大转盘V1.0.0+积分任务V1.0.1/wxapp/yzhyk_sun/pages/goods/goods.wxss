.container {
    overflow: hidden;
}

.body {
    height: 100%;
}

.search {
    width: 710rpx;
    height: 64rpx;
    margin: 12rpx 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 10rpx;
    background: #f2f2f2;
}

.search view {
    background: #fff;
    width: 100%;
    height: 64rpx;
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #999;
    font-size: 30rpx;
}

.sIcon {
    width: 28rpx;
    height: 28rpx;
    margin-right: 20rpx;
}

.goods {
    width: 750rpx;
    background: #fff;
    height: 100%;
    overflow: hidden;
    display: flex;
}

.nav {
    height: 100%;
    width: 200rpx;
    background: #fafafa;
    padding-bottom: 100rpx;
}

.nav view {
    height: 102rpx;
    box-sizing: border-box;
    width: 200rpx;
    line-height: 102rpx;
    text-align: center;
    font-weight: 700;
    font-size: 24rpx;
    background: #fafafa;
    border-left: 8rpx solid #fafafa;
}

.nav .on {
    box-sizing: border-box;
    color: #ff640f;
    border-left: 8rpx solid #ff640f;
    background: #fff;
    border-top: 1rpx solid #cbcbcb;
    border-bottom: 1rpx solid #cbcbcb;
}

.goodsCont {
    height: 100%;
    background: #fff;
    width: 536rpx;
    padding-left: 14rpx;
    overflow-y: scroll;
}

.rightNav {
    display: flex;
    margin-top: 6rpx;
    padding-bottom: 4rpx;
    border-bottom: 1rpx solid #ececec;
}

.minNav {
    display: flex;
    flex-wrap: wrap;
    width: 480rpx;
    height: 96rpx;
    overflow: hidden;
}

.show {
    height: auto;
}

.minNav view {
    padding: 0rpx 36rpx;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 6rpx;
    border: 1rpx solid #dcdcdc;
    margin-right: 20rpx;
    margin-top: 20rpx;
    font-size: 24rpx;
}

.minNav .cur {
    background: #ff640f;
    border: 1rpx solid #ff640f;
    color: #fff;
}

.arrow {
    height: 70rpx;
    width: 36rpx;
    margin-top: 20rpx;
    padding-right: 20rpx;
    line-height: 70rpx;
}

.arrow image {
    width: 32rpx;
    height: 22rpx;
    transform: rotate(180deg);
}

.arrow1 image {
    transform: rotate(0deg);
}

.goodsList {
    padding-top: 8rpx;
    padding-bottom: 220rpx;
}

.ipxx {
    padding-bottom: 280rpx;
}

.goodsLft {
    width: 160rpx;
    height: 160rpx;
    border-radius: 8rpx;
    border: 1rpx solid #ececec;
    margin-right: 22rpx;
    position: relative;
}

.buying {
    position: absolute;
    top: 0rpx;
    font-size: 22rpx;
    font-weight: 700;
    color: #f33030;
    height: 28rpx;
    display: flex;
    align-items: center;
    width: 160rpx;
    background: rgba(255,255,255,0.5);
}

.hotIcon {
    width: 15rpx;
    height: 22rpx;
    margin-right: 16rpx;
    margin-left: 32rpx;
}

.goodsImg {
    height: 158rpx;
    width: 158rpx;
    border-radius: 10rpx;
}

.goodsItem {
    display: flex;
    padding: 12rpx 20rpx 12rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
}

.goodsRight {
    width: 344rpx;
    height: 160rpx;
    position: relative;
}

.goodsName {
    display: -webkit-box!important;
    width: 250rpx;
    font-size: 26rpx;
    line-height: 36rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.goodsName_view {
    display: flex;
    line-height: 36rpx;
    justify-content: space-between;
}

.loook_goods {
    font-size: 22rpx;
    color: #999;
    flex: 1;
    width: 100rpx;
}

.goodsDet {
    width: 344rpx;
    position: absolute;
    bottom: 10rpx;
    display: flex;
    justify-content: space-between;
}

.goodsPrice {
    font-size: 32rpx;
    color: #f33030;
    font-weight: 700;
}

.oldPrice {
    position: relative;
    font-size: 24rpx;
    color: #999;
    font-weight: 500;
    margin-bottom: 10rpx;
    display: inline-block;
}

.oldPrice::after {
    content: " ";
    display: inline-block;
    width: 100%;
    height: 2rpx;
    background: #999;
    position: absolute;
    left: 0rpx;
    top: 10rpx;
    transform: rotate(3deg);
}

.opear {
    display: flex;
    position: absolute;
    right: 0rpx;
    bottom: 0rpx;
    align-items: center;
}

.reduce {
    height: 56rpx;
    width: 56rpx;
    border-radius: 50%;
    border: 2rpx solid #999;
    display: flex;
    justify-content: center;
    align-items: center;
}

.reduce image {
    width: 26rpx;
    height: 5rpx;
}

.add {
    border: 2rpx solid #ff640f;
}

.add image {
    width: 26rpx;
    height: 27rpx;
}

.goodsNum input {
    width: 60rpx;
    height: 56rpx;
    text-align: center;
    line-height: 56rpx;
}

.more {
    text-align: center;
    padding: 10rpx;
    color: #999;
    margin-top: 30rpx;
}

.emptyGoods {
    width: 358rpx;
    height: 296rpx;
    margin-top: 100rpx;
    margin-left: 96rpx;
}

.opear_btn {
    padding: 10rpx 30rpx;
    border: 1rpx solid #ff640f;
    color: #ff640f;
    background: #ffffff;
}