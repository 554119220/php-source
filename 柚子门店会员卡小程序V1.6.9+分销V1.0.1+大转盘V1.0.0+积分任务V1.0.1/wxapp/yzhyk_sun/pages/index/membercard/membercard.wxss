page {
    background: #fff;
}

.head {
    background-color: #262932;
    background-image: url(http://cgkqd.img48.wal8.com/img48/569611_20170429191245/152419589306.png);
    background-size: contain;
    height: 340rpx;
    width: 750rpx;
    padding-top: 50rpx;
    z-index: 9;
}

.headCont {
    position: relative;
    width: 650rpx;
    height: 340rpx;
    margin: 0rpx auto;
    z-index: 1;
    border-radius: 16rpx;
    box-shadow: 0 0 12rpx 0rpx rgb(38,41,50,0.11);
}

.bgCards {
    width: 650rpx;
    height: 340rpx;
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    border-radius: 16rpx;
}

.memberCard {
    padding: 0rpx 50rpx;
    width: 550rpx;
    z-index: 111;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: absolute;
    top: 70rpx;
}

.mcLeft {
    display: flex;
}

.bgLogo {
    height: 100rpx;
    width: 100rpx;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 4rpx solid #fff;
}

.bgLogo image {
    width: 86rpx;
    height: 86rpx;
    border-radius: 50%;
}

.mcName {
    font-size: 32rpx;
    color: #8b572a;
    font-weight: 700;
    height: 84rpx;
    padding: 8rpx 0rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 24rpx;
}

.mcName view:last-child {
    font-size: 28rpx;
}

.wrap {
    background: #fff;
    margin-top: -46rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.barcode {
    margin-top: 78rpx;
}

.barcode>canvas {
    width: 490rpx;
    height: 110rpx;
}

.qrcode>canvas {
    width: 420rpx;
    height: 420rpx;
}

.notice {
    padding: 32rpx 0rpx 60rpx;
}

.session {
    background: #fff;
    padding: 0rpx 20rpx 0rpx 30rpx;
}

.list {
    height: 90rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx solid #ececec;
}

.integral {
    font-weight: 700;
    font-size: 32rpx;
    display: flex;
    align-items: center;
}

.integral text {
    margin-right: 20rpx;
}

.remark {
    width: 750rpx;
    text-align: center;
    font-size: 26rpx;
    color: #666;
    margin-top: 60rpx;
}