button::after {
    border: none;
}

swiper {
    height: 750rpx;
}

.slide-image {
    width: 750rpx;
    height: 750rpx;
}

.head {
    position: relative;
    height: 750rpx;
    width: 750rpx;
}

.light {
    height: 100rpx;
    width: 710rpx;
    padding: 0rpx 20rpx;
    background: #ff640f;
    z-index: 99;
    position: absolute;
    bottom: 0rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #fff;
}

.lightLeft {
    display: flex;
    align-items: center;
}

.lightPrice {
    font-size: 72rpx;
    font-weight: 700;
}

.lightPrice text {
    font-size: 50rpx;
}

.lightPrice .moneyIcon {
    font-size: 30rpx;
    line-height: 24rpx;
}

.lightPrice .arrPrice {
    font-size: 72rpx;
}

.oldprice {
    font-size: 24rpx;
    margin-left: 30rpx;
}

.oldprice view {
    margin: 6rpx 0rpx;
}

.countDown {
    font-weight: 700;
}

.session {
    background: #fff;
    padding-left: 30rpx;
    margin-top: 20rpx;
}

.sList {
    border-bottom: 1rpx solid #ececec;
    font-size: 32rpx;
    padding: 24rpx 0rpx;
    line-height: 42rpx;
    display: flex;
    align-items: flex-start;
}

.sList:last-child {
    border-bottom: 0rpx solid #ececec;
}

.sList text:first-child {
    display: inline-block;
    color: #666;
    margin-right: 4rpx;
}

.sList text:last-child {
    display: inline-block;
    width: 520rpx;
}

.price {
    color: #f33030;
    font-weight: 700;
}

.cartIcon {
    width: 92rpx;
    height: 92rpx;
    border-radius: 50%;
}

.shareIcon {
    height: 32rpx;
    width: 32rpx;
    margin-right: 24rpx;
}

.btnGroup {
    display: flex;
    justify-content: space-between;
    background: #fff;
    padding: 0rpx 30rpx;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    width: 690rpx;
    height: 98rpx;
    box-shadow: 0rpx -3rpx 3rpx 0rpx rgba(180,180,180,0.212);
}

.carts {
    height: 104rpx;
    width: 104rpx;
    border-radius: 50%;
    margin-top: -28rpx;
    box-shadow: 0rpx -3rpx 3rpx 0rpx rgba(180,180,180,0.212);
    position: relative;
}

.carts text {
    position: absolute;
    top: 10rpx;
    right: -26rpx;
    display: block;
    width: 50rpx;
    height: 30rpx;
    line-height: 30rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(253,71,116)0%,rgb(253,194,209)100%);
    border-radius: 15rpx;
    font-weight: 700;
    color: #fff;
    text-align: center;
}

.carts button {
    height: 104rpx;
    width: 104rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.groupRight {
    display: flex;
    width: 500rpx;
}

.groupRight button {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 66rpx;
    width: 230rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    margin-top: 16rpx;
    font-size: 32rpx;
    color: #ff640f;
    margin-left: 40rpx;
    box-sizing: border-box;
    border: 2rpx solid #ff640f;
    background: #fff;
    border-radius: 20rpx;
}

.groupRight .add {
    background: #ff640f;
    color: #fff;
}

.groupRight .opear {
    display: flex;
    right: 0rpx;
    align-items: center;
    height: 98rpx;
}

.reduce {
    height: 56rpx;
    width: 56rpx;
    border-radius: 50%;
    border: 2rpx solid #999;
    display: flex;
    justify-content: center;
    align-items: center;
}

.reduce image {
    width: 26rpx;
    height: 5rpx;
}

.addBtn {
    border: 2rpx solid #ff640f;
}

.addBtn image {
    width: 26rpx;
    height: 27rpx;
}

.goodsNum input {
    width: 60rpx;
    height: 56rpx;
    text-align: center;
    line-height: 56rpx;
}

button::after {
    border: none;
}

.wrap {
    background: #fff;
    margin-top: 20rpx;
    padding-bottom: 200rpx;
}

.session-title {
    font-size: 30rpx;
    font-weight: 700;
    text-align: center;
    height: 82rpx;
    line-height: 82rpx;
}

.detail image {
    width: 750rpx;
    height: 750rpx;
}

.detail {
    line-height: 42rpx;
}

.noGoods {
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    color: rgb(153,153,153);
}

.noGoods>image {
    width: 260rpx;
    height: 242rpx;
    margin-bottom: 30rpx;
    margin-top: 100rpx;
}