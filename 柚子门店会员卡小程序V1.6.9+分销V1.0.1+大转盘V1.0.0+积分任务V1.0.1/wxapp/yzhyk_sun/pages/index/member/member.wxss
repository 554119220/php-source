.head {
    background-color: #262932;
    background-size: contain;
    height: 296rpx;
    width: 750rpx;
    padding-top: 50rpx;
    z-index: 9;
}

.headCont {
    position: relative;
    width: 650rpx;
    height: 296rpx;
    margin: 0rpx auto;
    z-index: 1;
    border-radius: 16rpx 16rpx 0rpx 0rpx;
    overflow: hidden;
}

.bgCards {
    width: 650rpx;
    height: 296rpx;
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    border-radius: 16rpx 16rpx 0rpx 0rpx;
}

.memberCard {
    padding: 0rpx 50rpx;
    width: 550rpx;
    z-index: 111;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: absolute;
    top: 70rpx;
}

.mcLeft {
    display: flex;
}

.bgLogo {
    height: 100rpx;
    width: 100rpx;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 4rpx solid #fff;
}

.bgLogo image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}

.mcName {
    font-size: 32rpx;
    color: #8b572a;
    font-weight: 700;
    height: 84rpx;
    padding: 8rpx 0rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 24rpx;
}

.mcName view:last-child {
    font-size: 28rpx;
}

.mcRight {
    width: 150rpx;
    height: 62rpx;
    border-radius: 6rpx;
    color: #fff;
    background: #8b572a;
    display: flex;
    align-items: center;
    justify-content: center;
}

.order-nav {
    height: 80rpx;
    display: flex;
    align-items: center;
    background: #fff;
    justify-content: space-around;
    margin-top: 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.order-nav view {
    display: inline-block;
    height: 80rpx;
    line-height: 80rpx;
    box-sizing: border-box;
    color: #666;
    font-size: 30rpx;
}

.order-nav .on {
    border-bottom: 4rpx solid #ff640f;
    color: #ff640f;
}

.block {
    margin-top: 20rpx;
    background: #fff;
    padding-bottom: 30rpx;
}

.blockTitle {
    display: flex;
    align-items: center;
    height: 84rpx;
    justify-content: center;
}

.blockTitle>text {
    margin: 0rpx 20rpx;
    font-weight: 700;
    font-size: 32rpx;
}

.licon {
    width: 20rpx;
    height: 30rpx;
}

.ricon {
    transform: rotateY(180deg);
}

.rules {
    padding: 0rpx 20rpx 50rpx 20rpx;
    font-size: 28rpx;
    line-height: 39rpx;
    color: #666;
}

.umember {
    margin: 0rpx 20rpx;
    border: 2rpx solid #ff640f;
    border-radius: 10rpx;
    padding: 20rpx;
}

.swiperItem {
    height: 60rpx;
    display: flex;
    align-items: center;
    line-height: 32rpx;
}

.swiperItem text {
    color: #ff640f;
}

.uthumb {
    height: 40rpx;
    width: 40rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

swiper {
    height: 180rpx;
}

swiper-item {
    height: 60rpx!important;
}

.session {
    background: #fff;
    padding-left: 20rpx;
    margin-top: 10rpx;
}

.list {
    display: flex;
    align-items: center;
    justify-content: space-betweenp;
    padding-right: 20rpx;
    border-bottom: 1rpx solid #dcdcdc;
}

.phone {
    height: 98rpx;
    font-size: 30rpx;
    font-weight: 700;
    border-bottom: 0rpx solid #dcdcdc;
    justify-content: space-between;
}

.phone>input {
    height: 58rpx;
    background: #f8f8f8;
    border: 1rpx solid #ECECEC;
    border-radius: 10rpx;
    font-size: 28rpx;
    color: #666;
    margin-left: 30rpx;
    padding-left: 20rpx;
    font-weight: 500;
    width: 536rpx;
}

.lists {
    width: 710rpx;
    justify-content: space-between;
    padding: 30rpx 20rpx 20rpx 0rpx;
}

.lists:last-child {
    border-bottom: 0rpx solid #dcdcdc;
}

.cards {
    margin-bottom: 16rpx;
}

.cards>text:first-child {
    font-size: 32rpx;
    margin-right: 30rpx;
}

.cards>text:last-child {
    font-weight: 700;
    color: #ff5e5e;
}

.sub {
    height: 62rpx;
    width: 150rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    color: #fff;
    background: #ff640f;
    font-size: 30rpx;
    line-height: 62rpx;
    border-radius: 32rpx;
}

.sub::after {
    border: none;
}

.btn {
    padding-bottom: 30rpx;
}

.active {
    margin-top: 50rpx;
    width: 710rpx;
    height: 90rpx;
    font-size: 34rpx;
    line-height: 90rpx;
    background: #ff640f;
    color: #fff;
    margin-left: 0rpx;
    margin-right: 0rpx;
}

.pay_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.pay_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

.pay_title {
    height: 130rpx;
    font-size: 34rpx;
    display: flex;
    padding: 0rpx 20rpx;
    align-items: center;
    justify-content: space-between;
    border-bottom: 12rpx solid #cbcbcb;
}

.dTitle {
    width: 676rpx;
    text-align: center;
    color: #222;
}

.cancel {
    width: 34rpx;
    height: 34rpx;
}

.pay_content {
    height: 420rpx;
}

.choose-pay {
    background: #fff;
}

.section {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
}

.radio {
    width: 730rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
    font-size: 32rpx;
    color: #999;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
    padding-right: 20rpx;
}

.radio radio {
    width: 48rpx;
    height: 48rpx;
}

.radio:last-child {
    border-bottom: 0rpx solid #ececec;
}

.deti {
    display: flex;
    align-items: center;
}

.wxicon {
    width: 34rpx;
    height: 34rpx;
    margin-left: 24rpx;
    margin-right: 20rpx;
}

.pay-foot {
    border-top: 1rpx solid #acacac;
    height: 130rpx;
    padding: 20rpx 20rpx;
}

.pay-foot button {
    height: 90rpx;
    background: #ff640f;
    font-size: 34rpx;
    color: #fff;
}