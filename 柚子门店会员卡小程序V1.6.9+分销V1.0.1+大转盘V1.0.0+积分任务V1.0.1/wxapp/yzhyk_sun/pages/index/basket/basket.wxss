.head {
    width: 750rpx;
    height: 72rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #232932;
}

.head view {
    height: 72rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.addrIcon {
    width: 26rpx;
    height: 36rpx;
    margin-right: 20rpx;
}

.session {
    background: #fff;
}

.goodsItem {
    display: flex;
    padding: 20rpx 0rpx;
    border-top: 1rpx solid #ececec;
    background: #fff;
    margin-left: 20rpx;
}

.goodsItem:first-child {
    border-top: 0rpx solid #f00;
}

.goodsLft {
    width: 180rpx;
    height: 180rpx;
    border-radius: 10rpx;
    border: 1rpx solid #dcdcdc;
    margin-right: 22rpx;
}

.goodsImg {
    height: 178rpx;
    width: 178rpx;
    border-radius: 10rpx;
}

.goodsRight {
    width: 504rpx;
    height: 180rpx;
    position: relative;
}

.goodsName {
    font-size: 25rpx;
    line-height: 36rpx;
}

.goodsDet {
    width: 504rpx;
    position: absolute;
    bottom: 16rpx;
    display: flex;
    justify-content: space-between;
}

.goodsPrice {
    font-size: 31rpx;
    color: #f33030;
    font-weight: 700;
    position: absolute;
    bottom: 2rpx;
}

.opear {
    display: flex;
    position: absolute;
    right: 0rpx;
    bottom: -16rpx;
    align-items: center;
}

.reduce {
    height: 56rpx;
    width: 56rpx;
    border-radius: 50%;
    border: 2rpx solid #999;
    display: flex;
    justify-content: center;
    align-items: center;
}

.reduce image {
    width: 26rpx;
    height: 5rpx;
}

.add {
    border: 2rpx solid #ff640f;
}

.add image {
    width: 26rpx;
    height: 27rpx;
}

.goodsNum input {
    width: 60rpx;
    height: 56rpx;
    text-align: center;
    line-height: 56rpx;
}

.balance {
    position: fixed;
    bottom: 0rpx;
    z-index: 999;
    background: #fff;
}

.balBot {
    background: #fff;
    height: 98rpx;
    width: 750rpx;
    display: flex;
    justify-content: space-between;
    box-shadow: 0 -3rpx 3rpx 0rpx rgba(180,180,180,0.212);
    position: relative;
}

.submit button {
    width: 220rpx;
    height: 98rpx;
    font-size: 32rpx;
    background: #ff640f;
    line-height: 97rpx;
    font-weight: 700;
    color: #FFF;
    border-radius: 0rpx;
    border: 0rpx solid #ff640f;
}

.btn button {
    background: #999;
}

.total {
    display: flex;
    width: 450rpx;
    padding-left: 30rpx;
    align-items: center;
    justify-content: space-between;
}

.totalLeft {
    height: 104rpx;
    width: 104rpx;
    border-radius: 50%;
    margin-top: -48rpx;
    box-shadow: 0 -3rpx 3rpx 0rpx rgba(180,180,180,0.212);
    display: flex;
    justify-content: center;
    align-items: center;
}

.totalRight {
    font-size: 35rpx;
    color: #ff640f;
    font-weight: 700;
}

.totalPrice {
}

.icon34 {
    height: 92rpx;
    width: 92rpx;
    border-radius: 50%;
}

.noGoods {
    margin-top: 100rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    color: #666;
    font-size: 28rpx;
}

.noGoods>image {
    width: 260rpx;
    height: 242rpx;
    margin-bottom: 30rpx;
}

.badge_text {
    right: -6rpx;
}