}

.m_dowm {
    margin-top: 24rpx;
    display: flex;
    align-items: center;
}

.icon24 {
    width: 38rpx;
    height: 29rpx;
}

.icon26 {
    width: 28rpx;
    height: 29rpx;
}

.m_dowm text {
    margin-left: 24rpx;
}

.block {
    background: #fff;
    margin-top: 20rpx;
    box-shadow: 0rpx -4rpx 16rpx 0rpx rgba(123,92,13,0.125);
}

.block-up {
    margin: 0rpx 20rpx;
    display: flex;
    height: 92rpx;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx solid #ececec;
    padding-left: 10rpx;
}

.rIcon {
    height: 20rpx;
    width: 13rpx;
    margin-left: 20rpx;
}

.block-up view {
    display: flex;
    align-items: center;
    color: #666;
}

.block-down {
    display: flex;
    justify-content: space-around;
    align-items: center;
    height: 154rpx;
    font-size: 24rpx;
    font-weight: 700;
}

.block-down view {
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
    position: relative;
}

.block-down>view .orderNum {
    position: absolute;
    top: -16rpx;
    right: -10rpx;
    display: inline-block;
    width: 26rpx;
    height: 26rpx;
    font-size: 20rpx;
    border-radius: 50%;
    border: 2rpx solid #ff640f;
    font-weight: 500;
    color: #ff640f;
    background: #fff;
    text-align: center;
    overflow: hidden;
    line-height: 26rpx;
}

.icn {
    width: 68rpx;
    height: 68rpx;
    margin-bottom: 20rpx;
}

.icon {
    width: 30rpx;
    height: 34rpx;
    margin-right: 34rpx;
}

.session {
    background: #fff;
    padding-left: 30rpx;
    margin-top: 20rpx;
    box-shadow: 0rpx -4rpx 16rpx 0rpx rgba(123,92,13,0.125);
}

.session .list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-right: 20rpx;
    padding-left: 10rpx;
    border-bottom: 1rpx solid #ececec;
    height: 92rpx;
}

.session .list:last-child {
    border-bottom: 1rpx solid #fff;
}

.list view {
    display: flex;
    align-items: center;
}

.ses-last {
    margin-bottom: 100rpx;
}

.listRight {
    display: flex;
    align-items: center;
    font-size: 24rpx;
    color: #222;
}

.list button {
    margin-left: 0rpx;
    height: 92rpx;
    background: #fff;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    font-size: 28rpx;
    color: #222;
    display: flex;
    align-items: center;
    width: 650rpx;
}

.list button::after {
    border: none;
}

.mask {
    position: fixed;
    top: 0;
    border: 0;
    left: 0;
    range: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1001;
    width: 100%;
    height: 100%;
}

.codeImg {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -250rpx;
    margin-top: -250rpx;
    width: 500rpx;
    height: 500rpx;
    z-index: 1002;
    background: #fff;
}

.codeImg>image {
    width: 500rpx;
    height: 500rpx;
}

.codeImg>canvas {
    width: 500rpx;
    height: 500rpx;
}

.integral-view {
    width: 750rpx;
    box-shadow: 0rpx -4rpx 16rpx 0rpx rgba(123,92,13,0.125);
    background: #ffffff;
}

.integral-top {
    margin: 0rpx 20rpx 0 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid #ececec;
}

.integral-bot {
    height: 130rpx;
    display: flex;
    justify-content: space-around;
    margin-top: 10rpx;
    padding-bottom: 20rpx;
}

.integral-item {
    display: flex;
    flex: 1;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
}

.icons {
    width: 68rpx;
    height: 68rpx;
}

