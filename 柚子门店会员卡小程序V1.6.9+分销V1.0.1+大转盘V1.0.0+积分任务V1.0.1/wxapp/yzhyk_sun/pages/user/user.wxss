.head {
    position: relative;
    height: 240rpx;
    width: 750rpx;
    display: flex;
    justify-content: center;
    background-size: cover;
}

.bghead {
    position: absolute;
    left: 0rpx;
    right: 0rpx;
    width: 750rpx;
    height: 240rpx;
}

.headCont {
    position: absolute;
    width: 710rpx;
    height: 240rpx;
    padding: 0rpx 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.code {
    position: absolute;
    bottom: 20rpx;
    right: 0rpx;
    display: flex;
    align-items: center;
    width: 128rpx;
    height: 52rpx;
    padding: 0rpx 20rpx;
    border-radius: 26rpx 0rpx 0rpx 26rpx;
    font-weight: 700;
    color: #fff;
    background: #ff640f;
}

.code>image {
    width: 28rpx;
    height: 28rpx;
    margin-left: 10rpx;
}

.uthumb {
    border: 2rpx solid #ff640f;
    box-shadow: 0rpx 0rpx 14rpx 0rpx rgba(0,0,0,0.11);
}

.uthumb,.uthumb image {
    width: 126rpx;
    height: 126rpx;
    border-radius: 50%;
}

.uthumb {
    margin-right: 24rpx;
}

.head-thumb>view:last-child {
    width: 400rpx;
    line-height: 36rpx;
}

.head-thumb {
    letter-spacing: 2rpx;
    z-index: 999;
    display: flex;
    height: 126rpx;
    align-items: center;
    font-size: 30rpx;
    font-weight: 700;
    flex-shrink: 0;
    flex: 1;
}

.member {
    width: 120rpx;
    height: 38rpx;
    background: #ff640f;
    border-radius: 19rpx;
    color: #fff;
    font-size: 26rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 18rpx;
}

.icon20 {
    margin-right: 10rpx;
    width: 36rpx;
    height: 26rpx;
}

.memberMsg {
    font-size: 26rpx;
}

.memberMsg text {
    margin-right: 20rpx;
    font-weight: 700;
}

.user-count {
    display: flex;
    padding: 15rpx 0rpx;
    background: #fff;
    box-shadow: 0 0 12rpx 0 rgba(3,3,3,0.063);
}

.user-money {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 375rpx;
    height: 120rpx;
    position: relative;
}

.voucher {
    position: absolute;
    top: 0rpx;
    right: 58rpx;
    width: 60rpx;
    height: 24rpx;
    border-radius: 12rpx;
    background: #f33030;
    color: #fff;
    font-size: 20rpx;
    line-height: 24rpx;
    text-align: center;
}

.user-money:first-child {
    width: 374rpx;
    border-right: 1rpx solid #ececec;
}

.icon {
    width: 30rpx;
    height: 34rpx;
}

.icon1 {
    width: 49rpx;
    height: 40rpx;
}

.m_up {
    margin-top: 24rpx;
}

.num {
    font-size: 42rpx;
