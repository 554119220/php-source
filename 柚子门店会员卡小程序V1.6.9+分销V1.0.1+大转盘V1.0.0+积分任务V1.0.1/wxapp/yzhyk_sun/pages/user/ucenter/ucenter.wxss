.session {
    margin: 30rpx 20rpx;
    width: 710rpx;
    background: #fff;
    border-radius: 12rpx;
    box-shadow: 0 0 8rpx 0 rgba(3,3,3,0.086);
    padding-bottom: 20rpx;
}

.session-title {
    height: 88rpx;
    margin: 0rpx 20rpx;
    line-height: 88rpx;
    border-bottom: 1rpx solid #dcdcdc;
}

.list {
    display: flex;
    align-items: center;
    padding: 0rpx 20rpx;
    height: 98rpx;
}

.listLeft {
    width: 107rpx;
    font-weight: 700;
    font-size: 30rpx;
    color: #999;
}

.listRight {
    width: 563rpx;
    display: flex;
    font-size: 30rpx;
    height: 98rpx;
    align-items: center;
}

.radio-group>radio {
    margin-right: 20rpx;
}

.phoneNum {
    font-weight: 700;
}

.member {
    width: 120rpx;
    height: 38rpx;
    background: #ff640f;
    border-radius: 19rpx;
    color: #fff;
    font-size: 26rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 30rpx;
}

.icon20 {
    margin-right: 10rpx;
    width: 36rpx;
    height: 26rpx;
}

.name {
    height: 8rpx;
    background: #f8f8f8;
    width: 539rpx;
    padding-left: 24rpx;
    border-radius: 10rpx;
}

.timeIcon {
    height: 30rpx;
    width: 30rpx;
    margin: 0rpx 20rpx 0rpx 22rpx;
}

.time {
    display: flex;
    align-items: center;
    height: 80rpx;
    width: 300rpx;
    border-radius: 10rpx;
    border: 1rpx solid #cbcbcb;
}

.picker {
    display: flex;
    align-items: center;
}

.save {
    color: #f33030;
    font-size: 26rpx;
    margin-left: 20rpx;
}

.submit {
    height: 125rpx;
}

.submit button {
    background: #ff640f;
    height: 94rpx;
    color: #fff;
    width: 672rpx;
    border: 1rpx solid rgb(202,79,12);
}