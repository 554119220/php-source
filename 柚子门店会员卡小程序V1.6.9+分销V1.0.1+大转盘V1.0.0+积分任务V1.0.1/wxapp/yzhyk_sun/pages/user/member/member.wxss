page {
    background: #fff;
}

.head {
    background-color: #262932;
    background-size: contain;
    height: 296rpx;
    width: 750rpx;
    padding-top: 50rpx;
    z-index: 9;
}

.headCont {
    position: relative;
    width: 650rpx;
    height: 296rpx;
    margin: 0rpx auto;
    z-index: 1;
    box-shadow: 0 0 12rpx 0rpx rgb(38,41,50,0.11);
    border-radius: 20rpx 20rpx 0 0;
    overflow: hidden;
}

.bgCards {
    width: 650rpx;
    height: 296rpx;
    position: absolute;
    left: 0rpx;
    top: 0rpx;
}

.memberCard {
    padding: 50rpx 32rpx 0rpx 50rpx;
    width: 568rpx;
    z-index: 111;
    position: absolute;
    border-radius: 50rpx 50rpx 0 0;
}

.mcLeft {
    display: flex;
}

.bgLogo {
    height: 84rpx;
    width: 84rpx;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 3rpx solid #fff;
}

.bgLogo image {
    width: 84rpx;
    height: 84rpx;
    border-radius: 50%;
}

.mcName {
    font-size: 32rpx;
    height: 82rpx;
    padding-bottom: 8rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 20rpx;
}

.mcName view:last-child {
    font-size: 28rpx;
    display: flex;
    align-items: center;
}

.vipIcon {
    width: 27rpx;
    height: 22rpx;
    margin-right: 10rpx;
}

.billIcon {
    width: 36rpx;
    height: 30rpx;
    margin-left: 22rpx;
}

.mzBill {
    display: flex;
    justify-content: space-between;
    color: #fff;
    font-size: 32rpx;
    align-items: center;
    margin-top: 30rpx;
}

.consum {
    margin-top: 26rpx;
}

.consum>text:last-child {
    font-weight: 700;
    font-size: 46rpx;
    display: inline-block;
    margin-top: 6rpx;
}

.bills {
    width: 220rpx;
    height: 68rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    border: 3rpx solid #fff;
    border-radius: 12rpx;
    font-weight: 700;
}

.hr {
    height: 20rpx;
    width: 750rpx;
    background: #f8f8f8;
}

.process {
    width: 614rpx;
    margin-left: 68rpx;
    padding-top: 20rpx;
    position: relative;
}

.process>progress {
    margin-top: 60rpx;
    width: 614rpx;
}

.totalIntegral {
    font-size: 30rpx;
    color: #ff640f;
    margin-top: 14rpx;
    text-align: right;
    font-weight: 700;
}

.progressTop {
    display: inline-block;
    position: absolute;
    height: 40rpx;
    padding: 0rpx 16rpx;
    color: #fff;
    background-color: #ff640f;
    border-radius: 10rpx;
    line-height: 40rpx;
    font-size: 30rpx;
    font-weight: 700;
    width: auto;
    margin-left: -40rpx;
}

.progressTop::after {
    content: " ";
    display: block;
    height: 0rpx;
    width: 0rpx;
    border-left: 12rpx solid transparent;
    border-top: 12rpx solid #ff640f;
    border-right: 12rpx solid transparent;
    transform: translateX(-50%);
    margin-left: 50%;
}

.proTips {
    text-align: center;
    font-size: 30rpx;
    margin-top: 10rpx;
}

.proTips>text {
    color: #ff640f;
}

.list {
    height: 130rpx;
    padding-left: 20rpx;
    display: flex;
    align-items: center;
}

.listHead {
    height: 110rpx;
    border-bottom: 1rpx solid #dcdcdc;
    font-size: 32rpx;
}

.mIcon {
    height: 48rpx;
    width: 48rpx;
}

.listBot {
    font-size: 30rpx;
    margin-left: 26rpx;
}

.listBot>view:last-child {
    color: #999;
    font-size: 24rpx;
    margin-top: 22rpx;
}

.block {
    padding: 20rpx;
    margin-top: 20rpx;
}

.block>view:first-child {
    font-size: 32rpx;
    font-weight: 700;
    margin-bottom: 26rpx;
}

.block>view:last-child {
    font-size: 26rpx;
    color: #666;
    line-height: 36rpx;
}

.flex-box {
    position: relative;
    border: 1rpx solid #e8e8e8;
    padding: 27rpx 42rpx 37rpx;
    flex-shrink: 0;
    min-width: 180rpx;
    margin-bottom: 16rpx;
    display: flex;
    align-items: center;
    box-shadow: 0rpx 4rpx 10rpx #e8e8e8;
    border-radius: 10rpx;
}

.list_new {
    padding: 20rpx;
}

.session {
    margin-top: 10rpx;
}

.flex-grade {
    height: 66rpx;
    border-right: 1px dashed #b0b0b0;
    line-height: 66rpx;
    margin-right: 30rpx;
    padding-right: 30rpx;
    max-width: 130rpx;
    overflow: hidden;
    text-overflow: ellipsis;
}

.flex-price-l {
    font-size: 32rpx;
    font-weight: 700;
    color: #ff640e;
    margin-bottom: 10rpx;
}

.flex-price-r {
    width: 350rpx;
    line-height: 38rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.flex-price-l text {
    font-size: 22rpx;
    font-weight: 400;
}

.open-soon {
    font-size: 26rpx;
    color: #fff;
    background-color: #ff640e;
    padding: 16rpx 26rpx;
    border-radius: 25rpx;
    position: absolute;
    right: 36rpx;
    top: 50%;
    transform: translateY(-50%);
}