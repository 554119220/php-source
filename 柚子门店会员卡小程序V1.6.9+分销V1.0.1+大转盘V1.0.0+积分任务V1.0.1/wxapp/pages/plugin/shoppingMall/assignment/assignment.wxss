@keyframes flipInY {
    0% {
        -webkit-transform: -webkit-perspective(300px) rotateY(90deg);
        opacity: 0;
    }

    0%,40% {
        animation-timing-function: ease-in;
    }

    40% {
        -webkit-transform: -webkit-perspective(300px) rotateY(-20deg);
    }

    60% {
        -webkit-transform: -webkit-perspective(300px) rotateY(10deg);
        opacity: 1;
    }

    80% {
        -webkit-transform: -webkit-perspective(300px) rotateY(-5deg);
    }

    to {
        -webkit-transform: -webkit-perspective(300px);
    }
}

.flipInY {
    backface-visibility: visible!important;
    animation-name: flipInY;
}

.animated {
    animation-duration: 1.5s;
    animation-fill-mode: middle;
}

