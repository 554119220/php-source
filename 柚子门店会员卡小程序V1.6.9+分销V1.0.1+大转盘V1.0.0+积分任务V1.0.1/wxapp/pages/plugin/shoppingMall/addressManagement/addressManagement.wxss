.wrap {
    background: #f2f2f2;
    height: 100%;
}

.set-address {
    width: 750rpx;
    border-top: solid 1px #e0e0e0;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 20rpx;
    background: #ffffff;
}

.address1 {
    width: 690rpx;
    border-bottom: solid 1px #e0e0e0;
    box-sizing: border-box;
    font-size: 30rpx;
    position: relative;
}

.address1-text {
    margin-top: 10rpx;
    position: relative;
}

.telephone {
    position: absolute;
    right: 0;
}

.address2-text {
    font-size: 30rpx;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
}

.address2 {
    width: 690rpx;
    height: 80rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.default-address {
    width: 200rpx;
}

.editor-address {
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-left: 250rpx;
}

.delete-address {
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-left: 42rpx;
}

.default-text {
    font-size: 40rpx;
}

.editor-text {
    font-size: 30rpx;
    margin-left: 10rpx;
}

.delete-text {
    font-size: 30rpx;
    margin-left: 10rpx;
}

.install {
    height: 29rpx;
    width: 29rpx;
    border-radius: 50%;
}

.editor {
    height: 27rpx;
    width: 27rpx;
}

.delete {
    width: 26rpx;
    height: 28rpx;
}

.no-data-box {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
    margin-top: 226rpx;
}

.no-data {
    width: 141rpx;
    height: 42rpx;
    border: solid 1px #f9f9f9;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    color: black;
    background: #f9f9f9;
}

.button-set {
    display: flex;
    flex-direction: row;
    justify-content: center;
    position: fixed;
    bottom: 20rpx;
    left: 23rpx;
}

.transverse-line {
    width: 173rpx;
    border-top: solid 1px #e7e7e7;
}

.manually-add {
    width: 328rpx;
    height: 100rpx;
    background: #fecf39;
    color: #ffffff;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    font-size: 28rpx;
    border-radius: 20rpx;
}

.add-circles {
    width: 328rpx;
    height: 100rpx;
    background: #fecf39;
    color: #ffffff;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    font-size: 30rpx;
    border-radius: 20rpx;
    margin-left: 46rpx;
}

.radio {
    transform: scale(0.7);
    margin-left: -35rpx;
}