.container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    box-sizing: border-box;
    font-size: 28rpx;
    line-height: 26rpx;
    color: #222;
}

.right {
    width: 12rpx;
    height: 18rpx;
}

.tab {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    background: #fff;
    border-top: 2rpx solid #acacac;
    height: 92rpx;
    padding-bottom: 4rpx;
    z-index: 99999;
}

.tabnav {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.tablist {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #8c8c8c;
}

.tablist button {
    background: none;
    padding: 0;
    margin: 0;
    line-height: auto;
    font-size: 0px;
}

.tablist button:after {
    border: 0px;
}

.tablist button view {
    font-size: 20rpx;
    color: #8c8c8c;
    line-height: 26rpx;
}

.tabimg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 8rpx;
    margin-bottom: 0rpx;
}

.foot {
    height: 120rpx;
}

.addrIcon {
    flex-shrink: 0;
}

.absolute-icon {
    position: absolute;
    top: -2rpx;
}

.inline {
    display: inline-block;
    margin-left: 50rpx;
}

.index {
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    bottom: 300rpx;
    right: 20rpx;
    z-index: 999;
    box-shadow: 0px 0px 18px 0px rgba(4,0,0,0.2);
}

.indexHome {
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
}

button::after {
    border: 0;
