    --fonta: #222;
    --fontb: #666;
    --fontc: #999;
    --white: #fff;
    --skin: #fe433f;
    --pink: #FFC8C7;
    --normal: linear-gradient(to right, #FF8044, rgb(254, 63, 63));
    --prevent: linear-gradient(to right, #999, #666);
}

