<template name="productList">
		<block wx:for="{{listArray.data}}" wx:for-item="item" wx:for-index="index" >
			<view catchtap="productDetails" class="product-List-left">
				<view class="picture-area">
					<image class="productlist1" src="{{listArray.other.img_root+item.icon}}"></image>
					<image class="underway" src="../../resource/images/integrationMall/tupian2.png"></image>
				</view>
				<view class="product-introduction">
					<text class="product-List1-text">仓鼠坐垫一体椅垫</text>
