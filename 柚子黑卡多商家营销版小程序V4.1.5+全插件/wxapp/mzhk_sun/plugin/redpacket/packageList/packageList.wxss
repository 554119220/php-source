.tel-hd { height: 88rpx; border-bottom: 1rpx solid #eee; padding: 0 20rpx; box-sizing: border-box; display: flex; justify-content: space-around; }
.tel-hd .hd-content { height: 88rpx; position: relative; }
.tel-hd text { font-size: 30rpx; line-height: 88rpx; color: #666666; }
.tel-hd .current { position: absolute; bottom: 0; left: 50%; transform: translateX(-50%); border-bottom: 4rpx solid #fe433f; width: 66rpx; }
.tel-hd .current1{ color: #fe433f; }
.may-use .may-use-b{ margin-top:42rpx; padding:0 30rpx; }
.may-use .use-list{ background-color: #ff3d49; border-radius:5px; display: flex; align-items: center; margin-bottom:20rpx; position: relative; }
.may-use .use-list1{ background-color: #d1d1d1; }
.may-use .use-list .use-list-l{ padding-left:5rpx; height: 170rpx; width: 530rpx; position: relative; display: flex; align-items: center; }
.may-use .use-list .use-price{ color: #fff; font-size: 50rpx; display: flex; align-items: center; }
.may-use .use-list .use-price wx-span{ font-size: 48rpx; line-height: 48rpx; }
.may-use .use-list .use-info{ margin-left:20rpx; color: #fff; }
.may-use .use-list .package-title{ font-size: 32rpx; line-height: 32rpx; }
.may-use .use-list .package-limit{ font-size: 24rpx; line-height: 24rpx; margin:12rpx 0 26rpx; }
.may-use .use-list .package-time{ font-size: 20rpx; line-height: 20rpx; }
.may-use .use-list .use-list-l::before{ content: ""; width: 20rpx; height:20rpx; background-color: #fff; border-radius: 50%; display: block; position: absolute; top:0; right: 0; transform: translate(50%,-50%); }
.may-use .use-list .use-list-l::after{ content: ""; width: 20rpx; height:20rpx; background-color: #fff; border-radius: 50%; display: block; position: absolute; bottom:0; right: 0; transform: translate(50%,50%); }
.may-use .use-list .use-list-r{ height: 170rpx; width: 180rpx; display: flex; justify-content: center; align-items: center; border-left: 1px dashed #fff; }
.use-list-r .go-use{ padding: 16rpx 26rpx; border-radius: 50rpx; font-size: 32rpx; border:1px solid #fff; color: #fff; }
.may-use .use-list .use-list-r image{ width: 128rpx; height: 128rpx; display: block; }
.count-down{ width: 180rpx; height: 46rpx; background-color: #353535; position: absolute; top:0; left: 0; border-radius: 8rpx 0 46rpx 0; }
.count-down{ display: flex; align-items: center; }
.count-down image{ width: 24rpx; height: 24rpx; display: block; margin:0 10rpx; }
.count-down view{ font-size:20rpx; color: #fff; }
.count-down view text{ color:#fff000; }
.use-price-man{ font-size: 24rpx; color: #fff;text-align: center; }
