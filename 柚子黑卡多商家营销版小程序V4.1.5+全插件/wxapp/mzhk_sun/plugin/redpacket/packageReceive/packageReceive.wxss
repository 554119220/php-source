page{ background-color: #e2514c; }
.top-bg{ position: relative; }
.top-bg image{ width: 750rpx; height: 299rpx; display: block; }
.top-bg .rule{ width: 134rpx; height: 60rpx; background-color: #f8db7f; border-radius:20rpx 0 0 20rpx; position: absolute; right: 0; top:50rpx; font-size:26rpx; color:#e93d33; text-align: center; line-height: 60rpx; }
.package-info{ box-sizing: border-box; width: 690rpx; padding:40rpx 25rpx; border-radius:5px; background-color: #fff5ec; margin: 0 auto; }
.package-info .chopper-package{ background-color: #ffffff; box-shadow: 0px 2px 5px #eaceae; display: flex; align-items: center; }
.package-info .chopper-package .chopper-l{ width: 200rpx; height:170rpx; position: relative; display: flex; flex-direction: column; justify-content: center; align-items: center; }
.package-info .chopper-package .chopper-l::before{ content: ""; width: 20rpx; height:20rpx; background-color: #fff5ec; border-radius: 50%; display: block; position: absolute; top:0; right: 0; transform: translate(50%,-50%); }
.package-info .chopper-package .chopper-l::after{ content: ""; width: 20rpx; height:20rpx; background-color: #fff5ec; border-radius: 50%; display: block; position: absolute; bottom:0; right: 0; transform: translate(50%,50%); }
.package-info .chopper-package .chopper-r{ width: 440rpx; height: 170rpx; border-left:1px dashed #9e9e9e; display: flex; flex-direction: column; justify-content: center; padding-left:42rpx; }
.package-info .chopper-package .chopper-price{ display: flex; color: #ff5050; font-size: 50rpx; justify-content: center; align-items: center; }
.package-info .chopper-package .chopper-price text{ font-size: 46rpx; line-height: 48rpx; }
.package-info .chopper-package .full{ font-size: 24rpx; color:#999; }
.chopper-r .chopper-title{ font-size: 32rpx; color:#ff5050; }
.chopper-r .chopper-limit{ font-size: 26rpx; color:#999999; margin-top:10rpx; }
.chopper-r .chopper-time{ font-size: 26rpx; color:#999999; margin-top:20rpx; }
.get-package{ padding:45rpx 0 35rpx; }
.invitation{ font-size:28rpx; color:#353535; text-align: center; }
.invitation text{ color:#ff5050; }
.get .get-t{ font-size: 30rpx; color: #353535; display: flex; align-items: center; justify-content: center; }
.get .get-t image{ margin-right:16rpx; width: 36rpx; height:36rpx; display: block; }
.get .get-b{ font-size: 24rpx; color: #ff5050; margin-top:20rpx; text-align: center; }
.use-btn .use-btn-con{ height: 84rpx; background: -webkit-linear-gradient(top, #fff742 , #ffc802); border-radius:42rpx; color: #ff3c17; font-size: 34rpx; line-height: 84rpx; text-align: center; font-weight: 700; box-shadow: 0 10rpx #ffa736; }
.good-friend{ margin-top:30rpx; padding:25rpx; background-color: #ffe7d2; display: flex; align-items: center; }
.good-friend .friend-l{ font-size:30rpx; color: #ff3c17; padding-right: 30rpx; border-right: 1px solid #ff3c17; display: flex; justify-content: center; align-items: center; flex-direction: column; }
.good-friend .friend-l text{ }
.good-friend .friend-r{ display: flex; align-items: center; padding-left:30rpx; flex-wrap: wrap; }
.good-friend .friend-r image{ width: 88rpx; height: 88rpx; display: block; margin-right:20rpx; border-radius: 50%; }
.use-title{ display: flex; align-items: center; justify-content: center; padding:55rpx 0 40rpx; }
.use-title .icon-line{ width: 150rpx; height: 1px; background-color: #fff; }
.use-title .icon-scommodity{ font-size: 30rpx; color: #fff; margin:0 15rpx; }
.use-shop{ padding:20rpx 20rpx 0 20rpx; background-color: #fff5ec; margin:0 30rpx 30rpx; border-radius:10rpx; display: flex; justify-content: space-between; align-items: center; flex-wrap: wrap; }
.use-shop .shop-list{ width: 310rpx; border-radius: 5px; margin-bottom:30rpx; }
.use-shop .shop-list .shop-list-img image{ width: 310rpx; height: 216rpx; display: block; }
.use-shop .shop-list .shop-list-title{ font-size: 26rpx; color: #494f52; margin-top:25rpx; height: 70rpx; overflow: hidden; }
.use-shop .shop-list .shop-list-price{ font-size: 30rpx; color: #fe5624; margin-top:30rpx; display: flex; align-content: center; justify-content: space-between }
.use-shop .shop-list .shop-list-price .shop-price-r{ font-size: 24rpx; color: #7f8487; text-decoration: line-through; }
.use-shop .shop-list .shop-list-discount{ display: flex; justify-content: space-between; align-items: center; margin-top:10rpx; }
.use-shop .shop-discount-l{ display: flex; justify-content: space-between; align-items: center; font-size: 26rpx; color: #353535; }
.use-shop .shop-discount-l text{ background-color: #353535; color: #d5c098; font-size: 22rpx; text-align: center; border-radius: 5px; margin-right:10rpx; padding:2rpx 4rpx; }
.use-shop .shop-discount-r{ color: #fe5624; font-size: 26rpx; }
.use-shop .shop-list .rob-btn{ width: 310rpx; height: 60rpx; background-color: #fe5624; line-height: 60rpx; text-align: center; color: #fff; margin-top: 25rpx; font-size:28rpx; margin:25rpx auto 0; border-radius:5px; }
.package-popup{ position: fixed; height: 100%; width: 100%; background-color: rgba(0,0,0,0.6); top:0; left: 0; }
.package-popup .package-bg .turn-off{ position: absolute; top:-80rpx; right: 60rpx; }
.package-popup .package-bg .turn-off image{ width: 46rpx; height: 46rpx; display: block; }
.package-popup .package-bg{ position: relative; top:50%; left: 50%; transform: translate(-50%,-50%); z-index: 66; }
.package-popup .package-bg image{ width: 687.5rpx; height: 700rpx; display: block; margin: 0 auto; }
.package-popup .package-bg-t{ width: 100%; position: absolute; top:90rpx; text-align: center; }
.package-popup .package-bg-t .package-bg-money{ font-size: 100rpx; color: #fc575a; text-align: center; }
.package-popup .package-bg-t .package-bg-money text{ font-size: 40rpx; }
.package-popup .package-bg-t .package-bg-receive{ font-size: 28rpx; color: #fc575a; margin-top: 60rpx; }
.package-popup .package-bg-t .package-bg-title{ font-size: 24rpx; color: #353535; margin-top: 30rpx; }
.package-popup .package-bg-b{ width: 100%; position: absolute; bottom:40rpx; text-align: center; }
.package-popup .package-bg-b .package-bg-invitation{ font-size: 28rpx; color:#fff; width: 450rpx; text-align: center; margin: 0 auto 30rpx; }
.package-popup .package-bg-b .package-bg-btn{ width: 350rpx; height: 84rpx; background: -webkit-linear-gradient(top, #fff742 , #ffc802); border-radius:42rpx; color: #ff3c17; font-size: 34rpx; line-height: 84rpx; text-align: center; font-weight: 700; box-shadow: 0 10rpx #ffa736; margin: 0 auto; }
.package-rule{ position:fixed; top: 0; left: 0; height: 100%; width: 100%; background-color: rgba(0,0,0,0.6) }
.package-rule .package-rule-content{ width: 600rpx; background-color: #fff; border-radius: 20rpx; padding: 30rpx; box-sizing: border-box; position: absolute; left: 50%; top:50%; transform: translate(-50%,-50%); }
.package-rule .package-rule-content .rule-title{ text-align: center; font-size: 32rpx; color: 000; margin-bottom:30rpx; }
.package-rule .package-rule-content .rule-list > view{ font-size: 28rpx; color: #333; line-height: 40rpx; }
.package-close{ width: 66rpx; height: 66rpx; position: fixed; left: 50%; transform: translateX(-50%); bottom:120rpx; border-radius: 50%; }
.package-close image{ width: 66rpx; height: 66rpx; }
