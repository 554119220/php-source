page{ height: 100%; }
.container { justify-content: space-around; align-items: center; }
.card { width: 600rpx; margin: 100rpx; flex: 1; background: #fff; display: flex; flex-direction: column; padding: 30rpx 0; justify-content: space-between; align-items: center; background: salmon; border-radius: 10rpx; }
.font-38 { font-size: 32rpx; color: #fff; text-align: center; font-weight: bold; line-height: 38rpx; max-height: 76rpx; overflow: hidden; }
.font-42 { font-size: 35rpx; color: #fff; text-align: center; font-weight: bold; line-height: 38rpx; max-height: 114rpx; overflow: hidden; }
.font-26 { font-size: 26rpx; color: #fff; text-align: center; }
.line-height { line-height: 34rpx; padding: 0 30rpx; }
.icon { width: 30rpx; height: 30rpx; align-self: flex-end; }
.icon-view{ padding: 0 30rpx; align-self: flex-end; }
.card-img { width: 160rpx; height: 160rpx; margin: 0 auto; border-radius: 50%; }
.lines { height: 3rpx; width: 100%; margin: 10rpx 0; background: #fff; }
.line { width: 60rpx; height: 5rpx; background: #fff; }
.sumbit { width: 600rpx; height: 80rpx; border: 1rpx solid #ececec; margin-bottom: 80rpx; color: #fff; line-height: 80rpx; font-size: 26rpx; background-color:#FF7F00; }
.no{ margin-top:200rpx; height:270rpx; width:750rpx; }
