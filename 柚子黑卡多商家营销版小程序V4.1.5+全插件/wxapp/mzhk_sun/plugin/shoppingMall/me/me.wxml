<login bindtogetuserinfo="updateUserInfo" modalHidden="{{is_modal_Hidden}}"></login>
<view class="wrap">
	<view class="top">
		<image class="bg" src="../../resource/images/me/bg.png"></image>
		<view class="top-content">
			<view>
				<image class="user-name-touxiang" src="{{me.img}}"></image>
			</view>
			<view class="button">
				<text class="user-name">{{me.name}}</text>
			</view>
			<view class="top-bottom">
				<view>
					<image class="jifen" src="../../resource/images/me/jifen.jpg"></image>
					<text class="top-bottom-left">
						<text>{{me.integral}}</text>
						<text class="unit">分</text>
					</text>
				</view>
			</view>
			<view></view>
		</view>
		<view class="list">
			<view catchtap="myOrder" class="my-order">
				<image class="dingdan" src="../../resource/images/me/dingdan.jpg"></image>
				<text class="my-order-text">我的订单</text>
			</view>
			<view catchtap="myMark" class="my-order">
				<image class="dingdan" src="../../resource/images/me/make.jpg"></image>
				<text class="my-order-text">我的收藏</text>
			</view>
			<view catchtap="pointsDetails" class="my-order">
				<image class="dingdan" src="../../resource/images/me/mingxi.jpg"></image>
				<text class="my-order-text">积分明细</text>
			</view>
		</view>
	</view>
	<view class="merchants-information">
		<view catchtap="addressManagement" class="address-management">
			<text class="address-management-text">地址管理</text>
			<text class="arrow">></text>
		</view>
		<view catchtap="aboutUs" class="about-us">
			<text class="about-us-text">关于我们</text>
			<text class="arrow">></text>
		</view>
	</view>
</view>
<view style="height:200rpx;width:750rpx;background:#f1f1f1"></view>
<view class="technical-support">
	<text class="technical-support-text"></text>
</view>
<view class="navigation">
	<view catchtap="home" class="navigation-box1">
		<block wx:if="false" >
			<image class="tupian-pic1" src=""></image>
		</block>
		<block wx:else>
			<image class="tupian-pic1" src="../../resource/images/navigation/page.png"></image>
		</block>
		<view class="navigation-text">首页</view>
	</view>
	<view catchtap="integrationMall" class="navigation-box2">
		<block wx:if="false" >
			<image class="tupian-pic1" src=""></image>
		</block>
		<block wx:else>
			<image class="tupian-pic1" src="../../resource/images/navigation/welfare.png"></image>
		</block>
		<view class="navigation-text">福利</view>
	</view>
	<view catchtap="assignment" class="navigation-box3">
		<block wx:if="false" >
			<image class="tupian-pic1" src=""></image>
		</block>
		<block wx:else>
			<image class="tupian-pic1" src="../../resource/images/navigation/task.png"></image>
		</block>
		<view class="navigation-text">任务</view>
	</view>
	<view catchtap="me" class="navigation-box4">
		<block wx:if="{{variable}}" >
			<image class="tupian-pic1" src="../../resource/images/navigation/select-me.png"></image>
		</block>
		<block wx:else>
			<image class="tupian-pic" src=""></image>
		</block>
		<view class="navigation-text">我的</view>
	</view>
</view>
