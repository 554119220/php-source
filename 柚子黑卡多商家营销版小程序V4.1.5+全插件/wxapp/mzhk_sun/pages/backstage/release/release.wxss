page {
    background-color: #f0f0f0;
}

.heiRad90 {
    height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin: 20rpx 20rpx 0 20rpx;
    background: #fff;
    padding: 0 20rpx;
    border-radius: 15rpx;
    font-size: 30rpx;
}

.heiRad90 input {
    flex: 1;
    text-align: right;
    height: 90rpx;
}

.heiRad90>picker {
    display: block;
    height: 100%;
    width: 100%;
    line-height: 90rpx;
}

.heiRad90 .picker {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.heiRad {
    margin: 20rpx 20rpx 0 20rpx;
    background-color: #fff;
    padding: 20rpx;
    border-radius: 15rpx;
}

.video-upload image {
    width: 140rpx;
    height: 140rpx;
    display: flex;
    margin-top: 20rpx;
}

.video-upload video {
    margin-top: 20rpx;
    width: 690rpx;
}

.video-box {
    position: relative;
}

.video-box image {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.order-nav {
    height: 80rpx;
    display: flex;
    align-items: center;
    background: #fff;
    justify-content: space-around;
}

.order-nav view {
    display: inline-block;
    height: 80rpx;
    line-height: 80rpx;
    box-sizing: border-box;
    color: #666;
    font-size: 30rpx;
}

.order-nav .on {
    border-bottom: 4rpx solid #f5ac32;
    color: #f5ac32;
}

.order-nav text {
    padding: 0rpx 10rpx;
}