<view class="container">
    <view>
        <view class="header">
            <image class="head_back" src="{{url}}{{background.dt_logo}}"></image>
        </view>
    </view>
    <view class="shop_bg" style="background:{{background.fontcolor}}">
        <view class="titles">
     购买以下商品可参与抽奖
    </view>
        <view bindtap="toCjGoods" class="type_details" data-bid="{{item.bid}}" data-gid="{{item.gid}}" data-lid="{{item.lid}}" wx:for="{{list.data}}">
            <image class="shop_img" src="{{url}}{{item.pic}}"></image>
            <view class="infos">
                <view style="margin-top:5rpx;">
                    <view class="nowrap">{{item.gname}}</view>
                    <view class="nums">已售 {{item.buynum}} 件</view>
                </view>
                <view class="time_view">
                    <image class="time_icon" src="../../../style/static/time.png"></image>
           {{item.astime}}-{{item.antime}}
         </view>
            </view>
            <view class="type_btn 1">立即购买</view>
        </view>
        <view bindtap="toIndex" class="index">
            <view>
                <image class="indexHome" src="/style/images/tab3.png"></image>
            </view>
        </view>
    </view>
</view>
