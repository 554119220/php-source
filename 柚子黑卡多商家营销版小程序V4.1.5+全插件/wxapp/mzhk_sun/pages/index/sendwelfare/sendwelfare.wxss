swiper {
    height: 404rpx;
}

.slide-image {
    width: 750rpx;
    height: 404rpx;
}

.block {
    background: #fff;
    padding: 22rpx 0rpx 0rpx 30rpx;
    border-bottom: 1rpx solid #cbcbcb;
}

.block-up {
    border-bottom: 1rpx solid #ececec;
    display: flex;
    flex-direction: column;
    color: #666;
    font-size: 26rpx;
    padding-bottom: 34rpx;
}

.title {
    line-height: 42rpx;
    font-size: 36rpx;
    font-weight: 700;
    color: #222;
    margin-bottom: 26rpx;
}

.addrIcons {
    width: 30rpx;
    height: 32rpx;
    margin-right: 20rpx;
}

.addrIcon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 20rpx;
}

.block-down {
    display: flex;
    justify-content: space-between;
    padding-right: 30rpx;
    align-items: center;
    height: 120rpx;
    color: #222;
}

.addr {
    display: flex;
    align-items: center;
}

.phone {
    width: 42rpx;
    height: 42rpx;
}

.session-title {
    display: flex;
    align-items: center;
    font-size: 32rpx;
    justify-content: center;
    padding: 60rpx 0rpx 34rpx;
}

.session {
    background: #fff;
    margin-top: 20rpx;
    margin-bottom: 150rpx;
}

.session-title image {
    margin-right: 20rpx;
}

.details image {
    width: 100%;
}

.navFix {
    display: flex;
    justify-content: center;
    height: 98rpx;
    width: 750rpx;
    position: fixed;
    bottom: 0rpx;
    background: #edd087;
    line-height: 98rpx;
    text-align: center;
    color: #fff;
    font-size: 34rpx;
    background-image: -webkit-linear-gradient(270deg,rgb(237,208,135)0%,rgb(235,174,113)100%);
}

.navFix view {
    height: 98rpx;
    width: 750rpx;
}

.bname {
    padding-bottom: 15rpx;
    display: flex;
    align-items: center;
}

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.drawer_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

.drawer_title {
    font-size: 32rpx;
    line-height: 30rpx;
    color: #222;
    font-weight: 700;
    padding: 24rpx 20rpx;
    display: flex;
    justify-content: space-between;
}

.drawer_content {
    height: 500rpx;
    overflow-y: scroll;
}

.pay_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.pay_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

.pay_title {
    height: 130rpx;
    font-size: 34rpx;
    display: flex;
    padding: 0rpx 20rpx;
    align-items: center;
    justify-content: space-between;
    border-bottom: 12rpx solid #cbcbcb;
}

.dTitle {
    width: 676rpx;
    text-align: center;
    color: #222;
}

.cancel {
    width: 34rpx;
    height: 34rpx;
}

.pay_content {
}

.choose-pay {
    background: #fff;
}

.section {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
}

.radio {
    width: 730rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
    font-size: 32rpx;
    color: #999;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
    padding-right: 20rpx;
}

.radio radio {
    width: 48rpx;
    height: 48rpx;
}

.radio:last-child {
    border-bottom: 0rpx solid #ececec;
}

.deti {
    display: flex;
    align-items: center;
}

.wxicon {
    width: 34rpx;
    height: 34rpx;
    margin-left: 24rpx;
    margin-right: 20rpx;
}

.pay-foot {
    border-top: 1rpx solid #acacac;
    padding: 20rpx 20rpx;
}

.pay-foot button {
    height: 90rpx;
    background: #ef8200;
    font-size: 34rpx;
    color: #fff;
}

.price {
    padding: 5rpx 0 20rpx;
}

.price text:first-child {
    display: inline-block;
    height: 36rpx;
    line-height: 36rpx;
    font-size: 36rpx;
    color: #f33030;
    margin-right: 12rpx;
}

.price text:nth-child(2) {
    text-decoration: line-through;
}