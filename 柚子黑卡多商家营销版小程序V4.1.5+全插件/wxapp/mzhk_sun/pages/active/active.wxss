#mz {
    margin-top: 20rpx;
    padding: 30rpx 50rpx;
    background: #fff;
}

#mz-img {
    margin-top: 20rpx;
    padding: 30rpx 50rpx;
    background: #fff;
}

.mz-card {
    background: #000;
    color: #f5ac32;
    position: relative;
    height: 380rpx;
    width: 650rpx;
    border-radius: 30rpx;
    box-shadow: 0rpx 2rpx 21rpx rgba(0,0,0,0.5);
    background-size: cover;
}

.mz-logo {
    width: 600rpx;
    position: absolute;
    top: 50rpx;
    left: 50%;
    margin-left: -300rpx;
}

.mz-logo ._mzLogo {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 42rpx;
    color: #f5ac32;
    font-weight: 700;
}

._mzLogo text {
    display: block;
    width: 600rpx;
    overflow: hidden;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
}

.hklogo {
    width: 80rpx;
    height: 80rpx;
    margin-bottom: 30rpx;
    border-radius: 50%;
}

.uthumb {
    width: 82rpx;
    height: 82rpx;
    border-radius: 50%;
    overflow: hidden;
    display: block;
}

.mz-right {
    text-align: center;
}

.mz-right text {
    display: inline-block;
    margin-bottom: 20rpx;
}

.rightNow {
    width: 190rpx;
    height: 70rpx;
    border-radius: 12rpx;
    line-height: 70rpx;
    text-align: center;
    background: #412d2e;
    font-size: 30rpx;
    border: 1rpx solid #f5ac32;
}

.mz-cont {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 246rpx 30rpx 0rpx;
}

.mz-awater {
    display: flex;
    line-height: 40rpx;
}

.mz-pic {
    border: 2rpx solid #f5ac32;
    width: 82rpx;
    height: 82rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.order-nav {
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #fff;
}

.order-nav view {
    display: inline-block;
    height: 100rpx;
    line-height: 100rpx;
    box-sizing: border-box;
    color: #666;
    font-size: 30rpx;
}

.order-nav .on {
    border-bottom: 1rpx solid #f33030;
    color: #f33030;
}

.order-nav text {
    padding: 0rpx 20rpx;
}

.order-nav view:first-child .nav {
    border-right: 1rpx solid #ececec;
}

.session {
    background: #faf9f9;
    padding: 30rpx 30rpx 30rpx;
}

.list {
    background: #fff;
    margin-bottom: 30rpx;
    box-shadow: 0rpx 0rpx 24rpx 0rpx rgba(3,3,3,0.086);
    position: relative;
}

.list-up {
    width: 690rpx;
    height: 390rpx;
    position: relative;
}

.list-up image {
    width: 690rpx;
    height: 390rpx;
}

.activeType {
    position: absolute;
    right: 12rpx;
    top: 12rpx;
    width: 150rpx;
    height: 48rpx;
    background: #f25551;
    color: #fff;
    border-radius: 10rpx;
    text-align: center;
    line-height: 48rpx;
}

.brand {
    display: inline-block;
    padding: 0rpx 50rpx 0rpx 30rpx;
    margin-top: 20rpx;
    background: #f5ac32;
    color: #fff;
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
}

.lst {
    padding: 0rpx 30rpx 0rpx;
}

.list-title {
    font-weight: 700;
    margin-bottom: 20rpx;
    margin-top: 16rpx;
    line-height: 34rpx;
    display: flex;
    align-items: center;
}

.list-title>text {
    display: inline-block;
}

.list-title .vip {
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    width: 60rpx;
    font-size: 22rpx;
    color: #fff;
    background: #f25551;
    margin-right: 12rpx;
}

.list-time {
    color: #999;
    font-size: 24rpx;
    display: flex;
    justify-content: space-between;
    padding-bottom: 32rpx;
}

.list-time text {
    margin-right: 10rpx;
}

.list-time .num {
    color: #f5ac32;
}

.btn {
    width: 154rpx;
    height: 62rpx;
    background: #f5ac32;
    border-radius: 10rpx;
    text-align: center;
    line-height: 62rpx;
    color: #fff;
}

.pass,.pass image {
    position: absolute;
    right: 12rpx;
    top: 12rpx;
    width: 113rpx;
    height: 98rpx;
    z-index: 12;
}

.mask {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    position: absolute;
    top: 0rpx;
    left: 0rpx;
}

.banner-box {
    position: relative;
    z-index: 22;
}

.nav {
    position: relative;
    display: flex;
    flex-direction: row;
    width: 750rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: #fff;
    box-shadow: 0 -1px 1px #ccc inset;
    border-top: 1rpx solid #f2f2f2;
}

.nav-left {
    flex: 1;
    display: flex;
    flex-direction: row;
}

.nav-left-item {
    flex: 1;
    text-align: center;
    font-size: 30rpx;
}

.nav-left-itemchoose {
    position: relative;
    color: #fe433f;
}

.nav-right {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    position: relative;
    width: 160rpx;
    padding: 0 20rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 30rpx;
}

.nav-right-name {
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    font-weight: 600;
}

.nav-right::after {
    content: '';
    position: absolute;
    top: 20rpx;
    left: 0;
    width: 2rpx;
    height: 40rpx;
    background: #ececec;
}

.alert-select {
    position: absolute;
    top: 80rpx;
    width: 730rpx;
    padding: 20rpx 0 0 20rpx;
    height: 400rpx;
    float: left;
    background: #fff;
    overflow: scroll;
    z-index: 26;
    box-shadow: 0 5rpx 10rpx rgba(0,0,0,0.2);
}

.alert-select-item {
    display: inline-block;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 20rpx;
    margin-right: 20rpx;
    margin: 0 20rpx 20rpx 0;
    box-shadow: 0 0 1px #666 inset;
    border-radius: 30rpx;
}

.select-choose {
    color: #fff;
    background: #fe433f;
    box-shadow: 0 0 1px #fe433f inset;
}

.mask-click {
    z-index: 24;
    background: rgba(0,0,0,0);
}

.active-end {
    width: 276rpx;
    height: 231rpx;
}

.active-mask {
    position: absolute;
    z-index: 20;
    width: 100%;
    height: 400rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: rgba(0,0,0,0.4);
}

.triangle {
    display: inline-block;
    position: relative;
    top: 6rpx;
    margin-left: 10rpx;
    width: 0;
    height: 0;
    border: 10rpx solid transparent;
    border-top-color: #999;
}

.choose {
    position: relative;
    color: #fe433f;
}

.choose::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 20%;
    width: 60%;
    height: 4rpx;
    border-radius: 2rpx;
    background: #fe433f;
}

.taggle-arrow {
    height: 0;
    width: 0;
    margin-left: 10rpx;
    border-right: 10rpx solid transparent;
    border-top: 12rpx solid #666;
    border-left: 10rpx solid transparent;
}