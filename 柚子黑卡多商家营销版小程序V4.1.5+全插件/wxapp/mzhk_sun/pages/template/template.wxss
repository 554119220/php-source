.technical {
    width: 750rpx;
    display: flex;
    padding: 50rpx 0rpx 0 0;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;
    color: rgb(153,153,153);
}

.supportIcon {
    width: 64rpx;
    height: 64rpx;
    margin-right: 24rpx;
}

.supportPhone {
    width: 34rpx;
    height: 32rpx;
    margin-bottom: 8rpx;
}

.tecSup {
    line-height: 42rpx;
}

.dialogPhone {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-left: 60rpx;
    font-size: 20rpx;
}

#ordertopay .drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

#ordertopay .drawer_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

#ordertopay .drawer_title {
    font-size: 32rpx;
    line-height: 30rpx;
    color: #222;
    font-weight: 700;
    padding: 24rpx 20rpx;
    display: flex;
    justify-content: space-between;
}

#ordertopay .drawer_content {
    height: 500rpx;
    overflow-y: scroll;
}

#ordertopay .pay_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

#ordertopay .pay_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

#ordertopay .pay_title {
    height: 130rpx;
    font-size: 34rpx;
    display: flex;
    padding: 0rpx 20rpx;
    align-items: center;
    justify-content: space-between;
    border-bottom: 12rpx solid #cbcbcb;
}

#ordertopay .dTitle {
    width: 676rpx;
    text-align: center;
    color: #222;
}

#ordertopay .cancel {
    width: 34rpx;
    height: 34rpx;
}

#ordertopay .choose-pay {
    background: #fff;
}

#ordertopay .section {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
}

#ordertopay .radio {
    width: 730rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
    font-size: 32rpx;
    color: #999;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
    padding-right: 20rpx;
}

#ordertopay .radio radio {
    width: 48rpx;
    height: 48rpx;
}

#ordertopay .radio:last-child {
    border-bottom: 0rpx solid #ececec;
}

#ordertopay .deti {
    display: flex;
    align-items: center;
}

#ordertopay .wxicon {
    width: 34rpx;
    height: 34rpx;
    margin-left: 24rpx;
    margin-right: 20rpx;
}

#ordertopay .pay-foot {
    border-top: 1rpx solid #acacac;
    padding: 20rpx 20rpx;
}

#ordertopay .pay-foot button {
    height: 90rpx;
    background: #ef8200;
    font-size: 34rpx;
    color: #fff;
}

#mz-img {
    margin-top: 70rpx;
    padding: 30rpx 50rpx;
    background: #fff;
}

#mz-img .mz-card {
    overflow: hidden;
    background: #fff;
}

#mz-img .mz-card swiper {
    height: 380rpx;
    margin: 0;
}

#mz-img .mz-card image {
    width: 650rpx;
    height: 380rpx;
}

.header_views {
    display: flex;
    flex-direction: column;
    box-shadow: 0rpx 0rpx 10rpx #dcdcdc;
    width: 650rpx;
    padding: 0 20rpx;
    margin: 20rpx auto 20rpx auto;
    background: #ffffff;
    border-radius: 10rpx;
}

.shop_header_view {
    display: flex;
    height: 120rpx;
    align-items: center;
    position: relative;
    background: #fff;
    padding: 15rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.shop_header_btn {
    padding: 0 10rpx;
    height: 60rpx;
    border-radius: 30rpx;
    background: red;
    color: #ffffff;
    line-height: 60rpx;
    font-size: 26rpx;
    text-align: center;
    position: absolute;
    right: 0;
    top: 40rpx;
}

.shop_header_imgs {
    width: 50rpx;
    height: 50rpx;
    border-radius: 25rpx;
}

.header_font {
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 26rpx;
}

.header_add {
    width: 100rpx;
    height: 50rpx;
    border-radius: 10rpx;
    line-height: 50rpx;
    text-align: center;
    color: #ffffff;
    background: #33CC00;
    font-size: 22rpx;
    padding: 0rpx 20rpx;
}

.process_h {
    display: flex;
    flex-direction: row;
    padding: 30rpx 20rpx;
    align-items: cneter;
    background: #fff;
}

.pro_wg_h {
    display: flex;
    align-items: center;
    flex: 1;
}

.pro_wg_h .pro_wg_logo_h {
    margin-right: 20rpx;
}

.pro_wg_h .pro_wg_logo_h,.pro_wg .pro_wg_add_h {
    width: 80rpx;
    height: 80rpx;
}

.pro_wg_add_h {
    width: 80rpx;
    height: 80rpx;
}

.pro_wg_h .pro_wg_title_h {
    flex: 1;
    line-height: 37rpx;
    overflow: hidden;
}

.pro_wg_h .pro_wg_title_h view {
    height: 37rpx;
    overflow: hidden;
}

.pro_wg_h .pro_wg_title_h view:nth-child(2) {
    color: #bbb;
    font-size: 24rpx;
}

.norm_modal {
    position: fixed;
    display: flex;
    flex-direction: column;
    width: 750rpx;
    left: 0;
    bottom: 0;
    background: #ffffff;
    z-index: 1001;
}

.norm_info {
    display: flex;
    padding-bottom: 20rpx;
    flex-direction: column;
    padding: 30rpx 30rpx 30rpx 30rpx;
}

.norm_img {
    width: 120rpx;
    height: 120rpx;
    flex-shrink: 0;
    margin-right: 20rpx;
}

.norm_infos {
    display: flex;
}

.info_detail {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.norm_price {
    color: #f33030;
    font-size: 28rpx;
}

.norm_num {
    font-size: 24rpx;
    color: #999;
}

.norm_name {
    font-size: 24rpx;
    color: #000;
    font-weight: bold;
}

.norm_main {
    margin-top: 30rpx;
}

.main_title {
    font-size: 26rpx;
    font-weight: bold;
    line-height: 45rpx;
}

.norm_tel {
    height: 80rpx;
    font-size: 26rpx;
    font-weight: 300;
    line-height: 80rpx;
    width: 750rpx;
    padding: 0 30rpx;
    border-top: 1rpx solid #ececec;
    border-bottom: 1rpx solid #ececec;
    margin-bottom: 50rpx;
}

.norm_pay {
    width: 750rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 28rpx;
    color: #ffffff;
    background-image: -webkit-linear-gradient(0deg,rgb(255,134,0)0%,rgb(255,105,14)100%);
    text-align: center;
}

.main_num {
    height: 60rpx;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #000;
    margin-top: 25rpx;
}

.adds {
    width: 52rpx;
    height: 52rpx;
}