.order-nav{ height:80rpx; display: flex; align-items: center; background: #fff; justify-content: space-around; }
.order-nav view{ display: inline-block; height:80rpx; line-height: 80rpx; box-sizing: border-box; color:#666; font-size: 30rpx; }
.order-nav .on{ border-bottom: 4rpx solid #f33030; color:#f33030; }
.order-nav text{ padding:0rpx 10rpx; }
.list{ background:#fff; margin-top:20rpx; font-size:28rpx; }
.order-num{ display: flex; font-size: 28rpx; justify-content: space-between; align-items: center; height:82rpx; padding: 0rpx 20rpx; border-bottom:1rpx solid #ececec; }
.list-up{ display: flex; align-items: center; justify-content: space-between; padding:20rpx 0rpx; border-bottom:1rpx solid #ececec; font-weight:700; }
.list-left{ display: flex; padding-left:20rpx; }
.list-right{ padding-right:20rpx; }
.left-det{ width:460rpx; display: flex; flex-direction: column; margin-left:20rpx; color:#666; font-size:24rpx; position: relative; }
.left-title{ color:#222; font-size:28rpx; line-height:44rpx; margin-bottom:4rpx; }
.price{ font-size:30rpx; color:#f33030; position: absolute; bottom: 0rpx; }
.img{ height:180rpx; width:180rpx; }
.list-down{ height:100rpx; background-color: #fff; display: flex; justify-content: flex-end; padding:0rpx 20rpx; align-items: center; }
.list-down .btn{ width:150rpx; height:56rpx; font-size:26rpx; color:#222; text-align: center; line-height: 56rpx; justify-content: center; border:1rpx solid #999; margin-left:14rpx; border-radius: 10rpx; position: relative; }
.list-down .submit{ color:#f33030; border:1rpx solid #f33030; }
.orderStu{ color:#f33030; }
.no{ margin-top:200rpx; height:270rpx; width:750rpx; }
.package-popup{ position: fixed; height: 100%; width: 100%; background-color: rgba(0,0,0,0.6); top:0; left: 0; }
.package-popup .package-bg .turn-off{ position: absolute; top:-80rpx; right: 60rpx; }
.package-popup .package-bg .turn-off image{ width: 46rpx; height: 46rpx; display: block; }
.package-popup .package-bg{ position: relative; top:50%; left: 50%; transform: translate(-50%,-50%); z-index: 66; }
.package-popup .package-bg image{ width: 687.5rpx; height: 700rpx; display: block; margin: 0 auto; }
.package-popup .package-bg-t{ width: 100%; position: absolute; top:140rpx; text-align: center; }
.package-popup .package-bg-t .package-bg-money{ font-size: 100rpx; color: #fc575a; text-align: center; }
.package-popup .package-bg-t .package-bg-money text{ font-size: 40rpx; }
.package-popup .package-bg-t .package-bg-receive{ font-size: 28rpx; color: #fc575a; margin-top: 60rpx; }
.package-popup .package-bg-t .package-bg-title{ font-size: 24rpx; color: #353535; margin-top: 30rpx; }
.package-popup .package-bg-b{ width: 100%; position: absolute; bottom:40rpx; text-align: center; }
.package-popup .package-bg-b .package-bg-invitation{ font-size: 28rpx; color:#fff; margin-bottom:30rpx; }
.package-popup .package-bg-b .package-bg-btn{ width: 350rpx; height: 84rpx; background: -webkit-linear-gradient(top, #fff742 , #ffc802); border-radius:42rpx; color: #ff3c17; font-size: 34rpx; line-height: 84rpx; text-align: center; font-weight: 700; box-shadow: 0 10rpx #ffa736; margin: 0 auto; }
.get-coupons{ width: 690rpx; height: 800rpx; border-radius: 10rpx; position: fixed; top: 58%; left: 50%; transform: translate(-50%,-50%); background-color: #eee; padding:0 20rpx; box-sizing: border-box; background: -webkit-linear-gradient(top, #fb6101 , #f97d00); }
.get-coupons .coupons-title{ height: 88rpx; line-height: 80rpx; font-size: 30rpx; text-align: center; position: relative; color: #fff; }
.get-coupons .coupons-img { }
.get-coupons .coupons-img image{ width: 56rpx; height: 158rpx; display: block; position: absolute; right: 50rpx; top:-140rpx; transform: translateY(-50%); z-index: -1; }
.may-use .may-use-b{ margin-top:0rpx; margin-bottom:0rpx; }
.may-use .use-list{ background-color: #fff; border-radius:5px; display: flex; align-items: center; margin-bottom:20rpx; position: relative; }
.may-use .use-list1{ background-color: #d1d1d1; }
.may-use .use-list .use-list-l{ padding-left:5rpx; height: 170rpx; width: 160rpx; position: relative; display: flex; align-items: center; }
.may-use .use-list .use-price{ color: #fe4c1c; font-size: 30rpx; display: flex; align-items: center; }
.may-use .use-list .use-price wx-span{ font-size: 36rpx; line-height: 48rpx; }
.may-use .use-list .use-info{ margin-left:20rpx; color: #232323; width: 325rpx; overflow: hidden; }
.may-use .use-list .use-price-man{ font-size: 22rpx; color: #fe4c1c; margin-left:10rpx; }
.may-use .use-list .package-title{ font-size: 28rpx; line-height: 32rpx; overflow: hidden; width: 325rpx; white-space: nowrap; text-overflow: ellipsis; }
.may-use .use-list .package-limit{ font-size: 24rpx; line-height: 24rpx; margin:12rpx 0 26rpx; overflow: hidden; width: 325rpx; white-space: nowrap; text-overflow: ellipsis; }
.may-use .use-list .package-time{ font-size: 22rpx; line-height: 22rpx; }
.may-use .use-list .use-list-l::before{ content: ""; width: 20rpx; height:20rpx; background-color: #fc6200; border-radius: 50%; display: block; position: absolute; top:0; right: 0; transform: translate(50%,-50%); }
.may-use .use-list .use-list-l::after{ content: ""; width: 20rpx; height:20rpx; background-color: #fc6200; border-radius: 50%; display: block; position: absolute; bottom:0; right: 0; transform: translate(50%,50%); }
.may-use .use-list .use-list-r{ height: 170rpx; width: 530rpx; display: flex; justify-content: center; align-items: center; border-left: 1px dashed #e8e8e8; }
.use-list-r .go-use{ padding: 10rpx 16rpx; border-radius: 10rpx; font-size: 24rpx; color: #fff; background-color: #fe6100; }
.may-use .use-list .use-list-r image{ width: 128rpx; height: 128rpx; display: block; }
.one-key-collection{ position: absolute; bottom: 22rpx; border-radius: 40rpx; background: -webkit-linear-gradient(top, #fbde14 , #dcb208); left: 50%; transform: translateX(-50%); color: #fc541b; height: 66rpx; width: 650rpx; text-align: center; line-height: 66rpx; font-size: 30rpx; }
.icon002{ position: absolute; left: 50%; transform: translateX(-50%); top: -180rpx; z-index: 0; }
.icon002 image{ width: 690rpx; height: 264rpx; display: block; }
.get-coupons-cao{ position: fixed; top: 0; left: 0; width: 100%; height: 100%; background-color: #7f7f7f; z-index: 0 }
.be-without{ position: absolute; top: 50%; left: 50%; transform: translate(-50%,-50%); }
.be-without image{ width: 276rpx; height: 322rpx; display: block; }
.be-without view{ font-size: 30rpx; color: #222; margin-top:10rpx; }
