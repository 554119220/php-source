<import src="../../../../templates/loading/loading.wxml"></import>
<import src="../../../../templates/setTab.wxml"></import>
<template is="setTabScroll" data="{{tabList:tabList,currentIndex:currentIndex,scrollNav:scrollNav}}"></template>
<view style="height:80rpx;"></view>
<view class="myearning-history" wx:if="{{dataList.list.length>0}}">
    <view class="commission" wx:for="{{dataList.list}}" wx:key="key">
        <view class="child">
            <view class="flex user">
                <view class="flex100-7 ellipsis">
                    <block wx:if="{{paramObj.status=='staff'}}">
                        <image class="circle fl" src="{{item.user_info.avatarUrl?item.user_info.avatarUrl:globalData.userDefault}}"></image>
                        <text class="name ellipsis">{{item.user_info.nickName?item.user_info.nickName:'客户'}}</text>
                    </block>
                    <block wx:else>
                        佣金:<em>￥{{item.extract_money}}</em>
                    </block>
                    <text class="not">
                        <block wx:if="{{item.waiting}}">({{item.waiting==2?'已入账':'未入账'}})</block>
                    </text>
                </view>
                <view class="flex100-3 tr">{{item.create_time4}}</view>
            </view>
            <view class="product rel">
                <image class="abs br-10" src="{{item.img?item.img:globalData.productDefault}}"></image>
                <view class="content">
                    <view class="name clamp2">{{item.title}}</view>
                    <view class="text">售价:<text>￥{{item.price}}</text>　佣金:<text>{{paramObj.status=='staff'?'￥'+item.extract_money_single:item.extract+'%'}}</text>
                        <text class="fr">X{{item.buy_number}}</text>
                    </view>
                </view>
            </view>
        </view>
    </view>
</view>
<template is="loading" data="{{...dataList,loading:dataList.loading}}"></template>
<view class="{{globalData.isIphoneX?'isIphoneX':''}}"></view>
