page {
    background: #f4f4f8;
}

view,button {
    display: block;
    overflow: initial;
}

.setTab-box-view {
    position: fixed;
}

.swiper-tab-list {
    font-size: 28rpx;
    color: #323232;
}

.btn-hover {
    opacity: 0.8;
}

.showCheck-sec {
    width: 510rpx;
    height: 270rpx;
    padding: 65rpx 55rpx;
    top: 50%;
    left: 50%;
    margin: -200rpx 0rpx 0rpx -310rpx;
    background: white;
    z-index: 2;
    overflow: hidden;
}

.showCheck-sec .title {
    font-size: 38rpx;
    color: #323232;
    font-weight: bold;
}

.showCheck-sec input {
    background: #fafafa;
    border: 1px solid #dcdcdc;
    width: 92%;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0rpx 4%;
    color: #969696;
    font-size: 30rpx;
    margin: 30rpx 0rpx;
    text-align: center;
}

.uCenter-index-info-list-sec {
    width: auto;
    padding-left: 100rpx;
    background: white;
}

.uCenter-index-info-list-sec .child {
    height: 99rpx;
    border-bottom: 1rpx solid #e6e6e6;
}

.uCenter-index-info-list-sec .child:nth-last-child(1),.uCenter-index-info-list-sec .child .flex {
    border: transparent;
}

.uCenter-index-info-list-sec .child i.abs {
    left: -60rpx;
    line-height: 30rpx;
    top: 37rpx;
}

.uCenter-index-info-list-sec .child .flex,.uCenter-index-info-list-sec .child .flex .flex100-5 {
    margin-left: 0rpx;
    height: 100rpx;
    line-height: 100rpx;
}