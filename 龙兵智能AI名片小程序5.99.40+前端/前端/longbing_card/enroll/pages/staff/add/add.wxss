@import "../../../../../__wuBaseWxss__/0.wxss";

@import "../../../../../__wuBaseWxss__/1.wxss";

@import "../../../../../__wuBaseWxss__/2.wxss";

page {
    background: #f4f4f4;
}

.pd-lg {
    padding: 26rpx 30rpx;
}

.fill-base .iconfont {
    max-width: 40rpx;
    margin-right: -4rpx;
}

.show-image {
    width: 200rpx;
    height: 200rpx;
}

.show-image:nth-child(3n) {
    margin-right: 0rpx;
}

picker {
    height: 53rpx;
    line-height: 53rpx;
}

.upload-images {
    width: 200rpx;
    height: 200rpx;
    float: left;
    margin: 0rpx 25rpx 25rpx 0rpx;
}

.upload-images:nth-child(3n-3) {
    margin-left: 0rpx;
}

.upload-images em {
    top: -16rpx;
    right: -16rpx;
    line-height: 36rpx;
    display: block;
    width: 36rpx;
    height: 36rpx;
    line-height: 36rpx;
    background: #898989;
    transform: rotate(45deg);
    vertical-align: middle;
    display: flex;
    align-items: center;
    justify-content: center;
}

.upload-images em i {
    font-size: 20rpx;
    color: white;
}

.imgs-icon {
    width: 200rpx;
    height: 200rpx;
    line-height: 200rpx;
    display: block;
    margin-bottom: 20rpx;
    margin-left: 10rpx;
    background: #f3f4f7;
}

.imgs-icon i {
    font-size: 80rpx;
    color: #b4b4b4;
}

.tabbar-box::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid rgba(226,226,226,1);
    color: rgba(226,226,226,1);
    transform-origin: 0 0;
    transform: scaley(0.5);
    box-sizing: border-box;
}

.tabbar-box-item {
    overflow: initial;
}

.tabbar-box-item__i {
    font-size: 48rpx;
    line-height: 48rpx;
    display: block;
    margin-top: 10rpx;
}

.tabbar-box-item__img_fabu {
    width: 96rpx;
    height: 96rpx;
    margin-top: -38rpx;
}