<view class="fill-base b-1px-b">
    <view class="reserve-list">
        <view catchtap="goUrl" class="child flex-center" data-url="/longbing_card/enroll/pages/detail/detail?id={{data.id}}&uid={{to_uid}}">
            <image class="img bg-f4f6f8 br-10" mode="aspectFill" src="{{data.cover}}"></image>
            <view class="flex-1">
                <view class="f-title ellipsis">{{data.title}}</view>
                <view class="f-icontext c-paragraph-sm ellipsis mt-sm mb-sm">
          {{data.start_time}} ～ {{data.end_time}}
        </view>
                <view class="f-icontext c-paragraph-sm ellipsis">限额人数{{data.sign_limit}}</view>
            </view>
        </view>
    </view>
</view>
<view class="space-xxl"></view>
<view class="text-center">
    <i class="iconfont icon-xianshi_xuanzetianchong" style="font-size:120rpx;color:#e93636;"></i>
    <view class="c-caption f-caption">活动发布成功</view>
</view>
<view class="space-xl"></view>
<view class="flex-center enroll-rank-child">
    <view catchtap="goUrl" class="f-title br-10 c-title" data-method="redirectTo" data-url="/longbing_card/enroll/pages/staff/add/add">
      继续发布活动
    </view>
    <button class="f-title br-10 active c-base" openType="share">
      分享
    </button>
</view>
