.setTab-box-view {
    position: fixed;
}

.scroll-item-de {
    padding: 0rpx 38rpx;
}

.tabbar-box::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid rgba(226,226,226,1);
    color: rgba(226,226,226,1);
    transform-origin: 0 0;
    transform: scaley(0.5);
    box-sizing: border-box;
}

.tabbar-box-item {
    overflow: initial;
}

.tabbar-box-item__i {
    font-size: 48rpx;
    line-height: 48rpx;
    display: block;
    margin-top: 10rpx;
}

