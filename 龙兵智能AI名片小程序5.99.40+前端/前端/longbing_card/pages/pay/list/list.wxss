page {
    background: #f4f4f4;
}

.list-jian {
    width: 254rpx;
    height: 55rpx;
    line-height: 55rpx;
    margin: 50rpx auto;
}

.list-jian .img {
    width: 100%;
    height: 25rpx;
    top: 18rpx;
}

.list-jian .text {
    width: 100%;
    height: 55rpx;
    line-height: 55rpx;
    top: 0rpx;
    color: #1e1e1e;
    font-size: 32rpx;
}

.pay-ok {
    width: 100%;
    height: auto;
    display: block;
    margin-top: 100rpx;
}

.pay-ok i {
    font-size: 76rpx;
    color: #ee234e;
    display: block;
}

.pay-ok .money {
    font-size: 48rpx;
    color: #1b1b1b;
    margin: 15rpx 0rpx 5rpx 0rpx;
}

.pay-ok .money em,.pay-ok .time {
    font-size: 26rpx;
}

.return-btn {
    width: 686rpx;
    height: 96rpx;
    line-height: 96rpx;
    display: block;
    border-radius: 8rpx;
    margin: 90rpx auto;
}