page {
    background: #f6f5fa;
}

.import-img-bg {
    width: 252rpx;
    height: 180rpx;
    margin: 0rpx auto;
}

.import-btn-import {
    width: 686rpx;
    height: 96rpx;
    line-height: 96rpx;
    border-radius: 8rpx;
}

.import-article-link {
    width: 664rpx;
    line-height: 40rpx;
    height: 120rpx;
    padding: 20rpx 20rpx;
    display: block;
    background: #f7f7f7;
    margin: 0rpx auto;
    outline: 0;
}

.tabbar-box::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid rgba(226,226,226,1);
    color: rgba(226,226,226,1);
    transform-origin: 0 0;
    transform: scaley(0.5);
    box-sizing: border-box;
}

.tabbar-box-item {
    overflow: initial;
}

.tabbar-box-item__i {
    font-size: 48rpx;
    line-height: 48rpx;
    display: block;
    margin-top: 10rpx;
}

.tabbar-box-item__img_fabu {
    width: 96rpx;
    height: 96rpx;
    margin-top: -38rpx;
}