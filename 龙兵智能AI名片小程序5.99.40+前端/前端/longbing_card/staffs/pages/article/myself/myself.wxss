@import "../../../../../__wuBaseWxss__/0.wxss";

@import "../../../../../__wuBaseWxss__/1.wxss";

page {
    background: #f6f5fa;
}

.flex-x-around .tc {
    width: 50%;
}

.rank-img {
    top: 80rpx;
}

.article-child {
    padding: 30rpx 4%;
}

.article-img {
    width: 130rpx;
    height: 130rpx;
    display: block;
}

.article-title {
    line-height: 36rpx;
    height: 36rpx;
    max-width: 520rpx;
}

.article-text,.article-text i {
    color: #c8c8c8;
}

.article-text text {
    width: 100rpx;
    height: 42rpx;
    line-height: 42rpx;
    border-radius: 8rpx;
    background: #eeeeee;
    display: block;
}

.b-1px-t:before {
    border-top: 1px solid rgba(238,238,238,1);
    color: rgba(238,238,238,1);
}

.scroll-item-active {
    color: #11c95e;
}

.bottom-split {
    background: #11c95e;
}

.tabbar-box::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid rgba(226,226,226,1);
    color: rgba(226,226,226,1);
    transform-origin: 0 0;
    transform: scaley(0.5);
    box-sizing: border-box;
}

.tabbar-box-item {
    overflow: initial;
}

.tabbar-box-item__i {
    font-size: 48rpx;
    line-height: 48rpx;
    display: block;
    margin-top: 10rpx;
}

.tabbar-box-item__img_fabu {
    width: 96rpx;
    height: 96rpx;
    margin-top: -38rpx;
}