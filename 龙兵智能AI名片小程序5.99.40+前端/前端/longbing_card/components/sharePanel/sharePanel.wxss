@import "../../../__wuBaseWxss__/4.wxss";

@import "../../../__wuBaseWxss__/6.wxss";

button::after {
    border: none;
}

button {
    background: none;
}

.fix-show-share-sec {
    bottom: 0rpx;
    width: 100%;
    height: auto;
    display: block;
    background: white;
    z-index: 9999999991;
}

.fix-show-share-sec .flex {
    padding: 50rpx 5%;
}

.fix-show-share-sec .flex100-5 i {
    font-size: 100rpx;
    display: block;
    line-height: 100rpx;
}

.fix-show-share-sec .flex100-5 i.icon-weixin1 {
    color: #3aae49;
}

.fix-show-share-sec .flex100-5 i.icon-pengyouquan-copy {
    color: #67b83f;
}

.fix-show-share-sec .flex100-5 .title {
    width: 100%;
    font-size: 28rpx;
    color: #333;
    line-height: 34rpx;
    margin-top: 12rpx;
}

.fix-show-share-sec .cancel {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
    background: #efeff4;
    color: #333;
    border-radius: 0rpx;
}

.fix-painter-share {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    bottom: 0rpx;
    color: white;
    background: white;
    border-radius: 0rpx;
}

.fix-painter-share button {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    color: white;
    background: #1aad19;
    border-radius: 0rpx;
}