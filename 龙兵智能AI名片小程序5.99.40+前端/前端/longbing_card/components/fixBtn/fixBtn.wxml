<form bindsubmit="formSubmit" class="fixBottomBtn-sec {{isIphoneX?'isIphoneX':''}}" reportSubmit="true">
    <button class="btn fix" data-delta="{{fixBtn.delta}}" data-method="{{fixBtn.method||'navigateTo'}}" data-status="{{fixBtn.delta||fixBtn.url?'toJumpUrl':'toConfirm'}}" data-url="{{fixBtn.url}}" disabled="{{fixBtn.disabled||false}}" formType="submit" hoverClass="btn-hover" style="background:{{fixBtn.color}}" type="primary">
        {{fixBtn.text}}
    </button>
</form>
