.footer-company-img {
    width: 100%;
    height: auto;
    padding: 30rpx 0rpx;
    display: block;
}

.footer-company-img .image {
    width: 193rpx;
    height: 87rpx;
    display: block;
    margin: 0rpx auto;
}

.footer-company-img .text {
    width: 90%;
    margin: 0rpx 5%;
    text-align: center;
    font-size: 22rpx;
    line-height: 28rpx;
    margin-bottom: 5rpx;
    color: #aeaeae;
}