<view bindtap="{{copyright.click_copy_way==4?'previewImage':'toCopyright'}}" class="footer-company-img tc" data-img="{{copyright.click_copy_show_img}}" data-url="{{copyright.click_copy_way==1?'tel:':''}}{{copyright.click_copy_content}}" wx:if="{{copyright.logo_switch!=3}}">
    <image class="image" mode="widthFix" src="{{copyright.copyright}}" wx:if="{{copyright.logo_switch==0}}"></image>
    <view class="text" style="margin-top:20rpx" wx:if="{{copyright.logo_switch==1}}">
        <text>{{copyright.logo_text}}</text>
    </view>
    <block wx:if="{{copyright.logo_switch==2}}">
        <image class="image" mode="widthFix" src="{{copyright.copyright}}"></image>
        <view class="text" style="margin-top:20rpx">
           {{copyright.logo_text}}
        </view>
    </block>
</view>
