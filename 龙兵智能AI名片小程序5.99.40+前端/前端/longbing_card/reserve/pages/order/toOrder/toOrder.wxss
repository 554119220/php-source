@import "../../../../../__wuBaseWxss__/0.wxss";

@import "../../../../../__wuBaseWxss__/1.wxss";

@import "../../../../../__wuBaseWxss__/2.wxss";

page {
    background: #f4f4f4;
}

.form-list {
    padding: 0rpx 4%;
}

.form-list .child {
    height: 90rpx;
}

.form-list .child .c-title {
    width: 160rpx;
}

.form-list .child .c-title em {
    color: #fc3c3b;
    margin-right: 5rpx;
}

.form-list .child .arrow {
    width: 0rpx;
    height: 0rpx;
    border-left: 16rpx solid transparent;
    border-right: 16rpx solid transparent;
    border-top: 16rpx solid #969696;
}

.form-list .remark {
    width: auto;
    height: 130rpx;
    padding: 30rpx;
    background: #f4f4f4;
    outline: none;
}

.reserve-btn .text {
    line-height: 100rpx;
    background: #fc3c3b;
}

.form-list .child picker {
    height: 90rpx;
    line-height: 90rpx;
}

.form-list .child picker i {
    transform: rotate(90deg);
}