<import src="/sudu8_page/resource/wxParse/wxParse.wxml"></import>
<view class="bg">
    <block wx:if="{{active_is==1}}">
        <image mode="widthFix" src="{{bg}}" style="width:100%;display:block"></image>
        <view bindtap="role_block" class="role_down"></view>
        <view class="shibai_con" wx:if="{{shibaila==1}}">
            <image mode="widthFix" src="../images/fail.png"></image>
            <view class="failTit">很遗憾，当前红包已失效 -_-</view>
            <view bindtap="makenewbao" class="failBtn" wx:if="{{zibao==1}}"></view>
            <view bindtap="makenewbaos" class="failBtn" data-num="{{request_num}}" wx:if="{{sxb==1}}"></view>
        </view>
        <view class="shibai_con" wx:if="{{shibaila==2}}">
            <image mode="widthFix" src="../images/fail.png"></image>
            <view class="failTit">
          您来晚了，红包已拆完-_-
        </view>
            <view bindtap="makenewbao" class="failBtn" wx:if="{{zibao==1}}"></view>
            <view bindtap="makenewbaos" class="failBtn" data-num="{{request_num}}" wx:if="{{sxb==1}}"></view>
        </view>
        <view class="con2" wx:if="{{tsuccessed==1}}">
            <image class="successIcon" mode="widthFix" src="../images/successIcon.png"></image>
            <image class="line2 line2l" mode="widthFix" src="../images/line2.png"></image>
            <image class="line2 line2r" mode="widthFix" src="../images/line2.png"></image>
            <view class="con21">
                <view class="regBox">
                    <view class="regBoxTit2">Congratulations!</view>
                    <view class="regBoxTit2" style="margin-top:10rpx; font-size:40rpx;">恭喜您获得{{getscore}}{{types}}!</view>
                    <view bindtap="makenewbao" class="regBoxBtn" wx:if="{{successed==0}}">发起新的红包</view>
                    <view bindtap="makenewbaos" class="regBoxBtn" data-num="{{request_num}}" wx:if="{{successed==1}}">发起新的红包</view>
                    <view class="regBoxBtn2">查看我的</view>
                </view>
            </view>
            <view class="con21 con22" style="margin-top:30rpx">
                <view class="regBox">
                    <view class="regBoxTit3">———— 看看大家手气 ———— </view>
                    <view class="successUser">
                        <view class="tlist2 t1 bd" style="display:inline-block;float:left">
                            <image src="{{touxiang1}}"></image>
                            <view class="teamer">团长</view>
                        </view>
                        <view style="display:inline-block;float:left">{{nickname}}</view>
                        <view class="tlist2Money">{{score}}
                <span>{{types}}</span>
                        </view>
                        <view class="zuijia" wx:if="{{bignum==score}}">手气最佳</view>
                    </view>
                    <view class="successUser" wx:for="{{arr}}">
                        <view class="tlist2 t1 bd" style="display:inline-block;float:left">
                            <image src="{{item.avatar}}"></image>
                        </view>
                        <view style="display:inline-block;float:left">{{item.nickname}}</view>
                        <view class="tlist2Money">{{item.score}}
                <span>{{types}}</span>
                        </view>
                        <view class="zuijia" wx:if="{{bignum==item.score}}">手气最佳</view>
                    </view>
                </view>
            </view>
        </view>
        <view class="con" style="height:{{h}}rpx">
            <view class="regBox" wx:if="{{hb==1}}">
                <view class="regBoxTit">剩余时间：
            <block wx:if="{{sytime>0}}">{{stime}}</block>
                </view>
                <view>
                    <view class="tlist t1 bd" wx:if="{{touxiang1!=''}}">
                        <image src="{{touxiang1}}"></image>
                        <view class="teamer">团长</view>
                    </view>
                    <view class="tlist " wx:if="{{touxiang1==''}}">
                        <image src="../images/touxiang_bg.jpg"></image>
                    </view>
                    <view class="tlist t1 bd" wx:for="{{arr}}">
                        <image src="{{item}}"></image>
                    </view>
                </view>
                <view class="hbNotice">还差{{cha_num}}人即可瓜分{{jine}}{{types}}，快邀好友来拆</view>
                <button class="regBoxBtn" openType="share">邀请好友拆红包</button>
            </view>
            <view class="regBox" wx:if="{{really==1}}">
                <view class="regBoxTit">填写个人信息参加活动</view>
                <input bindinput="userNameInput" class="regBoxIpt" placeholder="您的姓名" type="text" value="">您的姓名</input>
                <input bindinput="userTelInput" class="regBoxIpt" maxlength="11" placeholder="联系方式" type="number" value="">联系方式</input>
                <view bindtap="really_sub" class="regBoxBtn">邀请好友拆红包</view>
            </view>
            <view class="regBox" wx:if="{{click_cy==1}}">
                <view class="regBoxTit">剩余时间：
            <block wx:if="{{sytime>0}}">{{stime}}</block>
                </view>
                <view class="hbNotice">还差{{cha_num}}人即可瓜分{{jine}}{{types}}啦，点击按钮参与</view>
                <view bindtap="click_cy" class="regBoxBtn">点击拆红包</view>
                <view bindtap="gethongbao" class="regBoxBtn2" style="margin-top:30rpx;">发起新的红包</view>
            </view>
            <view class="regBox" wx:if="{{cyed==1}}">
                <view class="regBoxTit">剩余时间：
            <block wx:if="{{sytime>0}}">{{stime}}</block>
                </view>
                <view class="hbNotice">还差{{cha_num}}人即可瓜分{{jine}}{{types}}啦，点击按钮参与</view>
                <view class="regBoxBtn3">参与成功</view>
                <view bindtap="gethongbao" class="regBoxBtn2">发起新的红包</view>
            </view>
            <block wx:if="{{success_cy==2}}">
                <block wx:if="{{jindu==1}}">
                    <view class="mb20" style="color：#5e5a5b;text-align:center;margin-top:30rpx;">已成功参与，还差{{cha_num}}人即可瓜分{{jine}}{{types}}啦</view>
                    <view class="mb20" style="color:#000000;text-align:center">
                        <block wx:if="{{sytime>0}}">
              {{stime}}
            </block>
                    </view>
                </block>
                <view bindtap="gethongbao" style="width:500rpx;height:80rpx;padding:0;line-height:80rpx;background:#ded80a;border-radius:50rpx;color:#ffffff;text-align:center;margin:0 auto;font-size:32rpx;margin-top:30rpx">发起个人红包</view>
                <view style="background:#a1387b;margin-top:-2rpx">
                    <block wx:if="{{jindu==2}}">
                        <view style="text-align:center">恭喜您，已成功瓜分红包 ~ </view>
                        <view style="width:670rpx;padding:17rpx; margin:0 auto;background:#f6ebf3;border-radius:10rpx">
                            <view style="text-align:center">红包瓜分详情</view>
                            <view style="overflow:hidden;line-height:110rpx;margin-bottom:20rxp;padding:10rpx;position:relative">
                                <view class="tlist t1 bd" style="display:inline-block;float:left">
                                    <image src="{{touxiang1}}"></image>
                                    <view class="teamer">团长</view>
                                </view>
                                <view style="display:inline-block;float:left">{{nickname}}</view>
                                <view style="display:inline-block;float:right">{{score}}{{types}}</view>
                                <view style="position:absolute;right:0;bottom:0;" wx:if="{{bignum==score}}">
                                    <image mode="widthFix" src="../images/zuijia.jpg" style="width:152rpx;"></image>
                                </view>
                            </view>
                            <view style="overflow:hidden;line-height:110rpx;margin-bottom:20rxp;padding:10rpx;position:relative" wx:for="{{arr}}">
                                <view class="tlist t1 bd" style="display:inline-block;float:left">
                                    <image src="{{item.avatar}}"></image>
                                </view>
                                <view style="display:inline-block;float:left">{{item.nickname}}</view>
                                <view style="display:inline-block;float:right">{{item.score}}</view>
                                <view style="position:absolute;right:0;bottom:0;" wx:if="{{bignum==item.score}}">
                                    <image mode="widthFix" src="../images/zuijia.jpg" style="width:152rpx;"></image>
                                </view>
                            </view>
                        </view>
                    </block>
                </view>
            </block>
        </view>
    </block>
    <block wx:if="{{active_is==2}}">
    活动不存在
  </block>
    <block wx:if="{{active_is==3}}">
    活动已过期
  </block>
    <block wx:if="{{active_is==4}}">
    活动未开始
  </block>
</view>
<view bindtap="role_close" class="role" style="overflow:visible" wx:if="{{role==1}}">
    <view style="width:560rpx;background:#fff;border-radius:10rpx;margin:314rpx auto 0;padding:40rpx;position:relative;overflow:visible">
        <image src="../images/close.png" style="width:60rpx;height:60rpx;position:absolute;right:-30rpx;top:-30rpx"></image>
        <view class="title" style="color:#ea3c3b;text-align:center;margin-bottom:20rpx;font-size:30rpx">拆红包活动规则</view>
        <view style="line-height:50rpx">
            <template is="wxParse" data="{{wxParseData:article.nodes}}"></template>
        </view>
    </view>
</view>
<view bindtap="tan_close" class="tan" wx:if="{{tan==1}}">
    <image class="tanBg" mode="widthFix" src="../images/share.png"></image>
    <view class="tanBox">
        <view>恭喜您拆分到
        <span>{{initiator}}</span>{{types}}</view>
        <view>邀请
        <span>{{cha_num}}</span>位好友后即可领取!</view>
        <button class="tanBoxBtn" openType="share"></button>
    </view>
</view>
