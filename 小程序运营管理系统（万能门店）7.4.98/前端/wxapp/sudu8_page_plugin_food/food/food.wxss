}

view {
    overflow: visible!important;
}

.jiajian {
    font-size: 40rpx;
    float: left;
    line-height: 40rpx;
}

.head {
    padding: 30rpx;
    line-height: 60rpx;
    word-break: break-all;
    width: 100%;
    box-sizing: border-box;
}

.foodlogo {
    width: 160rpx;
    height: 160rpx;
    float: left;
    border-radius: 6rpx;
    box-shadow: 0 0 6rpx #fff;
    margin-right: 20rpx;
}

.head view {
    word-break: break-all;
    overflow: auto;
}

.ftitle1 {
    font-size: 36rpx;
    font-weight: 700;
    color: #fff;
}

.ftitle2 {
    font-size: 24rpx;
    color: #fff;
    line-height: 36rpx;
    height: 36rpx;
    overflow: hidden;
    margin: 10rpx 0;
}

.P_title {
    display: flex;
    padding: 0 60rpx;
    justify-content: space-between;
    background: #fff;
    font-size: 30rpx;
    margin-bottom: 10rpx;
    width: 630rpx;
    margin: 0 auto;
}

.P_business,.P_order {
    width: 20%;
    padding: 20rpx;
    text-align: center;
}

.P_active {
    border-bottom: 4rpx solid #2094FE;
}

.P_business_P {
    border-radius: 6rpx;
    margin: 0 auto;
}

.S_top {
    margin-top: 10rpx;
    background: #fff;
    padding: 20rpx 40rpx;
}

.S_title {
    font-weight: 700;
    border-bottom: 2rpx solid #eee;
    padding-bottom: 20rpx;
    margin-bottom: 16rpx;
}

.S_top>.iconfont {
    line-height: 60rpx;
    font-size: 28rpx;
    color: #666;
}

.S_top>.iconfont:before {
    margin-right: 20rpx;
    font-size: 40rpx;
}

.S_top>.icon-zhekou:before {
    color: #f74d4d;
}

.slist {
    margin-bottom: 16rpx;
}

.title {
    display: block;
}

.goods {
    display: flex;
    width: 100%;
    background: #fff;
    margin-top: 10rpx;
}

.menu-wrapper {
    width: 200rpx;
    background: #f3f5f7;
}

.menu-item {
    width: 100%;
    box-sizing: border-box;
    height: 90rpx;
    padding: 0 24rpx;
    line-height: 90rpx;
    color: #999;
    font-size: 24rpx;
    text-align: center;
    overflow: hidden!important;
    white-space: nowrap;
    text-overflow: ellipsis;
    border-bottom: 1px solid #eeeeee;
}

.menu-item.current {
    color: #333;
    background: #fff;
    border-left: 8rpx solid #FFD946;
}

.ltem_list {
    display: flex;
    width: 65%;
}

.foods-wrapper {
    width: 550rpx;
}

.foods-item {
    position: relative;
    margin: 0 18px;
    padding-bottom: 18px;
    height: 120rpx;
    border-bottom: 1px solid rgba(248,248,248,1);
    padding-top: 18px;
}

.food-grouping:last-child .foods-item:last-child {
    border: none;
    margin-bottom: 0;
}

.foods-item .icon {
    display: inline-block;
    width: 120rpx;
    height: 120rpx;
    flex: 0 0 120rpx;
    margin-right: 20rpx;
}

.foods-item .title {
    display: block;
    color: #2c2c2c;
    font-size: 0.8rem;
}

.foods-item .content {
    position: relative;
}

.foods-item .price {
    font-size: 0.8rem;
    color: #FF5035;
    position: absolute;
    bottom: 0;
    width: 200rpx;
}

.unit {
    font-size: 0.68rem;
    color: #999;
}

.slide image {
    width: 100%;
    height: 100%;
}

.cartcontrol-wrap {
    position: absolute;
    right: 0;
    bottom: 20px;
}

.cartcontrol {
    font-size: 0;
}

.cart-decrease {
    display: inline-block;
    padding: 6px;
    opacity: 1;
    transform: translate3d(0,0,0);
    width: 40rpx;
    height: 40rpx;
    background-repeat: no-repeat;
    background-size: contain;
    background-position: center;
    padding: 0;
    float: left;
}

.cart-add {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    padding: 6px;
    padding: 0;
    line-height: 24px;
    font-size: 24px;
    color: rgb(0,160,220);
    background-repeat: no-repeat;
    background-size: contain;
    background-position: center;
}

.inner {
    display: inline-block;
    line-height: 24px;
    font-size: 24px;
    color: rgb(0,160,220);
    transition: all 0.4s linear;
    transform: rotate(0);
}

.inner.move-enter-active,.inner.move-leave-active {
    transition: all 0.4s linear;
}

.inner .move-enter,.inner.move-leave-active {
    opacity: 0;
    transform: translate3d(24px,0,0);
}

.inner {
    transform: rotate(180deg);
}

.icon-remove_circle_outline {
}

.cart-count {
    display: inline-block;
    vertical-align: top;
    width: 84rpx;
    padding-top: 2rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 30rpx;
    color: rgb(147,153,159);
    float: left;
}

.icon-add_circle {
}

.shopcart {
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 50;
    width: 100%;
    height: 48px;
}

.shopcart .content {
    display: flex;
    background: #141d27;
    font-size: 0;
    color: rgba(255,255,255,0.4);
}

.shopcart .content .content-left {
    flex: 1;
}

.shopcart .logo-wrapper {
    display: inline-block;
    vertical-align: top;
    position: relative;
    top: -10px;
    margin: 0 12px;
    padding: 6px;
    width: 56px;
    height: 56px;
    box-sizing: border-box;
    border-radius: 50%;
    background: #141d27;
    overflow: visible!important;
}

.shopcart .logo {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    text-align: center;
    background: #2b343c;
    background-size: 100% 100%;
}

.shopcart .logo.highlight {
    background: rgb(0,160,220);
}

.shopcart {
    line-height: 44px;
    font-size: 24px;
    color: #80858a;
}

.shopcart.highlight {
    color: #fff;
}

.shopcart .num {
    position: absolute;
    top: 0;
    right: 0;
    width: 24px;
    height: 16px;
    line-height: 16px;
    text-align: center;
    border-radius: 16px;
    font-size: 9px;
    font-weight: 700;
    color: #fff;
    background: rgb(240,20,20);
    box-shadow: 0 4px 8px 0 rgba(0,0,0,0.4);
}

.shopcart .price {
    display: inline-block;
    vertical-align: top;
    margin-top: 12px;
    line-height: 24px;
    padding-right: 12px;
    box-sizing: border-box;
    font-size: 16px;
    font-weight: 700;
}

.shopcart .price.highlight {
    color: #fff;
}

.shopcart .content-right {
    flex: 0 0 105px;
    width: 105px;
}

.content-right .pay {
    height: 48px;
    line-height: 48px;
    text-align: center;
    font-size: 12px;
    font-weight: 700;
    background: #2b333b;
}

.content-right .pay.enough {
    background: #00b43c;
    color: #fff;
}

.shopcart-list {
    position: fixed;
    left: 0;
    bottom: 49px;
    z-index: -1;
    width: 100%;
    background: #fff;
    z-index: 10000;
}

.food {
    border-bottom: 1px solid rgba(7,17,27,0.1);
}

.list-header {
    height: 40px;
