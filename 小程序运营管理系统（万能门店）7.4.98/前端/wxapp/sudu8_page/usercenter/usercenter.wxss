.ripple {
    background-color: rgba(0,0,0,0.8);
    border-radius: 100%;
    height: 10px;
    width: 10px;
    margin-top: -90px;
    position: absolute;
    -webkit-transform: scale(0);
}

@-webkit-keyframes ripple {
    100% {
        transform: scale(12);
        background-color: transparent;
    }
}

.prompt {
    margin-bottom: 30rpx;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.prompt>text {
    margin: auto;
    display: block;
    text-align: center;
    float: left;
    vertical-align: middle;
    height: 80rpx;
    line-height: 80rpx;
    width: 440rpx;
}

.prompt>view {
    width: 100rpx;
    height: 2rpx;
    background-color: #969696;
    vertical-align: middle;
    margin-top: 38rpx;
}

.prompt>view:first-child {
    float: left;
}

.prompt>view:last-child {
    float: right;
}

.prompt::after {
    content: '';
    display: block;
    clear: both;
}