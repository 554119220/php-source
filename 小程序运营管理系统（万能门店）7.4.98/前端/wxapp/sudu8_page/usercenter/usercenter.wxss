    max-height: 500rpx;
}

.coupon::-webkit-scrollbar {
    display: none;
}

.coupon>view {
    width: 600rpx;
    height: 180rpx;
    margin: 0 auto 20rpx;
    display: block;
    box-shadow: rgba(162,162,162,0.2) 0rpx 0rpx 40rpx;
    border-radius: 10rpx;
