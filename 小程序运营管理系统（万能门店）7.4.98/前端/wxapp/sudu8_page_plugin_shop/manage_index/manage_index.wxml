<view class="mTop">
    <navigator url="../manage_shop/manage_shop">
        <image src="{{dataindex.logo}}"></image>
    </navigator>
    <view>{{dataindex.name}}</view>
</view>
<view class="mData">
    <view class="mDatat1">今日店铺收入</view>
    <navigator url="../manage_tixian/manage_tixian">
        <view class="mDatat2">{{dataindex.todayOrderMoney}}<span>元</span>
        </view>
    </navigator>
    <view class="mDataList">
        <navigator class="mDataListLine" url="../manage_order/manage_order">
            <view>总付款订单</view>
            <view class="mDataListLineNum">{{dataindex.allOrderNum}}</view>
        </navigator>
        <navigator class="mDataListLine" url="../manage_prolist/manage_prolist">
            <view>在售商品</view>
            <view class="mDataListLineNum">{{dataindex.allProNum}}</view>
        </navigator>
        <navigator class="mDataListLine" url="../manage_prolist/manage_prolist">
            <view>浏览数量</view>
            <view class="mDataListLineNum">{{dataindex.allViewNum==null?0:dataindex.allViewNum}}</view>
        </navigator>
    </view>
</view>
<view class="mMenu">
    <navigator class="mMenuRow" url="../manage_prolist/manage_prolist">
        <image src="../img/mpro.png"></image>
        <view>商品管理</view>
    </navigator>
    <navigator class="mMenuRow" url="../manage_order/manage_order">
        <image src="../img/morder.png"></image>
        <view>订单详情</view>
    </navigator>
    <navigator class="mMenuRow" url="../manage_tixian/manage_tixian">
        <image src="../img/mmoney.png"></image>
        <view>提现管理</view>
    </navigator>
    <navigator class="mMenuRow" url="../manage_shop/manage_shop">
        <image src="../img/mshop.png"></image>
        <view>店铺信息</view>
    </navigator>
</view>
