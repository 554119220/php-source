body {
    background-color: #F6F6F6;
}

.wallet-top {
    height: 330px;
    margin: 30rpx auto;
    border-radius: 14rpx;
    background: #6671E4;
    overflow: visible;
}

.wallet-img {
    width: 276rpx;
    height: 120rpx;
    margin-top: 108rpx;
}

.wallet-info {
    font-size: 32rpx;
    font-weight: 500;
    color: white;
    margin-top: 60rpx;
}

.wallet-money {
    font-size: 28rpx;
    font-weight: 500;
    color: white;
    margin-top: 12rpx;
}

.wallet-sum {
    font-size: 74rpx;
    font-weight: bolder;
}

.wallet-tool {
    background-color: white;
    border-radius: 10rpx;
    height: 140rpx;
    font-size: 32rpx;
    font-weight: 600;
}

.deal-box img {
    width: 80rpx;
    margin-top: 30rpx;
}

.record-box {
    width: 68rpx;
    height: 68rpx;
    margin: 36rpx auto;
}

.tool-text {
    padding-right: 0;
    padding-left: 0;
    margin-top: 15%;
    color: black;
    font-weight: normal;
}

.take-box {
    width: 100%;
    margin: 15px auto;
    background-color: #ffffff;
    border-radius: 14rpx;
}

.take-box .btn-primary {
    background-color: #6671E4;
    border-color: #6671E4;
    margin-top: 30px;
    font-size: 15px;
}

.text-one {
    font-size: 15px;
    margin-top: 17px;
    font-weight: 500;
}

.text-two {
    color: #5E6AE2;
    font-size: 13px;
}

.text-three {
    color: #949494;
    margin-top: 12px;
    font-size: 12px;
}

.take-input {
    margin-top: 18px;
}

.input-group-addon {
    background-color: white;
    border-bottom: 1px solid #EDEDED;
    border-left: 0px;
    border-top: 0px;
    border-right: 0px;
    border-radius: 0px;
}

.form-control {
    border-bottom: 1px solid #EDEDED;
    border-left: 0px;
    border-top: 0px;
    border-right: 0px;
    border-radius: 0px;
    box-shadow: inset 0 0px 0px rgba(0,0,0,0);
}

input[type="radio"]+label::before {
    content: "\a0";
    display: inline-block;
    vertical-align: middle;
    font-size: 18px;
    width: 1em;
    height: 1em;
    margin-right: .4em;
    border-radius: 50%;
    border: 5px solid #eee;
    text-indent: .15em;
    line-height: 1;
}

input[type="radio"]:checked+label::before {
    background-color: #6671E4;
    background-clip: content-box;
}

input[type="radio"] {
    position: absolute;
    clip: rect(0,0,0,0);
}

.sz-top {
    background-color: white;
    height: 48px;
    padding-top: 20px;
}

.sz-top img {
    width: 22px;
