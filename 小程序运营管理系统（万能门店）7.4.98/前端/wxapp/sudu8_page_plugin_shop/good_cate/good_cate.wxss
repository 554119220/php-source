page {
    background: #ffffff;
}

.goods {
    display: flex;
    width: 100%;
    background: #fff;
}

.menu-wrapper {
    flex: 0 0 100px;
    width: 100px;
    background: #f3f5f7;
}

.menu-item {
    display: table;
    height: 90rpx;
    width: 100%;
    box-sizing: border-box;
    padding: 0 24rpx;
    line-height: 90rpx;
    color: #999;
    font-size: 0.8rem;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    border-bottom: 1px solid #eee;
}

.menu-item.current {
    color: #333;
    background: #fff;
    border-left: 8rpx solid #ffd946;
}

.foods-wrapper {
    width: 650rpx;
}

.foods-item {
    position: relative;
    margin: 20rpx;
    border-bottom: 1px solid #eee;
    overflow: hidden;
    padding-bottom: 20rpx;
}

.food-grouping:last-child .foods-item:last-child {
    border: none;
    margin-bottom: 0;
}

.foods-item .icon {
    width: 180rpx;
    float: left;
}

.foods-item .title {
    display: block;
    color: #2c2c2c;
    font-size: 0.8rem;
}

.foods-item .content {
    width: 300rpx;
    float: left;
    margin-left: 20rpx;
}

.slld1 {
