.swiper-box .wx-swiper-dot {
    display: inline-flex;
    justify-content: space-between;
    border-radius: 50%;
    margin-left: 15rpx;
}

.swiper-box .wx-swiper-dot::before {
    content: '';
    flex-grow: 1;
    border: 2rpx solid #6A819B;
    border-radius: 50%;
}

.swiper-box .wx-swiper-dot-active::before {
    background: #fff;
    border-radius: 50%;
    border-color: #fff;
}

.swiperbox {
    position: relative;
}

