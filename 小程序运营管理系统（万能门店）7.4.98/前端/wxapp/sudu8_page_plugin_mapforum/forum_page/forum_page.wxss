.forum_page_dynamic_img {
    width: 100%;
    height: 375rpx;
    display: block;
    border-radius: 12rpx;
    margin: 30rpx auto;
}

page {
    background-color: #F8F8F8;
}

.hbj {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.flex1 {
    flex: 1;
}

.dynamic_single {
    padding: 50rpx 30rpx 40rpx;
    background-color: #fff;
}

.dynamic_tx {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    margin-right: 24rpx;
}

.dynamic_tx2 {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin-right: 24rpx;
}

.dynamic_view1_center {
    width: 440rpx;
}

.dynamic_name_text1 {
    max-width: 220rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    display: inline-block;
    font-size: 28rpx;
    color: #434343;
    font-weight: bold;
}

.dynamic_name_text2 {
    font-size: 24rpx;
    color: #969696;
    font-family: Arial,Helvetica,sans-serif;
    margin-left: 10rpx;
    vertical-align: 8rpx;
    font-weight: 400;
}

.forum_card_single {
    padding: 2rpx 20rpx;
    border-radius: 40rpx;
    font-size: 24rpx;
    color: #fff;
    margin-right: 10rpx;
}

.forum_phone {
    width: 40rpx;
    height: 40rpx;
}

.dynamic_name {
    font-size: 28rpx;
    color: #434343;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-weight: bold;
}

.dynamic_view1_right {
    font-size: 28rpx;
    color: #2f74fd;
}

.forum_page_line {
    height: 40rpx;
    width: 2rpx;
    background-color: #eee;
    margin: 0 20rpx;
}

.dynamic_view1_right image {
    width: 20rpx;
    height: 24rpx;
    vertical-align: middle;
    margin-right: 10rpx;
}

.dynamic_content {
    font-size: 30rpx;
    color: #434343;
    line-height: 50rpx;
    margin-top: 20rpx;
}

.dynamic_content2 {
    font-size: 26rpx;
    color: #434343;
    line-height: 40rpx;
    margin-top: 20rpx;
}

.dynamic_imgbox {
    margin-top: 20rpx;
    font-size: 0;
}

.dynamic_img {
    width: 160rpx;
    height: 160rpx;
    margin-right: 16rpx;
    margin-bottom: 20rpx;
    border-radius: 12rpx;
}

.dynamic_img:nth-child(4n) {
    margin-right: 0;
}

.dynamic_single_bot_view {
    width: 150rpx;
    margin-right: 10rpx;
}

.dynamic_single_bot_text1 {
    font-size: 36rpx;
    color: #CACACA;
    margin-right: 10rpx;
}

.dynamic_single_bot_text2 {
    font-size: 24rpx;
    color: #838383;
}

.plbox {
    margin-top: 15rpx;
    background-color: #fff;
    margin-bottom: 15rpx;
}

.plhead {
    padding: 26rpx 30rpx 0;
}

.plhead_left {
    font-size: 24rpx;
    color: #838383;
    flex: 1;
}

.plhead_right {
    font-size: 24rpx;
    color: #838383;
}

.pl_single {
    padding: 50rpx 30rpx 40rpx;
    border-bottom: 2rpx solid #F8F8F8;
}

.comment {
    width: 100%;
    height: 100rpx;
    padding: 0 30rpx;
    background-color: #F8F8F8;
    position: fixed;
    bottom: 0;
    left: 0;
    box-sizing: border-box;
}

.comment_left {
    width: 614rpx;
    height: 72rpx;
    padding: 0 20rpx;
    line-height: 72rpx;
    color: #434343;
    font-size: 28rpx;
    background-color: #fff;
    box-sizing: border-box;
    border-radius: 12rpx;
    margin: 0;
}

.comment_right {
    width: 47rpx;
    height: 43rpx;
}

.reply_con {
    background: #F6F6F6;
    padding: 30rpx;
    border-radius: 10rpx;
    box-sizing: border-box;
    margin-top: 20rpx;
}

.reply_list {
    margin-bottom: 30rpx;
}

.reply_list:last-child {
    margin-bottom: 0;
}

.forum_page_like {
    font-size: 36rpx;
    color: #CACACA;
    margin-right: 10rpx;
    vertical-align: middle;
}

.dynamic_time {
    font-size: 24rpx;
    color: #969696;
    font-family: Arial,Helvetica,sans-serif;
}

.forum_address {
    margin: 10rpx auto;
}

.forum_address_left {
    width: 21rpx;
    height: 25rpx;
    margin-right: 10rpx;
}

.forum_address_right {
    width: 655rpx;
    height: 38rpx;
    line-height: 38rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 24rpx;
    color: #969696;
}

.alert_del {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 8000;
}

.alert_delbtn {
    width: 690rpx;
    height: 80rpx;
    background-color: #fff;
    text-align: center;
    line-height: 80rpx;
    border-radius: 20rpx;
    position: absolute;
    left: 30rpx;
    bottom: 120rpx;
    font-size: 28rpx;
    color: #0061CE;
}

.alert_delbtn2 {
    width: 690rpx;
    height: 80rpx;
    background-color: #fff;
    text-align: center;
    line-height: 80rpx;
    border-radius: 20rpx;
    position: absolute;
    left: 30rpx;
    bottom: 20rpx;
    font-size: 28rpx;
    color: #0061CE;
}

.zhezhao {
    position: fixed;
    width: 120rpx;
    height: 196rpx;
    top: 20%;
    right: 0;
    background: rgba(0,0,0,0);
    overflow: hidden;
    z-index: 10;
}

.xf {
    position: absolute;
    top: 0;
    right: 20rpx;
    width: 90rpx;
    height: 196rpx;
    z-index: 10;
}

.xf>image {
    width: 90rpx;
    height: 90rpx;
    display: block;
}

.xf>image:last-child {
    margin-top: 16rpx;
}

.forum_page_dynamic_video {
    width: 100%;
    height: 375rpx;
    display: block;
    margin: 30rpx auto;
}

.forum_page_dynamic_audio {
    width: 100%;
    height: 375rpx;
    display: block;
    margin: 30rpx auto;
}

.audio {
    width: 100%;
}

.audio_bg {
    width: 164rpx;
    height: 164rpx;
    display: inline-block;
    position: relative;
    margin: 30rpx 30rpx 30rpx 0;
    background-color: #f6f6f6;
    border-radius: 10rpx;
    overflow: hidden;
}

.audio_bg_view {
    width: 100%;
    left: 0;
    top: 20rpx;
    position: absolute;
    overflow: hidden;
}

.audio_img {
    width: 88rpx;
    height: 88rpx;
    border-radius: 50%;
    margin: auto;
    display: block;
    animation: slideshow 10s linear infinite;
}

@keyframes slideshow {
    0% {
        left: 0;
    }

    100% {
        left: -100%;
    }
}

.audio_title {
    font-size: 24rpx;
    text-align: center;
    margin-top: 10rpx;
}

.audio_control {
    padding: 0 65rpx;
    text-align: center;
    line-height: 65rpx;
}

.audio_time {
    font-size: 20rpx;
    color: #838383;
    display: inline-block;
    float: left;
}

.audio_control_img {
    width: 26rpx;
    height: 32rpx;
}

slider {
    float: left;
}

.audio_progress {
    margin: 0 20rpx;
}

.dynamic_title {
    color: #434343;
    font-size: 32rpx;
    font-weight: 800;
    margin-top: 20rpx;
}

.down_block {
    height: 30rpx;
    line-height: 30rpx;
    position: relative;
    top: 0rpx;
    right: 30rpx;
    display: block;
}

.click_down {
    position: absolute;
    z-index: 10;
    width: 226rpx;
    top: 120rpx;
    right: 30rpx;
    padding: 0 10rpx 10rpx;
    box-sizing: border-box;
    overflow: hidden;
}

.forum_down {
    width: 30rpx;
    height: 30rpx;
    display: block;
    margin: auto;
}

.forum_x {
    transform: rotate(180deg);
}

#triangle-up {
    width: 0;
    height: 0;
    border-left: 16rpx solid transparent;
    border-right: 16rpx solid transparent;
    border-bottom: 16rpx solid rgba(0,0,0,0.4);
    margin-left: 154rpx;
}

.down {
    width: 206rpx;
    box-shadow: rgba(162,162,162,0.2) 0 4rpx 20rpx;
    text-align: center;
    margin: auto;
    background-color: rgba(0,0,0,0.4);
    height: 66rpx;
    line-height: 66rpx;
    overflow: hidden;
}

.down>view {
    width: 68rpx;
    height: 66rpx;
    text-align: center;
    display: inline-block;
}

.down image {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
    margin: auto;
}