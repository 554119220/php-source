page {
    background-color: #fff;
}

.hbj {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.flex1 {
    flex: 1;
}

.relase_title {
    font-size: 28rpx;
    color: #434343;
    padding: 30rpx 30rpx 20rpx;
}

.relase_textarea {
    padding: 10rpx 30rpx;
    width: 100%;
    height: 236rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #232323;
}

.relase_imgbox {
    padding: 20rpx 30rpx;
    margin-top: 30rpx;
    font-size: 0;
    overflow: visible;
}

.relase_img_single {
    margin-right: 16rpx;
    position: relative;
    width: 160rpx;
    height: 160rpx;
    display: inline-block;
    margin-bottom: 20rpx;
    overflow: visible;
    background-color: #F8F8F8;
    text-align: center;
    border-radius: 10rpx;
}

.relase_img_single:nth-child(4n) {
    margin-right: 0;
}

.relase_img {
    width: 160rpx;
    height: 160rpx;
    display: block;
    border-radius: 12rpx;
}

.relase_close_box {
    padding: 20rpx;
    position: absolute;
    right: -30rpx;
    top: -30rpx;
    z-index: 1000;
}

.relase_close {
    width: 36rpx;
    height: 36rpx;
    border-radius: 36rpx;
    background-color: #2F74FD;
    position: relative;
}

.relase_close image {
    width: 18rpx;
    height: 18rpx;
    position: absolute;
    margin: auto;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    display: block;
}

.add_img {
    width: 160rpx;
    height: 160rpx;
    border-radius: 12rpx;
    background-color: #F8F8F8;
    display: inline-block;
    margin-right: 16rpx;
    position: relative;
    margin-bottom: 20rpx;
}

.relase_img_single_img {
    position: absolute;
    width: 98rpx;
    height: 98rpx;
    margin: auto;
    left: 0;
    top: 14rpx;
    right: 0;
    display: block;
}

.relase_img_single_text {
    position: absolute;
    top: 104rpx;
    display: block;
    height: 40rpx;
    font-size: 24rpx;
    width: 160rpx;
    color: #bbbbbb;
    z-index: 100;
}

.sfzd {
    padding: 0 30rpx;
    margin-top: 36rpx;
    margin-bottom: 45rpx;
}

.sfzd_left_select {
    width: 36rpx;
    height: 36rpx;
    background-color: #EEE;
    border-radius: 50%;
}

.sfzd_left_selected {
    width: 36rpx;
    height: 36rpx;
    background-color: #2f74fd;
    border-radius: 50%;
    position: relative;
}

.sfzd_left_selected image {
    width: 20rpx;
    height: 14rpx;
    position: absolute;
    margin: auto;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    display: block;
}

.sfzd_right {
    font-size: 28rpx;
    color: #434343;
    margin-left: 20rpx;
}

.day_money {
    padding: 0 30rpx;
    margin-top: 40rpx;
}

.day_money_left {
    font-size: 28rpx;
    color: #232323;
    flex: 1;
}

.day_money_right {
    font-size: 28rpx;
    color: #232323;
    text-align: right;
    font-family: Arial,Helvetica,sans-serif;
}

.day_money_right text {
    font-size: 18rpx;
}

.relase_day {
    margin-top: 24rpx;
    padding: 0 30rpx;
}

.relase_day_input {
    font-size: 24rpx;
    color: #838383;
    width: 180rpx;
    text-align: right;
}

.all_money_left {
    font-size: 28rpx;
    color: #2f74fd;
    flex: 1;
}

.all_money_right {
    font-size: 28rpx;
    color: #2f74fd;
    text-align: right;
    font-family: Arial,Helvetica,sans-serif;
}

.all_money_right text {
    font-size: 18rpx;
}

.relase_btn {
    margin: 54rpx auto;
    width: 440rpx;
    height: 80rpx;
    text-align: center;
    border-radius: 80rpx;
    background-color: #2F74FD;
    line-height: 80rpx;
    color: #fff;
    font-size: 28rpx;
}

.release_position {
    height: 100rpx;
    border-bottom: 2rpx solid #F3F3F3;
    padding: 0 30rpx;
}

.relase_picker {
    height: 85rpx;
    width: 100%;
    padding: 0 30rpx;
    position: relative;
    box-sizing: border-box;
}

.relase_yjt {
    width: 14rpx;
    height: 27rpx;
    display: block;
    position: absolute;
    margin: auto;
    top: 0;
    bottom: 0;
    right: 30rpx;
}

.release_position_icon {
    font-size: 44rpx;
    color: #000;
}

.release_position_text {
    font-size: 28rpx;
    color: #232323;
    margin-left: 20rpx;
}

.release_position_img {
    width: 14rpx;
    height: 27rpx;
}

.release_position_input {
    border: 0;
    padding: 0;
    font-size: 28rpx;
    color: #232323;
    text-align: right;
    margin: 0;
    position: relative;
    height: 54rpx;
    line-height: 54rpx;
}

.hqwx {
    position: absolute;
    height: 52rpx;
    line-height: 52rpx;
    top: 0rpx;
    right: 0rpx;
    background-color: #2F74FD;
    font-size: 24rpx;
    color: #fff;
    text-align: center;
    z-index: 500;
}

.alertbox {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.7);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 9000;
}

.alert {
    width: 520rpx;
    height: 248rpx;
    background-color: #fff;
    border-radius: 12rpx;
    position: absolute;
    margin: auto;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    box-sizing: border-box;
    padding-top: 60rpx;
}

.alert_title {
    font-size: 28rpx;
    color: #434343;
    text-align: center;
}

.alert_btnbox {
    margin-top: 40rpx;
    text-align: center;
}

.alert_btnbox view {
    width: 192rpx;
    height: 60rpx;
    border-radius: 60rpx;
    color: #fff;
    font-size: 28rpx;
    display: inline-block;
    line-height: 60rpx;
    text-align: center;
    margin-right: 36rpx;
}

.alert_btnbox view:last-child {
    margin-right: 0;
}

.title {
    padding: 0 30rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    width: 100%;
    height: 86rpx;
    line-height: 86rpx;
    overflow: hidden;
}

.title>view {
    width: 90rpx;
}

.title>input {
    flex: 1;
    vertical-align: middle;
    border: 0;
    box-shadow: none;
    height: 70rpx;
}

.title>input:focus {
    outline: none;
    box-shadow: none;
    border: 0;
}

.release_position_image {
    width: 46rpx;
    height: 46rpx;
}

.zhezhao {
    position: fixed;
    width: 750rpx;
    height: 100%;
    top: 0px;
    background: rgba(0,0,0,0.5);
    overflow: hidden;
    z-index: 9999;
}

.tanchu_view {
    position: absolute;
    z-index: 10000;
    left: 0;
    right: 0;
    bottom: 0;
    overflow: hidden;
    background-color: #fff;
}

.tanchu_view>button {
    background: #fff;
    color: #434343;
    border-bottom: 1rpx solid #f6f6f6!important;
    padding: 0;
    height: 114rpx;
    line-height: 114rpx;
}

.tanchu_view>button:hover {
    background: #f6f6f6;
    color: #434343;
    outline: none;
    border: none;
}

.tanchu_view>button::after {
    border: none;
}

.luyin {
    position: fixed;
    width: 750rpx;
    height: 100%;
    top: 0px;
    background: rgba(0,0,0,0);
    overflow: hidden;
    z-index: 9999;
}

.luyin_view,.luyin_view2 {
    width: 300rpx;
    height: 300rpx;
    padding: 20rpx;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    background-color: rgba(0,0,0,0.5);
    display: block;
    border-radius: 10rpx;
    text-align: center;
}

.luyin_view>image,.luyin_view2>image {
    width: 200rpx;
    height: 200rpx;
    display: block;
    margin: auto;
}

.luyin_view>text,.luyin_view2>text {
    color: #fff;
    font-size: 30rpx;
    width: 100%;
}