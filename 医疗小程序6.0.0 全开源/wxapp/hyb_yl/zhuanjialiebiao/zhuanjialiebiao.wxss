page {
    background: #eee;
    padding-bottom: 120rpx;
}

.person__top__wrapper {
    width: 100%;
    box-sizing: border-box;
    padding-left: 28rpx;
    padding-right: 28rpx;
    padding-top: 24rpx;
    padding-bottom: 24rpx;
    border-top: 1rpx solid #e5e5e5;
    border-bottom: 1rpx solid #e5e5e5;
    height: 178rpx;
    margin-top: 30rpx;
    background-color: #fff;
    position: relative;
}

.person__top__avatar {
    border: 1rpx solid #e5e5e5;
    width: 130rpx;
    height: 130rpx;
    overflow: hidden;
    box-sizing: content-box;
    float: left;
}

.person__top__avatar image {
    width: 130rpx;
    height: 130rpx;
    border-radius: 7rpx;
}

.person__top__userinfo {
    right: 0;
    overflow: hidden;
    position: absolute;
    left: 182rpx;
    box-sizing: border-box;
    top: 44rpx;
    color: #000;
    font-family: '微软雅黑';
    font-weight: 500;
    bottom: 44rpx;
}

.person__top__userinfo .h2 {
    width: 300rpx;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 36rpx;
}

.person__top__userinfo .h3 {
    width: 300rpx;
    height: 60rpx;
    font-size: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.person__top__userinfo .h4 {
    width: 300rpx;
    height: 30rpx;
    font-size: 20rpx;
}

.person__top__userinfo::after {
    content: " ";
    display: inline-block;
    height: 17rpx;
    width: 17rpx;
    border-width: 2rpx 2rpx 0 0;
    border-color: #c6c6cb;
    border-style: solid;
    transform: matrix(0.71,0.71,-0.71,0.71,0,0);
    position: absolute;
    top: 50%;
    margin-top: -10rpx;
    right: 30rpx;
}

.person__top__userinfo image {
    display: inline-block;
    height: 34rpx;
    width: 34rpx;
    top: 50%;
    margin-top: -17rpx;
    position: absolute;
    right: 58rpx;
}

button::after {
    border: none;
}

.person__top__wrapper {
    margin-top: 0;
    border-top: none;
}

.search__top .search__input {
    display: block;
    height: 30px;
    text-overflow: clip;
    overflow: hidden;
    white-space: nowrap;
    font-family: UICTFontTextStyleBody;
    min-height: 30px;
    width: 96%;
    box-sizing: border-box;
    padding: 1% 8%;
    font-size: 32rpx;
    line-height: 30px;
    border: 1px solid #949494;
    margin: 2% auto;
    border-radius: 25px;
}

.inputsearch {
    width: 100%;
    box-sizing: border-box;
    position: relative;
}

.search__active {
    position: absolute;
    top: 27%;
    left: 5%;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.lf {
    float: left;
}

.rt {
    float: right;
}

.search {
    width: 100%;
    padding: 20rpx;
    background: #fff;
    overflow: hidden;
}

.search-input {
    position: relative;
    width: 90%;
    float: left;
}

.search-input image {
    width: 35rpx;
    height: 35rpx;
    position: absolute;
    top: 15rpx;
    left: 15rpx;
}

.search-input input {
    width: 100%;
    box-sizing: border-box;
    height: 60rpx;
    line-height: 60rpx;
    border: 1rpx solid #8E8E8E;
    font-size: 28rpx;
    color: #8E8E8E;
    border-radius: 10rpx;
    padding-left: 55rpx;
}

.search-fenlei {
    float: right;
    width: 7%;
    height: 45rpx;
    border: 1rpx solid #8E8E8E;
    border-radius: 10rpx;
    text-align: center;
    margin-top: 8rpx;
}

.modal {
    width: 100%;
    height: 92%;
    background: rgba(0,0,0,0.5);
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
}

.modal-fenlei {
    background: #fff;
    width: 100%;
    min-height: 50%;
    padding: 20rpx;
    padding-left: 5rpx;
    border-top: 1rpx solid #eee;
}

.modal-fenlei view {
    display: inline-block;
    width: 22%;
    padding: 10rpx;
    border: 1rpx solid #8E8E8E;
    font-size: 28rpx;
    color: #8E8E8E;
    text-align: center;
    margin-bottom: 20rpx;
    margin-left: 20rpx;
    border-radius: 10rpx;
}

.box {
    width: 100%;
    background: #fff;
    margin-top: 20rpx;
}

.box:first-child {
    margin-bottom: 20rpx;
}

.box-top {
    width: 100%;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #f4f4f4;
    overflow: hidden;
}

.box-top view:first-child {
    width: 30rpx;
    height: 30rpx;
    margin: 0 20rpx;
    position: relative;
    top: -5rpx;
}

.box-top view:last-child {
    width: 80%;
    font-size: 28rpx;
    color: #8E8E8E;
    letter-spacing: 3rpx;
}

.box-bottom-list {
    width: 100%;
    padding: 20rpx;
    overflow: hidden;
}

.box-bottom-list-img {
    width: 130rpx;
    height: 130rpx;
    position: relative;
}

.box-bottom-list-img .box-bottom-list-imgzj {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.box-bottom-list-img .box-bottom-list-imgzj image {
    border-radius: 50%;
}

.box-bottom-list-img .box-bottom-list-imgpai {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    bottom: 10rpx;
    right: 10rpx;
}

.box-bottom-list-jieshao {
    width: 79%;
}

.zj-name text {
    display: inline-block;
    font-size: 28rpx;
    color: #8E8E8E;
    margin-right: 10rpx;
}

.zj-name text:last-child {
    padding: 3rpx 10rpx 5rpx;
    border: 1rpx solid #fa8d87;
    color: #fa8d87;
    font-size: 22rpx;
    border-radius: 10rpx;
}

.zj-yiyuan {
    width: 100%;
    font-size: 26rpx;
    color: #8E8E8E;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: 8rpx;
}

.zj-jieshao {
    width: 100%;
    font-size: 22rpx;
    color: #B8B8B8;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: 7rpx;
}

.footer {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    padding: 10rpx;
    box-sizing: border-box;
    background: #fff;
    border-top: 1rpx solid #C5C5C5;
}

.footer view {
    display: inline-block;
    width: 25%;
}

.footer view image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 0 auto;
}

.footer view text {
    display: block;
    width: 100%;
    font-size: 25rpx;
    color: #AFAFAF;
    text-align: center;
    margin-top: 5rpx;
}