page {
    background: #fff;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.tab {
    width: 100%;
    border-bottom: 1rpx solid #eee;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 6000;
    background: #fff;
}

.tab text {
    display: inline-block;
    width: 49.8%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
    color: #999;
}

.tab text:last-child {
    border-left: 1rpx solid #eee;
}

.tab text.onon {
    color: #242424;
    font-weight: bold;
}

.questions {
    width: 100%;
    background: #fff;
    margin-top: 90rpx;
}

.doctor-bottom {
    width: 100%;
    padding: 0 20rpx;
}

.doctor-bottom-list {
    width: 100%;
    padding: 20rpx 0;
}

.doctor-wen {
    width: 100%;
    max-height: 120rpx;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    font-size: 25rpx;
    color: #8E8E8E;
    letter-spacing: 3rpx;
    line-height: 40rpx;
}

.doctor-asd {
    width: 100%;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
    overflow: hidden;
}

.doctor-asd navigator {
    display: inline-block;
}

.doctor-asd image {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    position: relative;
    top: 10rpx;
}

.doctor-asd text {
    display: inline-block;
    font-size: 25rpx;
    color: #B8B8B8;
    margin-left: 10rpx;
}

.doctor-asd text.txt-xian {
    width: 6rpx;
    height: 6rpx;
    background: #242424;
    border-radius: 50%;
    position: relative;
    top: -4rpx;
}

.questions-zan {
    float: right;
    width: 20%;
    font-size: 25rpx;
    color: #242424;
    text-align: right;
    margin-top: 5rpx;
}

.questions-zan image {
    width: 30rpx;
    height: 30rpx;
}

.questions-zan text {
    position: relative;
    top: 6rpx;
    right: 10rpx;
    color: #242424;
}

.empty {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    background: #f6f6f6;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 5000;
    padding-top: 300rpx;
}

.empty view:first-child {
    width: 100%;
    height: 360rpx;
}

.empty view:first-child image {
    width: 100%;
    height: 100%;
}

.empty view:last-child {
    width: 100%;
    padding: 10rpx;
    text-align: center;
    font-size: 30rpx;
    color: #bec1c3;
}

.fudai-imgs {
    width: 100%;
    padding: 20rpx 0;
}

.fudai-imgs image {
    width: 110rpx;
    height: 110rpx;
    margin-left: 10rpx;
}

.fudai-imgs image:first-child {
    margin-left: 0;
}

