<view class="box">
    <view class="box-list" wx:for="{{fenli}}" wx:key="guanzhu">
        <view class="box-list-left">
            <image src="{{item.u_thumb}}"></image>
        </view>
        <view class="box-list-right">
            <view>姓名: {{item.u_name}}</view>
            <view>
                <text>与TA对话</text>
            </view>
        </view>
    </view>
</view>
<view class="empty" wx:if="{{myguan.length==0}}">
    <view>
        <image src="/hyb_yl/images/empty.png"></image>
    </view>
    <view>抱歉！还没有内容</view>
</view>
