<view class="tab">
    <text bindtap="tab" class="{{current==index?'onon':''}}" data-index="{{index}}" wx:for="{{tab}}" wx:key="key">{{item}}</text>
</view>
<view wx:if="{{current==0}}">
    <view class="questions">
        <view class="doctor-bottom">
            <view class="doctor-bottom-list" wx:for="{{wenda}}" wx:key="wenda">
                <view class="doctor-asd">
                    <image src="{{item.q_thumb}}"></image>
                    <text style="color:#242424;">{{item.q_username}}</text>
                </view>
                <view class="doctor-wen">
                    <text class="asdf">问</text> {{item.question}}
          </view>
                <view class="fudai-imgs">
                    <image bindtap="previewImages" data-qid="{{item.qid}}" data-src="{{imgs}}" src="{{imgs}}" wx:for="{{item.user_picture}}" wx:for-item="imgs"></image>
                </view>
                <view class="doctor-asd">
                    <navigator openType="navigate" url="/hyb_yl/faxianxq/faxianxq">
                        <image src="{{item.q_docthumb}}"></image>
                        <text style="color:#242424;">{{item.q_dname}}</text>
                    </navigator>
                    <view bindtap="huida" class="questions-zan" data-id="{{item.qid}}" data-index="{{index}}">
                        <image src="/hyb_yl/images/huida.png"></image>
                    </view>
                </view>
                <view class="doctor-wen" wx:if="{{item.hd_question!=''}}">
                    <text class="asdf" style="border:1rpx solid #65EBD0;color:#65EBD0;">答</text> {{item.hd_question}}
          </view>
                <view class="doctor-wen" wx:if="{{item.hd_question!=''}}" wx:for="{{item.da}}">
                    <text class="asdf" style="border:1rpx solid #65EBD0;color:#65EBD0;">答</text> {{item}}
            </view>
                <view class="doctor-wen" wx:for="{{da}}">
                    <text class="asdf" style="border:1rpx solid #65EBD0;color:#65EBD0;">答</text> {{item.da}}
            </view>
            </view>
        </view>
    </view>
    <view class="empty" wx:if="{{wenda.length==0}}">
        <view>
            <image src="/hyb_yl/images/empty.png"></image>
        </view>
        <view>抱歉！还没有内容</view>
    </view>
</view>
<view wx:if="{{current==1}}">
    <view class="questions">
        <view class="doctor-bottom">
            <view class="doctor-bottom-list" wx:for="{{wenda1}}" wx:key="wenda">
                <view class="doctor-asd">
                    <image src="{{item.q_thumb}}"></image>
                    <text style="color:#242424;">{{item.q_username}}</text>
                </view>
                <view class="doctor-wen" data-id="{{item.qid}}">
                    <text class="asdf">问</text> {{item.question}}
          </view>
                <view class="fudai-imgs">
                    <image bindtap="previewImage" data-qid="{{item.qid}}" data-src="{{imgs}}" src="{{imgs}}" wx:for="{{item.user_picture}}" wx:for-item="imgs"></image>
                </view>
                <view class="doctor-asd">
                    <navigator openType="navigate" url="/hyb_yl/faxianxq/faxianxq">
                        <image src="{{item.q_docthumb}}"></image>
                        <text style="color:#242424;">{{item.q_dname}}</text>
                    </navigator>
                    <view bindtap="huida" class="questions-zan" data-id="{{item.qid}}" data-index="{{index}}">
                        <image src="/hyb_yl/images/huida.png"></image>
                    </view>
                </view>
                <view class="doctor-wen" wx:if="{{item.hd_question!=''}}">
                    <text class="asdf" style="border:1rpx solid #65EBD0;color:#65EBD0;">答</text> {{item.hd_question}}</view>
            </view>
        </view>
    </view>
    <view class="empty" wx:if="{{wenda1.length==0}}">
        <view>
            <image src="/hyb_yl/images/empty.png"></image>
        </view>
        <view>抱歉！还没有内容</view>
    </view>
</view>
<view class="form" hidden="{{hide}}">
    <form bindreset="formsubmit" bindsubmit="formsubmit">
        <view catchtap="close" class="close"></view>
        <view class="form-groud">
            <input name="wenti" type="text" value="{{values}}"></input>
            <button formType="submit" plain="true" size="mini">发送</button>
        </view>
    </form>
</view>
