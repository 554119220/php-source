.count-down { width: 750rpx; height: 120rpx; border-bottom: 20rpx solid #efeff4; }
.count-time { height: 100rpx; line-height: 40rpx; font-weight: 600; font-size: 50rpx; text-align: center; }
.count-tilte { height: 56rpx; line-height: 60rpx; text-align: center; }
.content { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; padding: 20rpx; border-bottom: 20rpx solid #efeff4; }
.c-l { display: block; width: 270rpx; height: 270rpx; margin-right: 20rpx; }
.c-r { position: relative; -webkit-flex: 1; flex: 1; }
.r-title { font-size: 32rpx; font-weight: 600; }
.r-bottom { position: absolute; bottom: 0; }
.r-price { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; }
.r-price>view:first-child { color: var(--skin); font-size: 36rpx; font-weight: 600; }
.r-price>view:last-child { text-decoration: line-through; font-size: 26rpx; color: var(--fontb); }
.process-title { font-size: 26rpx; }
.process-title text { color: var(--fontc); }
.process-left { -webkit-flex: 1; flex: 1; }
.process-right { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; width: 100rpx; }
.flex-row { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; padding: 20rpx; border-bottom: 20rpx solid #efeff4; }
.process-rank { margin-top: 20rpx; }
.process-rank, .process-rank>view { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-flex-wrap: wrap; flex-wrap: wrap; -webkit-align-items: center; align-items: center; font-size: 20rpx; color: var(--fontb); }
.process-rank>view { margin-right: 20rpx; }
.process-rank>view>text { display: inline-block; background: var(--fontc); width: 24rpx; height: 24rpx; line-height: 24rpx; margin-right: 5rpx; text-align: center; border-radius: 12rpx; color: var(--white); }
.group-img { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-flex-wrap: wrap; flex-wrap: wrap; -webkit-align-items: center; align-items: center; -webkit-justify-content: center; justify-content: center; padding: 40rpx 20rpx 0; }
.group-img-item { position: relative; width: 90rpx; height: 90rpx; margin: 0 20rpx 20rpx 0; }
.item-img { display: block; width: 90rpx; height: 90rpx; line-height: 90rpx; text-align: center; border-radius: 45rpx; border: 2rpx solid var(--skin); }
.item-dot { width: 90rpx; height: 90rpx; line-height: 90rpx; text-align: center; font-size: 60rpx; border: 2rpx solid var(--fontc); color: var(--fontc); border-radius: 50%; }
.group-f { position: absolute; top: 4rpx; left: -20rpx; width: 60rpx; line-height: 30rpx; text-align: center; border-radius: 4rpx; background: var(--skin); color: var(--white); font-size: 20rpx; }
.num-less { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-flex-wrap: wrap; flex-wrap: wrap; -webkit-align-items: center; align-items: center; -webkit-justify-content: center; justify-content: center; line-height: 60rpx; padding: 20rpx 0; border-bottom: 20rpx solid #efeff4; }
.num-less text { color: var(--skin); }
.num-suc { line-height: 80rpx; color: var(--skin); text-align: center; }
.btn-a,.btn-b { width: 660rpx; height: 90rpx; line-height: 90rpx; text-align: center; background: var(--skin); color: var(--white); border-radius: 10rpx; margin-bottom: 20rpx; }
.btn-b { background: #FF8044; }
