.swiper-tab {display: -webkit-flex;display: flex;height: 89rpx;line-height: 89rpx;text-align: center;border-bottom: 1px solid #eee;-webkit-justify-content: space-around;justify-content: space-around;background: #fff; font-size: 30rpx; color: var(--fontb)}
.on,.swiper-tab .active {height: 88rpx;line-height: 89rpx;border-bottom: 2rpx solid var(--skin);color: var(--skin); font-size: 30rpx}
.item{background: var(--white)}
.list{padding:30rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; box-sizing: border-box; border-bottom: 1rpx solid #ececec;}
.list-lef{ width: 100rpx; height: 100rpx; border-radius: 50%;}
.list-cont{ margin-left:20rpx; -webkit-flex: 1; flex: 1;}
.list-cont .v2{ margin-top:25rpx; font-size: 24rpx; color: var(--fontb)}
.list-rig{ font-size: 30rpx; color:var(--skin); font-weight: 600}
