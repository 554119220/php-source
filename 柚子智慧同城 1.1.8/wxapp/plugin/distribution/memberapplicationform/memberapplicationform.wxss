page{ padding-bottom: 150rpx; background: #f8f8f8}
.ma-tit{ padding-left:20rpx; line-height: 90rpx;}
.btn-wrap{ margin:100rpx 0 0 20rpx; width: 710rpx; height: 90rpx; line-height: 90rpx; text-align: center; background: var(--skin); border-radius: 6rpx; font-size: 34rpx; color: var(--white)}
.white-wrap{ margin-left:20rpx; width:690rpx; padding-left:20rpx; background: var(--white); border-radius: 15rpx;}
.ma-txt{ height: 89rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; line-height: 89rpx; border-bottom: 1rpx solid #dcdcdc;}
.ma-txt:last-child{border-bottom: 0 }
.ma-txt .inp1{ padding-right: 20rpx; -webkit-flex: 1; flex: 1; text-align: right}
.banner{ margin:20rpx 0 0 20rpx; display: block; width: 710rpx; height: 520rpx; border-radius: 10rpx;}
.bg-gray{ background: var(--fontc)}
.fail-reason{ margin:0 20rpx; margin-top:20rpx; padding:20rpx 0 20rpx 20rpx; display: -webkit-flex; display: flex; background: var(--white); border-radius: 15rpx;}
.fail-reason .v2{ -webkit-flex: 1; flex: 1}
.check-warm{ margin:30rpx 0 0 30rpx;display: -webkit-flex;display: flex; -webkit-align-items: center; align-items: center;}
.popWin{ width: 80%; position: fixed; left: 50%; top: 50%; -webkit-transform: translate(-50%,-50%); transform: translate(-50%,-50%); z-index: 12; background: #fff; border-radius: 20rpx;}
.popWinTit,.popWinBot{ height: 88rpx; line-height: 88rpx; font: 32rpx; text-align: center;}
.popWinTit{border-bottom: 1rpx solid #eee;}
.popWinBot{border-top: 1rpx solid #eee; color: #e9472c}
.popWinCont{ box-sizing: border-box; padding:30rpx;height:80vh;overflow:auto;}
.mask{position: fixed; top:0; right: 0; bottom: 0; left: 0; z-index: 11; background: rgba(0,0,0,0.3)}
.txtRed{ color: var(--skin)}
