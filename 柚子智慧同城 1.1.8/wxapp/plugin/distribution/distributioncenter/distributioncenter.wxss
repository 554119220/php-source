page{ padding-bottom: 100rpx; background: #F8F8F8}
.header{ height: 360rpx; background: linear-gradient(to bottom,#FE433F,#FF7A22); text-align: center;}
.header .img1{ margin-top:60rpx; width:160rpx; height: 160rpx; border-radius: 50%; border:2rpx solid var(--white)}
.name{ margin-top:20rpx; color: var(--white) }
.card{ margin:-30rpx 0 0 30rpx; padding:20rpx 0; position: relative; width: 690rpx; border-radius: 10rpx; background: var(--white);box-shadow:0px 0px 14px 0px rgba(5,2,2,0.2);}
.level{ position: relative; top:-25rpx; left: 50%; -webkit-transform: translateX(-50%); transform: translateX(-50%); display: -webkit-flex; display: flex; width: 200rpx; -webkit-align-items: center; align-items: center; -webkit-justify-content: center; justify-content: center; height: 52rpx; line-height: 52rpx; border-radius: 26rpx; border:1rpx solid var(--white); background: linear-gradient(to bottom,#FE433F,#FF7A22)}
.level-img{ width: 26rpx; height: 30rpx;}
.level-txt{ margin-left:20rpx; font-size: 26rpx; font-weight: 600; color: var(--white)}
.card-money{ padding:0 30rpx; box-sizing: border-box;}
.cm-tit{ font-size: 26rpx;}
.cm-money{ margin-top:12rpx; display: -webkit-flex; display: flex;}
.cm-money .v1{ -webkit-flex: 1; flex: 1; font-size: 38rpx; font-weight: 600}
.cm-money .v2{ padding:0 23rpx; height: 34rpx; line-height: 34rpx; border:2rpx solid var(--skin); border-radius: 17rpx; font-size: 26rpx; color: var(--skin)}
.accumulate{ margin-top:32rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; padding:0 30rpx; box-sizing: border-box; font-size: 26rpx;}
.accumulate .v1{ -webkit-flex: 1; flex: 1;}
.accumulate text{ color: var(--skin)}
.right-arrow::after { border-width: 2rpx 2rpx 0 0; }
.item{ margin-top:30rpx; padding:0 30rpx 50rpx 30rpx; display: -webkit-flex; display: flex; -webkit-flex-wrap: wrap; flex-wrap: wrap; -webkit-justify-content: space-between; justify-content: space-between; background: var(--white); box-sizing: border-box;}
.list{ margin-top:30rpx; width: 330rpx; height: 200rpx; border-radius: 6rpx; text-align: center;}
.list image{ margin-top:36rpx; width: 70rpx; height: 60rpx;}
.list .v2{ margin-top:11rpx;}
.commission{ background: #FFEED4}
.order{ background: #FFE9F1}
.detail{ background: #FFEDE6}
.team{ background: #E6FFF3}
.qrcode{ background: #E6F1FF}
.withdrawal{ background: #e3feee}
