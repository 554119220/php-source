page{padding-bottom: 100rpx}
.list{margin-top:20rpx; padding:5rpx 0 20rpx 30rpx; background: var(--white)}
.list-tit{ height: 59rpx; line-height: 59rpx; border-bottom: 1rpx solid #ececec; color: var(--skin); font-size: 26rpx}
.list-money{ margin-top:13rpx; padding-right: 30rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; box-sizing: border-box;}
.list-money .v1{ -webkit-flex: 1; flex: 1; font-size: 30rpx;}
.list-money .v2{ margin-left:30rpx; color: var(--skin); font-weight: 600}
.list-time{ margin-top:18rpx; font-size: 24rpx;}
.noMore{ margin-top:30rpx; text-align: center; color: var(--fontb)}
