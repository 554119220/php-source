page{ padding-bottom: 100rpx}
.swiper-tab {display: -webkit-flex;display: flex;height: 89rpx;line-height: 89rpx;text-align: center;border-bottom: 1px solid #eee;-webkit-justify-content: space-around;justify-content: space-around;background: #fff; font-size: 30rpx; color: var(--fontb)}
.on,.swiper-tab .active {height: 88rpx;line-height: 89rpx;border-bottom: 2rpx solid var(--skin);color: var(--skin); font-size: 30rpx}
.item{ padding-left:30rpx; background: var(--white)}
.list{ margin-top:20rpx; padding-bottom: 20rpx;}
.list-tit{ height: 59rpx; line-height: 59rpx; border-bottom: 1rpx solid #ececec; color: var(--skin); font-size: 26rpx}
.list-money{ margin-top:13rpx; padding-right: 30rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; box-sizing: border-box;}
.list-money .v1{ -webkit-flex: 1; flex: 1; font-size: 30rpx;}
.list-money .v2{ color: var(--skin); font-weight: 600}
.list-time{ margin-top:18rpx; font-size: 24rpx;}
.noMore{ margin-top:30rpx; text-align: center; color: var(--fontb)}
.list-money .txt1{ font-size: 24rpx; color:var(--fontb)}
