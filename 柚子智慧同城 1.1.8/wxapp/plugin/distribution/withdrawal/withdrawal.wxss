page{padding-bottom: 100rpx}
.header,.withdrawal-money,.widthdrawal-style{ background: var(--white)}
.wm-money,.wx-list,.check-warm{ display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center;}
.wm-money{ border-bottom: 1rpx solid #ececec}
.header{ padding-left:30rpx; height: 100rpx; line-height: 100rpx; font-size: 40rpx;}
.withdrawal-money{ margin-top:20rpx; padding:30rpx; box-sizing: border-box;}
.wm-tit{ font-size: 26rpx; color: var(--fontb)}
.wm-money{ margin-top:20rpx; padding-bottom: 10rpx;}
.wm-unit{ font-size: 40rpx; font-weight: 600}
.wm-num{ margin-left: 15rpx; -webkit-flex: 1; flex: 1}
.wm-warm{ margin-top:22rpx; font-size: 26rpx; color: var(--skin)}
.widthdrawal-style{ margin-top:20rpx; padding-bottom: 10rpx;}
.ws-tit{ padding:26rpx 0 0 30rpx}
.ws-item{ padding:20rpx 30rpx 0 30rpx; display: -webkit-flex; display: flex; -webkit-flex-wrap: wrap; flex-wrap: wrap; -webkit-justify-content: flex-start; justify-content: flex-start;}
.wx-list{ margin:0 30rpx 20rpx 0; width: 205rpx; height: 88rpx; -webkit-justify-content: center; justify-content: center; line-height: 88rpx; border:1rpx solid #ececec; border-radius: 6rpx;}
.wx-list:nth-child(3n + 0){margin-right: 0}
.wx-list .img1{ width: 40rpx; height: 40rpx; margin-right: 12rpx}
.ali-img .img1{ width: 50rpx; height: 40rpx;}
.check-warm{ margin:30rpx 0 0 30rpx}
.btn{ margin:60rpx 0 0 45rpx; width: 660rpx; height: 88rpx; line-height: 88rpx; text-align: center; background: var(--skin); color: var(--white); border-radius: 10rpx}
.payList { display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; height: 100rpx;border-top: 1rpx solid #f2f2f2;}
.payList .v1{width: 120rpx;text-align: right;margin-right: 30rpx;}
.payList input{ -webkit-flex: 1; flex: 1}
.txtRed{ color: var(--skin)}
.wx-list.active{ border:1rpx solid var(--skin)}
.popWin{ width: 80%; position: fixed; left: 50%; top: 50%; -webkit-transform: translate(-50%,-50%); transform: translate(-50%,-50%); z-index: 12; background: #fff; border-radius: 20rpx;}
.popWinTit,.popWinBot{ height: 88rpx; line-height: 88rpx; font: 32rpx; text-align: center;}
.popWinTit{border-bottom: 1rpx solid #eee;}
.popWinBot{border-top: 1rpx solid #eee; color: #e9472c}
.popWinCont{ box-sizing: border-box; padding:30rpx;height:80vh;overflow:auto;}
.mask{position: fixed; top:0; right: 0; bottom: 0; left: 0; z-index: 11; background: rgba(0,0,0,0.3)}
