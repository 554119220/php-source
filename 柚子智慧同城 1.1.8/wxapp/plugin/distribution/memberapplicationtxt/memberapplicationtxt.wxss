page{ padding-bottom: 250rpx; background: #f8f8f8}
.banner{ display: block; width: 100%; height: 520rpx;}
.mas-wrap,.ma-swiper,.s-list{ height: 82rpx;}
.ma-left{ margin:0 22rpx 0 20rpx; width: 32rpx; height: 32rpx;}
.ma-swiper{padding-right: 30rpx; -webkit-flex: 1; flex: 1; box-sizing: border-box; line-height: 82rpx;}
.s-list{ display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 1;overflow: hidden;}
.exclusive,.distribution{ margin-top:20rpx; padding:27rpx 0 30rpx 0;}
.exclusive-tit{ -webkit-justify-content: center; justify-content: center;}
.exclusive-tit image{ width: 20rpx; height: 30rpx;}
.exclusive-tit .img1{ margin-right: 22rpx; -webkit-transform:rotateY(180deg); transform:rotateY(180deg);}
.exclusive-tit .img2{ margin-left: 22rpx;}
.distribution-cont{ display: -webkit-flex; display: flex; -webkit-flex-wrap: wrap; flex-wrap: wrap;}
.dc-list{ padding-top: 30rpx; width: 33.333%; text-align: center; font-size: 24rpx}
.dc-list .img1{ width: 82rpx; height: 82rpx; border-radius: 50%;}
.dc-list view{ margin-top:10rpx;}
.footer{ position: fixed; left: 0; bottom: 50rpx; width: 100%; height: 128rpx; background: var(--white);}
.btn-wrap{ margin:19rpx 0 0 20rpx; width: 710rpx; height: 90rpx; line-height: 90rpx; text-align: center; background: var(--skin); border-radius: 6rpx; font-size: 34rpx; color: var(--white)}
.exclusive-cont{padding:20rpx 20rpx 0 20rpx; box-sizing: border-box;}
