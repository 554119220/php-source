page{ background: #f8f8f8}
.hedaer{ padding:0 20rpx; height: 150rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; background: var(--skin); color: var(--white)}
.hedaer-lef{ -webkit-flex: 1; flex: 1;}
.hl-tit{ font-size: 26rpx;}
.hl-money{ margin-top:12rpx;}
.hl-money text{ font-size: 32rpx; font-weight: 600}
.hedaer-rig{ padding:0 20rpx; height: 60rpx; line-height: 60rpx; border-radius: 6rpx; border:1rpx solid var(--white)}
.item{ margin-top:20rpx; padding-left:20rpx; background: var(--white)}
.list{ padding-right: 20rpx; box-sizing: border-box; height: 89rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; line-height: 89rpx; border-bottom: 1rpx solid #ececec}
.list:last-child{border-bottom: 0}
.list-lef{ -webkit-flex: 1; flex: 1;}
.list-rig{ color: var(--skin)}
.right-arrow::after { border-width: 2rpx 2rpx 0 0; border-color: var(--skin); }
.btn{ margin:80rpx 0 0 45rpx; width: 660rpx; height: 88rpx; line-height: 88rpx; text-align: center; background: var(--skin); font-size: 30rpx; color: var(--white); border-radius: 10rpx;}
.popWin{ width: 80%; position: fixed; left: 50%; top: 50%; -webkit-transform: translate(-50%,-50%); transform: translate(-50%,-50%); z-index: 12; background: #fff; border-radius: 20rpx;}
.popWinTit,.popWinBot{ height: 88rpx; line-height: 88rpx; font: 32rpx; text-align: center;}
.popWinTit{border-bottom: 1rpx solid #eee;}
.popWinBot{border-top: 1rpx solid #eee; color: #e9472c}
.popWinCont{ box-sizing: border-box; padding:30rpx;height:80vh;overflow:auto;}
.mask{position: fixed; top:0; right: 0; bottom: 0; left: 0; z-index: 11; background: rgba(0,0,0,0.3)}
