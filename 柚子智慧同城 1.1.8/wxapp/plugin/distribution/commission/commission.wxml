<import  src="/zhy/template/wxParse/wxParse.wxml"  />
<import  src="/zhy/template/temloading/temloading.wxml"  />
<block wx:if="{{show}}">
	<view class="wrap">
		<view class="hedaer">
			<view class="hedaer-lef">
				<view class="hl-tit">分销佣金</view>
				<view class="hl-money">￥<text>{{partner.allcommission}}</text>
				</view>
			</view>
			<view bindtap="onCommissiondetailTap" class="hedaer-rig">佣金明细</view>
		</view>
		<view class="item">
			<view class="list">
				<view class="list-lef">可提现佣金</view>
				<view class="list-rig">{{balance}}元</view>
			</view>
			<view class="list">
				<view class="list-lef">已提现佣金</view>
				<view class="list-rig">{{partner.withdraw_money}}元</view>
			</view>
			<view class="list">
				<view class="list-lef">待打款佣金</view>
				<view class="list-rig">{{partner.wait_money}}元</view>
			</view>
			<view bindtap="checkWarm" class="list">
				<view class="list-lef">用户须知</view>
				<view class="right-arrow"></view>
			</view>
			<block wx:if="{{popWin}}">
				<view class="mask"></view>
			</block>
			<block wx:if="{{popWin}}">
				<view class="popWin">
					<view class="popWinTit">用户须知</view>
					<view class="popWinCont">
						<template  data="{{wxParseData:detail.nodes}}" is="wxParse" />
					</view>
					<view bindtap="agree" class="popWinBot">我已阅读</view>
				</view>
			</block>
		</view>
		<view bindtap="onApplicationTap" class="btn">我要提现</view>
	</view>
</block>
<block wx:if="{{!show}}">
	<block></block>
</block>
