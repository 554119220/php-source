.right-arrow {
    display: inline-block;
    position: relative;
    width: 30rpx;
    height: 30rpx;
}

.right-arrow::after {
    display: inline-block;
    content: " ";
    height: 16rpx;
    width: 16rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #c7c7cc;
    border-style: solid;
    transform: matrix(0.71,0.71,-0.71,0.71,0,0);
    position: absolute;
    top: 50%;
    right: 4rpx;
    margin-top: -9rpx;
}

.down-arrow {
    display: inline-block;
    position: relative;
    width: 40rpx;
    height: 30rpx;
}

.down-arrow::after {
    display: inline-block;
    content: " ";
    height: 18rpx;
    width: 18rpx;
    border-width: 0 2rpx 2rpx 0;
    border-color: #111;
    border-style: solid;
    transform: matrix(0.71,0.71,-0.71,0.71,0,0);
    transform-origin: center;
    transition: transform 0.3s;
    transition: transform 0.3s,-webkit-transform 0.3s;
    position: absolute;
    top: 50%;
    right: 10rpx;
    margin-top: -10rpx;
}

.down-arrow.active::after {
    transform-origin: center;
    transform: rotate(-135deg);
    transition: transform 0.3s;
    transition: transform 0.3s,-webkit-transform 0.3s;
}

.triangle {
    display: inline-block;
    position: relative;
    top: 6rpx;
    margin-left: 10rpx;
    width: 0;
    height: 0;
    border: 10rpx solid transparent;
