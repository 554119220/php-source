page{background: #fff}
.container{ padding-bottom: 100rpx; }
.colorLine { margin-top: 30rpx; }
.info{ margin-bottom: 30rpx; }
.footer{ display: -webkit-flex; display: flex; position: fixed; bottom: 0rpx; left: 0rpx; width: 100%; border-top: 1rpx solid #ddd; }
.footer{ display: -webkit-flex; display: flex; position: fixed; bottom: 0rpx; left: 0rpx; height: 70rpx; width: 100%; padding:15rpx 0; background-color: #f87d6d; }
.footer>button{ -webkit-flex: 1; flex: 1; height: 70rpx; line-height: 70rpx; font-size: 32rpx; background-color: #f87d6d; border-radius: 0rpx; }
.footer>button:nth-child(2){ border-left: 1rpx solid #f6c3c3; }
.pay-footer{ border-top: 1rpx solid #ececec; padding:15rpx 20rpx; }
.pay-footer>button{ width: 710rpx; height: 90rpx; line-height: 90rpx; background-color: #f87d6d; font-size: 34rpx; color: #fff; border-radius: 16rpx; }
.choicePay{ -webkit-animation: showModel 0.3s ease-in-out; animation: showModel 0.3s ease-in-out; }
@-webkit-keyframes showModel{ 0%{ -webkit-transform: translateY(1000rpx); transform: translateY(1000rpx) }
100%{ -webkit-transform: translateY(0rpx); transform: translateY(0rpx) }
}@keyframes showModel{ 0%{ -webkit-transform: translateY(1000rpx); transform: translateY(1000rpx) }
100%{ -webkit-transform: translateY(0rpx); transform: translateY(0rpx) }
}.infoList>view.red{ color: #f87d6d; font-weight: 700; }
.myAddress { color: #666; background: #fff; }
.maCont { display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; padding: 25rpx 30rpx 0 0; }
.maLef { width: 80rpx; }
.maLef image { width: 30rpx; height: 38rpx; margin: 5rpx 0 0 30rpx; }
.maRig { -webkit-flex: 1; flex: 1; }
.maName { display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; line-height: 40rpx; }
.maAddress { line-height: 36rpx; margin-top: 10rpx; }
.dishesLine { width: 100%; height: 10rpx; margin-top: 30rpx; }
.maBot { display: -webkit-flex; display: flex; height: 70rpx; -webkit-justify-content: flex-end; justify-content: flex-end; -webkit-align-items: center; align-items: center; }
.maBot image { width: 30rpx; height: 30rpx; margin-right: 20rpx; }
.maBot text { margin-right: 30rpx; color: #999; }
.colorLine { width: 100%; display: block; height: 8rpx; }
.orderNum { padding: 0 30rpx; height: 90rpx; line-height: 90rpx; border-bottom: 1rpx solid #eee; background: #fff; font-size: 30rpx; color: #666; }
.bort20 { border-top: 20rpx solid #efeff4; }
.orderNum .txtRed { font-size: 28rpx; float: right; }
.qdd { display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; background: #fff; padding: 20rpx; border-bottom: 1rpx solid #eee; }
.qddLefImg { width: 160rpx; height: 160rpx; margin-right: 20rpx; }
.qddRigCont { display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; }
.qddRig { -webkit-flex: 1; flex: 1; }
.qddRigTit, .qddRigCont>view:nth-child(1) { font-size: 26rpx; }
.qddRigCont>view:nth-child(1) { color: #ff552e; }
.qddChoice { background: #fff; margin-top: 20rpx; }
.information { border-top: 20rpx solid #f2f2f2; background: #fff; padding: 15rpx 30rpx 25rpx; font-size: 26rpx; }
.infoList { display: -webkit-flex; display: flex; margin-top: 15rpx; }
.infoList > view:nth-child(1) { white-space: nowrap; }
.infoList > view:nth-child(2) { color: #999; margin-left: 10rpx; }
.verification { border-top: 20rpx solid #f2f2f2; padding: 0 30rpx 40rpx; background: #fff; }
.vfInfo { padding: 20rpx 0; line-height: 36rpx; }
.vfInfo > text:nth-child(1) { color: #333; }
.vfInfo > text:nth-child(2) { color: #999; margin-left: 10rpx; }
.qrCode { display: block; margin: 0 auto; margin-top: 20rpx; }
.vfWram { border-top: 20rpx solid #f2f2f2; background: #fff; padding: 0 30rpx; }
.vfWramcont { color: #999; padding: 20rpx 0 30rpx 0; }
.vfWramcont view { margin-bottom: 10rpx; }
.infoList>view.red{ color: #f87d6d; font-weight: 700; }
