page{ padding-top: 90rpx; }
.flex-status { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-justify-content: space-between; justify-content: space-between; }
.navi-btn { padding: 0 20rpx; height: 56rpx; line-height: 56rpx; border: 1rpx solid #ddd; border-radius: 8rpx; margin-left: 15rpx; }
.swiper-tab { position: fixed; left: 0; top: 0; width: 100%; display: -webkit-flex; display: flex; height: 89rpx; border-bottom: 1px solid #eee; -webkit-justify-content: space-between; justify-content: space-between; }
.swiper-tab { display: -webkit-flex; display: flex; height: 89rpx; text-align: center; border-bottom: 1px solid #eee; -webkit-justify-content: space-between; justify-content: space-between; background: #fff }
.tab-item { line-height: 89rpx; font-size: 30rpx; }
.on,.swiper-tab .active { height: 90rpx; line-height: 88rpx; border-bottom: 2rpx solid #e9472c; color: #e9472c; }
.swiper-tab view:nth-child(1) { margin-left: 30rpx; }
.swiper-tab view:last-child { margin-right: 30rpx; }
