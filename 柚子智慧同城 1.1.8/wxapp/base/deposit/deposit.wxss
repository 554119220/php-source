.info { margin-bottom: 20rpx; padding: 30rpx 15rpx; background: var(--skin); color: #fff; }
.info view:nth-child(1) { margin-bottom: 10rpx; font-size: 36rpx; font-weight: 600; }
.info view:nth-child(2) text { color: #FEAF64; }
.cash { padding: 15rpx 15rpx 0; background: #fff; }
.cash-title { color: #999; }
.cash-input { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; height: 68rpx; line-height: 68rpx; font-size: 36rpx; border-bottom: 1px solid #eee; }
.cash-input view { margin-right: 10rpx; }
.admire { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-flex-wrap: wrap; flex-wrap: wrap; line-height: 32rpx; padding: 20rpx 0; color: #999; }
.admire icon { margin: 0rpx 10rpx 0 0; }
.accept { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; height: 90rpx; padding: 0 40rpx; color: #999; }
.accept text { display: inline-block; margin-left: 10rpx; color: var(--skin); text-decoration: underline; }
wx-checkbox .wx-checkbox-input { width: 24rpx; height: 24rpx; }
wx-checkbox .wx-checkbox-input.wx-checkbox-input-checked::before { width: 24rpx; height: 24rpx; line-height: 24rpx; text-align: center; font-size: 24rpx; color: var(--skin); background: transparent; transform: translate(-50%, -50%) scale(1); -webkit-transform: translate(-50%, -50%) scale(1); }
.btn { width: 400rpx; height: 90rpx; line-height: 90rpx; margin-top: 20rpx; background: var(--skin); border-radius: 10rpx; color: #fff; }
