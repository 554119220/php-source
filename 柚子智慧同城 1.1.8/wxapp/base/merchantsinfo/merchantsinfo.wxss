@import"/wxss/f0_0.wxss";
.item-info,.facility,.introduce,.address-box{background: #fff;}
.facility{ padding: 20rpx; border-radius: 10rpx; box-sizing: border-box; display: -webkit-flex; display: flex; -webkit-flex-wrap: wrap; flex-wrap: wrap;}
.fac-list{ margin:10rpx 0; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center;}
.fac-list image{ margin:0 10rpx 0 20rpx; width: 30rpx; height: 30rpx;}
.goods2{ margin:20rpx 0; padding-bottom: 20rpx; background: var(--white)}
.tit80{ display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; height: 80rpx; border-bottom: 1rpx solid #dcdcdc}
.tit80 .tit-lef{ margin-left:30rpx; border-left:6rpx solid #ff552e; font-size:28rpx; padding-left:9rpx; }
.tit80 .tit-rig{ -webkit-flex: 1; flex: 1; padding-right: 20rpx; box-sizing: border-box; text-align: right; color:var(--fontc);}
.goods-item2{ margin-top:20rpx; padding-bottom: 10rpx; display: -webkit-flex; display: flex;}
.goods-list{ margin-left: 32rpx; width: 330rpx;}
.goods-list .img1{ width: 330rpx; height: 330rpx;}
.gi-tit{ width: 330rpx;}
.pop-allow{ position: fixed; top:0; right: 0; bottom: 0; left: 0; z-index: 27; background: rgba(0, 0, 0, 0.3);}
.star-icon { margin-right: 16rpx; width: 140rpx;height: 22rpx;}
.msg-center-e{ display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center;}
.item-info { padding:20rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; border-bottom: 1px solid #ececec; }
.item-info-left { display: block; width: 120rpx; height: 120rpx; margin-right: 20rpx; border-radius: 50%; }
.item-info-right { -webkit-flex: 1; flex: 1; }
.item-info-name { font-size: 30rpx; font-weight: 600; }
.item-info-hot { display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; margin-top: 16rpx; }
.star-icon { display: block; width: 190rpx; height: 30rpx; }
.item-info-hot .v2 { margin-left:30rpx; -webkit-flex: 1; flex: 1; color: var(--fontc); font-size: 26rpx; }
.item-info-hot .v3{ color: var(--fontc);font-size: 26rpx}
.item-info-name,.item-info-desc{ display: -webkit-flex; display: flex;}
.item-info-name{ margin-top:10rpx; position: relative;}
.item-info-desc { -webkit-justify-content: space-between; justify-content: space-between; margin-top: 20rpx; color: var(--fontb); font-size: 26rpx; }
.item-info-desc .v1{ width: 320rpx;}
.item-info-desc .v2{ color: #0ca8f6}
.item-info-name .tel { position: absolute; top: -10rpx; right: 0; display: block; width: 60rpx; height: 60rpx; }
.address{position: absolute; margin:6rpx 0 0 -30rpx; width: 20rpx; height: 25rpx;}
.ab-top,.ab-bot{padding: 0 20rpx 0 60rpx; color: #999;}
.ab-top{position: relative; padding-top: 20rpx; display: -webkit-flex; display: flex;}
.address-num{position: absolute; top:20rpx; right: 20rpx; color: #0ca8f6;}
.ab-bot{ margin-top:15rpx; display: -webkit-flex; display: flex;}
.tel{color: #e9472c}
.time{ -webkit-flex: 1; flex: 1}
.swiper750,.swiper750 image{ display: block; width: 750rpx; height: 340rpx;}
.rich{ padding:20rpx; box-sizing: border-box}
.address-txt{ width: 500rpx}
.spell { padding: 20rpx 0; margin-bottom: 20rpx; background: #fff; white-space: nowrap; }
.spell-item { margin-left:30rpx; display: inline-block; width: 240rpx; height: 400rpx; box-shadow: 0 0 1px #ccc inset; border-radius: 10rpx; overflow: hidden; }
.spell-item-pic { display: block; width: 240rpx; height: 240rpx; margin-bottom: 10rpx; border-top-left-radius: 10rpx; border-top-right-radius: 10rpx; }
.spell-item-name { width: 240rpx; margin: 0 10rpx; }
.spell-item-price { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-justify-content: space-between; justify-content: space-between; -webkit-align-items: center; align-items: center; padding: 0 10rpx; }
.spell-item-price>view:nth-child(1) { color: var(--skin); font-weight: 600; }
.spell-item-price>view:nth-child(2) { font-size: 22rpx; color: var(--fontc); text-decoration: line-through; }
.spell-item-btn { width: 190rpx; height: 48rpx; line-height: 48rpx; margin: 14rpx auto 0; text-align: center; background: var(--skin); color: #fff; font-size: 26rpx; border-radius: 6rpx; }
.panic {}
.panic-item { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; padding: 20rpx; border-bottom: 1px solid #ececec; background: var(--white); }
.panic-item-nums { position: absolute; bottom: 70rpx; left: 0; width: 100%; display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-justify-content: space-between; justify-content: space-between; }
.panic-item-left { position: relative; width: 240rpx; height: 240rpx; margin-right: 20rpx; overflow: hidden; border-radius: 10rpx; }
.panic-item-left image { width: 100%; height: 100%; }
.panic-item-left view { position: absolute; bottom: 0; left: 0; width: 100%; height: 52rpx; line-height: 52rpx; text-align: center; font-size: 22rpx; color: var(--white); background: var(--skin); }
.panic-item-right { position: relative; -webkit-flex: 1; flex: 1; }
.panic-item-name { font-weight: 600; }
.panic-item-price { position: absolute; bottom: 0; left: 0; display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; width: 100%; }
.panic-price { -webkit-flex: 1; flex: 1; }
.panic-price-new { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; font-size: 26rpx; }
.panic-price-a>text:nth-child(1) { display: inline-block; margin-right: 10rpx; color: var(--skin); font-size: 26rpx; font-weight: 600; }
.panic-price-a>text:nth-child(2) { color: var(--fontc); font-size: 22rpx; text-decoration: line-through; }
.panic-price-b text { background: rgba(254, 67, 63, 0.4); color: var(--skin); padding: 0 20rpx; }
.panic-price-c text { font-size: 22rpx; }
.panic-btn { width: 168rpx; height: 62rpx; line-height: 62rpx; text-align: center; font-size: 26rpx; border-radius: 34rpx; color: var(--white); background: var(--skin); }
.introduce{ margin-top: 20rpx; padding-bottom: 50rpx;}
.qrcode{ display: block; margin:0 auto; margin-top:42rpx; width: 500rpx; height: 500rpx;}
