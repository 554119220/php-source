.search { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; padding: 18rpx 20rpx; background: #fff; margin-bottom: 20rpx; }
.search input { -webkit-flex: 1; flex: 1; height: 64rpx; line-height: 64rpx; padding: 0 10rpx; margin-right: 20rpx; background: #f8f8f8; box-shadow: 0 0 1px #ccc inset; border-radius: 10rpx; }
.search button { width: 110rpx; height: 64rpx; line-height: 64rpx; background: var(--skin); border-radius: 10rpx; }
.van-icon-banner { color: var(--white); }
.nav { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; background: var(--white); margin-bottom: 1rpx; }
.nav-item { -webkit-flex: 1; flex: 1; height: 80rpx; line-height: 80rpx; text-align: center; }
.choose { color: var(--skin); }
.content { background: var(--white); padding: 0 20rpx; }
.item { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; padding: 20rpx 0; border-bottom: 1px solid #ececec; }
.item-left { display: block; width: 200rpx; height: 200rpx; margin-right: 20rpx; border-radius: 10rpx; }
.item-right { position: relative; -webkit-flex: 1; flex: 1; }
.item-name { font-weight: 600; }
.item-price { position: absolute; bottom: 0; width: 100%; font-size: 34rpx; color: var(--skin); font-weight: 600; }
.is-address .txt1{ width: 360rpx;}
.is-headtit{ width: 420rpx;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
