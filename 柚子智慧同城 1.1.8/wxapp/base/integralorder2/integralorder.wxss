page{ background: #f2f2f2; padding-bottom: 200rpx; }
.qdd { display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; background: #fff; padding: 20rpx 30rpx; border-top: 1rpx solid #eee; }
.qddLefImg { width: 130rpx; height: 130rpx; margin-right: 30rpx; }
.qddRigCont { display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; }
.qddRig { -webkit-flex: 1; flex: 1; }
.qddRigTit, .qddRigCont>view:nth-child(1) { font-size: 30rpx; }
.qddRigCont>view:nth-child(1) { color: #f87d6d; }
.qddChoice { background: #fff; border-top: 20rpx solid #f2f2f2; }
.courier, .invite { height: 100rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; border-bottom: 1rpx solid #eee; -webkit-justify-content: space-between; justify-content: space-between; font-size: 30rpx; padding: 0 30rpx; }
.qddCoupons, .qddMessage, .inviteSty { border-top: 20rpx solid #f2f2f2; height: 100rpx; background: #fff; display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; padding: 0 30rpx; -webkit-align-items: center; align-items: center; font-size: 30rpx; }
.qddMessage input, .inviteSty input { text-align: right; color: #999; }
.inviteSty { margin-top: 0; }
.inviteSty:first-child { border-bottom: 1rpx solid #eee; }
.qddWin { position: fixed; z-index: 11; left: 50%; top: 50%; width: 80%; height: 280rpx; background: #fff; margin: -140rpx 0 0 -40%; border-radius: 10rpx; text-align: center; font-size: 30rpx; }
.qddWin view:nth-child(1) { color: #999; }
.qddWin view:nth-child(2) { color: #f87d6d; }
.qddWin view:nth-child(1), .qddWin view:nth-child(2) { margin-top: 70rpx; }
.qddFooter, .qddFooter2 { position: fixed; left: 0; bottom: 0; width: 100%; height: 100rpx; background: #fff; display: -webkit-flex; display: flex; }
.qddFooter > view:nth-child(2), .qddfRig, .qddDctive, .qddBefore, .qddAfter { width: 260rpx; background: #999; color: #fff; font-size: 34rpx; line-height: 100rpx; text-align: center; }
.qddFooter > view:nth-child(1), .qddfLef { -webkit-flex: 1; flex: 1; color: #f87d6d; height: 100rpx; line-height: 100rpx; text-align: center; }
.qddFooter > view:nth-child(1) > text:nth-child(2), .qddfLef > text:nth-child(2) { font-size: 34rpx; }
.qddDctive, .qddAfter { background: #f87d6d; }
.qddBefore, .qddAfter { border-radius: 0; }
.express { display: -webkit-flex; display: flex; -webkit-flex-direction: column; flex-direction: column; }
.radio { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-justify-content: space-between; justify-content: space-between; padding: 0 30rpx; line-height: 100rpx; }
.express label:first-child { border-bottom: 1px solid #eee; }
.choose-address { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-justify-content: space-between; justify-content: space-between; -webkit-align-items: center; align-items: center; height: 90rpx; line-height: 90rpx; }
.choose-address view { font-size: 28rpx; color: #333; }
.choose-address image { width: 10rpx; height: 20rpx; }
.btn { background: #f87d6d; }
.fee { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-justify-content: space-between; justify-content: space-between; height: 90rpx; line-height: 90rpx; width: 690rpx; padding: 0 30rpx; position: fixed; bottom: 100rpx; background: #fff; border-bottom: 1px solid #eee; }
.fee view:last-child { color: #f87d6d; }
.shopflex { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; }
.shoplist { -webkit-flex: 1; flex: 1; }
.shoplist-in { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; width: 100%; }
.picker { -webkit-flex: 1; flex: 1; }
.shoplist image { margin-top: 6rpx; width: 14rpx; height: 28rpx; }
.address { width: 690rpx; }
.button-hover { background: #fff; }
.webChat, .meetPay { height: 120rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; padding: 0 30rpx; border-top: 1rpx #eee solid; }
.payIcon { width: 60rpx; height: 60rpx; margin-right: 30rpx; }
.choicepayTit { line-height: 80rpx; text-align: center; font-size: 30rpx; }
.iconRight { width: 34rpx; height: 24rpx; }
.webChat view:nth-child(2), .meetPay view:nth-child(2) { -webkit-flex: 1; flex: 1; }
.pay-footer{ border-top: 1rpx solid #ececec; padding:15rpx 20rpx; }
.pay-footer>button{ width: 710rpx; height: 90rpx; line-height: 90rpx; background-color: #f87d6d; font-size: 34rpx; color: #fff; border-radius: 16rpx; }
.myAddress { color: #666; background: #fff; }
.maCont { display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; padding: 25rpx 30rpx 0 0; }
.maLef { width: 80rpx; }
.maLef image { width: 30rpx; height: 38rpx; margin: 5rpx 0 0 30rpx; }
.maRig { -webkit-flex: 1; flex: 1; }
.maName { display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; line-height: 40rpx; }
.maAddress { line-height: 36rpx; margin-top: 10rpx; }
.dishesLine { width: 100%; height: 10rpx; margin-top: 30rpx; }
.maBot { display: -webkit-flex; display: flex; height: 70rpx; -webkit-justify-content: flex-end; justify-content: flex-end; -webkit-align-items: center; align-items: center; }
.maBot image { width: 30rpx; height: 30rpx; margin-right: 20rpx; }
.maBot text { margin-right: 30rpx; color: #999; }
.btnWrap { margin-top: 40rpx; }
.btnWrap button { width: 80%; height: 90rpx; text-align: center; line-height: 90rpx; background: #e9472c; color: #fff; }
.c6 { color: #666; }
.addressHref { border-top: 1rpx solid #eee; }
