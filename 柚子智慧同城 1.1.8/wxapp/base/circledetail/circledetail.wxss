page{padding-bottom:120rpx;}
.is-bot{ color: #999; font-size:24rpx;}
.detail-img image{ display: block; width: 100%; margin-top:20rpx;}
.is-midtext{ font-size:30rpx;}
.note-phone{height: 90rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; -webkit-justify-content: space-between; justify-content: space-between; color: #999}
.note-txt{ background: #fff}
.red-btn{width:140rpx;background:#ff552e;color:#fff;text-align:center;line-height:50rpx;border-radius:10rpx;font-size:30rpx;}
.notetxt-red{ line-height:59rpx; border-top:1rpx solid #eee; color: #ff552e; font-size:24rpx; }
.comments-tit{ height: 80rpx; font-size:30rpx; border-bottom:1rpx solid #eee;}
.comments-tit view:nth-child(1){ border-left:6rpx solid #ff552e; margin-left:30rpx;padding-left:9rpx}
.comments-tit view:nth-child(2){ color: #ff552e; margin-right: 30rpx;}
.no-flat{ text-align: center; color: #999; margin-top:68rpx;}
.comments-cont{ min-height: 100rpx;}
.bot-com{ width: 100%; position: fixed; left: 0; bottom:0;}
.pics-box{ margin-top:20rpx;}
.pics-box image{ display: block; width: 100%; margin-bottom: 20rpx;}
.is-address{ display: -webkit-flex; display: flex; padding-bottom: 10rpx;}
.is-address image{ width: 20rpx; height: 25rpx; margin:6rpx 10rpx 0 0;}
.is-time{ color: #999; font-size: 24rpx;}
.is-mid{padding-bottom: 20rpx;}
.release-footer{ position: fixed; left: 0; bottom:0; width: 100%; height: 100rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; background: #f8f8f8; border-top:1rpx solid #eee}
.rl-btn,.release-footer .v2{display: -webkit-flex;display: flex; -webkit-align-items: center; align-items: center;}
.release-footer .v2{ -webkit-flex: 1; flex: 1}
.comments-btn{ width: 220rpx; height: 100rpx; line-height: 100rpx; text-align: center; background: #ff552e; font-size: 30rpx; color: #fff;}
.com-pop-tit{line-height: 80rpx; background:#ff552e;text-align: center; color: #fff}
.com-pop-tit .v1{ position: absolute; left: 30rpx;}
.com-pop-cont{ width: 100%; height: 350rpx; color: #999; padding: 30rpx; box-sizing: border-box;}
.com-pop-bot{ border-top:1rpx solid #eee;}
.cancel-btn{ margin:25rpx auto; width: 80%; height: 66rpx; line-height: 66rpx; background: #ff552e; border: none; color: #fff; font-size: 30rpx;text-align: center; border-radius: 10rpx;}
.no-flat{ margin-top:20rpx; color: #999; text-align: center;}
.rl-btn{ font-size: 28rpx}
.content{ margin:5rpx 0 0 150rpx; padding:0 30rpx 30rpx 0; box-sizing: border-box;}
.c9{font-size: 24rpx; color: #999}
.we-talk{ background: #f8f8f8;}
.talk-wrap{ display: -webkit-flex; display: flex; margin-top:10rpx; padding:15rpx; box-sizing: border-box}
.talk-lef{ margin-right:20rpx; width: 50rpx; height: 50rpx; border-radius: 50%;}
.talk-rig{ padding-top:8rpx; -webkit-flex: 1; flex: 1;}
.talk-rig .txt1{ color: #0ca8f6}
.talk-rig .txt2{ color: #999; font-size: 24rpx;}
.talk-rig .txt3{ margin:0 20rpx 0 10rpx; color: #0ca8f6}
.talk-rig .txt4{ font-size: 24rpx;}
.talk-bot{padding-bottom: 10rpx; font-size: 24rpx; color: #999; text-align: right; border-bottom: 1rpx solid #eee}
.talk-bot .txt2{ font-size: 26rpx; margin:0 20rpx; color: #0ca8f6}
.to-home{ display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center;}
.to-home image{ margin:0 0 0 40rpx; width: 60rpx; height: 60rpx}
.transmit{ margin-left:40rpx}
