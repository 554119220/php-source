page{ padding-bottom: 120rpx; background: #fe8e8c}
.cd-wrap{ margin:20rpx 0 0 20rpx; padding-bottom: 50rpx; width: 710rpx; border:1rpx solid var(--skin); border-radius: 10rpx;}
.store-detail .img1{ margin-right:24rpx; width: 80rpx; height: 80rpx; border-radius: 50%;-webkit-flex-shrink:0;flex-shrink:0;}
.as-cont .img1{ width: 24rpx; height: 32rpx;}
.as-cont .img3{ padding-left:25rpx; width: 68rpx; height: 68rpx; border-left:1rpx solid #dcdcdc;}
.store-detail{ padding:10rpx 50rpx; position: relative; box-sizing: border-box; -webkit-justify-content: center; justify-content: center; font-size: 34rpx; font-weight: 600; border-bottom: 1rpx dashed var(--skin);}
.store-detail::before{position: absolute; content: ''; left: -8rpx; top:92rpx; width: 16rpx; height: 16rpx; background: #fe8e8c; border-radius: 50%; border-top:1rpx solid var(--skin); border-right:1rpx solid var(--skin); -webkit-transform: rotate(45deg); transform: rotate(45deg)}
.store-detail::after{position: absolute; content: ''; right: -8rpx; top:92rpx; width: 16rpx; height: 16rpx; background: #fe8e8c; border-radius: 50%; border-top:1rpx solid var(--skin); border-right:1rpx solid var(--skin); -webkit-transform: rotate(-135deg); transform: rotate(-135deg)}
.store-card{ text-align: center;}
.sc-num{ margin-top:30rpx; font-size: 46rpx; font-weight: 600; color: var(--skin);}
.sc-how{ margin-top:10rpx; font-size: 28rpx; color: #666;}
.sc-btn{ margin:33rpx 0 0 6rpx; width: 660rpx; height: 84rpx; line-height: 84rpx; border-radius: 10rpx; background: linear-gradient(to right, #FF8044, #FE433F); font-size: 32rpx; color: var(--white); text-align: center;}
.sc-people{ margin-top:33rpx; font-size: 24rpx; color: #666;}
.notice{ margin-top:42rpx;}
.left-red-tit{ margin-left:30rpx; padding-left:16rpx; position: relative; font-size:30rpx; font-weight: 600;}
.left-red-tit::before{ position: absolute; content: ''; left: 0; top:7rpx; width: 6rpx; height: 30rpx; border-radius: 3rpx; background: var(--skin); overflow: hidden}
.apply-store{ margin-top:36rpx;}
.as-cont{ margin:24rpx 0 0 46rpx;}
.as-cont .v2{ margin-left:20rpx; width: 500rpx;}
.notice .v1{ margin-top:17rpx; padding:0 30rpx;}
.btn-wrap{ margin-top:30rpx; padding:0 20rpx; display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between;}
.btn-wrap .v1,.btn-wrap .v2{ width: 320rpx; height: 68rpx; line-height: 68rpx; text-align: center; font-size: 30rpx; background: var(--white); border-radius: 34rpx; box-shadow: 0 0 10px rgba(255, 237, 33, 0.43);}
.sc-time{ margin-top:23rpx; font-size: 24rpx;}
.sc-time text{ margin-right:20rpx;}
.notice-cont{ margin:14rpx 0 0 20rpx;}
.verification-qr{ margin:40rpx 20rpx 0 20rpx; border-top:1rpx solid #dcdcdc;}
.vq-tit{ margin-top:27rpx; font-size: 32rpx; font-weight: 600}
.consumer{ margin-top:36rpx; text-align: center; color: var(--skin)}
.vq-warm{ font-size: 26rpx; text-align: center;}
.vq-code{ margin:8rpx auto 0 auto; width: 276rpx; height: 276rpx;}
.vq-inp{ margin:40rpx 0 0 46rpx; width: 560rpx; padding-left:20rpx; height: 68rpx; line-height: 68rpx; border-radius: 10rpx; background: #f8f8f8; border:2rpx solid #ececec;}
