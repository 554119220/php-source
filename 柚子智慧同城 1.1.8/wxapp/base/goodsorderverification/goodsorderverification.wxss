page{padding-bottom: 98rpx;}
.order-status { width: 750rpx; height: 120rpx; line-height: 120rpx; margin-bottom: 20rpx; text-align: center; font-size: 32rpx; color: var(--white); background: linear-gradient(to right, #ff9492, #fe433f); }
.order-status>view:nth-child(1) { line-height: 60rpx; font-size: 28rpx; }
.order-status>view:nth-child(2) { line-height: 40rpx; font-size: 36rpx; font-weight: 600; }
.goods-info { background: var(--white); }
.goods-title { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; height: 80rpx; line-height: 80rpx; border-bottom: 1px solid #dcdcdc; }
.goods-titles { padding-left: 20rpx; -webkit-flex: 1; flex: 1; }
.concat-tel { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; -webkit-justify-content: center; justify-content: center; padding-right: 20rpx; }
.concat-tel image { display: block; width: 34rpx; height: 34rpx; margin-right: 20rpx; }
.goods-content { padding: 20rpx; }
.goods-content { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; }
.goods-content-left { display: block; width: 180rpx; height: 180rpx; }
.goods-content-right { -webkit-flex: 1; flex: 1; }
.goods{ display: -webkit-flex; display: flex; padding: 20rpx; background: #fff; border-bottom: 1rpx solid #eee;}
.g-lef{ width: 180rpx; height: 180rpx; margin-right: 20rpx; border-radius: 12rpx;}
.g-rig{ -webkit-flex: 1; flex: 1; box-sizing: border-box;}
.gr-top{ margin-top:7rpx; display: -webkit-flex; display: flex; font-weight: 600}
.gr-top view:nth-child(1),.gr-cont .v1{ -webkit-flex: 1; flex: 1}
.gr-cont{ margin-top:28rpx; display: -webkit-flex; display: flex; }
.gr-cont .v1{ font-size: 24rpx; color: var(--fontb)}
.gr-cont .v2{ font-weight: 600;}
.gr-bot{ margin-top:37rpx; font-size: 34rpx; font-weight: 600; color: var(--skin)}
.info{ margin-top:20rpx; padding:10rpx 0; background: #fff}
.info-list{padding:15rpx 0; display: -webkit-flex; display: flex;}
.info-list .v1{ padding-left:20rpx; width: 160rpx; color: var(--fontb)}
.info-list .v2{ -webkit-flex: 1; flex: 1; padding-right: 30rpx; box-sizing: border-box}
.order-code{ margin-top:20rpx; padding:20rpx 0}
.oc-head{ display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center;}
.och-lef{ padding-left:20rpx; -webkit-flex: 1; flex: 1; font-size: 24rpx; box-sizing: border-box;}
.och-lef .v2{ margin-top:12rpx;}
.och-rig{ display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center;}
.och-rig .v2{ margin-right: 30rpx; font-size: 34rpx; color: var(--skin); font-weight: 600}
.goi-footer{ position: fixed; left: 0; bottom: 0; width: 100%; height: 98rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; background: var(--white); -webkit-justify-content: flex-end; justify-content: flex-end; border-top:1rpx solid #ececec}
.goi-footer view{ margin-right: 20rpx; padding:0 30rpx; height: 60rpx; line-height: 60rpx; font-size: 26rpx; border-radius: 30rpx; border:1rpx solid #222}
.store{ padding:0 20rpx; display: -webkit-flex; display: flex; height: 80rpx; -webkit-align-items: center; align-items: center; border-bottom: 1rpx solid #ececec}
.store-name{ -webkit-flex: 1; flex: 1; font-size: 30rpx;}
.store-tel{ display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center;}
.store-tel .img1{ margin-right: 16rpx; width: 34rpx; height: 34rpx;}
.info-list .v1.txt-red,.info-list.txt-red{ color: var(--skin)}
.project-list {padding: 36rpx 20rpx 82rpx;}
.icna { width: 29rpx; height: 30rpx; }
.icnr { width: 29rpx; height: 3rpx; }
.choose { display: -webkit-flex; display: flex; margin-top: 16rpx; }
#choose-num { display: inline-block; width: 62rpx; height: 50rpx; border-top: 1rpx solid #ececec; border-bottom: 1rpx solid #ececec; text-align: center; min-height: 0rpx; color: #222; font-weight: 700; font-size: 32rpx; }
.choose view { width: 58rpx; height: 50rpx; border: 1rpx solid #ececec; display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-justify-content: center; justify-content: center; -webkit-align-items: center; align-items: center; font-size: 34rpx; color: #DAD7D7; }
.goi-footer view.red-btn{border:1rpx solid var(--skin); color: var(--white)}
