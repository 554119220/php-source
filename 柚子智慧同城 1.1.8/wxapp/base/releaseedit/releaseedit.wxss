page{padding-bottom:50rpx;}
.header-rig image{width: 30rpx; height: 30rpx;}
.add-img{ width: 150rpx; height: 150rpx; border-radius: 8rpx}
.bg-white{ background: #fff; padding-bottom:20rpx;}
.header-lef{ color: #000; font-size:30rpx; margin-left:30rpx}
.header-rig{ color: #999; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; margin-right:30rpx}
.header-rig image{ margin-right:14rpx;}
.header{ display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; padding-top:30rpx;}
.describe{ width: 100%; height: 240rpx; color: #999;}
.content-add{ width: 150rpx; height: 150rpx; display: inline-block; margin-right:20rpx;}
.content,.upload{ padding:0 30rpx;}
.delete image{ position: absolute; width: 40rpx; height: 40rpx; top: 2rpx; right: 2rpx;}
.content{margin-top:20rpx;}
.edit-user{background: #fff;}
.eu-item{ display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; -webkit-justify-content: space-around; justify-content: space-around; box-sizing: border-box; min-height: 90rpx; border-bottom:1rpx solid #f2f2f2; color: #000; padding: 0 30rpx;}
.eu-item .inp2{ -webkit-flex: 1; flex: 1; text-align: right;}
.eu-item:last-child{border-bottom:0;}
.switch-change{ height: 90rpx; display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; background: #fff; color: #999; -webkit-align-items: center; align-items: center;}
.switch-change view{margin-left:30rpx;}
.switch-change wx-switch{ margin-right:20rpx;}
.btn-orange{ width: 80%; line-height: 90rpx; background: #ff552e; border-radius: 10rpx; color: #fff; font-size:34rpx; margin-top:100rpx;}
.switch-choice{ line-height: 90rpx; background: #fff; color: #ff552e; text-align: center; border-top:2rpx solid #f2f2f2}
.switch-choice text:nth-child(1){ position: absolute; left: 30rpx;}
.eu-item input{ color: #999}
.drawer-screen{ z-index: 12}
.drawer-screen {width:100%;height:100%;position:fixed;top:0;left:0;z-index:10;background:rgba(0,0,0,0.5);overflow:hidden;}
.drawer-box {width:750rpx;overflow:hidden;position:fixed;bottom:0rpx;left:0;z-index:11;background:#fff;font-size:30rpx;}
.db-tit{ line-height: 90rpx; background:#ff552e;text-align: center; color: #fff}
.db-item{ max-height: 350rpx; overflow-y: scroll; color: #000; text-align: center;}
.db-item view{ line-height: 88rpx; border-bottom:1rpx solid #eee;}
.cancel{ font-size:30rpx; width: 80%; margin:25rpx auto; line-height: 66rpx; border:1rpx solid #ff552e; color: #ff552e; text-align: center; border-radius: 10rpx;}
.upload-wrap{ padding:0 30rpx;}
.address,.placeh{ color: var(--fontc)}
.item-rf{display: -webkit-flex;display: flex; -webkit-align-items: center; align-items: center; -webkit-justify-content: flex-end; justify-content: flex-end; font-size: 28rpx; color:var(--fontc); background: var(--white); line-height: 28rpx}
.item-rf .v1{ height: 30rpx; line-height: 30rpx}
.popWin{ width: 90%; position: fixed; left: 50%; top: 50%; -webkit-transform: translate(-50%,-50%); transform: translate(-50%,-50%); z-index: 12; background: #fff; border-radius: 20rpx;}
.popWinTit,.popWinBot{ height: 88rpx; line-height: 88rpx; font: 32rpx; text-align: center;}
.popWinTit{border-bottom: 1rpx solid #eee;}
.popWinBot{border-top: 1rpx solid #eee; color: #e9472c}
.popWinCont{ box-sizing: border-box; padding:30rpx;height:80vh;overflow:auto;}
.mask{position: fixed; top:0; right: 0; bottom: 0; left: 0; z-index: 11; background: rgba(0,0,0,0.3)}
.posting_fee{ -webkit-flex: 1; flex: 1; text-align: right; color: var(--skin); font-weight: 600}
