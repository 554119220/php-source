.arrowRig { width: 12rpx; height: 20rpx; }
.balanceMoney { padding: 0 30rpx; height: 300rpx; background: #e9472c; color: #fff; }
.balanceMoney view:nth-child(1) { padding-top: 60rpx; }
.balanceMoney view:nth-child(2) { margin-top: 50rpx; font-size: 90rpx; }
.el-input { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; height: 90rpx; line-height: 90rpx; padding: 0 20rpx; margin: 20rpx 0 0; border-top: 1px solid #eee; border-bottom: 1px solid #eee; }
.el-input view { margin-right: 20rpx; }
.el-input input { -webkit-flex: 1; flex: 1; height: 90rpx; line-height: 90rpx; }
.admire { padding: 20rpx; display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-align-items: center; align-items: center; background: #eee; }
.admire text { margin-left: 10rpx; color: #999; }
.title { height: 80rpx; line-height: 80rpx; padding: 0 20rpx; color: #999; }
.choose-nav { display: -webkit-flex; display: flex; -webkit-flex-direction: row; flex-direction: row; -webkit-flex-wrap: wrap; flex-wrap: wrap; }
.choose-nav-item { display: -webkit-flex; display: flex; -webkit-flex-direction: column; flex-direction: column; -webkit-align-items: center; align-items: center; -webkit-justify-content: center; justify-content: center; width: 210rpx; height: 100rpx; margin: 0 20rpx 20rpx; box-shadow: 0 0 1px #e9472c; border-radius: 10rpx; color: #e9472c; }
.btn { width: 690rpx; height: 90rpx; line-height: 90rpx; margin: 50rpx auto 0; text-align: center; background: #e9472c; color: #fff; border-radius: 10rpx; }
