.btnbox {
    padding: 40rpx 30rpx;
}

.exa-qtype {
    border-radius: 0rpx;
    margin-top: -5rpx;
}

.itembox {
}

.itembox>label {
    display: block;
    line-height: 84rpx;
}

.itembox-total {
    margin-top: 40rpx;
    line-height: 64rpx;
    color: #576b95;
    font-size: 28rpx;
    padding: 20rpx 0rpx;
    border-top: 1rpx solid #ddd;
}

.exaday-btn {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin-top: 30rpx;
}

.exaday-btn-top {
    width: 300rpx;
}

.exaday-btn-bottom {
    width: 690rpx;
    margin-top: 30rpx;
    margin-bottom: 60rpx;
}

.exaanswerdetails-info {
    padding-top: 0rpx;
    font-size: 28rpx;
    color: #888;
    line-height: 64rpx;
}

.exaanswerdetails-info .til {
    color: #888;
    font-weight: bold;
}

.exaanswerdetails-items {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
}

.exaanswerdetails-items button {
    width: 160rpx;
    margin-bottom: 30rpx;
    margin-top: 10rpx;
}

.exaanswerdetails-items button text {
    font-size: 28rpx;
}