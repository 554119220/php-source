.imgtop {
    width: 100%;
    height: 360rpx;
}

.title {
    height: 68rpx;
    line-height: 68rpx;
    border-bottom: 1rpx solid #ddd;
    margin: 0 30rpx;
}

.img-phone {
    height: 36rpx;
    width: 36rpx;
}

.box1 {
    padding: 0 30rpx;
    border-bottom: 10rpx solid #f5f5f5;
    line-height: 60rpx;
}

.box1-list {
    display: flex;
    align-items: center;
}

.box1-list .shop {
    margin-left: 40rpx;
    margin-right: 30rpx;
}

.list-box {
    padding: 10rpx 0;
    border-bottom: 10rpx solid #f5f5f5;
}

.list {
    line-height: 44rpx;
    padding: 0 30rpx;
    font-size: 26rpx;
    display: flex;
    align-items: center;
}

.list-b {
    width: 50%;
    display: inline-block;
    margin-right: 30rpx;
}

.container {
    padding-bottom: 98rpx;
}

.fix-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98rpx;
    line-height: 98rpx;
    border-radius: 0;
}