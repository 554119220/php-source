page {
    padding-bottom: 50rpx;
}

.list {
    display: flex;
    padding: 30rpx;
}

.list>view {
    flex: 0 0 auto;
    padding-right: 30rpx;
    position: relative;
}

.list>view image {
    height: 160rpx;
    width: 160rpx;
}

.num {
    position: absolute;
    top: 0;
    right: 30rpx;
    height: 40rpx;
    width: 40rpx;
    border-radius: 50%;
    background: #ff5046;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
}

.list2 {
    margin-top: -1rpx;
    border-top: 1rpx solid #ddd;
    border-bottom: 1rpx solid #ddd;
    height: 90rpx;
    display: flex;
    align-items: center;
    padding: 0 30rpx;
}

.btn {
    margin: 95rpx 45rpx 0;
    background-color: #ff5046;
    height: 90rpx;
    color: #fff;
    line-height: 90rpx;
    text-align: center;
    box-sizing: border-box;
}

.btn1 {
    margin: 30rpx 45rpx 0;
    background-color: #fff;
    height: 90rpx;
    border: 2rpx solid #ff5046;
    color: #ff5046;
    line-height: 90rpx;
    text-align: center;
    box-sizing: border-box;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    border-radius: 50px;
}

.gobackindex image {
    width: 50px;
    height: 50px;
    border-radius: 50%;
}

.alcohol-top {
    height: 490rpx;
    background: -webkit-linear-gradient(top,#ff5846,#ff7b46);
    position: relative;
    padding: 145rpx 0 110rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.alcohol-top .deposit-l {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    height: 100%;
}

.alcohol-top .deposit-l .deposit-title {
    font-size: 30rpx;
    color: #fff;
}

.alcohol-top .deposit-l .deposit-num {
    font-size: 110rpx;
    color: #fff;
    line-height: 110rpx;
    margin-bottom: 20rpx;
}

.h134 {
    height: 134rpx;
    background-color: #ffa68c;
    width: 2rpx;
    position: absolute;
    bottom: 110rpx;
    left: 50%;
    transform: translateX(-50%);
}

.deposit-bg {
    position: absolute;
    bottom: 0rpx;
    left: 0rpx;
}

.deposit-bg image {
    width: 750rpx;
    height: 62rpx;
    display: block;
}

.explain {
    margin-top: 80rpx;
    padding: 0 30rpx;
}

.explain .explain-title {
    height: 110rpx;
    border-bottom: 2rpx solid #f7f7f7;
    display: flex;
    align-items: center;
}

.explain .explain-title .explain-l {
    height: 48rpx;
    width: 6rpx;
    background-color: #ff5046;
    margin-right: 20rpx;
}

.explain .explain-title .explain-r {
    font-size: 32rpx;
    color: #222222;
    font-weight: 700;
}

.explain .explain-content {
    margin-top: 45rpx;
    font-size: 28rpx;
    color: #484848;
    line-height: 50rpx;
}