<view class="container">
    <view class="alcohol-top">
        <view class="deposit-l">
            <view class="deposit-title">已存数量</view>
            <view class="deposit-num">{{y_num}}</view>
        </view>
        <view class="deposit-l">
            <view class="deposit-title">可存数量</view>
            <view class="deposit-num">{{k_num}}</view>
        </view>
        <view class="h134"></view>
        <view class="deposit-bg">
            <image src="/style/images/icon/icon104.png"></image>
        </view>
    </view>
    <button bindtap="goWineorder" class="btn">提取</button>
    <button bindtap="goWinetwo" class="btn1">存入</button>
    <view class="explain">
        <view class="explain-title">
            <view class="explain-l"></view>
            <view class="explain-r">存酒说明</view>
        </view>
        <rich-text class="explain-content" nodes="{{alcoholExplain}}"></rich-text>
    </view>
</view>
<view class="gobackindex">
    <navigator openType="redirect" url="/ymktv_sun/pages/booking/index/index">
        <image src="/ymktv_sun/resource/images/home2.png"></image>
    </navigator>
</view>
