.icon-img {
    width: 26rpx;
    height: 26rpx;
    margin-right: 20rpx;
}

.img {
    width: 180rpx;
    height: 180rpx;
    flex: 0 0 auto;
    margin-right: 30rpx;
}

.list {
    padding: 30rpx;
    display: flex;
    border-bottom: 10rpx solid #f5f5f5;
    font-size: 28rpx;
    line-height: 55rpx;
}

.list-name {
    font-size: 30rpx;
    display: flex;
    justify-content: space-between;
    border-bottom: 22rpx;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    border-radius: 50px;
}

.gobackindex image {
    width: 50px;
    height: 50px;
    border-radius: 50%;
}