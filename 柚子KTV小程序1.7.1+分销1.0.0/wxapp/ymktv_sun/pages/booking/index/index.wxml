<view class="container padding-bottom-153">
    <view class="index-head-wrap">
        <swiper autoplay="{{autoplay}}" duration="{{duration}}" indicatorDots="{{indicatorDots}}" interval="{{interval}}">
            <swiper-item style="position:relative;" wx:for="{{banner.lb_imgs}}">
                <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                    <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
                </form>
                <image class="slide-image" mode="aspectFill" src="{{url}}{{item}}"></image>
            </swiper-item>
        </swiper>
        <view class="bg_fff index-head-add-wrap">
            <view class="index-head-add">
                <view class="leftArea">
                    <view class="font-30">
                        <image class="icon_index_36" src="../../../resource/images/new/index-shop.png"></image>{{Branch.b_name}}</view>
                    <view bindtap="godaohang" class="font-26 eellipsis" style="position:relative;">
                        <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                            <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
                        </form>
                        <image class="icon_index_36" src="../../../resource/images/new/index_add.png"></image>地址：{{Branch.address}}</view>
                </view>
                <view bindtap="dianhua" class="rightArea" style="position:relative;">
                    <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                        <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
                    </form>
                    <image mode="widthFix" src="../../../../ymktv_sun/resource/images/phone.png"></image>
                </view>
            </view>
        </view>
    </view>
    <view class="navList_box" wx:if="{{nav_list.nav.length<=0&&showdefaultnav}}">
        <view bindtap="{{item.herf}}" style="position:relative;" wx:for="{{navList}}">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
            </form>
            <view>
                <image class="navList-img" src="{{item.imgSrc}}"></image>
            </view>
            <view>{{item.txt}}</view>
        </view>
    </view>
    <view class="navList_box" wx:if="{{nav_list.nav.length>0}}">
        <view bindtap="gotoUrl" data-url="{{item.url}}" style="position:relative;" wx:for="{{nav_list.nav}}">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
            </form>
            <view>
                <image class="navList-img" src="{{item.pic}}"></image>
            </view>
            <view>{{item.title}}</view>
        </view>
    </view>
    <view class="index-activity color_fff">
        <view bindtap="goCollectlist" class="index-activity-list" style="position:relative;" wx:if="{{shop.is_jkopen==1}}">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
            </form>
            <image src="/ymktv_sun/resource/images/jika.png"></image>
            <view class="leftArea ">
                <view class="font-28 index-activity-title">集卡</view>
                <view class="font-24 index-activity-txt">
                    <view>集齐所有卡片</view>免费领奖</view>
            </view>
        </view>
        <view bindtap="goBargainlist" class="index-activity-list bg_list_2" style="position:relative;" wx:if="{{shop.is_bargainopen==1}}">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
            </form>
            <image src="/ymktv_sun/resource/images/kanjia.png"></image>
            <view class="leftArea">
                <view class="font-28 index-activity-title">砍价</view>
                <view class="font-24 index-activity-txt">
                    <view>邀好友</view>砍到最低价
        </view>
            </view>
        </view>
    </view>
    <view bindtap="goCard" class="index-activity-time-wrap bg_fff" data-id="{{jicard.id}}" data-pic="{{jicard.thumb}}" style="position:relative;" wx:if="{{shop.is_jkopen==1&&jicard.id!=null&&jicard.id!=''}}">
        <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
            <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
        </form>
        <view class="index-activity-time ">
            <view class="index-activity-time-mask"></view>
            <view wx:if="{{shop.is_jkopen==1&&jicard!=null&&jicard!=''&&jicard.thumb!=''}}">
                <image class="index-activity-time-img" mode="aspectFill" src="{{url}}{{jicard.thumb}}"></image>
            </view>
            <view class="index-activity-time-detail">
                <view class="font-30">距离结束仅剩</view>
                <view class="activeTime">
                    <view class="time">{{jicard.clock}}</view>
                </view>
            </view>
            <view class="font-26 index-activity-time-title">集卡</view>
        </view>
        <view class="activity-bottom">
            <view class="font-26">{{jicard.title}}</view>
            <view class="activity-bottom-txt">
                <view style="padding-left:10rpx">
                    <image class="small-head" src="{{item}}" wx:if="{{item!=false}}" wx:for="{{jicard.img}}"></image>
                    <text class="color_999999 font-26 ">{{jicard.new_partnum}}人参与集卡</text>
                </view>
            </view>
            <view class="btn bg_fe5146">马上参与</view>
        </view>
    </view>
    <view bindtap="goBargain" class="index-activity-time-wrap bg_fff" data-id="{{kanbargain.id}}" data-pic="{{kanbargain.pic}}" style="position:relative;" wx:if="{{shop.is_bargainopen==1&&kanbargain!=''&&kanbargain!=null}}">
        <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
            <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
        </form>
        <view class="index-activity-time ">
            <view class="index-activity-time-mask"></view>
            <view wx:if="{{shop.is_bargainopen==1&&kanbargain.id!=''&&kanbargain.id!=null&&kanbargain.pic!=''}}">
                <image class="index-activity-time-img" mode="aspectFill" src="{{url}}{{kanbargain.pic}}"></image>
            </view>
            <view class="index-activity-time-detail">
                <view class="font-30">距离结束仅剩</view>
                <view class="activeTime">
                    <view class="time">{{kanbargain.clock}}</view>
                </view>
            </view>
            <view class="font-26 index-activity-time-title">砍价</view>
        </view>
        <view class="activity-bottom">
            <view class="font-26">{{kanbargain.gname}}</view>
            <view class="activity-bottom-txt font-26 color_999999">
        截止目前已有{{kanbargain.part_num}}人参与砍价
      </view>
            <view class="activity-bottom-txt font-26 color_999999">
        最低
        <text class="color_fe5146 font-32">¥ {{kanbargain.shopprice}}</text>
                <text class="txt-line-through">¥{{kanbargain.marketprice}}</text>
            </view>
            <view class="btn bg_fe5146">立即砍价</view>
        </view>
    </view>
    <view class="choose-box">
        <view>
            <view class="choose-box-title font-30 bg_fff">预约</view>
            <view class="page">
                <view class="page__bd">
                    <view class="weui-tab">
                        <scroll-view>
                            <view class="weui-navbar bg_fff margin-bottom-10">
                                <view bindtap="tabClick" class="weui-navbar__item font-26 {{activeIndex==index?'weui-bar__item_on':''}}" data-id="{{item.id}}" id="{{index}}" style="position:relative;" wx:for="{{tabs}}" wx:key="{{index}}">
                                    <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                                        <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
                                    </form>
                                    <view class="weui-navbar__title">{{item.type_name}}</view>
                                </view>
                                <view class="weui-navbar__slider" style="left: {{sliderLeft}}px; transform: translateX({{sliderOffset}}px); -webkit-transform: translateX({{sliderOffset}}px);"></view>
                            </view>
                        </scroll-view>
                        <view class="weui-tab__panel">
                            <view class="weui-tab__content">
                                <view bindtap="goBookingfirst" class="choose-box-list bg_fff " data-bid="0" data-id="{{item.id}}" data-pic="{{item.big_thumbnail}}" style="position:relative;" wx:for="{{RoomData}}">
                                    <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                                        <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
                                    </form>
                                    <view class="leftArea">
                                        <image class="choose-box-list-img" src="{{url}}{{item.big_thumbnail}}"></image>
                                    </view>
                                    <view class="rightArea">
                                        <view class="choose-box-list-title font-26">{{item.goods_name}}</view>
                                        <view class="choose-box-list-price color_fe5146 font-32">¥ {{item.goods_price}}</view>
                                        <view class="choose-box-list-price color_fe5146 " wx:if="{{item.vip_price}}">
                                            <text>会员价：¥ {{item.vip_price}}</text>
                                        </view>
                                        <view class="btn bg_fe5146" style="bottom:-10rpx">立即预约</view>
                                    </view>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <view class="youzi" wx:if="{{jszc.js_tel!=''||jszc.js_font!=''||jszc.js_logo!=''}}">
        <image src="{{url}}{{jszc.js_logo}}"></image>
        <view>
            <view>{{jszc.js_font}}</view>
            <view>合作电话：{{jszc.js_tel}}</view>
        </view>
        <button bindtap="dianbo" class="bg_fe5146 color_fff">拨打</button>
    </view>
</view>
<login bindtogetuserinfo="updateUserInfo" modalHidden="{{is_modal_Hidden}}"></login>
<view bindtap="goUrl" wx:if="{{adBtn==false&&indexTan.is_open!=0&&indexTan.is_open!=null}}">
    <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
        <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
    </form>
    <view class="mask"></view>
    <view class="new_ad_box">
        <icon catchtap="closeAd" color="#999" size="{{24}}" type="clear"></icon>
        <image src="{{url}}{{indexTan.img1}}"></image>
    </view>
</view>
<canvas canvasId="shareImg" class="canvas" style="width:580px;height:680px"></canvas>
<view bindtap="hidden" class="mask" hidden="{{hidden}}">
    <image class="posterBanner" mode="aspectFill" src="{{prurl}}"></image>
    <view catchtap="save" class="shareBtnbot">保存分享图</view>
</view>
<view wx:if="{{jurisDiction}}">
    <view catchtouchmove="preventTouchMove" class="mask2"></view>
    <view class="modalDlg2">
        <view catchtap="get" class="window3">检查到您没有打开定位权限，是否去设置打开呢</view>
    </view>
</view>
<import src="../../template/nav/nav.wxml"></import>
<template is="tabBar" data="{{tabBar:tabBarList,url:url}}" wx:if="{{nav_list.tab.length<=0}}"></template>
<template is="new_tabBar" data="{{tabBar:nav_list.tab,whichone:whichone}}" wx:if="{{nav_list.tab.length>0}}"></template>
