    margin-top: 10rpx;
}

.index-activity-time-title {
    color: #fff;
    background: #f39839;
    position: absolute;
    top: 22rpx;
    right: 40rpx;
    z-index: 3;
    height: 40rpx;
    line-height: 40rpx;
    padding: 0 8rpx;
}

.small-head {
