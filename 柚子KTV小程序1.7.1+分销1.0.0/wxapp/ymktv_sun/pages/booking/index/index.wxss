.container {
    background: #f5f5f5;
}

.index-head-wrap {
    position: relative;
    margin-bottom: 10rpx;
}

.index-head-add {
    position: relative;
    z-index: 2;
    height: 130rpx;
    border-radius: 10rpx;
    margin-top: -30rpx;
    background: #fff;
    box-shadow: 0 0 10rpx rgba(0,0,0,0.2);
    display: flex;
    align-items: center;
    background: #fff;
}

.index-head-add .leftArea {
    flex: 1 1 auto;
    padding-left: 30rpx;
    margin-right: 20rpx;
    min-width: 0;
}

.index-head-add-wrap {
    padding: 0 20rpx 30rpx 20rpx;
}

.index-head-add .leftArea .font-30 {
    margin-bottom: 8rpx;
}

.index-head-add .rightArea {
    flex: 0 0 auto;
    width: 120rpx;
