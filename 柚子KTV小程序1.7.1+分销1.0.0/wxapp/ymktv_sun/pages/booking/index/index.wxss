    margin-bottom: 10rpx;
}

.index-activity-list {
    height: 190rpx;
    width: 49%;
    display: flex;
    align-items: center;
    position: relative;
}

.index-activity-list .leftArea {
    flex: 1 1 auto;
    padding-left: 20rpx;
    position: absolute;
    top: 30rpx;
    left: 0rpx;
}

.index-activity-list .rightArea {
    flex: 0 0 auto;
    margin: 0 20rpx 0 40rpx;
}

.index-activity-list image {
    width: 350rpx;
    height: 190rpx;
}

.index-activity-title {
    padding-left: 5rpx;
}

.index-activity-txt {
