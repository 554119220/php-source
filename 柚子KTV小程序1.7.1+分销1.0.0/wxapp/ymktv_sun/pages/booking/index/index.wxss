    padding-bottom: 20rpx;
}

.index-activity-time {
    position: relative;
    padding: 20rpx;
    font-size: 0;
}

.index-activity-time-mask {
    position: absolute;
    top: 20rpx;
    left: 20rpx;
    right: 20rpx;
    bottom: 20rpx;
    background: rgba(0,0,0,0.2);
    z-index: 1;
}

.index-activity-time-img {
    width: 100%;
