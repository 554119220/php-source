}

.choose-box-title {
    text-align: center;
    height: 64rpx;
    line-height: 64rpx;
    position: relative;
}

.choose-box-title::before {
    content: " ";
    position: absolute;
    top: 30rpx;
    height: 1rpx;
    background: #fdee00;
    right: 50%;
    margin-right: 50rpx;
    width: 100rpx;
}

.choose-box-title::after {
    content: " ";
    position: absolute;
    top: 30rpx;
    height: 1rpx;
    background: #fdee00;
    left: 50%;
    margin-left: 50rpx;
    width: 100rpx;
}

