page {
    font-size: 28rpx;
    border-top: 1rpx solid #eee;
    padding-bottom: 100rpx;
}

.address,.goodsList,.bookTime,.messsage,.goodsMoney,.goodsOrder {
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #eee;
}

.goodsList,.messsage,.gm1,.goodsOrder {
    display: flex;
}

.address {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.address image {
    width: 48rpx;
    height: 48rpx;
}

.address view {
    width: 600rpx;
    line-height: 42rpx;
}

.gsImg {
    width: 150rpx;
    height: 150rpx;
    margin-right: 30rpx;
}

.gsTit {
    height: 72rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.gsRig {
    flex: 1;
}

.gsNum {
    margin-top: 12rpx;
    font-size: 26rpx;
    color: #999;
}

.gsMoney {
    margin-top: 4rpx;
    color: #ff5046;
}

.messsage view:nth-child(2) {
    flex: 1;
    color: #999;
}

.gm1 view:nth-child(2) {
    color: #ff5046;
}

.goodsOrder {
    justify-content: space-between;
}

.qrCode {
    position: relative;
    padding: 30rpx;
    text-align: center;
    border-bottom: 10rpx solid #eee;
}

.qrCodeimg {
    width: 280rpx;
    height: 280rpx;
    margin: 0 auto;
}

.qrCode view {
    margin-top: 10rpx;
    color: #999;
}

.deleteQrcode {
    position: absolute;
    bottom: 30rpx;
    right: 30rpx;
    width: 28rpx;
    height: 36rpx;
}