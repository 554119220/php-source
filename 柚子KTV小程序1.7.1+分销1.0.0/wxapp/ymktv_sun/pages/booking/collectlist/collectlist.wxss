.collectlist {
    display: flex;
    border-bottom: 1rpx solid #dddddd;
    align-items: center;
}

.collectlist .leftArea {
    flex: 0 0 auto;
    padding: 20rpx;
}

.collectlist .rightArea {
    flex: 1 1 auto;
    position: relative;
}

.collectlist-img {
    width: 190rpx;
    height: 190rpx;
}

.btn {
    color: #fff;
    font-size: 28rpx;
    border-radius: 25rpx;
    position: absolute;
    bottom: 77rpx;
    right: 20rpx;
    width: 140rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    background: #999999;
}

.collectlist-img-title {
    margin-bottom: 30rpx;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    border-radius: 50px;
}

.gobackindex image {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
}