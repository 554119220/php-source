.container {
    background: #f5f5f5;
    padding-bottom: 88rpx;
}

.card-swiper {
    position: relative;
}

.card-swiper-box {
    box-sizing: border-box;
    height: 140rpx;
    border: 1rpx solid #ff5046;
    padding: 10rpx;
    border-radius: 10rpx;
    position: absolute;
    bottom: -80rpx;
    left: 80rpx;
    right: 80rpx;
    background: #fff;
}

.card-time-detail {
    position: absolute;
    top: 40rpx;
    left: 40rpx;
    color: #fff;
    z-index: 2;
    width: 50%;
    height: 100rpx;
}

.countDownTimeView {
    display: flex;
}

.countDownTimeText {
    color: #ff5046;
    background: #fff;
    height: 36rpx;
    line-height: 36rpx;
    text-align: center;
    width: 36rpx;
    margin-top: 10rpx;
}

.small-mao {
    margin: 10rpx;
    height: 36rpx;
    line-height: 36rpx;
}

.txt-box {
    padding: 20rpx;
    background: #fff;
    margin-bottom: 10rpx;
}

.title {
    margin-bottom: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
    border-bottom: 1rpx solid red;
    display: inline-block;
}

.one {
    padding: 30rpx;
    line-height: 50rpx;
    margin-bottom: 10rpx;
}

.one-title {
    margin-bottom: 20rpx;
}

.two {
    justify-content: space-between;
    display: flex;
    text-align: center;
    padding: 0 30rpx;
}

.two image {
    width: 70rpx;
}

.three-img {
    width: 100%;
}

.fix-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    background: #ff5046;
    color: #fff;
}

.shareBtn {
    padding: 10rpx 10rpx 0 10rpx;
    position: fixed;
    right: 20rpx;
    bottom: 220rpx;
    width: 60rpx;
    height: 70rpx;
    background: rgba(254,80,71,0.8);
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    border-radius: 50%;
    line-height: 30rpx;
    z-index: 9;
}

.sharePoster {
    position: fixed;
    z-index: 11;
    left: 50%;
    top: 50%;
    width: 580rpx;
    margin: 0 0 0 -290rpx;
    background: #fff;
    transform: translateY(-50%);
}

.canvas {
    position: absolute;
    top: -3000rpx;
    z-index: 9999;
}

.previewImg {
    top: 0;
    right: 0;
    bottom: 100rpx;
    left: 0;
    z-index: 99999;
    width: 100%;
    position: fixed;
}

.preview button {
    width: 40%;
    position: absolute;
    bottom: 30rpx;
    left: 30%;
    z-index: 999;
    background: #e9472c;
    color: #fff;
}

.shareBtnbot {
    margin-left: -100rpx;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 99999;
    width: 200rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: red;
    font-size: 26rpx;
    background: #e5e1de;
    border-radius: 10rpx;
    text-align: center;
}

.fix-home {
    height: 70rpx;
    width: 70rpx;
    border-radius: 50%;
    position: fixed;
    bottom: 130rpx;
    right: 30rpx;
    z-index: 2;
}

.mask1 {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #7e7e7e;
    z-index: 9000;
    opacity: 0.3;
}

.modalDlg1 {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0rpx;
    left: 0;
    z-index: 9999;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.window1 {
    width: 600rpx;
    height: 80rpx;
    background: #ffffff;
    position: relative;
    margin-top: 300rpx;
    border-radius: 50rpx;
    font-size: 35 rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.window2 {
    width: 600rpx;
    height: 80rpx;
    background: #ffffff;
    position: relative;
    margin-top: 100rpx;
    border-radius: 50rpx;
    font-size: 35 rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    color: red;
}

.mask2 {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #7e7e7e;
    z-index: 9000;
    opacity: 0.3;
}

.modalDlg2 {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0rpx;
    left: 0;
    z-index: 9999;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.window3 {
    width: 580rpx;
    height: 60rpx;
    background: #ffffff;
    position: relative;
    margin-top: 500rpx;
    border-radius: 50rpx;
    font-size: 24rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    color: red;
}

.activeTime {
    display: flex;
    flex-direction: row;
    margin-left: -10rpx;
    margin-top: 10rpx;
    font-size: 30rpx;
}

.time {
    background: rgba(0,0,0,.5);
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    margin: 0 10rpx 0 10rpx;
    border-radius: 10rpx;
}