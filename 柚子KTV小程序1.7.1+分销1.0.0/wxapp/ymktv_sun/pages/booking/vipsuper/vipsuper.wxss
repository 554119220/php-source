page {
    padding-bottom: 50rpx;
}

.border-5 {
    border-bottom: 10rpx solid #f5f5f5;
}

.card {
    width: 690rpx;
    height: 330rpx;
    position: relative;
    margin: 30rpx auto;
    box-shadow: 0 2rpx 20rpx rgba(255,186,182,1);
    border-radius: 34rpx;
    background: -webkit-linear-gradient(top,#ff5846,#ff8046);
}

.card_bg {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    border-radius: 34rpx;
}

.icon_28 {
    width: 28rpx;
    height: 28rpx;
    margin-left: 20rpx;
}

.xufei {
    margin-left: 20rpx;
    font-size: 26rpx;
}

.icon_44 {
    width: 44rpx;
    height: 36rpx;
    margin: 0 20rpx;
}

.my-smg-img {
    width: 120rpx;
    height: 120rpx;
    border: 5rpx solid #fff;
    border-radius: 50%;
    box-sizing: border-box;
    margin-right: 25rpx;
}

.my-smg-img image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.leftTop {
    display: flex;
    position: absolute;
    align-items: center;
    top: 68rpx;
    left: 38rpx;
    line-height: 46rpx;
    color: #fff;
    font-size: 30rpx;
}

.nameimg {
    display: flex;
    align-items: center;
    font-size: 30rpx;
    color: #fff;
    font-weight: 700;
}

.bottom {
    display: flex;
    align-items: flex-end;
    padding: 40rpx 28rpx;
    justify-content: space-between;
}

.bottom_left1 {
    color: #fff;
    margin-top: -15rpx;
    margin-left: 10rpx;
    font-size: 26rpx;
}

.bottom_left2 {
    font-size: 24rpx;
    color: #ff5046;
    width: 140rpx;
    background: #fff;
    text-align: center;
    margin-top: 10rpx;
}

.bottom_right {
    position: absolute;
    bottom: 55rpx;
    left: 50rpx;
    font-size: 24rpx;
    color: #fff;
}

.equity {
    padding: 0 30rpx;
}

.title {
    font-size: 32rpx;
    border-bottom: 2rpx solid #f8f8f8;
    font-weight: 700;
    height: 110rpx;
    display: flex;
    align-items: center;
    color: #222;
}

.title .title-icon {
    height: 48rpx;
    width: 6rpx;
    background-color: #ff5046;
    display: block;
    margin-right: 20rpx;
}

.imgNav {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    margin-bottom: 30rpx;
}

.list-box {
    display: flex;
    flex-wrap: wrap;
    padding: 30rpx;
    text-align: center;
}

.list-box>view {
    width: 33.3%;
    font-size: 28rpx;
    margin-bottom: 30rpx;
}

.title2 {
    text-align: center;
    font-size: 28rpx;
    line-height: 48rpx;
    margin-top: 20rpx;
}

.msg {
    padding: 45rpx 0;
    line-height: 50rpx;
}

.open-member {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 37rpx;
    padding: 0 20rpx;
    height: 67rpx;
    border: 2rpx solid #fff;
    border-radius: 10rpx;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    line-height: 67rpx;
}

.member-equity {
    display: flex;
    margin-top: 45rpx;
    margin-bottom: 40rpx;
    flex-wrap: wrap;
}

.member-equity .equity-list {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 26rpx;
    line-height: 30rpx;
    color: #2e2e2f;
    width: 160rpx;
    height: 160rpx;
    border-radius: 10rpx;
    background-color: #fff;
    box-shadow: 0rpx 0rpx 20rpx #ebebeb;
    margin-right: 16rpx;
    margin-bottom: 20rpx;
    overflow: hidden;
}

.member-equity .equity-list>view {
    width: 160rpx;
    overflow: hidden;
    text-align: center;
}

.member-equity .equity-list:nth-last-of-type(4n) {
    margin-right: 0rpx;
}

.h20 {
    height: 20rpx;
    background-color: #f8f8f8;
}

.member-equity .equity-list image {
    width: 45rpx;
    height: 45rpx;
    margin-bottom: 22rpx;
}

.explain {
    padding: 0 30rpx;
}

.recharge {
    padding: 0 30rpx;
}

.member-recharge {
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-wrap: wrap;
}

.member-recharge .recharge-list {
    width: 330rpx;
    height: 240rpx;
    background-color: #fff;
    box-shadow: 0 0 20rpx #ebebeb;
    margin-top: 30rpx;
    border-radius: 20rpx;
    box-sizing: border-box;
    padding: 40rpx 0 25rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    position: relative;
}

.member-recharge .recharge-list .recharge-list-t {
    font-size: 22rpx;
    color: #111111;
    font-weight: 700;
}

.member-recharge .recharge-list .recharge-list-m {
    font-size: 34rpx;
    color: #ff5046;
}

.member-recharge .recharge-list .recharge-list-m text {
    font-size: 60rpx;
    color: #ff5046;
    font-weight: 700;
}

.member-recharge .recharge-list .recharge-list-b {
    width: 154rpx;
    height: 50rpx;
    background-color: #ff5046;
    color: #fff;
    border-radius: 25rpx;
    font-size: 28rpx;
    line-height: 50rpx;
    text-align: center;
}

.member-recharge .recharge-list .recharge-list-p {
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    padding: 5rpx 13rpx;
    background-color: #ff5046;
    font-size: 18rpx;
    color: #fff;
    border-radius: 8rpx;
}

.balance {
    font-size: 28rpx;
}

.member-equity button {
    margin: 0;
    padding: 0;
}