.container {
    border-top: 1rpx solid #ddd;
    padding-bottom: 98rpx;
}

.detail {
    padding: 20rpx 0;
    background: #fff;
}

.detail-title {
    height: 50rpx;
    line-height: 50rpx;
    border-bottom: 1rpx solid red;
    display: inline-block;
    margin: 0 30rpx 30rpx 30rpx;
}

.play_box {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 11;
}

.btn_wrap {
    padding: 150rpx 30rpx 150rpx 30rpx;
}

.play-container {
    padding: 0;
    align-items: center;
}

.play_colse_btn {
    position: absolute;
    top: 10rpx;
    right: 10rpx;
    z-index: 12;
}

.msg {
    display: flex;
    align-items: center;
    padding: 30rpx;
    line-height: 55rpx;
}

.msg-title {
    line-height: 44rpx;
}

.msg-btn {
    width: 140rpx;
    height: 50rpx;
    line-height: 50rpx;
    padding: 0;
    border-radius: 60rpx;
    margin: 0 0 10rpx 0;
}

.msg_input {
    border-bottom: 1rpx solid #999;
}

.list {
    display: flex;
    padding: 20rpx 86rpx 20rpx 20rpx;
    position: relative;
    border-bottom: 1rpx solid #ddd;
}

.list-img {
    width: 200rpx;
    height: 200rpx;
    flex: 0 0 auto;
    margin-right: 50rpx;
    margin-left: 30rpx;
    margin-top: -80rpx;
    border: 1rpx solid #ddd;
    border-radius: 10rpx;
    background: #fff;
}

.list-img image {
    max-width: 150rpx;
}

.flex-1-1 {
    position: relative;
}

.list-body {
    display: flex;
    justify-content: space-between;
    position: absolute;
    bottom: -1;
    left: 0;
    right: 0;
    line-height: 46rpx;
    align-items: center;
}

.goods-msg {
    padding: 30rpx;
}

.fix_bottom {
    position: fixed;
    bottom: 0;
    right: 0;
    left: 0;
    height: 98rpx;
    line-height: 98rpx;
    display: flex;
    align-items: center;
    padding: 0 10rpx;
    z-index: 2;
    box-shadow: 0 0 1rpx rgba(0,0,0,0.3);
}

.fix_bottom button {
    width: 210rpx;
    height: 70rpx;
    line-height: 70rpx;
    margin: 13rpx;
}

.fix_bottom-car {
    display: flex;
    flex-direction: column;
    align-items: center;
    line-height: 48rpx;
}

.play_nun {
    height: 88rpx;
    line-height: 88rpx;
    padding: 0 20rpx;
    border-bottom: 1rpx solid #ddd;
    justify-content: space-between;
    display: flex;
    align-items: center;
}

.num-box {
    display: flex;
    border: 1px solid #ddd;
    border-radius: 10rpx;
    width: 140rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.num-box-munus {
    width: 38rpx;
    border-right: 1rpx solid #ddd;
    text-align: center;
    line-height: 35rpx;
}

.num-box-add {
    width: 38rpx;
    border-left: 1rpx solid #ddd;
    text-align: center;
    line-height: 46rpx;
}

.num-input {
    text-align: center;
}

.shareBtn {
    padding: 10rpx 10rpx 0 10rpx;
    position: fixed;
    right: 20rpx;
    bottom: 220rpx;
    width: 60rpx;
    height: 70rpx;
    background: rgba(254,80,71,0.8);
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    border-radius: 50%;
    line-height: 30rpx;
    z-index: 9;
}

.sharePoster {
    position: fixed;
    z-index: 11;
    left: 50%;
    top: 50%;
    width: 580rpx;
    margin: 0 0 0 -290rpx;
    background: #fff;
    transform: translateY(-50%);
}

.canvas {
    position: absolute;
    top: -3000rpx;
    z-index: 9999;
}

.previewImg {
    top: 0;
    right: 0;
    bottom: 100rpx;
    left: 0;
    z-index: 99999;
    width: 100%;
    position: fixed;
}

.preview button {
    width: 40%;
    position: absolute;
    bottom: 30rpx;
    left: 30%;
    z-index: 999;
    background: #e9472c;
    color: #fff;
}

.shareBtnbot {
    margin-left: -100rpx;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 99999;
    width: 200rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: red;
    font-size: 26rpx;
    background: #e5e1de;
    border-radius: 10rpx;
    text-align: center;
}

.fix-home {
    height: 60rpx;
    width: 60rpx;
    border-radius: 50%;
    position: fixed;
    bottom: 130rpx;
    right: 30rpx;
    z-index: 2;
}

