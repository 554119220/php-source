.list {
    display: flex;
    border-bottom: 1rpx solid #ddd;
    padding: 20rpx 75rpx 20rpx 30rpx;
    align-items: center;
    position: relative;
}

.dijiang {
    margin-bottom: 120rpx;
}

.list-icon {
    width: 36rpx;
}

.list-img {
    width: 200rpx;
    height: 200rpx;
    border-radius: 10rpx;
    margin: 0 30rpx;
}

.lixt-num {
    height: 50rpx;
    line-height: 50rpx;
    display: flex;
    margin: 20rpx 0;
}

.num-box {
    display: flex;
    border: 1px solid #ddd;
    border-radius: 10rpx;
    width: 140rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-left: 20rpx;
}

.num-box-munus {
    width: 38rpx;
    border-right: 1rpx solid #ddd;
    text-align: center;
    line-height: 35rpx;
}

.num-box-add {
    width: 38rpx;
    border-left: 1rpx solid #ddd;
    text-align: center;
    line-height: 46rpx;
}

.num-input {
    text-align: center;
}

.list-body {
    position: relative;
    overflow: hidden;
}

.list-close-btn {
    position: absolute;
    top: -70rpx;
    right: -55rpx;
}

.fix-box {
    position: fixed;
    bottom: 0;
    right: 0;
    left: 0;
    height: 130rpx;
    display: flex;
    align-items: center;
    box-shadow: 0 0 1rpx rgba(0,0,0,0.3);
    padding: 0 30rpx;
    background: #fff;
}

.fix-box-btn {
    width: 300rpx;
    height: 70rpx;
    line-height: 70rpx;
}

.margin-0-20 {
    margin: 0 20rpx;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    border-radius: 50px;
}

.gobackindex image {
    width: 50px;
    height: 50px;
    border-radius: 50%;
}