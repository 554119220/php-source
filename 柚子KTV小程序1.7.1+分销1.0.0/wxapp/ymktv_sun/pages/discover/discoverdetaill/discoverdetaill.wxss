.container {
    padding-bottom: 98rpx;
}

.find-list {
    display: flex;
    align-items: center;
    padding: 30rpx;
}

.find-img {
    width: 98rpx;
    position: relative;
    margin-right: 30rpx;
}

.find-header {
    width: 98rpx;
    border-radius: 50%;
}

.sex {
    width: 24rpx;
    position: absolute;
    right: 10rpx;
    top: 80rpx;
    background: #fff;
    border-radius: 50%;
}

.list-attention {
    border: 1rpx solid #ff5046;
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0;
    background: #fff;
    margin: 0 0 0 30rpx;
}

.list-attention.no {
    border: 1rpx solid #999999;
    color: #999!important;
}

.list-img1 {
    width: 222rpx;
    height: 222rpx;
    margin-right: 10rpx;
    margin-bottom: 10rpx;
}

.list-img2 {
    width: 220rpx;
    height: 220rpx;
    margin-right: 20rpx;
}

.list-body {
    display: flex;
    padding: 0 30rpx 30rpx 30rpx;
    position: relative;
    flex-wrap: wrap;
}

.list-msg {
    position: relative;
    overflow: hidden;
}

.list-bootom {
    position: relative;
    height: 100rpx;
    align-items: center;
    border-bottom: 10rpx solid #f5f5f5;
    display: flex;
    align-content: center;
    padding: 0 34%;
    justify-content: space-between;
}

.list-bootom image {
    width: 36rpx;
    margin-right: 12rpx;
}

.eellipsis-2 {
    line-height: 44rpx;
    padding: 0 30rpx 30rpx 30rpx;
}

.suggest-list {
    display: flex;
    padding: 30rpx;
    border-bottom: 1rpx solid #ddd;
}

.suggest-container {
    line-height: 36rpx;
}

.fix-bottom {
    height: 98rpx;
    display: flex;
    position: fixed;
    z-index: 2;
    background: #fff;
    left: 0;
    right: 0;
    bottom: 0;
    box-shadow: 0 0 1rpx rgba(0,0,0,0.3);
    align-items: center;
}

.fix-bottom image {
    width: 36rpx;
    margin-right: 20rpx;
    vertical-align: -4rpx;
}

.fix-bottom-list {
    width: 50%;
    text-align: center;
    position: relative;
    height: 98rpx;
    line-height: 98rpx;
}

.fix-bottom-list::before {
    content: "";
    position: absolute;
    width: 2rpx;
    height: 54rpx;
    background: #dddddd;
    top: 21rpx;
    left: 0;
}

.fix-bottom-list:first-child::before {
    content: "";
    background: #fff;
}

.input_null {
    color: #C9C9C9;
}

.release {
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
    box-sizing: border-box;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    padding: 18rpx 0 18rpx 30rpx;
    background-color: #F7F8F7;
    font-size: 28rpx;
    z-index: 999;
}

.release .text {
    width: 604rpx;
    min-height: 34rpx;
    max-height: 102rpx;
    border-width: 15rpx 20rpx;
    border-style: solid;
    border-color: #ffffff;
    line-height: 34rpx;
    font-size: 28rpx;
    background-color: #ffffff;
    border-radius: 4rpx;
}

.release .submit {
    width: 116rpx;
    height: 64rpx;
    line-height: 64rpx;
    text-align: center;
    color: #66CC00;
    flex: 0 0 auto;
}

.suggestno {
    height: 120rpx;
    text-align: center;
    line-height: 160rpx;
    border-bottom: 1rpx solid #ddd;
    margin: 0 60rpx;
}