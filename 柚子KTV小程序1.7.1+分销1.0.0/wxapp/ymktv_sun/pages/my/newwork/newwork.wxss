.container {
    padding-bottom: 98rpx;
}

.top-box {
    height: 300rpx;
    position: relative;
}

.top-box_bg {
    width: 100%;
    height: 300rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}

.top-container {
    display: flex;
    padding: 86rpx 43rpx 0 43rpx;
    align-items: center;
}

.top-container .flex-0-0 image {
    width: 126rpx;
    height: 126rpx;
    border-radius: 50%;
    margin-right: 30rpx;
}

.title {
    line-height: 70rpx;
}

.top_list-box {
    display: flex;
    text-align: center;
    height: 128rpx;
    line-height: 52rpx;
    align-items: center;
    border-bottom: 10rpx solid #f5f5f5;
}

.top_list-box>view {
    width: 50%;
    border-left: 1rpx solid #ddd;
    height: 128rpx;
    padding-top: 16rpx;
    box-sizing: border-box;
}

.top_list-box>view:first-child {
    border-left: none;
}

.erweima {
    position: absolute;
    top: 30rpx;
    right: 0;
    background: #ff5046;
    border-top-left-radius: 20rpx;
    border-bottom-left-radius: 20rpx;
    width: 190rpx;
    height: 100rpx;
    padding: 20rpx;
    box-sizing: border-box;
    line-height: 35rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.img {
    width: 44rpx;
    height: 44rpx;
}

.width36 {
    width: 36rpx;
    height: 36rpx;
    margin-right: 20rpx;
}

.width360 {
    width: 36rpx;
    height: 36rpx;
}

.erweima image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
}

.list-tile {
    height: 94rpx;
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    position: relative;
}

.list-tile::after {
    content: "";
    left: 30rpx;
    right: 0;
    height: 1rpx;
    position: absolute;
    bottom: 0;
    background: #dddddd;
}

.list-tile .iconfont {
    margin-right: 20rpx;
}

.list1 {
    display: flex;
    padding: 20rpx 30rpx;
    text-align: center;
    line-height: 50rpx;
    border-bottom: 10rpx solid #f5f5f5;
}

.list1>view {
    width: 180rpx;
}

.list1>view:first-child {
    margin-right: 140rpx;
}

.list2 {
    display: flex;
    padding: 20rpx 30rpx;
    text-align: center;
    line-height: 50rpx;
    border-bottom: 10rpx solid #f5f5f5;
    flex-wrap: wrap;
}

.list2>view {
    width: 266rpx;
    display: flex;
    margin-left: 50rpx;
}

.font-box {
    width: 56rpx;
}

.list2>view:first-child {
    margin-bottom: 70rpx;
}

.new-input {
    height: 68rpx;
    border-radius: 10rpx;
    border: 1rpx solid #dddddd;
    margin: 0 20rpx;
    padding: 0 20rpx;
    font-size: 26rpx;
}

.btn4 {
    background: #ff5046;
    width: 150rpx;
    height: 50rpx;
    border-radius: 5rpx;
    margin: 30rpx auto;
    line-height: 50rpx;
}

.btn5 {
    background: #ff5046;
    line-height: 88rpx;
    height: 88rpx;
    border-radius: 10rpx;
    margin: 95rpx 20rpx 0 20rpx;
}

.mask,.codemask {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.goos {
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translate(-50%);
    z-index: 9999;
    background: #fff;
    width: 650rpx;
    padding-bottom: 30rpx;
    border-radius: 10rpx;
}

.goodsdet {
    width: 400rpx;
    font-size: 26rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.btn {
    display: flex;
    justify-content: space-around;
    margin-top: 30rpx;
}

.btn>button {
    height: 60rpx;
    width: 160rpx;
    line-height: 60rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    font-size: 28rpx;
    border: 1rpx solid #999;
    background: #fff;
}

.btn>button.confirm {
    background: #ef4c00;
    border: 1rpx solid #ef8200;
    color: #fff;
}

.list-left {
    margin-top: 10rpx;
    display: flex;
}

.img1 {
    height: 130rpx;
    width: 130rpx;
    margin-right: 20rpx;
}

.lists {
    padding: 20rpx;
}

.order-num {
    border-bottom: 1rpx solid #ececec;
    line-height: 40rpx;
    padding: 10rpx 0rpx;
    font-size: 30rpx;
}

.list-up {
    padding: 20rpx 0rpx 0rpx;
}

.left-title {
    line-height: 40rpx;
}