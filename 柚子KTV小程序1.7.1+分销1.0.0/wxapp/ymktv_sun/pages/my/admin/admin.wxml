<form bindsubmit="login">
    <view class="container">
        <image class="bg" src="{{url}}{{background}}" wx:if="{{background}}"></image>
        <view class="mask"></view>
        <view class="input-box">
            <view class="input-list">
                <image class="img" src="../../../resource/images/my/ad.png"></image>
                <input name="account" placeholder="请输入账号" placeholderClass="color_fff" type="text" value="{{account}}"></input>
            </view>
            <view class="input-list">
                <image class="img" src="../../../resource/images/my/mima.png"></image>
                <input name="password" placeholder="请输入密码" placeholderClass="color_fff" type="password" value="{{password}}"></input>
            </view>
        </view>
        <button class="color_fff font-32" formType="submit">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width:100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10;"></button>
            </form>
    登录
  </button>
    </view>
</form>
<view class="gobackindex">
    <navigator openType="redirect" style="position:relative;" url="/ymktv_sun/pages/booking/index/index">
        <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
            <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
        </form>
        <image src="/ymktv_sun/resource/images/home2.png"></image>
    </navigator>
</view>
