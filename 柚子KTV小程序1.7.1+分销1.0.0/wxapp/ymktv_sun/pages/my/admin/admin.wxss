page {
    height: 100%;
}

.container {
    position: relative;
    width: 100%;
    height: 100%;
}

.bg {
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
}

.input-box {
    position: absolute;
    z-index: 11;
    height: 170rpx;
    border: 1rpx solid #beb53e;
    border-radius: 20rpx;
    top: 50%;
    left: 50%;
    width: 600rpx;
    transform: translateX(-50%);
    background: rgba(255,255,255,0.3);
}

.input-list {
    display: flex;
    height: 85rpx;
    align-items: center;
    padding: 0 30rpx;
}

.input-list:first-child {
    border-bottom: 1rpx solid #beb53e;
}

.img {
    width: 36rpx;
    height: 36rpx;
    margin-right: 20rpx;
}

.input-list input {
    flex: 1 1 auto;
    font-size: 28rpx;
}

button {
    position: absolute;
    z-index: 11;
    height: 86rpx;
    border: 1rpx solid #ff5046;
    border-radius: 20rpx;
    bottom: 300rpx;
    left: 50%;
    width: 600rpx;
    transform: translateX(-50%);
    background: #ff5046;
    display: flex;
    align-items: center;
    justify-content: center;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    border-radius: 50px;
}

.gobackindex image {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
}