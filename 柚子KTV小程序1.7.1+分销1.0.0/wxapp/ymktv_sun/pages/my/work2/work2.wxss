.container {
    padding-bottom: 98rpx;
}

.top-box {
    height: 300rpx;
    position: relative;
}

.top-box_bg {
    width: 100%;
    height: 300rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}

.top-container {
    display: flex;
    padding: 86rpx 43rpx 0 43rpx;
}

.top-container .flex-0-0 image {
    width: 126rpx;
    height: 126rpx;
    border-radius: 50%;
    margin-right: 30rpx;
}

.title {
    line-height: 70rpx;
}

.top_list-box {
    display: flex;
    text-align: center;
    line-height: 52rpx;
}

.top_list-box>view {
    margin-right: 40rpx;
}

.erweima {
    position: absolute;
    top: 30rpx;
    right: 0;
    background: #ff5046;
    border-top-left-radius: 20rpx;
    border-bottom-left-radius: 20rpx;
    width: 170rpx;
    height: 100rpx;
    padding: 20rpx;
    box-sizing: border-box;
    line-height: 35rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.img {
    width: 44rpx;
    height: 44rpx;
}

.width36 {
    width: 36rpx;
    height: 36rpx;
    margin-right: 20rpx;
}

.width360 {
    width: 36rpx;
    height: 36rpx;
}

.erweima image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
}

.list-tile {
    height: 94rpx;
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    position: relative;
}

.list-tile::after {
    content: "";
    left: 30rpx;
    right: 0;
    height: 1rpx;
    position: absolute;
    bottom: 0;
    background: #dddddd;
}

.list-tile .iconfont {
    margin-right: 20rpx;
}

.list1 {
    display: flex;
    padding: 20rpx 30rpx;
    text-align: center;
    line-height: 50rpx;
    border-bottom: 10rpx solid #f5f5f5;
    justify-content: space-between;
    align-items: center;
}

.list1>view {
    width: 180rpx;
}

.list2 {
    display: flex;
    padding: 20rpx 30rpx;
    text-align: center;
    line-height: 50rpx;
    border-bottom: 10rpx solid #f5f5f5;
    flex-wrap: wrap;
}

.list2 image {
    vertical-align: -5rpx;
}

.list2>view {
    width: 266rpx;
    display: flex;
    margin-left: 50rpx;
}

.font-box {
    width: 56rpx;
}

.list2>view:first-child {
    margin-bottom: 70rpx;
}

.fix-bottom {
    position: fixed;
    height: 98rpx;
    display: flex;
    bottom: 0;
    right: 0;
    left: 0;
    box-shadow: 0 0 1rpx rgba(0,0,0,0.3);
    justify-content: space-between;
    line-height: 34rpx;
    align-items: center;
    background: #fff;
}

.fix-list {
    width: 33.3%;
    text-align: center;
    color: #999999;
}

.fix-list.active {
    color: #ff5046;
}

.new-input {
    height: 68rpx;
    border-radius: 10rpx;
    border: 1rpx solid #dddddd;
    margin: 30rpx 20rpx 0 20rpx;
    padding: 0 20rpx;
    font-size: 26rpx;
}

.btn4 {
    background: #ff5046;
    width: 120rpx;
    height: 50rpx;
    border-radius: 5rpx;
    margin: 30rpx auto;
    line-height: 50rpx;
    padding: 0;
}

.mask,.codemask {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.goos {
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translate(-50%);
    z-index: 9999;
    background: #fff;
    width: 650rpx;
    padding-bottom: 30rpx;
    border-radius: 10rpx;
}

.goodsdet {
    width: 400rpx;
    font-size: 26rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.btn {
    display: flex;
    justify-content: space-around;
    margin-top: 30rpx;
}

.btn>button {
    height: 60rpx;
    width: 160rpx;
    line-height: 60rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    font-size: 28rpx;
    border: 1rpx solid #999;
    background: #fff;
}

.btn>button.confirm {
    background: #ef4c00;
    border: 1rpx solid #ef8200;
    color: #fff;
}

.list-left {
    margin-top: 10rpx;
    display: flex;
}

.img1 {
    height: 130rpx;
    width: 130rpx;
    margin-right: 20rpx;
}

.lists {
    padding: 20rpx;
}

.order-num {
    border-bottom: 1rpx solid #ececec;
    line-height: 40rpx;
    padding: 10rpx 0rpx;
    font-size: 30rpx;
}

.list-up {
    padding: 20rpx 0rpx 0rpx;
}

.left-title {
    line-height: 40rpx;
}

.code {
    position: absolute;
    top: 20%;
    left: 50%;
    transform: translate(-50%);
    z-index: 1001;
    background: #fff;
    width: 650rpx;
    height: 600rpx;
    padding-bottom: 30rpx;
    border-radius: 10rpx;
}

.qrcode {
    display: flex;
    align-items: center;
    margin: 30rpx auto;
    width: 420rpx;
}

.qrcode>canvas {
    width: 420rpx;
    height: 420rpx;
}

.codetitle {
    text-align: center;
    margin-top: 30rpx;
    font-size: 34rpx;
    color: #ef8200;
}

.showEwm {
    height: 96rpx;
    width: 96rpx;
    border-radius: 50%;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0px 0px 18px 0px rgba(4,0,0,0.055);
    margin-top: 30rpx;
    margin-left: 620rpx;
}

.showEwm>image {
    width: 56rpx;
    height: 56rpx;
}

.userOpe {
    background-color: #fff;
    padding: 34rpx 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    margin-top: 20rpx;
}

.opeartion {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.opeartion image {
    width: 58rpx;
    height: 58rpx;
}

.opeartion text {
    font-size: 26rpx;
    color: #666;
    margin-top: 20rpx;
}

.curPrice {
    font-size: 26rpx;
}