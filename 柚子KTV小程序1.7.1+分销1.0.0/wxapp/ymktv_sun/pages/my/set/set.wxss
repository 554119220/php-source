.container {
    padding-bottom: 98rpx;
}

.set {
    height: 80rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx;
}

.btn {
    height: 50rpx;
    width: 80rpx;
    border-radius: 100rpx;
    padding: 5rpx 5rpx 5rpx 35rpx;
    box-sizing: border-box;
    position: relative;
    margin-right: 30rpx;
}

.btn_a {
    padding: 5rpx;
    background: #ddd;
}

.dot {
    height: 40rpx;
    width: 40rpx;
    border-radius: 50%;
    background: #fff;
}

.txt {
    padding: 0 30rpx;
    text-align: right;
    border-bottom: 1rpx solid #ddd;
    height: 40rpx;
    line-height: 30rpx;
}

button {
    height: 70rpx;
    line-height: 70rpx;
    width: 360rpx;
    margin: 30rpx auto;
}

.fix-bottom {
    position: fixed;
    height: 98rpx;
    display: flex;
    bottom: 0;
    right: 0;
    left: 0;
    box-shadow: 0 0 1rpx rgba(0,0,0,0.3);
    justify-content: space-between;
    line-height: 34rpx;
    align-items: center;
}

.fix-list {
    width: 33.3%;
    text-align: center;
    color: #999999;
}

.fix-list.active {
    color: #ff5046;
}

.bg_4fbefa {
    background: #fdee00;
}

.img {
    width: 44rpx;
    height: 44rpx;
}