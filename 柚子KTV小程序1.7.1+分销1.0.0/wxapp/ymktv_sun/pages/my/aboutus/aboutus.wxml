<view>
    <image mode="widthFix" src="{{url}}{{family.logo}}"></image>
    <view class="title">
    {{family.name}}公司简介
  </view>
    <view class="txt" style="text-align: justify">
        <rich-text nodes="{{family.details}}"></rich-text>
    </view>
    <view class="title">
    联系我们
  </view>
    <view class="txt">
        <view>全国免费服务热线：{{family.phone}}</view>
        <view>总部地址：{{family.address}}</view>
    </view>
    <button bindtap="dianhua" class="font-34 bg_ff5046 color_fff btn" style="position:relative;">
        <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
            <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10"></button>
        </form>
    联系我们
  </button>
</view>
<view class="gobackindex">
    <navigator openType="redirect" style="position:relative;" url="/ymktv_sun/pages/booking/index/index">
        <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
            <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:10;margin-top:0;"></button>
        </form>
        <image src="/ymktv_sun/resource/images/home2.png"></image>
    </navigator>
</view>
