<view class="elCont">
    <view class="address">
        <view>{{details.b_name}}</view>
    </view>
    <view class="elInfo">
        <view>存酒时间：{{details.addtime}}</view>
        <view>订单号：{{details.order_num}}</view>
    </view>
    <view class="goodsList">
        <image class="gsImg" mode="aspectFill" src="{{url}}{{details.imgsrc}}"></image>
        <view class="gsRig">
            <view class="gsTit">{{details.winename}}</view>
            <view class="gsNum">数量：{{details.winenum}}瓶</view>
            <view class="gsName">商户：{{details.b_name}}</view>
        </view>
    </view>
    <view class="qrCode">
        <view>请呼叫客服，或与前台人员核实确认</view>
        <canvas canvasId="qrcode" class="qrCodeimg" mode="aspectFill"></canvas>
        <view>消费时请出示此二维码</view>
    </view>
</view>
