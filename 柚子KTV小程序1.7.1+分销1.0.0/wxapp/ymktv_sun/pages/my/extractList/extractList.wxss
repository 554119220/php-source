page {
    font-size: 28rpx;
}

.elTab {
    height: 90rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    border-bottom: 1rpx solid #eee;
}

.elTabitem {
    line-height: 90rpx;
}

.elTab .active {
    line-height: 90rpx;
    color: #ff5046;
    border-bottom: 1rpx solid #ff5046;
}

.address,.elInfo,.goodsList {
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #eee;
}

.address {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.address image {
    width: 48rpx;
    height: 48rpx;
}

.address view {
    width: 600rpx;
    line-height: 42rpx;
}

.goodsList {
    display: flex;
}

.gsImg {
    width: 150rpx;
    height: 150rpx;
    margin-right: 30rpx;
}

.gsTit {
    height: 72rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.gsRig {
    flex: 1;
}

.gsNum {
    margin-top: 12rpx;
    font-size: 26rpx;
    color: #999;
}

.gsMoney {
    margin-top: 4rpx;
    color: #ff5046;
}

.qrCode {
    padding: 30rpx;
    text-align: center;
    border-bottom: 10rpx solid #eee;
}

.qrCodeimg {
    margin-top: 20rpx;
    width: 280rpx;
    height: 280rpx;
    margin: 0 auto;
}

.qrCode view {
    margin-top: 10rpx;
    color: #999;
}