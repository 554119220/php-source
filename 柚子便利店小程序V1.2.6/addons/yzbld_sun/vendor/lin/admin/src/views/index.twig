    <div class="content-wrapper" id="pjax-container">

        <div style="min-height: calc(100vh - 140px);">
            {% block content %}{% endblock %}
        </div>




        <script>
            $(function () {
                {% for item in script%}
                {{ item |raw}}
                {% endfor %}
            });
        </script>

        {% include 'partials/footer.twig' %}
    </div>



</div>

