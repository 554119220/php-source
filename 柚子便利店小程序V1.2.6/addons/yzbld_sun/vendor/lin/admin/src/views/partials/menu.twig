

{% if item['children'] %}
    <li class="treeview">
        <a href="#">
            <i class="fa {{item['icon']}}"></i>
            <span>{{item['title']}}</span>
            <i class="fa fa-angle-left pull-right"></i>
        </a>
        <ul class="treeview-menu">
            {% for item in  item["children"] %}
                {% include 'partials/menu.twig' with item %}
            {% endfor %}
        </ul>
    </li>
{% else %}
    <li>
        <a href="{{item['url']}}" class="{{item['active']}}" >
            <i class="fa {{item['icon']}}"></i>
            <span>{{item["title"]}}</span>
        </a>
    </li>

{% endif %}