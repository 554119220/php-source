<div {{attributes|raw}}>
    <div class="box-header with-border">
        <h3 class="box-title">{{title}}</h3>
        <div class="box-tools pull-right">
            {% for item in tools %}
                {{item|raw}}
            {% endfor %}
        </div><!-- /.box-tools -->
    </div><!-- /.box-header -->
    <div class="box-body" style="display: block;">
        {{content|raw}}
    </div><!-- /.box-body -->
</div>