<li class="dd-item" data-id="{{branch[keyName]}}">
   <div class="dd-handle">
      {{branch["title"]|raw}}
      <span class="pull-right dd-nodrag">
            <a href="{{path}}&id={{branch[keyName]}}"><i class="fa fa-edit"></i></a>
            <a href="javascript:void(0);" data-id="{{branch[keyName]}}" class="tree_branch_delete"><i class="fa fa-trash"></i></a>
        </span>
   </div>
   {% if branch['children'] %}
   <ol class="dd-list">
       {% for branch in branch['children'] %}
           {% include 'tree/branch.twig' with branch%}
       {% endfor %}
   </ol>
   {% endif %}
</li>