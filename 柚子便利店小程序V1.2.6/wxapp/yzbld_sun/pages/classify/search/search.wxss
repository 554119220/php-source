.searchIcon {
    height: 28rpx;
    width: 28rpx;
    padding: 0rpx 20rpx;
}

.searchForm {
    display: inline-block;
    background: #fff;
    width: 672rpx;
    height: 64rpx;
    padding: 17rpx 39rpx;
}

.search {
    display: flex;
    width: 672rpx;
    height: 64rpx;
    border-radius: 10rpx;
    align-items: center;
    background: #f8f8f8;
}

.search input {
    width: 604rpx;
    font-size: 30rpx;
}

.search button {
    padding-left: 0rpx;
    padding-right: 0rpx;
    height: 64rpx;
    display: flex;
    align-items: center;
    background: transparent;
}

.search button::after {
    border: none;
}

.sTitle {
    height: 78rpx;
    padding: 0rpx 30rpx;
    font-weight: 700;
    color: #999;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.delIcon {
    height: 32rpx;
    width: 32rpx;
}

.word {
    background: #fff;
    width: 710rpx;
    display: flex;
    flex-wrap: wrap;
    padding: 10rpx 20rpx 30rpx;
}

.word view {
    display: block;
    width: 120rpx;
    height: 70rpx;
    box-sizing: border-box;
    border: 1rpx solid #dcdcdc;
    margin: 20rpx 10rpx 0rpx;
    border-radius: 6rpx;
    text-align: center;
    line-height: 70rpx;
    font-size: 24rpx;
}