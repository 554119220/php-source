.header {
    height: 374rpx;
    width: 750rpx;
}

.aboutSrc {
    height: 374rpx;
    width: 750rpx;
}

.body {
    padding: 0rpx 20rpx;
    margin-top: 50rpx;
}

.desc {
    display: block;
    color: #666;
    line-height: 50rpx;
    text-indent: 60rpx;
    letter-spacing: 3rpx;
}

.desc view {
    text-indent: 60rpx;
}

.ct {
    display: flex;
    justify-content: center;
}

.contact {
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: 100rpx 0rpx 200rpx;
    color: #666;
}

.list {
    display: flex;
    align-items: center;
    margin-bottom: 10rpx;
}

.icon {
    width: 36rpx;
    height: 36rpx;
    margin-right: 24rpx;
}

.icon_text {
    font-size: 28rpx;
    line-height: 42rpx;
}

.wxParse {
    line-height: 36rpx;
}

.btn {
    position: fixed;
    bottom: 0;
    left: 0px;
    height: 128rpx;
    width: 100%;
    border-top: 1rpx solid #ececec;
    background: #fff;
}

.dialog {
    margin: 15rpx 20rpx;
    height: 98rpx;
    line-height: 98rpx;
    width: 710rpx;
    background: #ef8200;
    color: #fff;
    font-size: 34rpx;
}