<import src="../../wxParse/wxParse.wxml"></import>
<view class="container">
    <view>
        <view class="header">
            <image class="aboutSrc" src="{{aboutsrc}}"></image>
        </view>
        <view class="body">
            <view class="wxParse">
                <template is="wxParse" data="{{wxParseData:desc.nodes}}"></template>
            </view>
            <view class="ct">
                <view class="contact">
                    <view class="list">
                        <image class="icon" src="../../../../style/images/icon60.png"></image>
                        <text class="icon_text">{{phoneNum}}</text>
                    </view>
                    <view class="list">
                        <image class="icon" src="../../../../style/images/icon61.png"></image>
                        <text class="icon_text">{{email}}</text>
                    </view>
                    <view class="list">
                        <image class="icon" src="../../../../style/images/icon62.png"></image>
                        <text class="icon_text">{{addr}}</text>
                    </view>
                </view>
            </view>
        </view>
        <view class="btn">
            <button bindtap="toDialog" class="dialog">与我联系</button>
        </view>
    </view>
</view>
