.head {
    position: relative;
    height: 346rpx;
    width: 750rpx;
    display: flex;
    justify-content: center;
    background-size: cover;
}

.bghead {
    position: absolute;
    left: 0rpx;
    right: 0rpx;
    width: 750rpx;
    height: 346rpx;
}

.uthumb {
    margin-bottom: 26rpx;
    border: 2rpx solid #fff;
    box-shadow: 0rpx 0rpx 14rpx 0rpx rgba(0,0,0,0.11);
}

.uthumb,.uthumb image {
    width: 156rpx;
    height: 156rpx;
    border-radius: 50%;
}

.uid {
    display: inline-block;
    margin-top: 10rpx;
}

.head-thumb {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 60rpx;
    letter-spacing: 2rpx;
    z-index: 999;
}

.code {
    position: absolute;
    top: 60rpx;
    right: 0rpx;
    display: flex;
    align-items: center;
    width: 128rpx;
    height: 52rpx;
    padding: 0rpx 20rpx;
    border-radius: 26rpx 0rpx 0rpx 26rpx;
    font-weight: 700;
    color: #fff;
    background: #ef8200;
}

.code>image {
    width: 28rpx;
    height: 28rpx;
    margin-left: 10rpx;
}

.user-count {
    display: flex;
    padding: 15rpx 0rpx;
    background: #fff;
}

.user-money {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 375rpx;
    height: 120rpx;
}

.user-money:first-child {
    width: 374rpx;
    border-right: 1rpx solid #ececec;
}

.icon {
    width: 38rpx;
    height: 29rpx;
}

.icon1 {
    width: 49rpx;
    height: 40rpx;
}

.m_up {
    margin-top: 24rpx;
}

.num {
    font-size: 42rpx;
    color: #666;
    font-weight: 700;
}

.m_dowm {
    margin-top: 24rpx;
    display: flex;
    align-items: center;
}

.icon2 {
    width: 44rpx;
    height: 30rpx;
}

.m_dowm text {
    margin-left: 24rpx;
}

.block {
    background: #fff;
    margin-top: 20rpx;
}

.block-up {
    padding: 0rpx 30rpx;
    display: flex;
    height: 86rpx;
    justify-content: space-between;
    align-items: center;
}

.rightIcon {
    margin-left: 20rpx;
}

.block-up view {
    display: flex;
    align-items: center;
    color: #666;
}

.block-down {
    display: flex;
    justify-content: space-around;
    align-items: center;
    height: 154rpx;
    border-top: 1rpx solid #ececec;
    font-size: 22rpx;
    margin: 0rpx 20rpx;
}

.block-down view {
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #000;
}

.icn {
    width: 52rpx;
    height: 52rpx;
    margin-bottom: 20rpx;
}

.icon {
    width: 34rpx;
    height: 34rpx;
    margin-right: 34rpx;
}

.session {
    background: #fff;
    padding-left: 30rpx;
    margin-top: 20rpx;
}

.session .list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 30rpx;
    border-bottom: 1rpx solid #ececec;
    height: 92rpx;
    color: #666;
}

.session .list:last-child {
    border-bottom: 1rpx solid #fff;
}

.list view {
    display: flex;
    align-items: center;
}

.ses-last {
    margin-bottom: 100rpx;
}

.listRight {
    display: flex;
    align-items: center;
    font-size: 24rpx;
    color: #222;
}

.concat {
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    font-size: 28rpx;
    background: #fff;
    color: #666;
    display: flex;
    align-items: center;
}

.concat::after {
    border: none;
}

.mask {
    position: fixed;
    top: 0;
    border: 0;
    left: 0;
    range: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1001;
    width: 100%;
    height: 100%;
}

.codeImg {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -250rpx;
    margin-top: -250rpx;
    width: 500rpx;
    height: 500rpx;
    z-index: 1002;
    background: #fff;
    animation: fadeInDown 0.6s 1;
}

.codeImg>image {
    width: 500rpx;
    height: 500rpx;
}

.codeImg>canvas {
    width: 500rpx;
    height: 500rpx;
}

@keyframes fadeInDown {
    from {
        opacity: 0;
        transform: translate3d(0,-100%,0);
    }

    to {
        opacity: 1;
        transform: translate3d(0,0,0);
    }
}