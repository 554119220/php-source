.header {
    width: 750rpx;
    height: 120rpx;
    background-image: -webkit-linear-gradient(180deg,rgb(255,100,15)0%,rgb(255,155,66)100%);
    display: flex;
    justify-content: space-between;
}

.header>view {
    padding: 0rpx 20rpx;
    color: #fff;
    font-size: 32rpx;
    display: flex;
    align-items: center;
}

.btnGroup {
    display: flex;
}

.btnGroup>view {
    width: 144rpx;
    height: 52rpx;
    text-align: center;
    line-height: 52rpx;
    border: 1rpx solid #fff;
    border-radius: 6rpx;
    font-size: 30rpx;
    margin-right: 10rpx;
}

.right {
    width: 12rpx;
    height: 21rpx;
    margin-left: 20rpx;
}

.session {
    margin-top: 20rpx;
    background: #fff;
}

.shop {
    height: 82rpx;
    width: 710rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
    font-size: 30rpx;
}

.contact {
    display: flex;
    align-items: center;
    margin-left: 0rpx;
    margin-right: 0rpx;
    background: #fff;
    padding-right: 0rpx;
    padding-left: 0rpx;
    font-size: 28rpx;
}

.contact>image {
    width: 38rpx;
    height: 34rpx;
    margin-right: 16rpx;
}

.contact::after {
    border: none;
}

.sesList {
    height: 180rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-left: 20rpx;
    padding: 20rpx 20rpx 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
}

.sesList:last-child {
    border-bottom: 0rpx solid #ececec;
}

.sLeft {
    display: flex;
}

.goodImg {
    margin-right: 30rpx;
    height: 180rpx;
    width: 180rpx;
    border: 1rpx solid #dcdcdc;
    border-radius: 6rpx;
}

.sLeft image {
    height: 180rpx;
    width: 180rpx;
    border-radius: 6rpx;
}

.sRight {
    font-weight: 700;
    font-size: 30rpx;
}

.det {
    margin-top: 10rpx;
    position: relative;
    width: 428rpx;
}

.title {
    font-weight: 700;
    margin-bottom: 6rpx;
    line-height: 36rpx;
}

.specConn {
    font-size: 24rpx;
    color: #666;
    margin-top: 10rpx;
}

.goodsPrice {
    color: #ff640f;
    font-weight: 700;
    font-size: 34rpx;
    position: absolute;
    bottom: 10rpx;
}

.count {
    margin-left: 20rpx;
    padding: 20rpx 20rpx 0rpx 0rpx;
}

.count>view {
    margin-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
    font-size: 24rpx;
}

.countpri {
    color: #666;
}

.ordernum {
    padding: 20rpx;
    background: #fff;
    font-size: 24rpx;
    line-height: 42rpx;
}

.totalPrice {
    font-weight: 700;
    font-size: 26rpx;
    height: 82rpx;
    line-height: 82rpx;
    border-top: 1rpx solid #ececec;
    padding: 0rpx 20rpx 0rpx 0rpx;
}

.sum {
    height: 92rpx;
    line-height: 92rpx;
    padding-left: 20rpx;
}

.sum text {
    font-weight: 700;
    font-size: 34rpx;
    color: #ff640f;
}

.qrcode {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-bottom: 50rpx;
}

.qrcode>view {
    text-align: center;
    margin-top: 50rpx;
    font-size: 30rpx;
}

.qrcode>canvas {
    width: 420rpx;
    height: 360rpx;
}

.pay_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.pay_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

.pay_title {
    height: 130rpx;
    font-size: 34rpx;
    display: flex;
    padding: 0rpx 20rpx;
    align-items: center;
    justify-content: space-between;
    border-bottom: 12rpx solid #cbcbcb;
}

.dTitle {
    width: 676rpx;
    text-align: center;
    color: #222;
}

.cancel {
    width: 34rpx;
    height: 34rpx;
}

.pay_content {
    height: 420rpx;
}

.choose-pay {
    background: #fff;
}

.section {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
}

.radio {
    width: 730rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
    font-size: 32rpx;
    color: #999;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
    padding-right: 20rpx;
}

.radio radio {
    width: 48rpx;
    height: 48rpx;
}

.radio:last-child {
    border-bottom: 0rpx solid #ececec;
}

.deti {
    display: flex;
    align-items: center;
}

.wxicon {
    width: 34rpx;
    height: 34rpx;
    margin-left: 24rpx;
    margin-right: 20rpx;
}

.pay-foot {
    border-top: 1rpx solid #acacac;
    height: 130rpx;
    padding: 20rpx 20rpx;
}

.pay-foot button {
    height: 90rpx;
    background: #ff640f;
    font-size: 34rpx;
    color: #fff;
}

.qrcode1 {
    background-image: -webkit-linear-gradient(180deg,rgb(255,100,15)0%,rgb(255,155,66)100%);
    height: 66rpx;
    line-height: 66rpx;
    width: 600rpx;
    margin: 20rpx auto 0;
    border-radius: 33rpx;
    text-align: center;
    color: #fff;
}