.order-nav {
    width: 750rpx;
    height: 80rpx;
    display: flex;
    justify-content: space-around;
    background: #fff;
    line-height: 80rpx;
    border-top: 1rpx solid #cbcbcb;
}

.order-nav>view {
    flex: 1;
    text-align: center;
}

.nav {
    display: inline-block;
    height: 80rpx;
    font-size: 30rpx;
    color: #666;
    font-weight: 500;
}

.on {
    box-sizing: border-box;
    color: #ef8200;
    border-bottom: 4rpx solid #ef8200;
}

.wrap {
    margin-top: 20rpx;
}

.wrapheader {
    position: relative;
    height: 354rpx;
    width: 750rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(255,136,92) 0%,rgb(239,130,0) 100%);
}

.acount {
    padding-top: 60rpx;
    text-align: center;
    color: #fff;
    font-size: 32rpx;
    line-height: 32rpx;
}

.acount>view:first-child {
    font-weight: 700;
    font-size: 78rpx;
    margin-bottom: 20rpx;
    line-height: 78rpx;
}

.reflect {
    border-radius: 39rpx 0rpx 0rpx 39rpx;
    background-color: rgb(255,255,255);
    opacity: 0.851;
    position: absolute;
    right: 0rpx;
    top: 255rpx;
    width: 160rpx;
    height: 78rpx;
    text-align: center;
    line-height: 78rpx;
    color: #ef8200;
    font-size: 32rpx;
    font-weight: 700;
}

.bill {
    background: #F5F5F5;
    border-bottom: 1rpx solid #dcdcdc;
    height: 100rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 30rpx;
    font-weight: 700;
}

.billDet {
    color: #666;
    font-size: 24rpx;
    margin-top: 18rpx;
    font-weight: 500;
}

.billDet>text {
    margin-right: 20rpx;
}

.dateIcon {
    width: 38rpx;
    height: 36rpx;
}

.session {
    padding-left: 20rpx;
    width: 730rpx;
    background: #fff;
}

.list {
    display: flex;
    justify-content: space-between;
    padding-left: 10rpx;
    padding-right: 20rpx;
    height: 140rpx;
    align-items: center;
    border-bottom: 1rpx solid #ececec;
    font-size: 32rpx;
}

.list:last-child {
    border-bottom: 0rpx solid #ececec;
}

.time {
    font-weight: 700;
    font-size: 26rpx;
    color: #999;
    margin-top: 18rpx;
}

.money {
    font-size: 30rpx;
    font-weight: 700;
}

.recharge {
    color: #ff640f;
}

.order-cont {
    padding-bottom: 100rpx;
}

.lists {
    margin-top: 20rpx;
    font-size: 28rpx;
}

.order-num {
    display: flex;
    font-size: 28rpx;
    justify-content: space-between;
    align-items: center;
    height: 82rpx;
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.list-up {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-weight: 700;
}

.list-left {
    display: flex;
    padding-left: 20rpx;
}

.list-right {
    padding-right: 20rpx;
    display: flex;
    align-items: center;
    font-weight: 500;
}

.left-det {
    width: 450rpx;
    display: flex;
    flex-direction: column;
    margin-left: 20rpx;
    color: #666;
    font-size: 24rpx;
    position: relative;
}

.left-title {
    color: #222;
    font-size: 28rpx;
    line-height: 36rpx;
    margin-top: 14rpx;
}

.goodsImg {
    border: 1rpx solid #ececec;
}

.img {
    height: 180rpx;
    width: 180rpx;
}

.style {
    display: inline-block;
    margin-right: 20rpx;
    font-weight: 500;
}

.left-title {
    color: #222;
    font-size: 28rpx;
    line-height: 36rpx;
    margin-top: 14rpx;
}

.price {
    font-size: 30rpx;
    color: #f33030;
    position: absolute;
    bottom: 0rpx;
}

.goodsImg {
    border: 1rpx solid #ececec;
}

.img {
    height: 180rpx;
    width: 180rpx;
}

.list-down {
    height: 80rpx;
    background-color: #fff;
    display: flex;
    justify-content: flex-end;
    padding: 0rpx 20rpx;
    align-items: center;
    border-top: 1rpx solid #ececec;
}

.list-down .btn {
    width: 150rpx;
    height: 56rpx;
    font-size: 26rpx;
    color: #222;
    text-align: center;
    line-height: 56rpx;
    justify-content: center;
    border: 1rpx solid #666;
    margin-left: 14rpx;
    border-radius: 4rpx;
}

.list-down .submit {
    color: #ef8200;
    border: 1rpx solid #ef8200;
}

.icon1 {
    width: 18rpx;
    height: 26rpx;
    margin-right: 20rpx;
}

.icon2 {
    width: 24rpx;
    height: 26rpx;
    margin-right: 20rpx;
}

.arrow {
    width: 85rpx;
    height: 12rpx;
}

.lisstBot {
    margin-left: 20rpx;
    width: 730rpx;
    height: 76rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 26rpx;
    border-top: 1rpx solid #ececec;
}

.shopname {
    width: 200rpx;
}

.addr {
    width: 312rpx;
    display: flex;
    align-items: center;
}

.addr>text {
    width: 268rpx;
    display: inline-block;
    align-items: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    padding-right: 20rpx;
}

.no {
    margin-top: 200rpx;
    height: 270rpx;
    width: 750rpx;
}