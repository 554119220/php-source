page {
    background: #fff;
}

.status {
    width: 750rpx;
    height: 120rpx;
    font-size: 34rpx;
    line-height: 120rpx;
    color: #fff;
    text-align: center;
    background-image: -webkit-linear-gradient(180deg,rgb(239,130,0) 0%,rgb(239,159,64) 100%);
}

.session {
}

.lists {
    margin-top: 20rpx;
    font-size: 28rpx;
}

.order-num {
    display: flex;
    font-size: 28rpx;
    justify-content: space-between;
    align-items: center;
    height: 82rpx;
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.list-up {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-weight: 700;
}

.list-left {
    display: flex;
    padding-left: 20rpx;
}

.list-right {
    padding-right: 20rpx;
    display: flex;
    align-items: center;
    font-weight: 500;
}

.left-det {
    width: 450rpx;
    display: flex;
    flex-direction: column;
    margin-left: 20rpx;
    color: #666;
    font-size: 24rpx;
    position: relative;
}

.left-title {
    color: #222;
    font-size: 28rpx;
    line-height: 36rpx;
    margin-top: 14rpx;
}

.goodsImg {
    border: 1rpx solid #ececec;
}

.img {
    height: 180rpx;
    width: 180rpx;
}

.block {
    padding-left: 20rpx;
    width: 730rpx;
    padding-bottom: 150rpx;
}

.list,.list>view:first-child {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 90rpx;
}

.list {
    border-top: 1rpx solid #ececec;
    padding-right: 20rpx;
}

.remark {
    padding: 32rpx 0rpx;
}

.remark,.remark>view:first-child {
    height: auto;
    line-height: 36rpx;
}

.title {
    width: 160rpx;
}

.icon {
    width: 36rpx;
    height: 36rpx;
}

.distribute {
    padding-right: 20rpx;
    text-align: right;
    margin-top: 30rpx;
    font-size: 26rpx;
    font-weight: 700;
}

.distribute>text {
    color: #f23030;
}

.distribute>text:last-child {
    font-size: 34rpx;
}

.fixBot {
    display: flex;
    justify-content: center;
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    width: 750rpx;
    height: 98rpx;
    z-index: 107;
    background-color: rgb(249,249,249);
    box-shadow: 0px -3rpx 3rpx 0px rgba(180,180,180,0.212);
}

.btn {
    margin-top: -70rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 138rpx;
    height: 138rpx;
    border-radius: 50%;
    background-color: rgb(249,249,249);
    box-shadow: 0px -3rpx 3rpx 0px rgba(180,180,180,0.212);
}

.btn>view {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 20rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 40rpx;
    background: #ef8200;
    border-radius: 50%;
    color: #fff;
    font-size: 34rpx;
    text-align: center;
}

.btnOver>view {
    background: #999;
}