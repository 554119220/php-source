button {
    border-radius: 0rpx;
}

button::after {
    border: none;
}

.delIcon {
    height: 28rpx;
    width: 28rpx;
    margin-right: 20rpx;
}

.empty {
    height: 92rpx;
    border-top: 1rpx solid #dcdcdc;
    border-bottom: 1rpx solid #dcdcdc;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    padding: 0rpx 20rpx;
    width: 710rpx;
    background: #fff;
}

.session {
    background: #fff;
    margin-bottom: 200rpx;
}

.goodsItem {
    display: flex;
    padding: 20rpx 0rpx;
    border-top: 1rpx solid #ececec;
    background: #fff;
    margin-left: 20rpx;
}

.goodsItem:first-child {
    border-top: 0rpx solid #f00;
}

.goodsLft {
    width: 180rpx;
    height: 180rpx;
    border-radius: 10rpx;
    border: 1rpx solid #dcdcdc;
    margin-right: 22rpx;
}

.goodsImg {
    height: 178rpx;
    width: 178rpx;
    border-radius: 10rpx;
}

.goodsRight {
    width: 504rpx;
    height: 180rpx;
    position: relative;
}

.goodsName {
    font-size: 25rpx;
    line-height: 36rpx;
}

.goodsType {
    font-size: 25rpx;
    line-height: 36rpx;
    font-weight: 700;
    color: #ef8200;
}

.goodsDet {
    width: 504rpx;
    position: absolute;
    bottom: 16rpx;
    display: flex;
    justify-content: space-between;
}

.goodsPrice {
    font-size: 31rpx;
    color: #f33030;
    font-weight: 700;
    position: absolute;
    bottom: 2rpx;
}

.opear {
    display: flex;
    position: absolute;
    right: 0rpx;
    bottom: -16rpx;
    align-items: center;
}

.reduce {
    height: 56rpx;
    width: 56rpx;
    border-radius: 50%;
    border: 2rpx solid #999;
    display: flex;
    justify-content: center;
    align-items: center;
}

.reduce image {
    width: 26rpx;
    height: 5rpx;
}

.add {
    border: 2rpx solid #ef8200;
}

.add image {
    width: 26rpx;
    height: 27rpx;
}

.goodsNum input {
    width: 60rpx;
    height: 56rpx;
    text-align: center;
    line-height: 56rpx;
    font-size: 26rpx;
}

#addCarts {
    position: fixed;
    bottom: 138rpx;
    right: 0rpx;
    z-index: 9999;
    width: 120rpx;
    height: 98rpx;
    background: rgba(255,255,255,0.9);
    border: 4rpx solid #ececec;
    border-radius: 50% 0rpx 0rpx 50%;
}

#addCarts view {
    height: 98rpx;
    display: flex;
    align-items: center;
    padding: 0rpx 13rpx;
    position: relative;
}

#addCarts text {
    position: absolute;
    top: 4rpx;
    right: 20rpx;
    display: block;
    width: 50rpx;
    height: 30rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(253,71,116)0%,rgb(253,194,209)100%);
    border-radius: 15rpx;
    font-weight: 700;
    color: #fff;
    text-align: center;
}

.balance {
    position: fixed;
    bottom: 0rpx;
    z-index: 999;
}

.balTop {
    width: 750rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 60rpx;
    font-size: 24rpx;
    color: #666;
    font-weight: 700;
    background: #ffedd8;
    border-top: 2rpx solid #ef8200;
    margin-top: -150rpx;
}

.balTop text {
    color: #ef8200;
}

.rightIcon {
    margin-left: 16rpx;
    margin-top: -6rpx;
}

.balBot {
    background: #fff;
    height: 98rpx;
    width: 750rpx;
    border-top: 1rpx solid #dcdcdc;
    display: flex;
    justify-content: space-between;
    margin-top: -194rpx;
}

.submit button {
    width: 220rpx;
    height: 98rpx;
    font-size: 32rpx;
    background: #ef8200;
    line-height: 97rpx;
    font-weight: 700;
}

.over button {
    background: #ececec;
}

.total {
    display: flex;
    width: 500rpx;
    padding-left: 30rpx;
    align-items: center;
}

.totalLeft {
    font-size: 30rpx;
    font-weight: 700;
    margin-right: 40rpx;
}

.totalRight {
    font-size: 22rpx;
}

.totalPrice {
    font-size: 35rpx;
    color: #ef8200;
    font-weight: 700;
}