.session {
    font-size: 30rpx;
    color: #222;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0rpx 30rpx 0rpx 20rpx;
    height: 90rpx;
    background: #fff;
    margin-top: 30rpx;
}

.shop {
    margin-top: 20rpx;
}

.status {
    margin-left: 50rpx;
    font-size: 28rpx;
    color: #999;
}

.order-nav {
    width: 750rpx;
    height: 80rpx;
    display: flex;
    justify-content: space-around;
    background: #fff;
    line-height: 80rpx;
    border-top: 1rpx solid #cbcbcb;
}

.order-nav>view {
    flex: 1;
    text-align: center;
}

.nav {
    display: inline-block;
    height: 80rpx;
    font-size: 30rpx;
    color: #666;
    font-weight: 500;
}

.on {
    box-sizing: border-box;
    color: #ef8200;
    border-bottom: 4rpx solid #ef8200;
}

.no {
    margin-top: 200rpx;
    height: 270rpx;
    width: 750rpx;
}

.order-cont {
    padding-bottom: 100rpx;
}

.list {
    background: #fff;
    margin-top: 20rpx;
    font-size: 28rpx;
}

.order-num {
    display: flex;
    font-size: 28rpx;
    justify-content: space-between;
    align-items: center;
    height: 82rpx;
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.order-num view:first-child {
    font-weight: 700;
}

.list-up {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
    font-weight: 700;
}

.list-left {
    display: flex;
    padding-left: 20rpx;
}

.list-right {
    padding-right: 20rpx;
    white-space: nowrap;
}

.left-det {
    display: flex;
    flex-direction: column;
    margin-left: 20rpx;
    font-size: 24rpx;
    position: relative;
}

.style {
    display: inline-block;
    margin-right: 20rpx;
    font-weight: 500;
}

.left-title {
    color: #222;
    font-size: 28rpx;
    line-height: 36rpx;
    margin-top: 14rpx;
}

.price {
    font-size: 30rpx;
    position: absolute;
    bottom: 0rpx;
}

.goodsImg {
    border: 1rpx solid #ececec;
}

.img {
    height: 180rpx;
    width: 180rpx;
}

.list-down {
    height: 80rpx;
    background-color: #fff;
    display: flex;
    justify-content: flex-end;
    padding: 0rpx 20rpx;
    align-items: center;
}

.list-down:last-child {
    border-top: 1rpx solid #ececec;
}

.orderDet {
    justify-content: space-between;
    height: auto;
    padding: 16rpx 20rpx;
    line-height: 32rpx;
    font-size: 26rpx;
}

.sum {
    margin-right: 16rpx;
}

.actualPri {
    font-weight: 700;
    font-size: 30rpx;
    color: rgb(34,34,34);
}

.list-down .btn {
    height: 56rpx;
    display: flex;
    justify-content: center;
    margin-left: 14rpx;
    align-items: center;
    font-size: 26rpx;
    color: #666;
    border: 1rpx solid #666;
    border-radius: 4rpx;
    text-align: center;
    line-height: 56rpx;
    width: 150rpx;
}

.list-down .submit {
    color: #ef8200;
    border: 1rpx solid #ef8200;
}

.region {
    display: inline-block;
    width: 630rpx;
    word-break: break-all;
    white-space: pre-wrap;
    text-align: left;
}

.kilometer {
    margin-right: 20rpx;
    font-weight: 700;
}

.mask {
    position: fixed;
    top: 0;
    border: 0;
    left: 0;
    range: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1001;
    width: 100%;
    height: 100%;
}

.box {
    position: fixed;
    top: 50%;
    left: 50%;
    margin-left: -300rpx;
    margin-top: -400rpx;
    width: 600rpx;
    height: 560rpx;
    z-index: 1002;
    background: #fff;
    border-radius: 10rpx;
    animation: fadeInDown 0.6s 1;
}

@keyframes fadeInDown {
    from {
        opacity: 0;
        transform: translate3d(0,-100%,0);
    }

    to {
        opacity: 1;
        transform: translate3d(0,0,0);
    }
}

.box .title {
    margin-top: 50rpx;
    margin-bottom: 50rpx;
    text-align: center;
    font-size: 36rpx;
}

.box .lst {
    padding: 0rpx 30rpx;
    margin-bottom: 30rpx;
    display: flex;
    align-items: center;
    line-height: 34rpx;
}

.box .lst text {
    display: inline-block;
    width: 120rpx;
    margin-right: 20rpx;
    text-align: right;
    color: #666;
}

.box ._lst view {
    width: 400rpx;
    padding-left: 20rpx;
}

.box .formSub {
    margin-top: 80rpx;
    width: 220rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #fff;
    background: #ef8200;
    padding-left: 0rpx;
    padding-right: 0rpx;
    font-size: 34rpx;
}

.choose {
    display: flex;
}

.distribute {
    display: inline-block;
    width: 120rpx;
    height: 68rpx;
    border-top: 1rpx solid #ececec;
    border-bottom: 1rpx solid #ececec;
    text-align: center;
    min-height: 0rpx;
    color: #222;
    font-weight: 700;
    font-size: 32rpx;
}

.choose view {
    width: 68rpx;
    height: 68rpx;
    border: 1rpx solid #ececec;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.choose view.add {
    border: 1rpx solid #ef8200;
}

.icon {
    width: 26rpx;
    height: 5rpx;
}

.addicon {
    height: 27rpx;
}