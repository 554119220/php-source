.session {
    border-top: 1rpx solid #dcdcdc;
    padding-top: 20rpx;
    height: 100%;
}

.seckill-list {
    background: #fff;
    height: 100%;
}

.seckill {
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.rightIcon {
    margin-left: 20rpx;
}

.skUp {
    height: 110rpx;
    display: flex;
    justify-content: space-around;
    border-top: 1rpx solid #ececec;
    border-bottom: 1rpx solid #ececec;
}

.skUpTime {
    width: 187rpx;
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 20rpx;
}

.on {
    background: #ef8200;
    color: #fff;
}

.skUpTime view {
    font-size: 30rpx;
    font-weight: 700;
    padding-bottom: 14rpx;
}

.seckillStatus {
    background: #f5f5f5;
    height: 52rpx;
    padding: 0rpx 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #ef8200;
    font-size: 26rpx;
    border-bottom: 1rpx solid #cbcbcb;
}

.skItem {
    display: flex;
    margin-left: 20rpx;
    padding: 24rpx 20rpx 24rpx 0rpx;
    border-bottom: 1rpx solid #cbcbcb;
}

.skItem:last-child {
    border-bottom: 0rpx solid #f00;
}

.skImg {
    margin-right: 28rpx;
}

.skImg,.skImg image {
    width: 180rpx;
    height: 180rpx;
    border-radius: 4rpx;
    border: 2rpx solid #cbcbcb;
}

.skRight {
    width: 502rpx;
    position: relative;
}

.skTitle {
    font-size: 26rpx;
    line-height: 40rpx;
}

.skDown {
    width: 502rpx;
    display: flex;
    justify-content: space-between;
    position: absolute;
    bottom: 0rpx;
    left: 0rpx;
}

.timeBuyBtn {
    width: 180rpx;
    height: 62rpx;
    color: #fff;
    border-radius: 4rpx;
    background: #ef8200;
    text-align: center;
    line-height: 62rpx;
}

.over {
    background: #ccc;
}

.skPrice {
    font-size: 32rpx;
    color: #ef8200;
    font-weight: 700;
    position: absolute;
    bottom: 0rpx;
}

.oldPice {
    position: relative;
    display: inline-block;
    font-size: 28rpx;
    font-weight: 500;
    color: #999;
    margin-left: 40rpx;
}

.oldPice::before {
    content: " ";
    display: inline-block;
    height: 2rpx;
    width: 100%;
    background: #999;
    position: absolute;
    top: 10rpx;
    transform: rotate(3deg);
}

.skNum {
    font-size: 24rpx;
    color: #999;
    margin-top: 12rpx;
}

.skNum text:first-child {
    display: inline-block;
    height: 30rpx;
    color: #fff;
    background: #ef8200;
    padding: 0rpx 20rpx;
    line-height: 30rpx;
    border-radius: 6rpx;
    margin-right: 22rpx;
}

.body {
    height: 100%;
    overflow: hidden;
}

.scroll {
    height: 100%;
}

.wrap {
    padding-bottom: 200rpx;
}

#addCarts {
    position: fixed;
    bottom: 138rpx;
    right: 0rpx;
    z-index: 9999;
    width: 120rpx;
    height: 98rpx;
    background: rgba(255,255,255,0.9);
    border: 4rpx solid #ececec;
    border-radius: 50% 0rpx 0rpx 50%;
}

#addCarts view {
    height: 98rpx;
    display: flex;
    align-items: center;
    padding: 0rpx 13rpx;
    position: relative;
}

#addCarts text {
    position: absolute;
    top: 4rpx;
    right: 20rpx;
    display: block;
    width: 50rpx;
    height: 30rpx;
    line-height: 30rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(253,71,116)0%,rgb(253,194,209)100%);
    border-radius: 15rpx;
    font-weight: 700;
    color: #fff;
    text-align: center;
}