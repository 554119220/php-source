page {
    background-color: #f8f8f8;
    height: 100%;
    font-family: "Microsoft YaHei","Helvetica Neue","Hiragino Sans GB","\9ED1\4F53",Arial;
    font-size: 0rpx;
}

.container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    box-sizing: border-box;
    font-size: 28rpx;
    line-height: 26rpx;
    color: #222;
}

.rightIcon {
    width: 12rpx;
    height: 18rpx;
}

.cartbtn {
    height: 76rpx;
    width: 76rpx;
}

.tab {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    background: #fff;
    border-top: 2rpx solid #d5d5d5;
    height: 92rpx;
    padding-bottom: 4rpx;
    z-index: 99999;
}

.tabnav {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.tablist {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #8c8c8c;
}

.tabimg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 8rpx;
    margin-bottom: 0rpx;
}

.foot {
    height: 150rpx;
}

.bottom_padding {
    padding-bottom: 68rpx;
}

@import "yzbld_sun/pages/wxParse/wxParse.wxss";

button::after {
    border: none;
}