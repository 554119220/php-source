.navBlock {
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    box-shadow: 0 1rpx 3rpx #999;
}

.navBlock .navBox {
    height: 90rpx;
    line-height: 90rpx;
    box-sizing: border-box;
    border-bottom: 2rpx solid transparent;
    margin: 0 25rpx;
    font-size: 30rpx;
    text-align: center;
}

.navBlock .navBox.curr {
    border-color: #77D4C0;
    color: #77D4C0;
}

.navBlock .border {
    width: 0;
    height: 40rpx;
    border-left: 1px dotted #dcdcdc;
}

.navBlock .border:last-child {
    display: none;
}

.navBlank {
    height: 90rpx;
}

.list {
    padding: 20rpx 20rpx 5rpx;
}

.list .listBox {
    height: 178rpx;
    padding: 0 20rpx 0 50rpx;
    margin-bottom: 15rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.list .listBox .text {
    overflow: hidden;
    color: #fff;
}

.list .listBox .text1 {
    margin-bottom: 20rpx;
}

.list .listBox .text1 .flex-flex1 {
    overflow: hidden;
}

.list .listBox .text1 .title {
    font-size: 26rpx;
    line-height: 36rpx;
    height: 72rpx;
    overflow: hidden;
}

.list .listBox .text1 .title2 {
    font-size: 39rpx;
    line-height: 72rpx;
    height: 72rpx;
    overflow: hidden;
}

.list .listBox .text1 .title2 .sp1 {
    font-size: 80rpx;
}

.list .listBox .text1 .border {
    width: 0;
    height: 68rpx;
    border-left: 1px dotted #fff;
    margin: 0 20rpx;
}

.list .listBox .text1 .h1 {
    font-size: 32rpx;
    margin-bottom: 15rpx;
}

.list .listBox .text1 .h2 {
    font-size: 22rpx;
}

.list .listBox .text2 {
    font-size: 24rpx;
}

.list .listBox .lbtn {
    width: 126rpx;
    height: 126rpx;
    padding: 10rpx;
    border-radius: 50%;
    text-align: center;
    font-size: 32rpx;
    line-height: 40rpx;
    background-color: rgba(255,255,255,0.5);
}

.list .listBox .lbtn>text {
    display: block;
    width: 126rpx;
    height: 126rpx;
    border-radius: 50%;
    box-sizing: border-box;
    padding-top: 23rpx;
    background-color: #fff;
}

.list .listBox .lbtn .sp1 {
    color: #77D3BF;
}

.list .listBox .lbtn .sp2 {
    color: #FFE599;
}

.list .listBox .lbtn .sp3 {
    padding-top: 0;
    line-height: 126rpx;
}

.bg1 {
    background-image: url(data:image/png;base64,);
}

.bg2 {
    background-image: url(data:image/png;base64,);
}