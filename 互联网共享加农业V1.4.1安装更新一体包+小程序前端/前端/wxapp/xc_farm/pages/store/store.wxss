.nav {
    background: #f7f7f7;
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 30;
}

.back {
    width: 100%;
    position: relative;
    font-size: 4vw;
    line-height: 6vw;
}

.back .back_img {
    width: 100%;
    height: 270rpx;
    display: block;
}

.back .code {
    width: 68rpx;
    height: 75rpx;
    position: absolute;
    top: 0;
    right: 60rpx;
    border-radius: 0 0 75rpx 75rpx;
}

.back .code>image {
    width: 40rpx;
    height: 40rpx;
    margin: 10rpx auto;
    display: block;
}

.banner {
    margin: -100rpx 30rpx 0;
    background: #fff;
    height: 200rpx;
    position: relative;
    z-index: 33;
    font-size: 4vw;
    line-height: 6vw;
    padding-top: 80rpx;
    box-sizing: border-box;
}

.banner .img {
    width: 130rpx;
    height: 130rpx;
    border: #f7f7f7 6rpx solid;
    border-radius: 100%;
    position: absolute;
    left: 50%;
    transform: translate(-50%);
    top: -71rpx;
}

.banner .img .logo {
    width: 100%;
    height: 100%;
    display: block;
    border-radius: 100%;
}

.banner .img .tip {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-radius: 100%;
    background: rgba(0,0,0,0.5);
    color: #fff;
    font-size: 3.5vw;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

.banner .img .tip>image {
    width: 50rpx;
}

.banner .name {
    display: flex;
    align-items: center;
    justify-content: center;
}

.banner .name>image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin-left: 10rpx;
}

.banner .name .tip {
    background: #FFA64D;
    color: #fff;
    width: 120rpx;
    text-align: center;
    line-height: 44rpx;
    border-radius: 44rpx;
    margin-left: 10rpx;
}

.banner .nick {
    text-align: center;
    color: #B7B7B7;
    font-size: 3.5vw;
    margin-top: 4rpx;
}

.topTags {
    padding: 0 20rpx;
    width: 100%;
    box-sizing: border-box;
    z-index: 50;
    box-shadow: 0px 1px 3px #999;
    margin-top: 10rpx;
}

.topTags .tagList {
    font-size: 30rpx;
    color: #999;
    white-space: nowrap;
    height: 92rpx;
    line-height: 92rpx;
}

.topTags .tagList+.tagList {
    border-top: 2rpx solid #dcdcdc;
}

.topTags .tagBox {
    display: inline-block;
    height: 48rpx;
    line-height: 48rpx;
    padding: 0 40rpx;
    border: 1rpx solid #fff;
    border-radius: 25rpx;
}

.topTags .tagBox.curr {
    border-color: #79d5c1;
    color: #77D3BF;
}

.topBlank {
    height: 478rpx;
}

.list {
    margin-bottom: 8rpx;
}

.list .listBox {
    margin-bottom: 10rpx;
    width: 340rpx;
    padding: 25rpx 10rpx 15rpx 25rpx;
}

.list .listBox:nth-of-type(2n) {
    padding: 25rpx 25rpx 15rpx 10rpx;
}

.list .listBox .imgBlock {
    position: relative;
    width: 340rpx;
    height: 288rpx;
    margin-bottom: 10rpx;
}

.list .listBox .imgBlock .tags {
    position: absolute;
    left: 5rpx;
    top: 0;
}

.list .listBox .imgBlock .tag {
    display: inline-block;
    width: 54rpx;
    height: 54rpx;
    text-align: center;
    line-height: 46rpx;
    color: #fff;
    font-size: 28rpx;
    border-radius: 0 0 27rpx 27rpx;
    margin-right: 5rpx;
}

.list .listBox .imgBlock .tag.bg1 {
    background-color: #FF8000;
}

.list .listBox .imgBlock .tag.bg2 {
    background-color: #FCC601;
}

.list .listBox .imgBlock .tag.bg3 {
    background-color: #77D4C0;
}

.list .listBox image {
    vertical-align: bottom;
}

.list .listBox .h1 {
    font-size: 28rpx;
    color: #999;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-bottom: 15rpx;
}

.list .listBox .p {
    font-size: 20rpx;
    color: #999;
    padding-bottom: 10rpx;
    border-bottom: 1px dotted #dcdcdc;
    margin-bottom: 15rpx;
}

.list .listBox .p .sp1 {
    color: #77D2BF;
}

.list .listBox .info {
    font-size: 30rpx;
    color: #77D3BF;
}

.list .listBox .info .btn {
    width: 134rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
    border-radius: 24rpx;
    background-color: #77D4C0;
    font-size: 26rpx;
    color: #fff;
}

.shade {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 100;
}

.hbLayer {
    width: 586rpx;
    position: fixed;
    left: 50%;
    margin-left: -293rpx;
    top: 50%;
    transform: translateY(-50%);
}

.hbLayer .closeBtn {
    text-align: right;
    margin-bottom: 30rpx;
}

.hbLayer .closeBtn image {
    width: 40rpx;
    height: 40rpx;
}

.hbLayer .hbimg {
    margin-bottom: 20rpx;
}

.hbLayer .dlBtn {
    height: 78rpx;
    line-height: 78rpx;
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    background-color: #77d4c0;
    border-radius: 10rpx;
}

.hbLayer .dlBtn image {
    width: 26rpx;
    height: 26rpx;
    margin-right: 10rpx;
    vertical-align: baseline;
}

.list2 {
}

.list2 .listBox {
    padding: 25rpx;
    margin-bottom: 15rpx;
}

.list2 .listBox .imgBlock {
    height: 330rpx;
    margin-bottom: 20rpx;
    position: relative;
}

.list2 .listBox .imgBlock .imgshade {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
}

.list2 .listBox .imgBlock .imgshade image {
    width: 204rpx;
    height: 170rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    margin: -85rpx 0 0 -102rpx;
}

.list2 .listBox .h1 {
    font-size: 30rpx;
    color: #666;
    line-height: 1.2;
    margin-bottom: 20rpx;
}

.list2 .listBox .h2 {
    font-size: 26rpx;
    color: #999;
    line-height: 1.2;
    margin-bottom: 10rpx;
}

.list2 .listBox .h2 .sp1 {
    color: #666;
}

.list2 .listBox .other {
    font-size: 26rpx;
    color: #999;
}

.list2 .listBox .other .flex-flex1 {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.list2 .listBox .other .sp1 {
    color: #77D2BF;
}

.list2 .listBox .other .btn {
    width: 134rpx;
    height: 48rpx;
    line-height: 48rpx;
    border-radius: 24rpx;
    text-align: center;
    color: #fff;
    background-color: #DDDDDD;
    font-size: 26rpx;
}

.list2 .listBox .other .btn.curr {
    background-color: #DDDDDD;
}

.jwCont {
    padding: 25rpx 0;
}

.jwCont .sp1 {
    padding: 8rpx 20rpx;
    color: #fff;
    border-radius: 50rpx;
    margin-right: 6rpx;
}

.jwNav {
    width: 530rpx;
    border-radius: 40rpx;
    border: 1px solid #49CCB9;
    margin: 0 auto 25rpx;
    overflow: hidden;
}

.jwNav .navBox {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
}

.jwNav .navBox.curr {
    background-color: #49CCB9;
    color: #fff;
}

.formBlock {
    display: block;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.formBlock .inputBlock {
    padding: 25rpx 0;
    border-bottom: 1px dotted #dcdcdc;
    box-sizing: border-box;
    min-height: 100rpx;
}

.formBlock .inputBlock .name {
    width: 5em;
    font-size: 26rpx;
    color: #999;
}

.formBlock .inputBlock .input {
    font-size: 26rpx;
}

.formBlock .inputBlock .input .radioL {
    color: #333;
    margin-right: 25rpx;
}

.formBlock .inputBlock .input input {
    font-size: 26rpx;
    height: 36rpx;
    line-height: 36rpx;
}

.formBlock .inputBlock>image {
    width: 50rpx;
    height: 50rpx;
    display: block;
}

.formBlock .info {
    padding: 20rpx 0;
    text-align: center;
    font-size: 22rpx;
    color: #77D4C0;
}

.formBlock .submitBtn {
    height: 90rpx;
    line-height: 90rpx;
    background-color: #77D4C0;
    font-size: 28rpx;
    color: #fff;
}

.formBlock .submitBtn::after {
    border: none;
}

.formBlock .inputBlock .input .upload {
    width: 100%;
}