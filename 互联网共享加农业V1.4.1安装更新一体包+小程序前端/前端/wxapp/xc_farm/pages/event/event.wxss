.list {
}

.list .listBox {
    padding: 25rpx;
    margin-bottom: 15rpx;
}

.list .listBox .imgBlock {
    height: 330rpx;
    margin-bottom: 20rpx;
    position: relative;
}

.list .listBox .imgBlock .imgshade {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
}

.list .listBox .imgBlock .imgshade image {
    width: 204rpx;
    height: 170rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    margin: -85rpx 0 0 -102rpx;
}

.list .listBox .h1 {
    font-size: 30rpx;
    color: #666;
    line-height: 1.2;
    margin-bottom: 20rpx;
}

.list .listBox .h2 {
    font-size: 26rpx;
    color: #999;
    line-height: 1.2;
    margin-bottom: 10rpx;
}

.list .listBox .h2 .sp1 {
    color: #666;
}

.list .listBox .other {
    font-size: 26rpx;
    color: #999;
}

.list .listBox .other .flex-flex1 {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.list .listBox .other .sp1 {
    color: #77D2BF;
}

.list .listBox .other .btn {
    width: 134rpx;
    height: 48rpx;
    line-height: 48rpx;
    border-radius: 24rpx;
    text-align: center;
    color: #fff;
    background-color: #DDDDDD;
    font-size: 26rpx;
}

.list .listBox .other .btn.curr {
    background-color: #DDDDDD;
}