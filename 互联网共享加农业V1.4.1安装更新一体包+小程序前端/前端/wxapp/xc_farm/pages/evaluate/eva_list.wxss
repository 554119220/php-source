.title {
    padding: 25rpx 0;
    border-bottom: 1px solid #dcdcdc;
    margin: 0 30rpx;
}

.title .border {
    width: 7rpx;
    height: 36rpx;
    background-color: #77d4c0;
    margin-right: 30rpx;
}

.title .text {
    font-size: 30rpx;
    color: #666;
}

.list {
    padding: 0 25rpx;
}

.list .listBox {
    padding: 25rpx 0;
    border-bottom: 1px solid #dcdcdc;
}

.list .listBox:last-child {
    border-bottom: none;
}

.list .listBox .box1 {
    margin-bottom: 25rpx;
}

.list .listBox .box1 image {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.list .listBox .box1 .text {
    line-height: 1.3;
}

.list .listBox .box1 .text .h1 {
    font-size: 26rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-bottom: 10rpx;
}

.list .listBox .box1 .text .h2 {
    font-size: 22rpx;
    color: #999;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.list .listBox .box2 {
    padding-left: 90rpx;
    font-size: 30rpx;
    line-height: 46rpx;
}

.list .listBox .box3 {
    padding-left: 90rpx;
    overflow: hidden;
}

.list .listBox .box3>image {
    width: 120rpx;
    height: 120rpx;
    display: block;
    float: left;
    margin-top: 20rpx;
    margin-right: 20rpx;
}

.body .nolist {
    position: fixed;
    z-index: 50;
    left: 50%;
    top: 50%;
    width: 152rpx;
    height: 206rpx;
    margin: -103rpx 0 0 -76rpx;
}