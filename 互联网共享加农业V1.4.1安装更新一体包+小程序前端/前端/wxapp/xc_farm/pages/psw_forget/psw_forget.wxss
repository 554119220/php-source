.lajform {
    display: block;
    padding: 0 110rpx;
    box-sizing: border-box;
    width: 100%;
}

.lajform .inputBlock {
    padding: 0 40rpx;
    height: 100rpx;
    border-radius: 50rpx;
    border: 1px solid #77D4C0;
    margin-bottom: 40rpx;
    overflow: hidden;
}

.lajform2 .inputBlock {
    margin-bottom: 20rpx;
}

.lajform .inputBlock .icon {
    width: 40rpx;
    height: 45rpx;
    margin-right: 10rpx;
}

.lajform .inputBlock .input {
    overflow: hidden;
}

.lajform .inputBlock .input input {
    font-size: 28rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    color: #666;
}

.lajform .submitBtn {
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 50rpx;
    background-color: #77D4C0;
    color: #fff;
    font-size: 32rpx;
}

.lajform2 .submitBtn {
    margin-bottom: 20rpx;
}

.lajform .submitBtn::after {
    border: none;
}

.lajform .submitBtn+.navBlock {
    margin-bottom: 30rpx;
}

.lajform .navBlock {
}

.lajform .navBlock .navBox {
    font-size: 28rpx;
    color: #999999;
    line-height: 1.3;
}

.lajform .navBlock .border {
    width: 1px;
    height: 28rpx;
    background-color: #CCCCCC;
    margin: 0 30rpx;
}

.lajform .inputBlock .send {
    height: 100rpx;
    line-height: 100rpx;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    width: 8em;
    background-color: #77D4C0;
    padding-right: 40rpx;
    margin-right: -40rpx;
}