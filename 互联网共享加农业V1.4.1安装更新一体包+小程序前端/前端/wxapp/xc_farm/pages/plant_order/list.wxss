.body {
    background: #fff;
}

.nav {
    display: flex;
    align-items: center;
    padding: 30rpx;
    font-size: 4.5vw;
    line-height: 6.5vw;
    border-top: #EEEEEE 4rpx solid;
    border-bottom: #EEEEEE 4rpx solid;
}

.nav .simg {
    width: 40rpx;
    display: block;
    margin-left: 20rpx;
}

.list {
    font-size: 4vw;
    line-height: 6vw;
    padding: 30rpx;
}

.list .item {
    display: flex;
}

.list .item .itemT {
}

.list .item .itemT .yue {
    width: 80rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    border-radius: 100%;
    color: #fff;
}

.list .item .itemT .line {
    width: 6rpx;
    height: 100%;
    margin: 0 auto;
}

.list .item .itemB {
    flex: 1;
}

.list .item .itemB .sp1 {
    padding: 18rpx 20rpx;
}

.list .item .itemB .imgs {
    overflow: hidden;
}

.list .item .itemB .imgs>image {
    width: 130rpx;
    height: 130rpx;
    display: block;
    float: left;
    margin-bottom: 20rpx;
    margin-left: 20rpx;
}

.list .item .itemB .sp2 {
    color: #AAAAAA;
    text-align: right;
    font-size: 3.5vw;
    line-height: 5.5vw;
}