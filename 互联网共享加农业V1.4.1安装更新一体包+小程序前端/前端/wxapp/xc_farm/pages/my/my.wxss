.myCont1 {
    padding: 0 30rpx;
    height: 365rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-image: url(data:image/jpeg;base64,);
}

.myCont1 .imgBlock {
    width: 140rpx;
    height: 140rpx;
    border: 5rpx solid #a4e6dc;
    border-radius: 50%;
    overflow: hidden;
}

.myCont1 .imgBlock>image {
    border-radius: 100%;
}

.myCont1 .text {
    overflow: hidden;
    margin: 0 10rpx;
    color: #fff;
    line-height: 1.3;
}

.myCont1 .text .h1 {
    font-size: 32rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-bottom: 20rpx;
}

.myCont1 .text .auther {
    width: 23rpx;
    height: 23rpx;
}

.myCont1 .text .h2 {
    font-size: 26rpx;
}

.myCont1 .setting {
    width: 44rpx;
    height: 42rpx;
}

.myCont2 {
    margin-bottom: 10rpx;
    padding: 35rpx 0;
}

.myCont2 .navBox {
    text-align: center;
    font-size: 28rpx;
    color: #999;
}

.myCont2 .navBox .icon {
    width: 56rpx;
    height: 56rpx;
    border: 2rpx solid transparent;
    font-size: 28rpx;
    line-height: 56rpx;
    margin: 0 auto 20rpx;
    border-radius: 50%;
    text-align: center;
    position: relative;
}

.myCont2 .navBox .icon1 {
    border-color: #FEBFAA;
    color: #FEBFAA;
}

.myCont2 .navBox .icon2 {
    border-color: #21C5AA;
    color: #21C5AA;
}

.myCont2 .navBox .icon3 {
    border-color: #99CCFF;
    color: #99CCFF;
}

.myCont2 .navBox .icon image {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    right: -30rpx;
    top: -10rpx;
}

.myCont3 {
    height: 100rpx;
    padding: 0 30rpx;
    margin-bottom: 2px;
    font-size: 28rpx;
    color: #868686;
}

.myCont3 .text {
    overflow: hidden;
    margin: 0 20rpx;
}

.myCont3 .icon {
    width: 48rpx;
    height: 40rpx;
}

.myCont3 .sp1 {
    color: #AAAAAA;
    margin-right: 10rpx;
}

.myCont4 {
    margin-bottom: 10rpx;
    padding: 30rpx 0;
}

.myCont4 .navBox {
    text-align: center;
    font-size: 28rpx;
    color: #666;
}

.myCont4 .navBox .icon {
    width: 50rpx;
    height: 50rpx;
    margin: 0 auto 30rpx;
    position: relative;
}

.myCont4 .navBox .icon .num {
    min-width: 38rpx;
    height: 38rpx;
    line-height: 38rpx;
    text-align: center;
    box-sizing: border-box;
    padding: 0 10rpx;
    font-size: 32rpx;
    color: #fff;
    border-radius: 19rpx;
    background-color: #77D4C0;
    position: absolute;
    right: 0rpx;
    top: -10rpx;
    transform: translateX(50%);
}

.yin {
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 100;
    width: 100%;
    height: 100vh;
}

.yinBox {
    background: #fff;
    font-size: 4vw;
    line-height: 6vw;
    width: 600rpx;
    margin: 100rpx auto;
    border-radius: 20rpx;
}

.yinBox .yinClose {
    text-align: right;
    padding: 20rpx;
}

.yinBox .yinClose>image {
    width: 40rpx;
    height: 40rpx;
}

.yinBox .simg {
    width: 140rpx;
    height: 140rpx;
    border-radius: 100%;
    display: block;
    margin: 0 auto;
    border: #ddd 2rpx solid;
}

.yinBox .tip {
    margin-top: 30rpx;
    text-align: center;
    padding: 0 30rpx;
}

.yinBox .form {
    padding: 10rpx 70rpx;
}

.yinBox .form .item {
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
}

.yinBox .form .item>input {
    border: #ddd 2rpx solid;
    border-radius: 20rpx;
    flex: 1;
    line-height: 80rpx;
    height: 80rpx;
    box-sizing: border-box;
    padding: 0 30rpx;
}

.yinBox .form .item .getcode {
    color: #fff;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 20rpx;
    text-align: center;
    width: 200rpx;
    margin-left: 20rpx;
    font-size: 3.5vw;
    border: none;
    padding: 0;
}

.yinBox .form .item .getcode::after {
    display: none;
    content: '';
    border-radius: 0;
    border: none;
}

.yinBox .form .btn {
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-radius: 90rpx;
    color: #fff;
    font-size: 4.5vw;
    margin-bottom: 40rpx;
}

.bind {
    display: flex;
    align-items: center;
}

.bind>image {
    width: 30rpx;
    height: 30rpx;
    display: block;
    margin-left: 10rpx;
}