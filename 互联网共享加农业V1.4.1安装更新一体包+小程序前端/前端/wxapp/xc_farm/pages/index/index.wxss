.page {
    background: #fff;
    font-size: 3.5vw;
}

.page .detail-banner {
    height: 360rpx;
}

.page .detail-banner image {
    width: 100%;
    height: 100%;
}

.page .detail-head {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    padding: 0 35rpx;
    margin-top: 20rpx;
}

.page .detail-name {
    width: 570rpx;
    height: 76rpx;
    line-height: 38rpx;
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.page .toShare {
    width: 55rpx;
    text-align: center;
    font-size: 3vw;
}

.page .toShare image {
    width: 44rpx;
    height: 44rpx;
    margin: 0 auto;
    margin-bottom: 5rpx;
    color: #535353;
}

.page .detail-price {
    display: flex;
    align-items: baseline;
    padding: 0 35rpx;
    margin-top: 20rpx;
}

.page .detail-after {
    color: #e43d1b;
    font-weight: bold;
    margin-right: 10rpx;
    font-size: 4vw;
}

.page .detail-before {
    font-size: 3vw;
    color: #5e5e5e;
    text-decoration: line-through;
}

.page .goods-detai-box {
    margin-top: 30rpx;
    border-top: 5rpx solid #eee;
}

.page .goods-detai-box .detail-box-head {
    display: flex;
    align-items: center;
    padding: 30rpx 0;
    color: #e43d1b;
    font-weight: bold;
    border-bottom: 1rpx solid #eee;
    line-height: 36rpx;
}

.page .goods-detai-box .detail-box-head view:nth-child(1) {
    width: 8rpx;
    height: 36rpx;
    background: #e43d1b;
    margin-right: 30rpx;
}

.page .goods-detai-box .detail-img-box {
    padding: 40rpx 35rpx;
}

.page .goods-detai-box .detail-img-box image {
    max-width: 100%;
}

.page .bottom-line {
    height: 120rpx;
}

.page .detail-submit {
    padding: 0;
    height: 100rpx;
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    border-radius: 0;
    background: #e43d1b;
    font-size: 3.5vw;
    display: flex;
    align-items: center;
    justify-content: center;
}

.page .detail-submit::after {
    border: 0;
}

.page .detail-submit .submit {
    text-align: center;
    height: 70rpx;
}

.page .detail-submit .submit view {
    height: 35rpx;
    line-height: 35rpx;
    color: #fff;
}

.page .detail-submit .submit .detail-now-buy {
    font-size: 4vw;
    font-weight: bold;
    letter-spacing: 5rpx;
}

.page .detail-shadow {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.7);
}

.page .model-box {
    background: #fff;
    width: 600rpx;
    position: fixed;
    left: 75rpx;
    top: 300rpx;
    border-radius: 30rpx;
    padding: 30rpx 0;
    font-size: 4vw;
    font-weight: bold;
    text-align: center;
}

.page .shadow-state {
    display: flex;
    align-items: center;
    justify-content: center;
}

.page .shadow-state icon {
    margin-right: 20rpx;
}

.page .model-hint {
    margin-top: 25rpx;
}