.banner {
    padding: 50rpx 30rpx;
    position: relative;
    display: flex;
    align-items: center;
    font-size: 4vw;
    line-height: 6vw;
}

.banner .logo {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100%;
    display: block;
}

.banner .bannerC {
    margin-left: 20rpx;
    color: #fff;
}

.banner .bannerC>view:nth-child(2) {
    font-size: 3.5vw;
    line-height: 5.5vw;
}

.banner .box {
    position: absolute;
    top: 40rpx;
    right: 0;
    background: #fff;
    width: 180rpx;
    text-align: center;
    line-height: 80rpx;
    border-radius: 80rpx 0 0 80rpx;
}

.list {
    background: #fff;
    padding: 0 30rpx;
    font-size: 4vw;
    line-height: 6vw;
}

.list .item {
    display: flex;
    align-items: center;
    padding: 20rpx 0;
}

.list .item>view:nth-child(2) {
    flex: 1;
    padding: 0 10rpx;
}

.list .item .icon {
    width: 40rpx;
    display: block;
}

.list .item2 {
    border-top: #ededed 2rpx solid;
    padding: 20rpx 0;
    position: relative;
}

.list .item2>view:nth-child(1) {
    color: #666666;
}

.list .item2>view:nth-child(2) {
    font-size: 8vw;
    line-height: 10vw;
    margin-top: 10rpx;
}

.list .item2 .btn {
    position: absolute;
    bottom: 20rpx;
    right: 0;
    width: 180rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    border-radius: 50rpx;
    color: #fff;
    background: #d2d2d2;
}

.nav {
    background: #fff;
    margin-top: 10rpx;
    overflow: hidden;
    font-size: 4vw;
    line-height: 6vw;
}

.nav .item {
    display: flex;
    align-items: center;
    padding: 20rpx 30rpx;
    width: 50%;
    float: left;
    box-sizing: border-box;
    border-right: #ededed 2rpx solid;
    border-bottom: #ededed 2rpx solid;
}

.nav .item .itemL {
    width: 80rpx;
    height: 80rpx;
    display: block;
}

.nav .item .itemR {
    margin-left: 20rpx;
}

.nav .item .itemR>view:nth-child(2) {
    color: #999999;
    font-size: 3.5vw;
    line-height: 5.5vw;
}