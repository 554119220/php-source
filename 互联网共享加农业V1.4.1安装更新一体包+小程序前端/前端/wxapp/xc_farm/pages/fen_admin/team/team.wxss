.nav {
    font-size: 4vw;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 11;
    background: #fff;
    display: flex;
    height: 100rpx;
    line-height: 100rpx;
}

.nav>view {
    flex: 1;
    text-align: center;
    margin: 0 80rpx;
}

.nav_block {
    height: 100rpx;
}

.tip {
    padding: 20rpx 30rpx;
    font-size: 4vw;
    line-height: 6vw;
    display: flex;
    align-items: center;
}

.tip>image {
    width: 40rpx;
    height: 40rpx;
    display: block;
    margin-right: 10rpx;
}

.list {
    background: #fff;
    padding: 0 30rpx;
    font-size: 4vw;
    line-height: 6vw;
}

.list .item {
    display: flex;
    padding: 20rpx 0;
}

.list .item+.item {
    border-top: #ededed 2rpx solid;
}

.list .item .itemL {
    width: 88rpx;
    height: 88rpx;
    border-radius: 100%;
    display: block;
}

.list .item .itemR {
    flex: 1;
    margin-left: 10rpx;
}

.list .item .itemR .itemT {
    display: flex;
    align-items: center;
}

.list .item .itemR .itemT .icon {
    width: 40rpx;
    height: 40rpx;
    display: block;
    margin: 0 10rpx;
}

.list .item .itemR .itemT .sp1 {
    color: #999999;
    font-size: 3.5vw;
}