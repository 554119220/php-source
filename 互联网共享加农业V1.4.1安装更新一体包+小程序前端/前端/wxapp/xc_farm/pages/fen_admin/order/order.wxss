.banner {
    width: 100%;
    padding: 30rpx 0;
    font-size: 4vw;
    line-height: 6vw;
}

.banner .title {
    text-align: center;
    color: #fff;
    padding: 40rpx 0;
    font-size: 6vw;
}

.banner .banner_box {
    background: #fff;
    display: flex;
    align-items: center;
    margin: 0 30rpx;
    padding: 0 20rpx;
    height: 90rpx;
    border-radius: 10rpx;
}

.banner .banner_box>image {
    width: 60rpx;
    height: 60rpx;
    display: block;
}

.banner .banner_box>input {
    flex: 1;
    padding: 0 20rpx;
    text-align: center;
}

.nav {
    display: flex;
    background: #fff;
    margin-top: 10rpx;
    color: #999999;
    font-size: 4vw;
    line-height: 6vw;
}

.nav>view {
    line-height: 90rpx;
    text-align: center;
    flex: 1;
}

.list {
    font-size: 4vw;
    line-height: 6vw;
}

.list .item {
    background: #fff;
    padding: 20rpx;
    display: flex;
    align-items: center;
    border-top: #f7f7f7 4rpx solid;
}

.list .item .item_l {
    width: 140rpx;
    color: #fff;
    height: 50rpx;
    border-radius: 50rpx;
    line-height: 50rpx;
    text-align: center;
    margin-right: 10rpx;
    background: #999;
}

.list .item .item_l.sp1 {
    background: #FD5B68;
}

.list .item .item_l.sp2 {
    background: #49ccb9;
}

.list .item .item_l.sp3 {
    background: #ffc926;
}

.list .item .item_c {
    flex: 1;
    box-sizing: border-box;
    padding: 0 10rpx;
    border-left: #efefef 2rpx dashed;
}

.list .item .item_c .te1 {
    color: #B7B7B7;
}

.list .item .item_r {
    width: 20rpx;
    display: block;
}

.bottomBlank2 {
    height: 111rpx;
}

.rr {
    float: right;
}

.ff {
    margin-right: 30rpx;
}