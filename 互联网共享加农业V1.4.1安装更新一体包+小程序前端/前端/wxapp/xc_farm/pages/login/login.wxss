.body {
    background: #fff;
}

.lajform {
    display: block;
    padding: 0 110rpx 30rpx 110rpx;
    box-sizing: border-box;
    width: 100%;
}

.lajform .inputBlock {
    padding: 0 40rpx;
    height: 100rpx;
    border-radius: 50rpx;
    border: 1px solid #77D4C0;
    margin-bottom: 40rpx;
}

.lajform .inputBlock .icon {
    width: 40rpx;
    height: 45rpx;
    margin-right: 10rpx;
}

.lajform .inputBlock .input {
    overflow: hidden;
}

.lajform .inputBlock .input input {
    font-size: 28rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    color: #666;
}

.lajform .checkBox {
    display: flex;
    text-align: center;
    font-size: 26rpx;
    color: #77D4C0;
    margin-bottom: 40rpx;
    justify-content: center;
    align-items: center;
}

.lajform .checkBox checkbox {
    vertical-align: middle;
}

.lajform .submitBtn {
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 50rpx;
    background-color: #77D4C0;
    color: #fff;
    font-size: 32rpx;
    margin-bottom: 30rpx;
}

.lajform .submitBtn::after {
    border: none;
}

.lajform .navBlock {
}

.lajform .navBlock .navBox {
    font-size: 28rpx;
    color: #999999;
    line-height: 1.3;
}

.lajform .navBlock .border {
    width: 1px;
    height: 28rpx;
    background-color: #CCCCCC;
    margin: 0 30rpx;
}