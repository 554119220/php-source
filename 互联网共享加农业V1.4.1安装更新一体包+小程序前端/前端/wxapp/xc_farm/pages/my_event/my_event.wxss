.list {
}

.listBox {
    height: 220rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    margin-bottom: 10rpx;
    background-image: url('data:image/png;base64,');
}

.listBox .text {
    overflow: hidden;
    line-height: 1.2;
    margin: 0 30rpx;
}

.listBox .text .h1 {
    font-size: 26rpx;
    color: #666;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-bottom: 20rpx;
}

.listBox .text .h2 {
    font-size: 26rpx;
    color: #999;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-bottom: 20rpx;
}

.listBox .text .p {
    font-size: 26rpx;
    color: #999;
}

.listBox .text .p .flex-flex1 {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.listBox .text .p .sp1 {
    color: #77D3BF;
}

.listBox .qr {
    padding: 0 30rpx;
    text-align: center;
    font-size: 22rpx;
    color: #999;
}

.listBox .qr .imgBlock {
    width: 140rpx;
    height: 140rpx;
    position: relative;
    margin-bottom: 15rpx;
}

.listBox .qr .quanma {
    width: 100%;
    height: 100%;
}

.listBox .qr .qrshade {
    width: 140rpx;
    height: 140rpx;
    line-height: 140rpx;
    position: absolute;
    left: 0;
    top: 0;
    background-color: rgba(255,255,255,0.8);
    font-size: 22rpx;
    color: #333;
}

.menu {
    position: fixed;
    width: 350rpx;
    height: 400rpx;
    left: 200rpx;
    top: 200rpx;
    z-index: 2000;
}

.menu .menu_back {
    width: 100%;
    height: 100%;
}

.menu .menux {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    padding: 0 20rpx;
    line-height: 94rpx;
}

.menu .menux .title {
    height: 94rpx;
    border-bottom: #ddd 2rpx dashed;
    text-align: center;
}

.menu .menux .quanma {
    width: 300rpx;
    height: 300rpx;
    display: block;
    margin: 0 auto;
}

.canshow {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: #000;
    opacity: 0.5;
    z-index: 1200;
}