.detailCont1 {
    height: 360rpx;
}

.detailCont2 {
    margin-bottom: 5rpx;
    padding: 30rpx 30rpx 0;
    position: relative;
    background: #fff;
}

.detailCont2 .h1 {
    font-size: 30rpx;
    color: #555;
    line-height: 40rpx;
    margin-bottom: 10rpx;
    padding-right: 60rpx;
}

.detailCont2 .h2 {
    font-size: 24rpx;
    line-height: 34rpx;
    margin-bottom: 15rpx;
    padding-right: 60rpx;
}

.detailCont2 .p {
    display: flex;
    font-size: 26rpx;
    color: #999;
    padding-right: 60rpx;
    margin-bottom: 30rpx;
}

.detailCont2 .p .sp1 {
    font-size: 30rpx;
}

.detailCont2 .p .sp2 {
    color: #333;
    margin-right: 10rpx;
}

.detailCont2 .p .sp3 {
    text-decoration: line-through;
    margin-right: 10rpx;
}

.detailCont2 .shareBlock {
    width: 50rpx;
    position: absolute;
    right: 30rpx;
    top: 30rpx;
    text-align: center;
    font-size: 20rpx;
    color: #999;
}

.detailCont2 .shareBlock image {
    height: 50rpx;
    margin-bottom: 10rpx;
}

.detailCont2 .shareBlock button {
    display: none;
}

.detailCont2 .list {
    font-size: 4vw;
    line-height: 6vw;
    margin-top: 40rpx;
    padding-bottom: 10rpx;
}

.detailCont2 .list .listT>image {
    width: 20rpx;
}

.detailCont2 .list .listT>image.curr {
    transform: rotate(-90deg);
}

.detailCont2 .list .listT .sp1 {
    font-size: 3vw;
    padding: 0 10rpx;
}

.detailCont2 .list .progress {
    margin-top: 20rpx;
    position: relative;
    margin-bottom: 40rpx;
}

.detailCont2 .list .sp2 {
    position: absolute;
    line-height: 20px;
    text-align: center;
    width: 100%;
    top: 0;
    left: 0;
}

.detailCont2 .list .item {
    background: #f7f7f7;
    padding: 10rpx 22rpx;
}

.detailCont2 .list .item+.item {
    margin-top: 6rpx;
}

.detailCont2 .list .item .itemL {
    width: 70rpx;
    height: 70rpx;
    border-radius: 100%;
    display: block;
}

.detailCont2 .list .item .itemC {
    padding: 0 20rpx;
    width: 426rpx;
    box-sizing: border-box;
}

.detailCont2 .list .item .itemC>view {
    width: 386rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.detailCont2 .list .item .sp3 {
    font-size: 3.5vw;
}

.detailCont2 .numBlock {
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 30rpx;
    border-radius: 10rpx;
}

.detailCont2 .numBlock .nbtn {
    width: 50rpx;
    height: 60rpx;
    color: #fff;
}

.detailCont2 .numBlock .input {
    height: 50rpx;
    overflow: hidden;
    padding: 5rpx 0;
}

.detailCont2 .numBlock .input input {
    height: 50rpx;
    vertical-align: top;
    padding: 0;
    width: 50rpx;
    color: #fff;
}

.detailCont5 {
    padding: 20rpx 30rpx;
}

.detailCont5 .title {
    font-size: 28rpx;
    color: #999;
    text-align: center;
    margin-top: 20rpx;
}

.detailCont5 .title .border {
    width: 38rpx;
    height: 3rpx;
    background-color: #77D4C0;
    margin: 10rpx auto;
}

.detailCont5 .title .p {
    background-position: left center;
    background-repeat: repeat-x;
    background-size: 2px 2px;
    color: #ccc;
}

.detailCont5 .title .b {
    display: inline-block;
    padding: 0 20rpx;
}

.detailCont5 .edit {
    padding: 30rpx 0 1rpx;
    font-size: 26rpx;
    line-height: 40rpx;
    text-indent: 2em;
}

.detailCont5 .edit image {
    max-width: 100%;
    display: block;
    margin: 0 auto 20rpx;
}

.detailCont5 .edit>view {
    margin-bottom: 20rpx;
}

.detailCont5 .dcnav {
    border-bottom: 1px solid #e6e6e6;
}

.detailCont5 .dcnav .navBox {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
    border-bottom: 1px solid transparent;
}

.detailCont5 .dcnav .navBox.curr {
    color: #77d4c0;
    border-color: #77d4c0;
}

.detailCont5 .dcnav .border {
    width: 1px;
    height: 30rpx;
    background-color: #e6e6e6;
}

.detailCont5 .dcnav .border:last-child {
    display: none;
}

.detailCont5 .list {
    padding-top: 15rpx;
}

.detailCont5 .list .llistBox {
    padding: 15rpx 0;
    border-bottom: 1px solid #dcdcdc;
}

.detailCont5 .list .llistBox image {
    width: 108rpx;
    height: 88rpx;
}

.detailCont5 .list .llistBox .text {
    overflow: hidden;
    margin-left: 20rpx;
}

.detailCont5 .list .llistBox .text .h1 {
    font-size: 26rpx;
    line-height: 1.1;
    color: #666;
    margin-bottom: 30rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.detailCont5 .list .llistBox .text .p {
    font-size: 26rpx;
    color: #666;
    line-height: 30rpx;
}

.detailCont5 .list .llistBox .text .sp1 {
    color: #77D3BF;
}

.pinfo .list2 {
    padding: 0 40rpx 30rpx;
    margin-bottom: 5rpx;
}

.pinfo .list2 .llistBox {
    padding: 20rpx 0;
    border-bottom: 1px solid #eee;
    font-size: 28rpx;
    color: #666;
    line-height: 38rpx;
}

.pinfo .list2 .llistBox .name {
    color: #999;
}

.pinfo .list2 .llistBox .text {
    overflow: hidden;
}

.buyBlock {
    height: 100rpx;
    box-shadow: 0 -1px 3px #999;
    position: fixed;
    width: 100%;
    left: 0;
    bottom: 0;
    z-index: 50;
}

.buyBlock .navBox {
    width: 120rpx;
    text-align: center;
    position: relative;
    padding: 25rpx 0;
}

.buyBlock .navBox image {
    width: 50rpx;
    height: 50rpx;
}

.buyBlock .navBox .num {
    font-size: 18rpx;
    height: 24rpx;
    min-width: 24rpx;
    box-sizing: border-box;
    line-height: 24rpx;
    padding: 0 5rpx;
    background-color: #8CDBC9;
    color: #fff;
    border-radius: 12rpx;
    position: absolute;
    top: 20rpx;
    left: 50%;
    margin-left: 13rpx;
}

.buyBlock .border {
    width: 1px;
    height: 100%;
    background-color: #ddd;
}

.buyBlock.type1 .btn {
    color: #fff;
    font-size: 22rpx;
    text-align: center;
    height: 80rpx;
    padding-top: 20rpx;
}

.buyBlock.type2 .btn {
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.buyBlock .btn>text {
    display: block;
}

.buyBlock .btn text+text {
    margin-top: 10rpx;
}

.buyBlock .btn .sp1 {
    font-size: 26rpx;
}

.buyBlock .btn.bg1 {
    background-color: #8CDBC9;
}

.buyBlock .btn.bg2 {
    background-color: #77D4C0;
}

.buyBlock .btn.btn2 {
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    padding-top: 0;
}