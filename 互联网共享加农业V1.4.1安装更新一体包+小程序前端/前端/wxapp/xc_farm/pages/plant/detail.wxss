.detailCont1 {
    height: 360rpx;
}

.plantD1 {
    background: #fff;
    padding: 30rpx 20rpx;
    font-size: 4vw;
    line-height: 6vw;
}

.plantD1 .area {
    margin-top: 20rpx;
    overflow: hidden;
}

.plantD1 .area>image {
    width: 74rpx;
    height: 68rpx;
    display: block;
    float: left;
    margin-bottom: 10rpx;
}

.plantD1 .tip {
    justify-content: flex-end;
    font-size: 3.5vw;
    line-height: 5.5vw;
}

.plantD1 .tip .tipP {
    margin-left: 40rpx;
    align-items: center;
}

.plantD1 .tip .tipP>image {
    width: 24rpx;
    height: 24rpx;
    display: block;
    margin-right: 10rpx;
}

.plantD3 {
    margin-top: 6rpx;
    padding: 0 20rpx 20rpx;
    background: #fff;
    font-size: 4vw;
    line-height: 6vw;
}

.plantD3 .box1 {
    padding: 15rpx;
    font-size: 22rpx;
    color: #999;
}

.plantD3 .item {
    background: #f7f7f7;
}

.plantD3 .item+.item {
    margin-top: 10rpx;
}

.plantD3 .item .itemL {
    padding: 20rpx 10rpx;
}

.plantD3 .item .itemL .simg {
    width: 80rpx;
    height: 80rpx;
    display: block;
    border-radius: 100%;
}

.plantD3 .item .itemL .itemC {
    margin-left: 10rpx;
    font-size: 3.5vw;
    line-height: 5.5vw;
    width: 380rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.plantD3 .item .itemL .itemC>view {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.plantD3 .item .itemL .itemY {
    width: 68rpx;
    height: 68rpx;
    border-radius: 100%;
    text-align: center;
    line-height: 68rpx;
    border: #fff 6rpx solid;
    font-size: 3.5vw;
}

.plantD3 .item .itemR {
    height: 200rpx;
    width: 140rpx;
}

.plantD3 .item .itemR .itemBtn {
    width: 100rpx;
    height: 60rpx;
    background: #fff;
    margin: 70rpx auto;
    border-radius: 10rpx;
    text-align: center;
    line-height: 60rpx;
}

.plantD3 .item .sp1 {
    font-size: 3.5vw;
    line-height: 5.5vw;
}

.plantD2 {
    background: #fff;
    font-size: 4vw;
    line-height: 6vw;
    padding: 20rpx;
}

.plantD2 .title>text {
    border-bottom: #ddd 6rpx solid;
    padding: 10rpx 0;
}

.plantD2 .content {
    margin-top: 30rpx;
}

.plantD2 .content image {
    max-width: 100%;
}

.plantD2 .plantD2List {
    margin-top: 40rpx;
}

.plantD2 .plantD2List .item {
    display: flex;
    background: #f7f7f7;
    align-items: center;
    position: relative;
}

.plantD2 .plantD2List .item+.item {
    margin-top: 20rpx;
}

.plantD2 .plantD2List .item .simg {
    width: 150rpx;
    height: 150rpx;
    display: block;
}

.plantD2 .plantD2List .item .itemC {
    flex: 1;
    overflow: hidden;
    padding: 0 80rpx 0 20rpx;
}

.plantD2 .plantD2List .item .sp1 {
    font-size: 3.5vw;
}

.plantD2 .plantD2List .item .itemChoose {
    position: absolute;
    bottom: 0;
    right: 0;
    width: 0;
    height: 0;
    border-width: 0 0 80rpx 80rpx;
    border-style: solid;
    border-color: transparent transparent #eeeeee transparent;
}

.plantD2 .plantD2List .item .itemChoose>icon {
    position: absolute;
    top: 36rpx;
    right: 4rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.plantD2 .plantD2List .item .tip {
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    display: block;
    top: 20rpx;
    right: 20rpx;
}

.buyBlock {
    height: 100rpx;
    box-shadow: 0 -1px 3px #999;
    position: fixed;
    width: 100%;
    left: 0;
    bottom: 0;
    z-index: 50;
}

.buyBlock .navBox {
    width: 120rpx;
    text-align: center;
    position: relative;
    padding: 25rpx 0;
}

.buyBlock .navBox image {
    width: 50rpx;
    height: 50rpx;
}

.buyBlock .navBox .num {
    font-size: 18rpx;
    height: 24rpx;
    min-width: 24rpx;
    box-sizing: border-box;
    line-height: 24rpx;
    padding: 0 5rpx;
    background-color: #8CDBC9;
    color: #fff;
    border-radius: 12rpx;
    position: absolute;
    top: 20rpx;
    left: 50%;
    margin-left: 13rpx;
}

.buyBlock .border {
    width: 1px;
    height: 100%;
    background-color: #ddd;
}

.buyBlock.type1 .btn {
    color: #fff;
    font-size: 22rpx;
    text-align: center;
    height: 80rpx;
    padding-top: 20rpx;
}

.buyBlock.type2 .btn {
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.buyBlock .btn>text {
    display: block;
}

.buyBlock .btn text+text {
    margin-top: 10rpx;
}

.buyBlock .btn .sp1 {
    font-size: 26rpx;
}

.buyBlock .btn.bg1 {
    background-color: #8CDBC9;
}

.buyBlock .btn.bg2 {
    background-color: #77D4C0;
}

.buyBlock .btn.btn2 {
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    padding-top: 0;
}

.yin {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.7);
    z-index: 100;
}

.menu {
    position: fixed;
    background: #fff;
    width: 600rpx;
    top: 80rpx;
    left: 75rpx;
    border-radius: 20rpx;
    box-sizing: border-box;
    padding: 50rpx;
    font-size: 4vw;
    line-height: 6vw;
}

.menu .menu_simg {
    width: 100%;
    display: block;
    height: 320rpx;
}

.menu .sp1 {
    text-align: center;
    margin-top: 10rpx;
}

.menu .sp2 {
    text-align: center;
    color: #AAAAAA;
    font-size: 3.5vw;
    line-height: 5.5vw;
}

.menu .menu_content {
    border: #ddd 2rpx solid;
    margin-top: 8rpx;
    padding: 10rpx 20rpx;
}

.menu .menu_content .sp3 {
    height: 200rpx;
    overflow: scroll;
    font-size: 3.5vw;
    line-height: 5.5vw;
    margin-top: 10rpx;
}

.menu .menu_rule {
    border: #ddd 2rpx solid;
    margin-top: 8rpx;
    padding: 20rpx;
}

.menu .menu_rule .sp4 {
    font-size: 3.5vw;
    line-height: 5.5vw;
}

.menu .menu_close {
    position: absolute;
    bottom: -74rpx;
    left: 50%;
    transform: translate(-50%);
    display: flex;
    justify-content: center;
    align-items: center;
}

.shade {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 100;
}

.buyLayer {
    background-color: #fff;
    position: fixed;
    width: 100%;
    left: 0;
    bottom: 0;
    box-sizing: border-box;
}

.buyLayer .box1 {
    padding: 30rpx;
    position: relative;
    border-bottom: 1px solid #ededed;
    margin: 0 30rpx;
}

.buyLayer .box1 .imgBlock {
    width: 210rpx;
    height: 210rpx;
    border: 1px solid #fff;
    border-radius: 10rpx;
    margin-top: -110rpx;
    margin-right: 30rpx;
    box-shadow: 1px 1px 3px #aaa;
}

.buyLayer .box1 .imgBlock image {
    border-radius: 10rpx;
}

.buyLayer .box1 .text {
    overflow: hidden;
    line-height: 1.3;
}

.buyLayer .box1 .text .h1 {
    font-size: 36rpx;
    color: #77d4c0;
    margin-bottom: 15rpx;
}

.buyLayer .box1 .text .p {
    font-size: 26rpx;
}

.buyLayer .box1 .closeBtn {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    right: 0;
    top: 20rpx;
}

.buyLayer .box2 {
    padding: 30rpx;
}

.buyLayer .box2 .inputBlock {
}

.buyLayer .box2 .inputBlock .name {
    width: 3em;
    font-size: 26rpx;
    color: #999;
    line-height: 50rpx;
}

.buyLayer .box2 .inputBlock .input {
    overflow: hidden;
}

.buyLayer .box2 .inputBlock .radioBox {
    display: inline-block;
    font-size: 28rpx;
    color: #999;
    height: 50rpx;
    line-height: 50rpx;
    border: 1px solid #999;
    padding: 0 30rpx;
    border-radius: 25rpx;
    margin: 0 15rpx 30rpx 0;
}

.buyLayer .box2 .inputBlock .radioBox.curr {
    color: #77D3BF;
    border-color: #77D3BF;
}

.buyLayer .box2 .inputBlock .radioBox radio {
    display: none;
}

.buyLayer .box2 .inputBlock .numBlock {
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 30rpx;
    border: 1px solid #999;
    border-radius: 10rpx;
}

.buyLayer .box2 .inputBlock .numBlock .nbtn {
    width: 60rpx;
    height: 60rpx;
    color: #999;
}

.buyLayer .box2 .inputBlock .numBlock .input {
    height: 50rpx;
    overflow: hidden;
    padding: 5rpx 0;
    border-left: 1px solid #999;
    border-right: 1px solid #999;
}

.buyLayer .box2 .inputBlock .numBlock .input input {
    height: 50rpx;
    vertical-align: top;
    padding: 0;
    width: 70rpx;
    color: #77D4C0;
}

.buyLayer .btn {
    display: block;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    background-color: #77d4c0;
}

