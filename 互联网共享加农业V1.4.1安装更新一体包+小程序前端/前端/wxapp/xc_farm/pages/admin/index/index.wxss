.banner {
    width: 100%;
    height: 340rpx;
    padding: 40rpx 0;
    box-sizing: border-box;
    text-align: center;
}

.banner>image {
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto;
    border-radius: 100%;
    display: block;
    border: #a4e6dc 6rpx solid;
    margin-bottom: 20rpx;
}

.banner>view {
    color: #fff;
}

.nav {
    margin: -72rpx 30rpx 0;
    background: #fff;
    display: flex;
}

.nav .item {
    padding: 20rpx 0;
    flex: 1;
    text-align: center;
}

.nav .item>view:nth-child(1) {
    font-size: 6vw;
    line-height: 8vw;
}

.nav .item>view:nth-child(2) {
    font-size: 4vw;
    color: #BABABA;
    line-height: 6vw;
}

.nav2 {
    display: flex;
    margin-top: 10rpx;
}

.nav2 .item {
    flex: 1;
    padding: 30rpx 0;
    text-align: center;
    color: #fff;
    font-size: 4vw;
    line-height: 6vw;
}

.nav2 .item>image {
    width: 60rpx;
    height: 60rpx;
    display: block;
    margin: 0 auto 10rpx;
}

.list {
    background: #fff;
    padding: 0 30rpx;
    margin-top: 10rpx;
    font-size: 4vw;
    line-height: 6vw;
}

.list .title {
    padding: 20rpx 0;
    border-bottom: #efefef 2rpx dashed;
}

.list .content {
    overflow: hidden;
    color: #999999;
}

.list .content .item {
    width: 33.33333333333333%;
    float: left;
    padding: 30rpx 0;
    text-align: center;
}

.list .content .item>image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 0 auto 10rpx;
}

.bottomBlank2 {
    height: 111rpx;
}