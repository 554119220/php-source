.banner {
    width: 100%;
    height: 220rpx;
    padding: 50rpx 0;
    box-sizing: border-box;
    text-align: center;
    color: #fff;
    font-size: 6vw;
    background: #999;
}

.banner.sp1 {
    background: #49CCB9;
}

.banner.sp2 {
    background: #FD5B68;
}

.banner.sp3 {
    background: #FFC926;
}

.nav {
    margin: -72rpx 30rpx 0;
    background: #fff;
    display: flex;
    color: #777777;
}

.nav .item {
    padding: 20rpx 0;
    flex: 1;
    text-align: center;
    font-size: 4vw;
    line-height: 6vw;
}

.nav .item image {
    width: 60rpx;
    height: 60rpx;
    display: block;
    margin: 0 auto 10rpx;
}

.list {
    background: #fff;
    padding: 0 30rpx 20rpx;
    margin-top: 10rpx;
    font-size: 4vw;
    line-height: 6vw;
}

.list .title {
    padding: 20rpx 0;
    text-align: center;
    color: #B7B7B7;
}

.list .title>view:nth-child(2) {
    width: 100rpx;
    height: 4rpx;
    margin: 0 auto;
}

.list .item {
    display: flex;
    padding: 10rpx 0;
}

.list .item>text:nth-child(1) {
    color: #B7B7B7;
}

.list .item>view:nth-child(2) {
    flex: 1;
}

.list .item .sp {
    width: 140rpx;
    height: 44rpx;
    line-height: 44rpx;
    text-align: center;
    color: #fff;
    border-radius: 44rpx;
    background: #999;
}

.list .item .sp1 {
    background: #49CCB9;
}

.list .item .sp2 {
    background: #FD5B68;
}

.list .item .sp3 {
    background: #FFC926;
}

.bottomBlank2 {
    height: 111rpx;
}