.container {
    justify-content: flex-start;
    min-height: 100%;
    position: relative;
}

.tab {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    background: #fff;
    border-top: 2rpx solid #acacac;
    height: 98rpx;
}

.tabnav {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.tablist {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #8c8c8c;
}

.active {
    color: #f92c53;
}

.tabimg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 8rpx;
    margin-bottom: 6rpx;
}

.session {
    font-size: 30rpx;
    color: #222;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0rpx 30rpx 0rpx 20rpx;
    height: 90rpx;
    background: #fff;
    margin-top: 30rpx;
}

.shop {
    margin-top: 20rpx;
}

.status {
    margin-left: 50rpx;
    font-size: 28rpx;
    color: #999;
}

.outLogin {
    width: 750rpx;
    margin-top: 50rpx;
    display: flex;
    justify-content: center;
    position: fixed;
    bottom: 200rpx;
}

.outLogin button {
    width: 688rpx;
    height: 88rpx;
    line-height: 88rpx;
    color: #fff;
    font-size: 32rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(249,77,110)0%,rgb(255,158,177)100%);
}