.container {
    justify-content: flex-start;
    min-height: 100%;
    margin-top: 10rpx;
}

.order-nav {
    width: 750rpx;
    height: 94rpx;
    display: flex;
    justify-content: space-between;
    background: #fff;
    line-height: 94rpx;
    border-right: 1rpx solid #cbcbcb;
}

.order-nav view {
    width: 375rpx;
    height: 94rpx;
    text-align: center;
    font-size: 32rpx;
    color: #666;
    font-weight: 500;
}

.nav {
    display: inline-block;
    height: 94rpx;
}

.on {
    box-sizing: border-box;
    color: #f92c53;
    border-bottom: 4rpx solid #f92c53;
}

.list {
    background: #fff;
    padding: 0rpx 0rpx 30rpx;
    margin-top: 20rpx;
}

.ordernum {
    padding: 0rpx 20rpx;
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
    color: #222;
    height: 82rpx;
    align-items: center;
    border-bottom: 1rpx solid #ececec;
}

.ordernum>view:last-child {
    color: #f92c53;
}

.wrap {
    padding: 0rpx 20rpx;
    font-size: 26rpx;
    color: #666;
    display: flex;
    padding-top: 20rpx;
}

.stysrc {
    width: 88rpx;
    height: 88rpx;
    margin-right: 20rpx;
    border-radius: 50%;
}

.detail {
    display: flex;
    flex-direction: column;
    width: 594rpx;
}

.detail text {
    margin-bottom: 6rpx;
    line-height: 36rpx;
}

.shop {
    font-size: 32rpx;
    color: #222;
    font-weight: 700;
    margin-top: 6rpx;
}

.status {
    font-size: 26rpx;
    color: #f92c53;
    height: 116rpx;
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}

.cancel {
    height: 56rpx;
    width: 56rpx;
    display: flex;
    justify-content: center;
    align-items: flex-end;
}

.cancel image {
    width: 31rpx;
    height: 31rpx;
}

.foot {
    height: 150rpx;
}