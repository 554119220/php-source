.container {
    justify-content: flex-start;
    min-height: 100%;
}

.header {
    padding: 30rpx 30rpx 0rpx 30rpx;
    background: #f92c53;
}

.header-cont {
    width: 642rpx;
    height: 266rpx;
    background: #fab1bf;
    border-radius: 12rpx 12rpx 0rpx 0rpx;
    padding: 24rpx;
}

.user-info {
    display: flex;
    color: #fff;
    font-size: 32rpx;
}

.thumb {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    margin-right: 20rpx;
    border: 3rpx solid rgb(249,44,83);
}

.user-grade {
    font-size: 28rpx;
    margin-top: 10rpx;
}

.user-grade image {
    width: 26rpx;
    height: 22rpx;
    margin-right: 10rpx;
}

.record {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #fff;
    font-size: 32rpx;
    margin-top: 60rpx;
}

.score {
    display: flex;
    flex-direction: column;
}

._record {
    width: 220rpx;
    height: 68rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 2px solid #fff;
    border-radius: 10rpx;
}

.score text:nth-child(2) {
    font-size: 46rpx;
    font-weight: 700;
    margin-top: 10rpx;
}

._record image {
    width: 36rpx;
    height: 30rpx;
    margin-left: 24rpx;
}

.wrap {
    margin-top: 20rpx;
    background: #fff;
}

.list {
    padding: 0rpx 30rpx;
    height: 130rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 30rpx;
    color: #222;
    line-height: 46rpx;
}

.list-title {
    height: 110rpx;
    font-size: 32rpx;
    border-bottom: 1px solid #ececec;
}

.icon {
    width: 36rpx;
    height: 36rpx;
    margin-right: 28rpx;
}

.det {
    color: #999;
    font-size: 24rpx;
}

.block {
    padding: 40rpx 20rpx 20rpx;
    background: #fff;
}

.block>view:first-child {
    font-size: 32rpx;
    font-weight: 700;
    margin-bottom: 26rpx;
}

.block>view:last-child {
    font-size: 26rpx;
    color: #666;
    line-height: 36rpx;
}

.process {
    width: 614rpx;
    margin-left: 68rpx;
    padding-top: 20rpx;
    position: relative;
}

.process>progress {
    margin-top: 60rpx;
    width: 614rpx;
}

.totalIntegral {
    font-size: 30rpx;
    color: #f92c53;
    margin-top: 14rpx;
    text-align: right;
    font-weight: 700;
}

.progressTop {
    display: inline-block;
    position: absolute;
    height: 40rpx;
    padding: 0rpx 16rpx;
    color: #fff;
    background-color: #f92c53;
    border-radius: 10rpx;
    line-height: 40rpx;
    font-size: 30rpx;
    font-weight: 700;
    width: auto;
    margin-left: -40rpx;
}

.progressTop::after {
    content: " ";
    display: block;
    height: 0rpx;
    width: 0rpx;
    border-left: 12rpx solid transparent;
    border-top: 12rpx solid #f92c53;
    border-right: 12rpx solid transparent;
    transform: translateX(-50%);
    margin-left: 50%;
}

.proTips {
    text-align: center;
    font-size: 30rpx;
    margin-top: 10rpx;
}

.proTips>text {
    color: #f92c53;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    background: #ffffff;
    border-radius: 50px;
}

.gobackindex image {
    width: 50px;
    height: 50px;
    border-radius: 50%;
}