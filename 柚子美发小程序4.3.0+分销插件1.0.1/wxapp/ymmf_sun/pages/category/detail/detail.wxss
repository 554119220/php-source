.container {
    justify-content: flex-start;
    min-height: 100%;
}

.up {
    font-size: 32rpx;
    color: #222;
    background: #fff;
}

swiper {
    width: 750rpx;
    height: 752rpx;
}

.slide-image {
    width: 750rpx;
    height: 752rpx;
}

.title {
    padding: 34rpx 20rpx 14rpx;
}

.desc {
    font-size: 24rpx;
    color: #666;
    padding: 0rpx 20rpx;
    line-height: 40rpx;
}

.price {
    padding: 14rpx 20rpx 20rpx;
    font-size: 30rpx;
    color: #f33030;
    font-weight: 700;
}

.session {
    margin-top: 20rpx;
    font-size: 30rpx;
    color: #666;
    background: #fff;
    padding: 0rpx 20rpx 150rpx;
    line-height: 54rpx;
}

.session-title {
    font-weight: 700;
    font-size: 28rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    padding-top: 30rpx;
    padding-bottom: 2rpx;
}

.session-title:before,.session-title:after {
    content: " ";
    width: 62rpx;
    height: 2rpx;
    background: #cbcbcb;
    margin-right: 24rpx;
}

.session-title:after {
    margin-left: 24rpx;
}

.det-img {
    display: flex;
    justify-content: space-between;
    margin-top: 20rpx;
}

.img {
    width: 355rpx;
    height: 400rpx;
}

.share {
    width: 750rpx;
    height: 98rpx;
    background: rgb(40,183,163);
    box-shadow: 0rpx -3rpx 3rpx 0rpx rgba(180,180,180,0.078);
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    display: flex;
    justify-content: space-between;
}

.share button {
    background: #f92c53;
    color: #fff;
    font-size: 32rpx;
    width: 375rpx;
    border-radius: 0rpx;
    line-height: 98rpx;
}

.share .arcShare {
    background: #f95e7b;
}

.share button::after {
    border: none;
}

.share-X {
    width: 750rpx;
    height: 98rpx;
    background: #ffffff;
    box-shadow: 0rpx -3rpx 3rpx 0rpx rgba(180,180,180,0.078);
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    display: flex;
    justify-content: space-between;
    padding-bottom: 68rpx;
}

.share-X button {
    background: #f92c53;
    color: #fff;
    font-size: 32rpx;
    width: 375rpx;
    border-radius: 0rpx;
    line-height: 98rpx;
}

.share-X .arcShare {
    background: #f95e7b;
}

.share-X button::after {
    border: none;
}

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.drawer_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #f8f8f8;
}

.icon {
    width: 90rpx;
    height: 90rpx;
    margin-bottom: 30rpx;
}

.drawer_content {
    height: 240rpx;
    display: flex;
    justify-content: space-between;
}

.shareList {
    display: flex;
    flex-direction: column;
    height: 240rpx;
    width: 375rpx;
    justify-content: center;
    align-items: center;
    background: #fff;
    line-height: 26rpx;
    font-size: 28rpx;
}

.shareList view {
    display: inline-block;
}

.shareList view:last-child {
    font-size: 24rpx;
    color: #999;
    margin-top: 12rpx;
}

.drawer_footer {
    background: #fff;
    width: 750rpx;
    height: 100rpx;
    margin-top: 12rpx;
    font-size: 34rpx;
    text-align: center;
    line-height: 100rpx;
}

button::after {
    border: none;
}

.shareBtn {
    padding: 10rpx 10rpx 0 10rpx;
    position: fixed;
    right: 20rpx;
    bottom: 120rpx;
    width: 60rpx;
    height: 70rpx;
    background: rgba(254,80,71,0.8);
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    border-radius: 50%;
    line-height: 30rpx;
    z-index: 9;
}

.sharePoster {
    position: fixed;
    z-index: 11;
    left: 50%;
    top: 50%;
    width: 580rpx;
    margin: 0 0 0 -290rpx;
    background: #fff;
    transform: translateY(-50%);
}

.canvas {
    position: absolute;
    top: -3000rpx;
    z-index: 9999;
}

.previewImg {
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 9999;
    width: 100%;
    position: fixed;
}

.preview button {
    width: 40%;
    position: absolute;
    bottom: 30rpx;
    left: 30%;
    z-index: 999;
    background: #e9472c;
    color: #fff;
}

.shareBtnbot {
    margin-left: -100rpx;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 99999;
    width: 200rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: red;
    font-size: 26rpx;
    background: #e5e1de;
    border-radius: 10rpx;
    text-align: center;
}

.mask1 {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #7e7e7e;
    z-index: 9000;
    opacity: 0.3;
}

.modalDlg1 {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0rpx;
    left: 0;
    z-index: 9999;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.window1 {
    width: 600rpx;
    height: 80rpx;
    background: #ffffff;
    position: relative;
    margin-top: 300rpx;
    border-radius: 50rpx;
    font-size: 35 rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

