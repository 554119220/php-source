.container {
    justify-content: flex-start;
    font-size: 28rpx;
    line-height: 26rpx;
}

.list {
    display: flex;
    width: 730rpx;
    padding: 20rpx 0rpx 20rpx 20rpx;
    height: 108rpx;
    background: #fff;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 10rpx;
}

.shopImg {
    width: 108rpx;
    height: 108rpx;
    border-radius: 50%;
    margin-right: 24rpx;
}

.listLeft {
    display: flex;
}

.phoneIcon {
    width: 26rpx;
    height: 26rpx;
    margin-right: 20rpx;
}

.det {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.det>view {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    font-size: 26rpx;
    color: #999;
    width: 438rpx;
}

.det>view>text {
    display: block;
    width: 392rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.det>view:first-child {
    line-break: 32rpx;
    font-size: 30rpx;
    color: #222;
}

.listRight {
    height: 108rpx;
    border-left: 1rpx dashed #dcdcdc;
    width: 120rpx;
    font-size: 26rpx;
    line-height: 36rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.listRight view:last-child {
    color: #999;
}

.det .curShop>text {
    line-height: 34rpx;
    color: #f52c53;
    font-weight: 700;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    background: #ffffff;
    border-radius: 50px;
}

.gobackindex image {
    width: 50px;
    height: 50px;
    border-radius: 50%;
}