page {
    background-color: #f8f8f8;
    height: 100%;
    font-family: "Helvetica Neue","Hiragino Sans GB","Microsoft YaHei","\9ED1\4F53",Arial;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
}

button:after {
    border: none;
}

.foot {
    height: 90rpx;
    width: 100%;
}

.tab {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    background: #fff;
    border-top: 2rpx solid #acacac;
    height: 92rpx;
    padding-bottom: 4rpx;
    z-index: 99999;
}

.tab-X {
    position: fixed;
    width: 100%;
    bottom: 0rpx;
    background: #fff;
    border-top: 2rpx solid #acacac;
    height: 92rpx;
    padding-bottom: 72rpx;
    z-index: 99999;
}

.tabnav {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.tablist {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #8c8c8c;
}

.tabimg {
    width: 60rpx;
    height: 60rpx;
    margin-top: 8rpx;
    margin-bottom: 0rpx;
}

.foot {
    height: 120rpx;
}

#styleSecond .session1 {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 30rpx 26rpx;
    margin-top: 30rpx;
}

#styleSecond .session {
    width: 650rpx;
    margin-left: 20rpx;
    background: #fff;
    box-shadow: 0rpx 0rpx 14rpx 0rpx rgba(5,2,2,0.078);
    border-radius: 10rpx;
}

#styleSecond .session1 .lst {
    display: flex;
    align-items: center;
}

#styleSecond .session1 .lst>image {
    height: 88rpx;
    width: 88rpx;
    margin-right: 70rpx;
    border-radius: 50%;
    border: 2rpx solid #fff;
}

#styleSecond .session1 .toFx {
    width: 128rpx;
    height: 56rpx;
    line-height: 56rpx;
    border-radius: 28rpx;
    background-image: -webkit-linear-gradient(0deg,rgb(135,255,228) 0%,rgb(65,194,252) 100%);
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    margin-right: 0px;
}

#ordertopay .drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

#ordertopay .drawer_box {
    width: 750rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
}

#ordertopay .drawer_title {
    font-size: 32rpx;
    line-height: 30rpx;
    color: #222;
    font-weight: 700;
    padding: 24rpx 20rpx;
    display: flex;
    justify-content: space-between;
}

#ordertopay .cancel {
    width: 30rpx;
    height: 30rpx;
}

#ordertopay .drawer_content {
    height: 500rpx;
    overflow-y: scroll;
}

#ordertopay .choose-pay {
    background: #fff;
}

#ordertopay .section {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
}

#ordertopay .radio {
    width: 730rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
    font-size: 32rpx;
    color: #999;
    border-bottom: 1rpx solid #ececec;
    margin-left: 20rpx;
    padding-right: 20rpx;
}

#ordertopay .radio radio {
    width: 48rpx;
    height: 48rpx;
}

#ordertopay .radio:last-child {
    border-bottom: 0rpx solid #ececec;
}

#ordertopay .det {
    display: flex;
    align-items: center;
}

#ordertopay .wxicon {
    width: 34rpx;
    height: 34rpx;
    margin-left: 24rpx;
    margin-right: 20rpx;
}

#ordertopay .drawer-foot {
    border-top: 1rpx solid #acacac;
    height: 130rpx;
    padding: 20rpx 20rpx;
}

#ordertopay .drawer-foot button {
    height: 90rpx;
    background: #f92c53;
    font-size: 34rpx;
    color: #fff;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    background: #fff;
    border-radius: 50px;
}

.gobackindex image {
    width: 50px;
    height: 50px;
    border-radius: 50%;
}