<view class="full column">
    <image class="m-img" src="{{list.img_url}}"></image>
    <view class="title">{{list.company}}</view>
    <view class="version">版本号 {{list.versions}}</view>
    <view catch:tap="callTel" class="item betweens" data-phone="{{list.phone}}">
        <view class="item-left">来电咨询</view>
        <view class="item-right tel">{{list.phone}}</view>
    </view>
    <view class="item betweens">
        <view class="item-left">工作日</view>
        <view class="item-right">{{list.work_time}}</view>
    </view>
    <view catch:tap="copy" class="item betweens" data-content="{{list.email}}">
        <view class="item-left">工作邮箱</view>
        <view class="item-right">{{list.email}}</view>
    </view>
    <view catch:tap="copy" class="item betweens" data-content="{{list.wechat}}">
        <view class="item-left">微信公号</view>
        <view class="item-right">{{list.wechat}}</view>
    </view>
</view>
