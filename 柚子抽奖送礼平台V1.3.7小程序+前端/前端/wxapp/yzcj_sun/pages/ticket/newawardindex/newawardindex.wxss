.new_nav {
    height: 90rpx;
    display: flex;
    align-items: center;
}

.new_nav>view {
    display: flex;
    width: 50%;
    justify-content: center;
    align-items: center;
    height: 90rpx;
    font-size: 30rpx;
    font-weight: bold;
}

.list {
    border-top: 1rpx solid #d0d0d0;
    border-bottom: 16rpx solid #f0f0f0;
}

.list-img {
    display: flex;
    align-items: center;
    min-height: 320rpx;
    margin-top: 5rpx;
}

.list-img image {
    width: 100%;
    height: 350rpx;
}

.list-body {
    padding: 0 30rpx 30rpx 30rpx;
}

.list-txt {
    display: flex;
    flex-direction: column;
}

.list-tile {
    overflow: hidden;
    line-height: 48rpx;
    margin-bottom: 30rpx;
    position: relative;
}

.sponsor {
    font-size: 26rpx;
    color: #D05143;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.sponsor-new {
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border: 1rpx solid #ddd;
    display: flex;
    margin-top: 15rpx;
    margin-bottom: 15rpx;
}

.w_120s {
    padding: 0 5rpx;
}

.list-time {
    display: flex;
}

.list-icon {
    margin-right: 10rpx;
}

.list-time icon {
    vertical-align: -2px;
}

.state {
    height: 32rpx;
    line-height: 32rpx;
    margin-left: 10rpx;
    display: inline-block;
}

.icon {
    width: 32rpx;
    height: 32rpx;
}

.tabBar {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98rpx;
    background: #fff;
    z-index: 10;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    justify-content: space-around;
    border-top: 1rpx solid #ddd;
    box-shadow: 0rpx 3rpx 8rpx #b4b4b4;
    color: #bcbcbc;
    padding-bottom: 13rpx;
}

.tabBar-X {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98rpx;
    background: #fff;
    z-index: 10;
    display: flex;
    align-items: center;
    font-size: 20rpx;
    justify-content: space-around;
    border-top: 1rpx solid #ddd;
    box-shadow: 0rpx 3rpx 8rpx #b4b4b4;
    padding-bottom: 68rpx;
}

.tabBar>view {
    width: 20%;
}

.tabBar>view.negative {
    color: #bcbcbc!important;
}

.tabBar-X>view {
    width: 20%;
}

.tabBar-X>view.negative {
    color: #bcbcbc!important;
}

.mg {
    margin-top: 12rpx;
    text-align: center;
    margin-bottom: 4rpx;
    position: relative;
}

.tabBar image {
    width: 60rpx;
    height: 60rpx;
}

.tabBar-img {
    width: 68rpx!important;
    height: 68rpx!important;
    border-radius: 50%;
    margin-top: -43rpx;
    background-color: #fff;
    padding: 19rpx;
    box-shadow: 0rpx -3rpx 6rpx 0rpx rgba(180,180,180,0.2);
}

.position {
    position: absolute;
}

.tabBar-X image {
    width: 60rpx;
    height: 60rpx;
}

.border-bottom {
    height: 90rpx;
    border-bottom: 2rpx solid red;
    line-height: 90rpx;
}

.daily {
    width: 100%;
    background: #ffffff;
    margin-top: 20rpx;
    padding-bottom: 20rpx;
}

.daily-title {
    display: flex;
    width: 750rpx;
    height: 90rpx;
    align-items: center;
    justify-content: center;
    color: #222222;
    font-size: 30rpx;
    font-weight: bold;
}

.daily-icon {
    width: 24rpx;
    height: 14rpx;
    margin: 0 10rpx;
}

.daily-item {
    width: 710rpx;
    height: 593rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0px 0px 12px 0px rgba(4,0,0,0.12);
    border-radius: 12px;
    margin: 20rpx auto;
}

.daily-img {
    width: 710rpx;
    height: 417rpx;
}

.daily-detail {
    padding-left: 20rpx;
    height: 186rpx;
}

.prize {
    font-size: 28rpx;
    font-family: PingFang-SC-Regular;
    font-weight: 400;
    color: rgba(34,34,34,1);
    margin-bottom: 5rpx;
}

.data {
    font-size: 22rpx;
    font-family: PingFang-SC-Regular;
    font-weight: 400;
    color: rgba(102,102,102,1);
}

.list {
    border-top: 1rpx solid #d0d0d0;
    border-bottom: 16rpx solid #f0f0f0;
}

.list-img {
    display: flex;
    align-items: center;
    min-height: 320rpx;
    margin-top: 5rpx;
}

.list-img image {
    width: 100%;
    height: 350rpx;
}

.line {
    height: 60rpx!important;
    width: 1rpx!important;
    background: #DCDCDC;
}