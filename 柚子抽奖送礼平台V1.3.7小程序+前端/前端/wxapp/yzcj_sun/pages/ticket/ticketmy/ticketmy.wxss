page {
    background: #f0f0f0;
}

.container {
    overflow-x: hidden;
    padding-bottom: 98rpx;
}

.user {
    margin-top: 20rpx;
    display: flex;
    padding: 40rpx 30rpx;
    border-bottom: 1rpx solid #e6e6e6;
    align-items: center;
    box-sizing: border-box;
}

.user-header {
    width: 68rpx;
    border-radius: 50%;
    margin-right: 15rpx;
}

.user .icon-you {
    font-size: 24rpx;
}

.num {
    display: flex;
    margin-bottom: 20rpx;
    padding: 30rpx 0;
    box-sizing: border-box;
}

.num-list {
    width: 33.33%;
    text-align: center;
    height: 100rpx;
    position: relative;
    line-height: 50rpx;
}

.num-list::before {
    content: "";
    position: absolute;
    top: 0;
    bottom: 0;
    width: 1px;
    background: #e6e6e6;
    left: 0;
}

.num-list:first-child::before {
    background: #fff;
}

.list-box {
    margin-bottom: 20rpx;
}

.list {
    height: 100rpx;
    display: flex;
    padding: 0 20rpx;
    align-items: center;
    position: relative;
    box-sizing: border-box;
}

.list::after {
    content: "";
    bottom: 0;
    left: 20rpx;
    right: 0;
    height: 1px;
    background: #e6e6e6;
    position: absolute;
}

.list:last-child::after {
    background: #fff;
}

.contact-button {
    opacity: 0;
    top: 0;
    bottom: 0;
    text-align: center;
    position: absolute;
    z-index: 3;
}

.list-right {
    display: flex;
    justify-content: flex-end;
}

.list1::after {
    left: 0;
    height: 10rpx;
    background: #f5f5f5;
}

.tabBar {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98rpx;
    background: #fff;
    z-index: 10;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    justify-content: space-around;
    border-top: 1rpx solid #ddd;
    box-shadow: 0rpx 3rpx 8rpx #b4b4b4;
    color: #bcbcbc;
    padding-bottom: 13rpx;
}

.tabBar-X {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98rpx;
    background: #fff;
    z-index: 10;
    display: flex;
    align-items: center;
    font-size: 20rpx;
    justify-content: space-around;
    border-top: 1rpx solid #ddd;
    box-shadow: 0rpx 3rpx 8rpx #b4b4b4;
    padding-bottom: 68rpx;
}

.tabBar>view {
    width: 20%;
}

.tabBar>view.negative {
    color: #bcbcbc!important;
}

.tabBar-X>view {
    width: 20%;
}

.tabBar-X>view.negative {
    color: #bcbcbc!important;
}

.mg {
    margin-top: 12rpx;
    text-align: center;
    margin-bottom: 4rpx;
    position: relative;
}

.tabBar image {
    width: 60rpx;
    height: 60rpx;
}

.tabBar-img {
    width: 68rpx!important;
    height: 68rpx!important;
    border-radius: 50%;
    margin-top: -43rpx;
    background-color: #fff;
    padding: 19rpx;
    box-shadow: 0rpx -3rpx 6rpx 0rpx rgba(180,180,180,0.2);
}

.position {
    position: absolute;
}

.tabBar-X image {
    width: 60rpx;
    height: 60rpx;
}