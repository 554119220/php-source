    height: 60rpx;
    color: #ffffff;
    font-size: 26rpx;
    line-height: 60rpx;
    padding-left: 15rpx;
}

.tabBar {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98rpx;
    background: #fff;
    z-index: 10;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    justify-content: space-around;
    border-top: 1rpx solid #ddd;
    box-shadow: 0rpx 3rpx 8rpx #b4b4b4;
    color: #bcbcbc;
    padding-bottom: 13rpx;
}

.tabBar-X {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98rpx;
    background: #fff;
    z-index: 10;
    display: flex;
    align-items: center;
    font-size: 20rpx;
    justify-content: space-around;
    border-top: 1rpx solid #ddd;
    box-shadow: 0rpx 3rpx 8rpx #b4b4b4;
    padding-bottom: 68rpx;
}

.tabBar>view {
    width: 20%;
}

.tabBar>view.negative {
    color: #bcbcbc!important;
}

.tabBar-X>view {
    width: 20%;
}

.tabBar-X>view.negative {
    color: #bcbcbc!important;
}

.mg {
    margin-top: 12rpx;
