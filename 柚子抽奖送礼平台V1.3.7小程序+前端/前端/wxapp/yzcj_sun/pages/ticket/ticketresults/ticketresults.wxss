.xinfeng_wrap {
    position: fixed;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    background: rgba(0,0,0,0.3);
    z-index: 2;
}

.xinfeng-bottom {
    width: 750rpx;
    height: 459rpx;
    position: absolute;
    left: 50%;
    margin-left: -375rpx;
    bottom: 0;
    z-index: 12;
}

.xinfeng-top {
    width: 750rpx;
    height: 459rpx;
    position: absolute;
    left: 50%;
    margin-left: -375rpx;
    bottom: 0;
    z-index: 11;
    transform-style: preserve-3d;
}

.img-fanzhuan {
    position: absolute;
    width: 750rpx;
    top: 0;
    left: 0;
    transform-origin: 50% 0%;
    animation: flipBook1 1s;
    transform: rotateX(180deg);
    z-index: 8;
    opacity: 1;
}

@keyframes flipBook1 {
    0% {
        transform: rotateX(0deg);
    }

    100% {
        transform: rotateX(180deg);
    }
}

.xinfeng-after {
    width: 750rpx;
    height: 459rpx;
    position: absolute;
    left: 50%;
    margin-left: -375rpx;
    bottom: 0;
    z-index: 9;
}

.img-top {
    position: absolute;
    width: 750rpx;
    top: 0;
    left: 0;
}

.xin-txt {
    width: 690rpx;
    height: 80%;
    background: #fff;
    animation: show 1s;
    position: absolute;
    z-index: 11;
    left: 50%;
    margin-left: -345rpx;
    top: 60rpx;
    padding: 30rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
}

@keyframes show {
    0% {
        top: 600rpx;
        opacity: 0;
    }

    100% {
        top: 60rpx;
        opacity: 1;
    }
}

