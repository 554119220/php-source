page {
    background: #f2f2f2;
}

.top {
    border-bottom: 1rpx solid #e6e6e6;
    height: 100rpx;
    background: #ffffff;
}

.ordinary {
    width: 170rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    font-size: 32rpx;
    margin-left: 225rpx;
    display: inline-block;
    border-top-left-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
    margin-top: 20rpx;
}

.senior {
    width: 170rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    border-radius: 10rpx;
    font-size: 32rpx;
    display: inline-block;
    margin-top: 20rpx;
}

.select {
    background: #ffffff;
    color: #222222;
    border: 1rpx solid #eeeeee;
}

.unselect {
    background: #eeeeee;
    color: #989898;
}

.bg {
    background: #ffffff;
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    border-bottom: 1rpx solid #eeeeee;
}

.noborder {
    border-bottom: none;
}

.item {
    border-bottom: 1rpx solid #e6e6e6;
    height: 100rpx;
    width: 720rpx;
    line-height: 100rpx;
}

.bold {
    font-size: 32rpx;
    font-weight: bold;
    display: inline-block;
}

.font-26 {
    font-size: 26rpx;
    display: inline-block;
    margin-left: 10rpx;
    color: #a5a5a5;
}

checkbox {
    width: 32rpx;
    height: 32rpx;
    margin-right: 30rpx;
    float: right;
}

checkbox .wx-checkbox-input {
    width: 32rpx;
    height: 32rpx;
    border-radius: 50%;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    background: #c15f4a;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    width: 28rpx;
    height: 28rpx;
    line-height: 28rpx;
    text-align: center;
    font-size: 22rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}

.box .wx-checkbox-input.wx-checkbox-input-checked {
    background: #818181;
}

.explain {
    width: 690rpx;
    margin-left: 30rpx;
    font-size: 26rpx;
    color: #a1a1a1;
    float: left;
    margin-top: 30rpx;
}

.submit {
    width: 750rpx;
    height: 128rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    background: #ffffff;
}

.btn {
    width: 690rpx;
    height: 80rpx;
    background: #c15b4a;
    color: #ffffff;
    text-align: center;
    line-height: 80rpx;
    margin-left: 30rpx;
    margin-top: 24rpx;
}

