.fix-bottom {
    display: flex;
    justify-content: space-between;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    border-top: 1rpx solid #d0d0d0;
    height: 128rpx;
    line-height: 128rpx;
    background: #fff;
    padding: 0 30rpx;
    align-items: center;
    opacity: 1;
    z-index: 10;
    animation-name: mymove;
    animation-duration: 1s;
    animation-timing-function: 0.2s;
    animation-iteration-count: 1;
}

@keyframes mymove {
    from {
        bottom: -128rpx;
        opacity: 0;
    }

    to {
        bottom: 0;
        opacity: 1;
    }
}

