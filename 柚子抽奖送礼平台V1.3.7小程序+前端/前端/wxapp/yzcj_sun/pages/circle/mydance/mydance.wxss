.dis_hide {
    display: none;
}

.nav {
    border-top: 1rpx solid #ddd;
    display: flex;
    border-bottom: 1rpx solid #ddd;
    height: 90rpx;
    align-items: center;
    background: #fff;
}

.nav>view {
    font-size: 32rpx;
    width: 50%;
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
}

.header-img {
    height: 96rpx;
    width: 96rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.circle-top {
    padding: 20rpx;
    line-height: 46rpx;
}

.circle-msg {
    line-height: 50rpx;
    margin-bottom: 20rpx;
    padding: 0 20rpx;
}

.img-box {
    flex-wrap: wrap;
    padding: 20rpx 0 20rpx 20rpx;
    line-height: 0;
}

.img {
    width: 70%;
}

.imgs {
    width: 222rpx;
    height: 222rpx;
    margin-right: 15rpx;
    margin-bottom: 15rpx;
}

.icon-img {
    width: 36rpx;
    height: 36rpx;
    margin-right: 10rpx;
}

.circle-tip {
    height: 60rpx;
}

.img1 {
    width: 150rpx;
    height: 150rpx;
}

.img2 {
    width: 70rpx;
    height: 150rpx;
}

.img2:first-child {
    margin-right: 10rpx;
}

.img3:first-child {
    width: 70rpx;
    height: 150rpx;
    margin-right: 10rpx;
}

.img3:nth-child(2) {
    width: 70rpx;
    height: 70rpx;
    margin-bottom: 10rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.img3:nth-child(3) {
    width: 70rpx;
    height: 70rpx;
    bottom: 0;
    right: 0;
    position: absolute;
}

.img4:first-child {
    width: 70rpx;
    height: 70rpx;
    left: 0;
    top: 0;
    position: absolute;
}

.img4:nth-child(2) {
    width: 70rpx;
    height: 70rpx;
    right: 0;
    top: 0;
    position: absolute;
}

.img4:nth-child(3) {
    width: 70rpx;
    height: 70rpx;
    left: 0;
    bottom: 0;
    position: absolute;
}

.img4:nth-child(4) {
    width: 70rpx;
    height: 70rpx;
    right: 0;
    bottom: 0;
    position: absolute;
}

.img4:nth-child(n+5) {
    width: 0;
    height: 0;
}

.new-list {
    padding: 0 30rpx 30rpx;
}

.new-list-right {
    margin-right: 20rpx;
    position: relative;
    width: 150rpx;
    height: 150rpx;
}

.chenhezhong {
    color: #999;
    border: 1rpx solid #ddd;
    font-size: 24rpx;
    line-height: 24rpx;
    padding: 0 5rpx;
    position: absolute;
    bottom: 22rpx;
    left: 30rpx;
}

.border-5 {
    position: relative;
}

.w_500 {
    width: 500rpx;
    display: flex;
    padding-left: 30rpx;
}

.icon-addredd {
    width: 26rpx;
    height: 36rpx;
    margin-right: 20rpx;
}

.typeclass {
    font-size: 24rpx;
    color: #999;
    margin-left: 30rpx;
    border: 1rpx solid #ddd;
}

.flex-center {
    justify-content: flex-start;
}

.w120 {
    width: 120rpx;
}