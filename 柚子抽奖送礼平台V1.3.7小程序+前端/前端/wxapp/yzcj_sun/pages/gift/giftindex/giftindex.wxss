    justify-content: center;
    margin: 35rpx auto 50rpx auto;
}

.play-box {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background: #fff;
    z-index: 9999;
}

.play-list {
    text-align: center;
    font-size: 34rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-bottom: 1rpx solid #ddd;
}

.play-list:nth-last-child(2) {
    border-bottom: 10rpx solid #f0f0f0;
}

.picker-view {
    height: 100px;
    width: 700rpx;
    margin: -10rpx auto 0 auto;
}

picker-view-column {
    text-align: center;
    line-height: 34px;
}

.list-warn-box {
    background: #cd4b3d;
    position: absolute;
    height: 40rpx;
    line-height: 40rpx;
    padding: 0 20rpx;
    top: 50%;
    margin-top: -20rpx;
    right: 280rpx;
    font-size: 24rpx;
    color: #fff;
}

.list-warn-box::after {
    content: "";
    position: absolute;
    top: 50%;
    margin-top: -10rpx;
    right: -16rpx;
    border-top: 10rpx solid transparent;
    border-left: 20rpx solid #cd4b3d;
    border-bottom: 10rpx solid transparent;
}

.list-right {
    display: flex;
    align-items: center;
}

.list-input-2 {
    width: 70rpx;
    text-align: right;
    margin-right: 20rpx;
}

.gift4 {
    width: 700rpx;
    border-radius: 10rpx;
    margin: -40rpx auto 0 auto;
