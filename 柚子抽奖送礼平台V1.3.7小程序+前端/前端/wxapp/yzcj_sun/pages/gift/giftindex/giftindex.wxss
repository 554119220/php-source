page {
    background: #f0f0f0;
}

.top {
    height: 232rpx;
    background: #d25446;
    color: #fff;
    padding: 54rpx 0 0 58rpx;
    box-sizing: border-box;
    position: relative;
}

.line-40 {
    line-height: 40rpx;
    margin-bottom: 22rpx;
}

.hide {
    display: none;
}

.line-26 {
    line-height: 26rpx;
}

.guide {
    width: 86rpx;
    height: 72rpx;
    background: #fff;
    position: absolute;
    top: 32rpx;
    right: 24rpx;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    background: radial-gradient(#ec5d55,#fff);
    font-size: 20rpx;
    flex-direction: column;
}

.guide::before {
    content: "";
    position: absolute;
    width: 2rpx;
    background: #f1876f;
    top: -30rpx;
    height: 30rpx;
    left: 50%;
    transform: translateX(-50%);
}

.guide::after {
    content: "";
    position: absolute;
    width: 8rpx;
    height: 6rpx;
    border-radius: 50%;
    background: #d25446;
    top: 5rpx;
    left: 50%;
    transform: translateX(-50%);
}

.gift-choose {
    width: 700rpx;
    border-radius: 10rpx;
    height: 130rpx;
    display: flex;
    margin: -40rpx auto 0 auto;
    position: relative;
    background: #fff;
    padding: 0 45rpx;
    box-sizing: border-box;
    align-items: center;
    line-height: 42rpx;
}

.gift-choose .font-30 {
    margin-bottom: 5rpx;
    margin-top: 5rpx;
}

.icon_gift {
    width: 80rpx;
    height: 80rpx;
    flex: 0 0 auto;
    margin-right: 25rpx;
}

.title {
    height: 70rpx;
    display: flex;
    line-height: 46rpx;
    align-items: flex-end;
    justify-content: space-between;
    font-size: 26rpx;
    padding: 0 60rpx;
}

.choceType {
    display: flex;
    background: #fff;
    width: 700rpx;
    align-items: center;
    height: 95rpx;
    border-radius: 10rpx;
    margin: 0 auto;
    padding: 0 30rpx;
    box-sizing: border-box;
    justify-content: space-between;
    position: relative;
    font-size: 28rpx;
}

.color_blue {
    color: #425476;
}

.color_blue.iconfont {
    font-size: 24rpx;
}

.icon_36 {
    width: 36rpx;
    margin-right: 20rpx;
}

textarea {
    background: #fff;
    width: 700rpx;
    height: 125rpx;
    border-radius: 10rpx;
    margin: 0 auto;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    z-index: 1;
}

.total {
    display: flex;
    align-items: flex-end;
    height: 110rpx;
    justify-content: center;
}

.font-60 {
    line-height: 60rpx;
}

.font-30 {
    line-height: 30rpx;
}

.button_s {
    background: #d25446;
    color: #fff;
