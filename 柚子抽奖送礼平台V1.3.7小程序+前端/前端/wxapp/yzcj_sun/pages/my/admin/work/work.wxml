        </view>
    </view>
    <view class="list-tile font-28">
        <image class="width36" src="../../../../resource/images/my/t-gift.png"></image>礼物管理
  </view>
    <view class="list1">
        <view bindtap="goSell" data-index="0" style="position:relative">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:1000;"></button>
            </form>
            <view>
                <image class="width360" src="../../../../resource/images/my/shop4.png"></image>
            </view>
            <view class="font-26">
                <text class="color_999999">已上架  </text>
                <text class="color_ff8883">{{shelves}}</text>
            </view>
        </view>
        <view bindtap="goSell" data-index="1" style="position:relative">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:1000;"></button>
