<view class="container">
    <view class="top-box">
        <image class="top-box_bg" src="../../../../resource/images/20180710155940.png"></image>
        <view class="top-container">
            <view class="flex-0-0">
                <image src="{{url}}{{sponsor.logo}}"></image>
            </view>
            <view class="flex-1-1 color_fff font-32">{{sponsor.sname}} </view>
        </view>
        <view class="scancode">
            <image bindtap="codes" class="code" src="../../../../resource/images/my/code.png">
                <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                    <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:1000;"></button>
                </form>
            </image>
            <view bindtap="codes" style="position:relative">
                <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                    <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:1000;"></button>
                </form>
      扫码核销</view>
        </view>
    </view>
    <view class="list-tile font-28">
        <image class="width36" src="../../../../resource/images/my/adorder.png"></image>抽奖订单管理
  </view>
    <view class="list1">
        <view bindtap="goOrdery1" data-index="0" style="position:relative">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:1000;"></button>
            </form>
            <view>
                <image class="width360" src="../../../../resource/images/my/shop1.png"></image>
            </view>
            <view class="font-26">
                <text class="color_999999">待发货  </text>
                <text class="color_ff8883">{{waitorder1}}</text>
            </view>
        </view>
        <view bindtap="goOrdery1" data-index="1" style="position:relative">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:1000;"></button>
            </form>
            <view>
                <image class="width360" src="../../../../resource/images/my/shop2.png"></image>
            </view>
            <view class="font-26">
                <text class="color_999999">已发货  </text>
                <text class="color_ff8883">{{yiorder1}}</text>
            </view>
        </view>
        <view bindtap="goOrdery1" data-index="2" style="position:relative">
            <form bindsubmit="oddPushFormSubmit" reportSubmit="true" style="width: 100%; height:100%; position: absolute; top: 0; left: 0">
                <button formType="submit" plain="true" style="width: 100%;height:100%;border:1px solid transparent;background-color:transparent;position: absolute; top: 0; left: 0;-webkit-transform: scale(0.5);transform:scale(1);content:\"\";z-index:1000;"></button>
            </form>
            <view>
                <image class="width360" src="../../../../resource/images/my/shop3.png"></image>
            </view>
            <view class="font-26">
                <text class="color_999999">已成功 </text>
                <text class="color_ff8883">{{completeorder1}}</text>
            </view>
        </view>
    </view>
    <view class="list-tile font-28">
        <image class="width36" src="../../../../resource/images/my/adorder.png"></image>礼物订单管理
  </view>
