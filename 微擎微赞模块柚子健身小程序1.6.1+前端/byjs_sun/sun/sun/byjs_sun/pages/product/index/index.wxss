page{
  padding-bottom: 98rpx;
}
.slide-image{
  width: 100%;
  height: 350rpx;
}
.index-nav{
  height:485rpx;
  padding-bottom:30rpx;
  border-bottom:10rpx solid #f5f5f5;
}
.index-nav-img{
  width:750rpx;
  height:350rpx;
  border-radius:0 0 25rpx 25rpx;
}
.index-block{
  display:flex;
  flex-direction: row;
  justify-content:space-around;
  width:640rpx;
  height:180rpx;
  border-radius:10rpx;
  background:#3daddd;
  position: relative;
  z-index: 1;
  margin-left:55rpx;
  margin-top:-96rpx;
  box-shadow: 0 0 10rpx #ddd;
}
.index-block-list{
  font-size:30rpx;
  text-align: center;
}
.index-block-list .img{
  width:60rpx;
  height:60rpx;
}
.index-block .back{
  display: flex;
  flex-direction: row;
  justify-content:space-around;
  width:580rpx;
  height:140rpx;
  background:#fff;
  box-shadow: 0 0 10rpx #ddd;
  position: relative;
  top:30rpx;
  border-radius: 10rpx;
  padding-top:40rpx;
}

.fight-list-title{
  padding:20rpx;
  font-size:30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.fight-list-title-left{
  display: flex;
  align-items: center;
}
.fight-list-title-icon{
  width:4rpx;
  height:26rpx;
  background:#2aaefd;
  margin-right:20rpx;
}
.fight-list{
  padding-left:20rpx;
  display: flex;
  
  height: 235rpx;
}
.fight-list-block{
  width:170rpx;
  height:230rpx;
  margin-right:10rpx;
  flex: 0 0 auto
}
.fight-list-img{
  width:180rpx;
  height:170rpx;
}
.fight-list-block-img{
  width: '';
  height:100%;
  max-width:170rpx;
}
.fight-list-block-title{
  /* padding-left:10rpx; 
  padding-right:10rpx;*/
  height:62rpx;
  line-height:62rpx;
  font-size:28rpx;
  background:#fff;
  text-align: center;
}

.recommend{
  padding:20rpx 20rpx 0 20rpx;
  font-size:30rpx;
  color:#000;
}
.recommend-list{
  padding:20rpx;
  border-bottom:10rpx solid #f5f5f5;
}
.recommend-img-block{
  width:710rpx;
  height:200rpx;
  text-align: center;
}
.recommend-list-img{
  max-width: 100%;
  width:'';
  height:100%;
}
.recommend-title{
  font-size:28rpx;
  color:#000;
  padding-top:20rpx;
  padding-bottom:20rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-align: center;
}

.redEnvelope{
  position: fixed;
  width:100%;
  height:100%;
  background:rgba(0, 0, 0, 0.5);
  top:0;
  left:0;
  z-index: 3;
}
.redEnvelope-block{
  position: absolute;
  top:50%;
  left:50%;
  transform: translate3d(-50%,-50%,0);
  display: flex;
  flex-direction: row;
  align-items: flex-start;
}
.redEnvelope-hb{
  width:330rpx;
  height:505rpx;
}
.redEnvelope-hb-img{
  width:330rpx;
  height:505rpx;
  position: absolute;
  z-index: 3;
}
.redEnvelope-close{
  width:26rpx;
  height:26rpx;
  margin-top:80rpx;
  margin-left:20rpx;
}
.ImmediatelyOpen{
  position: absolute;
  z-index: 4;
  padding:10rpx 5rpx;
  font-size:26rpx;
  color:#ff3233;
  background:#fbda30;
  border-radius:10rpx;
  left:114rpx;
  bottom:50rpx;
}
.yes-ImmediatelyOpen{
  width:522rpx;
  height:837rpx;
  position: relative;
}
.yes-ImmediatelyOpen-img{
  position: absolute;
  z-index: 3;
  width:522rpx;
  height:837rpx;
}
.ImmediatelyOpen-text{
  text-align: center;
  position: relative;
  z-index: 4;
  padding-top:255rpx;
  font-size:36rpx;
  color:#fff;
}
.ImmediatelyOpen-text .title{
  padding-top:106rpx;
  padding-bottom:30rpx;
}
.ImmediatelyOpen-text .onView{
  margin-top:30rpx;
  width:165rpx;
  height:165rpx;
  text-align: center;
  line-height: 165rpx;
  background:#fbda30;
  color:#000;
  font-size:26rpx;
  margin-bottom:80rpx;
  position: relative;
  left:50%;
  transform: translateX(-50%);
  border-radius:50%;
}
.ImmediatelyOpen-text .over{
  font-size:24rpx;
}
.yes-ImmediatelyOpen .colse{
  width:26rpx;
  height:26rpx;
  position:absolute;
  z-index:4;
  top:260rpx;
  right:30rpx;
}
.mask{
position: fixed;
z-index: 10;
left: 0;
right: 0;
