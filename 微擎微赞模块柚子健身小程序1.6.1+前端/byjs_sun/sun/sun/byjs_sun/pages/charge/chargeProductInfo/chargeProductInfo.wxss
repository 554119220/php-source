.icon-condition{
  width:30rpx;
  height:30rpx;
  margin-right:20rpx;
}
.icon-shoppingCart{
  width:36rpx;
  height:36rpx;
  
}

.product-info-img-block{
  width:750rpx;
 
  text-align: center;
}
.product-info-img{
  height:100%;
  width:'';
  max-width: 100%;
}

.product-info{
  padding:20rpx 30rpx;
}
.product-info .title{
  font-size:30rpx;
  line-height:34rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;  
}
.product-info .picer{
  font-size:36rpx;
  color:#f1b000;
  padding-top:30rpx;
  padding-bottom:20rpx;
}
.product-info .reduce{
  font-size:26rpx;
  color: #999;
}
.product-info .reduce text{
  color:#e85544;
  font-size: 30rpx;
}
.product-info .fare{
  font-size:26rpx;
  color:#999;
  position: relative;
  padding-top:20rpx;
  padding-bottom:30rpx;
}
.product-info .fare text{
  position: absolute;
  right:60rpx;
}
.product-info .condition{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size:26rpx;
  color:#999;
}
.product-info .condition text{
  padding-right:44rpx;
}

.product-info-footer{
  height:126rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background: #fff
}
.product-info-footer-block{
  height:96rpx;
  padding-top:30rpx;
  display: flex;
  flex-direction: row;
  position: fixed;
  left: 0;
  bottom: 0;
  background: #fff;
  box-shadow: 0 0 1rpx rgba(0, 0, 0, 0.3)
}
.footer-shoppingCart{
  width:284rpx;
  text-align: center;
  font-size:24rpx;
}
.add-shoppingCart, .go-shoppingCart{
  width:210rpx;
  height:70rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height:70rpx;
  color:#fff;
  font-size:32rpx;
}
.add-shoppingCart{
  background:#3daddd;
  margin-right:28rpx;
  font-size:32rpx;
}
.go-shoppingCart{
  background:#e85544;
}
.select-product-type-wrap{ position: fixed;top: 0; left: 0; right: 0; bottom: 0; background: rgba(0, 0, 0, 0.3)}

.select-product-type{
  position: fixed;
  bottom:0;
  left: 0;
  z-index: 2;
  padding:30rpx;
  background:#fff;
  width:690rpx;
}
.tour-around-footer{
  position: relative;
}
.tour-around-footer-nav{
  border-top:10rpx solid #f5f5f5;
  display: flex;
  flex-direction: row;
  padding-left:20rpx;
  padding-right:20rpx;
  justify-content: space-between;
  border-bottom:1rpx solid #ddd;
}
.yes-a2{
  color:#2aaefd;
}
.tour-around-footer-list{
  border-bottom:1rpx solid #f5f5f5;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tour-around-footer-img-block{
  width:130rpx;
  height:120rpx;
}
.tour-around-footer-img{
  width:'';
  height:100%;
  max-width:100%;
}
.tour-around-footer-list-right{
  font-size:26rpx;
  padding-left:30rpx;
}
.tour-around-footer-title{
    text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  width:400rpx;
}
.tour-around-footer-bq{
  padding-top:10rpx;
  font-size:26rpx;
  color:#999;
}
.icon-close{
  width:23rpx;
  height:23rpx;
  position: absolute;
  right:20rpx;
  font-size:26rpx;

}
.select-title{
  font-size:26rpx;
  padding-top:30rpx;
  padding-bottom:30rpx;
}
.select-list{
  display: flex;
  flex-direction: row;
  position: relative;
}
.select-list .list{
  min-width:70rpx;
  height:50rpx;
  border:1rpx solid #ddd;
  border-radius:6rpx;
  font-size:24rpx;
  text-align: center;
  line-height:50rpx;
  margin-right:20rpx;
}
.select-list .list.active{
  border: 1px solid red;
  color: red
}
.select-list .right{
  position: absolute;
  right: 30rpx;
}
.icon-main{
  width:30rpx;
  height:4rpx;
  padding-left:10rpx;
  padding-right:10rpx;
}
.icon-add{
  width:30rpx;
  height:30rpx;
  padding-left:10rpx;
  padding-right:10rpx;
}
.count-text{
  width:100rpx;
  font-size:24rpx;
  border-left:1rpx solid #ddd;
  border-right:1rpx solid #ddd;
  height:50rpx;
  line-height:50rpx;
}
.block-list{
  display: flex;
  flex-direction: row;
}
.count{
  display: flex;
  flex-direction: row;
  width:186rpx;
  height:50rpx;
  text-align: center;
  align-items: center;
  border:1rpx solid #ddd;
  border-radius: 10rpx;
}
.chargeProductInfo-yes{
  width:690rpx;
  height:88rpx;
  border-radius:10rpx;
  font-size:34rpx;
  background:#e85544;
  color:#fff;
  text-align: center;
  line-height: 88rpx;
  margin-bottom: 30rpx;
  margin-top:120rpx;
  position: relative;
  left: 50%;
  transform: translateX(-50%);
}
.padding-bottom-126{  padding-bottom: 126rpx; padding-top: 26rpx;}


