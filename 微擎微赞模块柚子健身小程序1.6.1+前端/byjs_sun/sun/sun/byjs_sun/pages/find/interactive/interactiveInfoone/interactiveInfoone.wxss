  color:#999;
}
.talentBlock-sex{
  width: 24rpx;
  height:24rpx;
  position: absolute;
  right:20rpx;
  bottom:10rpx;
}
.talentBlock-head-info-img-block{
  position: relative;
}

.movingInfo-title{
  font-size:26rpx;
  padding-top:10rpx;
  line-height: 44rpx;
}
.movingInfo-img-list{
  padding-top:20rpx;
  padding-bottom:30rpx;
}

.movingInfo-img{
  max-width:90%;
}
.movingInfo-img-1{
  max-width:225rpx;
  max-height: 215rpx;
  margin-right:10rpx;
}
.icon-love{
  width:36rpx;
  height:36rpx;
  margin-right:10rpx;
  margin-left:70rpx;
}
.mmy-moving-info-assess{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  font-size:24rpx;
  padding-right: 20rpx;
}
.my-moving-info-like{
  font-size:26rpx;
  display: flex;
  flex-direction: row;
  margin-left:20rpx;
  align-items: center;
}
.icon-like{
  width:24rpx;
  height:24rpx;
}
.my-moving-info-box{
  padding-left:118rpx;
}
.my-moving-info-combox{
  background: #ddd;
  padding: 10rpx 10rpx 0;
  font-size: 26rpx;
  margin-top: 10rpx;
}
.my-moving-info-combox-list{
  padding-bottom: 10rpx;
}
.my-moving-info-title{
  font-size:26rpx;
  overflow:auto;
}
.icon-reply{
  position: absolute;
  right:20rpx;
  bottom:20rpx;
  width:120rpx;
  height:60rpx;
  border-radius:6rpx;
  background:#2aaefd;
  font-size:26rpx;
  text-align: center;
  line-height:60rpx;
  color:#fff;
}
.my-moving-info-list{
  padding:40rpx 20rpx 50rpx 20rpx;
  border-bottom:1rpx solid #ddd;
  position: relative;
}
.my-moving-info-right{
  display: flex;
  flex-direction: row;
  align-items: flex-start;
}
.no-txt{
  color: #999;
  font-size: 26rpx; 
  text-align: center;
  margin-top: 30rpx;
  border-bottom: 1rpx solid #ddd;
  line-height: 60rpx;
  padding-bottom: 10rpx;
}
.no-txt-addcom{
  color: #2aaefd;
}
.input_null{
color: #C9C9C9;
}
 
.release{
display: flex;
align-items: flex-end; /*底部对齐*/
justify-content: space-between; /*两端对齐*/
box-sizing: border-box;
position: fixed;
left: 0;
bottom: 0;
width: 100%;
padding: 18rpx 0 18rpx 30rpx;
background-color: #F7F8F7;
font-size: 28rpx;
z-index: 999;
}
.releaseform{
  width: 90%;
  display: flex;
  align-items: flex-end; /*底部对齐*/
  justify-content: space-between; /*两端对齐*/
  box-sizing: border-box;
}
.release .text{
width: 580rpx;
min-height: 34rpx;
max-height: 102rpx; /*最多显示三行*/
border-width: 15rpx 20rpx; /*使用padding与预期留白不一致，故使用border*/
border-style: solid;
border-color: #ffffff;
line-height: 34rpx;
font-size: 28rpx;
background-color: #ffffff;
border-radius: 4rpx;
}
.release .submit{
width: 165rpx;
height: 64rpx;
line-height: 64rpx;
text-align: center;
color: #2aaefd;
}
.container{
  padding-bottom: 98rpx;
}
.fix-bottom{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 98rpx;
  box-shadow: 0 0 1rpx rgba(0, 0, 0, 0.3);
  background: #fff;
  display: flex;
  justify-content: space-between;
}
.fix-bottom>view{
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}
.bluefontcolor{
  color: #33b2fd;
}