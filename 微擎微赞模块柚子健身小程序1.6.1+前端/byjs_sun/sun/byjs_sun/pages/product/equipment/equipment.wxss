page{
  line-height: 0
}
.img {
  width: 100%;
  height: 300rpx;
}
.list{
  width: 50%;
  padding: 20rpx 38rpx;
  line-height: 44rpx;
  border-bottom: 1rpx solid #ddd;
  box-sizing: border-box;
}
.list-img{
  width: 300rpx;
  height: 200rpx;
}
.name {
  font-size: 28rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.price{
  font-size: 30rpx;
  color: #f1b000
}
.list-box{
  display: flex;
  flex-wrap: wrap;
}
.fffff{
  color: #999;
  font-size: 36rpx;
  text-align: center;
  align-items: center;
  line-height: 200rpx;
}
