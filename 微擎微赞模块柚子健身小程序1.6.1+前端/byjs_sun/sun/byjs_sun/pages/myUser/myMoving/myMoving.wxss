
.moving-nav{
  display: flex;
  flex-direction: row;
  border-top:1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
  height:60rpx;
  line-height:60rpx;
}
.moving-nav .moving-nav-list:nth-child(1){
  border-right:1rpx solid #ddd;
}
.moving-nav-list{
  display:inline-flex;
  align-items: center;
  width:48%;
  justify-content:center;
  font-size:26rpx;
}
.moving-nav-talent{
  width:30rpx;
  height:36rpx;
  margin-right:20rpx;
}
.moving-nav-goWith{
  width:36rpx;
  height:36rpx;
  margin-right:20rpx;
}
.moving-on{
  color:#2aaefd;
}

.talentBlock{
  padding:30rpx 20rpx;
  border-bottom:10rpx solid #f5f5f5;
}
.talentBlock-head{
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  align-items:center;
}
.talentBlock-head-info{
  flex-direction: row;
  display:inline-flex;
  align-items:center;
}
.talentBlock-head-info-img{
  width:97rpx;
  height:97rpx;
  border-radius:50%;
  margin-right:20rpx;
}
.talentBlock-head-info-text .name{
  font-size:26rpx;
  color:#000;
  padding-bottom:10rpx;
}
.talentBlock-head-info-text .time{
  font-size:24rpx;
  color:#999;
}
.talentBlock-head .attention{
  font-size:26rpx;
  border:1rpx solid #2aaefd;
  color:#2aaefd;
  text-align: center;
  width:120rpx;
  height:60rpx;
  line-height:60rpx;
  border-radius:6rpx;
}
.talentBlock-center{
  padding-left: 120rpx;
  padding-top:10rpx;
  font-size:30rpx;
}
.talentBlock-center .title{
  width: 100%;
  line-height:50rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.talentBlock-center .showtitle{
  width: 100%;
  line-height:50rpx;
}
.talentitlebtn{
  color: #2aaefd;
  font-size: 30rpx;
}
.talentBlock-center-img-block{
  width:100%;
}
.talentBlock-center-img{
  width:'';
  max-width: 384rpx;
  height:100%;
}
.talentBlock-center-img-tow{
  max-width:190rpx;
  max-height:180rpx;
  margin-right: 5rpx;
}
.talentBlock-center-img-tow-one{
  max-width: 60%;
  max-height:300rpx;
}
.talentBlock-center-right{
  width:100%;
  height: 44rpx;
  position: relative;
}
.right-bottom{
  display: flex;
  position:absolute;
  right: 20rpx;
  top: 10rpx;
}
.right-bottom .love{
  margin-right:70rpx;
}
.love, .comment{
  font-size:24rpx;
}
.icon-love{
  width:36rpx;
  height:36rpx;
  margin-right:10rpx;
}
.talentBlock-sex{
  width: 24rpx;
  height:24rpx;
  position: absolute;
  right:20rpx;
  bottom:10rpx;
}
.talentBlock-head-info-img-block{
  position: relative;
}
.addAttention{
  color: #999 !important;
  border: 1px solid #f5f5f5 !important;
}
.traveltxt_red{
  color: red
}

.traval-detail{
  display: flex;
  border: 1px solid #eee;
  padding: 10rpx;
  margin-top: 20rpx;
  height: 120rpx;
  margin-left:119rpx;
  margin-right:3rpx;
}
 .traval-detail-image{
  flex: 0 0 auto;
  max-width: 45%;
  display: flex;
  justify-content: space-between;
  overflow: hidden;
  align-items: center;
  
}
.traval-detail-image image{
  width: 150rpx;
  margin-right: 20rpx;

} 
.traval-detail-body{
  flex: 1 1 auto;
}
.traval-detail-title{
  font-size: 30rpx;
  line-height: 44rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.traval-detail-price{
  color: red;
  font-size: 26rpx;
}

.traval-center-image{
   flex: 0 0 auto;
  max-width: 45%;
  display: flex;
  justify-content: space-between;
  overflow: hidden;
  height: 220rpx;
}
.traval-center-image image{
  margin-right: 20rpx;
  max-width:100%
}
