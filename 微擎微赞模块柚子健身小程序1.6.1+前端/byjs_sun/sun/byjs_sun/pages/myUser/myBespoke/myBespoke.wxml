<view class='my-bespoke' wx:for="{{list[0]}}">
  <view class='my-bespoke-list'>
    <view class='my-bespoke-list-left'>
      <view class='title'>{{item.course_name}}</view>
      <view class='cent'>课程教练：{{item.course_coach}}</view>
      <view class='cent'>课程时间：{{item.course_time}}</view>
    </view>
    <view class='my-bespoke-list-right'>
      <image src='{{url}}{{item.course_img}}' class='icon-top'></image>

    </view>
  </view>
</view>