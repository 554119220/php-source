.myOrder-nav{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 28rpx;
  height:84rpx;
  line-height: 84rpx;
  border-top:1rpx solid #ddd;
  border-bottom:1rpx solid #ddd;
}
.myOrder-nav .icon{
  width:2rpx;
  height:24rpx;
  background:#2aaefd;
}
.myOrder-nav .list{
  width:24%;
  text-align: center;
}
.yes-order{
  color:#2aaefd;
}

.travel-product{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding:30rpx;
  border-bottom:1rpx solid #ddd;
}
.product-img{
  width:146rpx;
  height:146rpx;
  margin-right:30rpx;
}
.img-bat{
  width:'';
  max-width:146rpx;
  height:100%;
}
.travel-product{
  padding:20rpx;
}
.product-list{
  display: flex;
  flex-direction: column;
  position: relative;
  height:146rpx;
}
.icon-deleted{
  width:28rpx;
  height:36rpx;
}
.product-title{
  font-size:24rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;  
}
.product-people{
  font-size:24rpx;
  position: absolute;
  bottom:0;
}
.delect-product{
  padding-top:20rpx;
  padding-bottom:20rpx;
  display: flex;
  justify-content: flex-end;
  padding-right:30rpx;
}

.product-pay{
  position: absolute;
  right:30rpx;
  font-size:26rpx;
  width:120rpx;
  height:60rpx;
  border-radius:6rpx;
  background:#f73245;
  color:#fff;
  bottom:0;
  text-align: center;
  line-height:60rpx;
}

.order-status{
  margin-left:30rpx;
  padding-right:30rpx;
  padding-top:30rpx;
  padding-bottom:30rpx;
  position: relative;
  border-bottom:1rpx solid #ddd;
  font-size:24rpx;
}
.order-status .title text{
  color:#2aaefd;
}
.order-status .picer{
  padding-top:20rpx;
}
.order-status .picer text{
  color:#e85544;
}
.order-status .goToPay{
  width:360rpx;
  height:70rpx;
  line-height:70rpx;
  border-radius:10rpx;
  background:#e85544;
  font-size: 32rpx;
  text-align: center;
  position: absolute;
  right:30rpx;
  top:30rpx;
  color:#fff;
}