.tabBar{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 98rpx;
  box-shadow: 0 0 1rpx rgba(0, 0, 0, 0.3);
  background: #fff;
  z-index: 10;
  display: flex;
  align-items: center;
  font-size: 22rpx;
  justify-content: space-between;
  border-top:1rpx solid #ddd 
}
.tabBar>view{
  width: 20%;
}
.tabBar>view.active{
  color: #2aaefd !important
}
.tabBar image{
  width: 58rpx;
  height: 58rpx;
}
.tabBar-img{
  width: 118rpx !important;
  height: 110rpx !important;
  border-radius: 50%;
  margin-top: -46rpx;
}
.txt-center{
  text-align: center
}
.padding-bottom-98{
  padding-bottom: 98rpx;
}

.mask{
  position: fixed;
  z-index: 10;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.6)
}
.new_ad_box image{
  width: 400rpx;
  height: 500rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-left: -200rpx;
  margin-top: -250rpx;
  border-radius: 20rpx;
  z-index: 11
}
.new_ad_box icon{
  position: fixed;
  z-index: 11;
  top:50%;
  margin-top: -300rpx;
   left: 50%;
  margin-left: 200rpx;
}