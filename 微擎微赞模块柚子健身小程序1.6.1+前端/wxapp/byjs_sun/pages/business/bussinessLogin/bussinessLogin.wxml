<view class="business">
    <image class="business-login-back" src="../../../../byjs_sun/resource/images/business/login-back.png"></image>
    <view class="business-block">
        <view class="business-input-user">
            <image class="icon-user" src="../../../../byjs_sun/resource/images/business/icon-user.png"></image>
            <input bindblur="user" class="business-input" placeholder="请输入账号" value=""></input>
        </view>
        <view class="business-input-pow">
            <image class="icon-user" src="../../../../byjs_sun/resource/images/business/icon-pow.png"></image>
            <input bindblur="pow" class="business-input" password="true" placeholder="请输入密码" value=""></input>
        </view>
        <view bindtap="goLogin" class="icon-login">登录</view>
    </view>
</view>
