.my-moving-info {
    padding-left: 20rpx;
    padding-right: 20rpx;
    border-bottom: 10rpx solid #f5f5f5;
    padding-bottom: 30rpx;
    border-top: 1rpx solid #f5f5f5;
}

.talentBlock-head {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.talentBlock-head-info {
    flex-direction: row;
    display: inline-flex;
    align-items: center;
}

.talentBlock-head-info-img {
    width: 97rpx;
    height: 97rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.talentBlock-head-info-text .name {
    font-size: 26rpx;
    color: #000;
    padding-bottom: 10rpx;
}

.talentBlock-head-info-text .time {
    font-size: 24rpx;
    color: #999;
}

.talentBlock-sex {
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    right: 20rpx;
    bottom: 10rpx;
}

.talentBlock-head-info-img-block {
    position: relative;
}

.movingInfo-title {
    font-size: 26rpx;
    padding-top: 10rpx;
    line-height: 40rpx;
}

.movingInfo-img-list {
    padding-top: 20rpx;
    padding-bottom: 30rpx;
    display: flex;
    flex-direction: row;
}

.movingInfo-img-block {
    width: 100%;
    height: 320rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.movingInfo-img {
    width: '';
    height: 100%;
    max-width: 100%;
}

.movingInfo-img-1 {
    width: '';
    height: 100%;
    max-width: 300rpx;
    margin-right: 30rpx;
}

.icon-love {
    width: 36rpx;
    height: 36rpx;
    margin-right: 10rpx;
    margin-left: 70rpx;
}

.mmy-moving-info-assess {
    display: flex;
    flex-direction: row;
    justify-content: center;
    font-size: 24rpx;
}

.my-moving-info-like {
    font-size: 26rpx;
    display: flex;
    flex-direction: row;
    margin-left: 20rpx;
    align-items: center;
}

.icon-like {
    width: 24rpx;
    height: 24rpx;
}

.my-moving-info-title {
    font-size: 26rpx;
}

.icon-reply {
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    width: 120rpx;
    height: 60rpx;
    border-radius: 6rpx;
    background: #2aaefd;
    font-size: 26rpx;
    text-align: center;
    line-height: 60rpx;
    color: #fff;
}

.my-moving-info-list {
    padding: 40rpx 20rpx 50rpx 20rpx;
    border-bottom: 1rpx solid #ddd;
    position: relative;
}

.my-moving-info-right {
    display: flex;
    flex-direction: row;
    align-items: flex-start;
}

.noText {
    padding-top: 50rpx;
    padding-bottom: 10rpx;
    text-align: center;
    font-size: 24rpx;
    color: #999;
    border-bottom: 1rpx solid #f5f5f5;
}

.talentBlock-head .attention {
    font-size: 26rpx;
    border: 1rpx solid #2aaefd;
    color: #2aaefd;
    text-align: center;
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 6rpx;
}

.talentBlock-head .attention.active {
    border: 1px solid #ddd;
    color: #999;
}