page {
    padding-bottom: 100rpx;
}

.address {
    border-bottom: 1rpx solid #eee;
}

.addressList1 {
    padding: 0 20rpx;
    height: 85rpx;
    display: flex;
    align-items: center;
}

.addressList1 view:nth-child(1) {
    flex: 1;
    font-size: 28rpx;
    color: #999;
}

.addressList1 image {
    width: 12rpx;
    height: 20rpx;
}

.addressList {
    padding: 14rpx 20rpx;
    font-size: 28rpx;
}

.addressList view:nth-child(2) {
    margin-top: 6rpx;
}

.bookInfo {
    padding: 20rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #eee;
}

.biLef {
    width: 340rpx;
    height: 240rpx;
    margin-right: 20rpx;
}

.biRig {
    flex: 1;
}

.biRig view:nth-child(1) {
    font-size: 30rpx;
}

.biRig view:nth-child(2) {
    margin-top: 10rpx;
    font-size: 26rpx;
    color: #999;
    line-height: 42rpx;
}

.timeAndconsume {
    padding: 20rpx;
    font-size: 28rpx;
    border-bottom: 1rpx solid #eee;
}

.tcTime,.tcConsume {
    display: flex;
}

.tcConsume {
    margin-top: 20rpx;
}

.tcTime view:nth-child(1),.tcConsume view:nth-child(1) {
    flex: 1;
}

.tcTime view:nth-child(2),.tcConsume view:nth-child(2) {
    color: #e85544;
}

.mbNote {
    padding: 0 20rpx;
    height: 85rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #eee;
}

.mbNote view:nth-child(1) {
    margin-right: 20rpx;
    font-size: 28rpx;
}

.mbNote input {
    flex: 1;
    font-size: 28rpx;
}

.mbFooter {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 98rpx;
    display: flex;
    align-items: center;
    border-top: 1rpx solid #eee;
}

.mbfSubtract,.mbfNum,.mbfAdd {
    height: 46rpx;
    line-height: 46rpx;
}

.mbfSubtract {
    margin-left: 20rpx;
    padding: 0 20rpx;
    border-left: 1rpx solid #ddd;
    border-top: 1rpx solid #ddd;
    border-bottom: 1rpx solid #ddd;
    border-top-left-radius: 6rpx;
    border-bottom-left-radius: 6rpx;
}

.mbfNum {
    padding: 0 30rpx;
    border: 1rpx solid #ddd;
    font-size: 26rpx;
}

.mbfAdd {
    padding: 0 20rpx;
    border-right: 1rpx solid #ddd;
    border-top: 1rpx solid #ddd;
    border-bottom: 1rpx solid #ddd;
    border-top-right-radius: 6rpx;
    border-bottom-right-radius: 6rpx;
    font-family: Arial,Helvetica,sans-serif;
}

.mbfResidue {
    margin-left: 20rpx;
    flex: 1;
    font-size: 26rpx;
}

.mbfBtn {
    margin-right: 20rpx;
    width: 280rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    color: #fff;
    border-radius: 10rpx;
    background: #e85544;
}