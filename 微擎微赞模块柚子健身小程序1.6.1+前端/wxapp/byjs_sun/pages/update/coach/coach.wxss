.paly_box {
    position: fixed;
    z-index: 11;
    width: 80%;
    left: 50%;
    margin-left: -40%;
    margin-top: -192rpx;
    top: 50%;
    border-radius: 4rpx;
}

.paly_box-list {
    height: 96rpx;
    line-height: 96rpx;
    border-bottom: 1rpx solid rgba(0,0,0,0.2);
    padding: 0 40rpx;
}

.icon_next {
    width: 36rpx;
    height: 20rpx;
}

.shop {
    height: 126rpx;
    display: flex;
    justify-content: space-between;
    border-bottom: 10rpx solid #f5f5f5;
    padding: 0 30rpx;
    font-size: 30rpx;
    align-items: center;
}

.coach_list {
    display: flex;
    align-items: center;
    padding: 30rpx;
    overflow: hidden;
    position: relative;
    border-bottom: 10rpx solid #f5f5f5;
    line-height: 58rpx;
}

.coach_list .flex-1-1 {
    position: relative;
    overflow: hidden;
}

.coach_list image {
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    flex: 0 0 auto;
    margin-right: 30rpx;
}

.coach_list button {
    font-size: 30rpx;
    color: #fff;
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    background: #e85544;
    border-radius: 20rpx;
    flex: 0 0 auto;
    margin-left: 30rpx;
    padding: 0;
}

.tran90 {
    transform: rotate(180deg);
}