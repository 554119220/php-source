.myFans-img-block {
    width: 750rpx;
    height: 300rpx;
    text-align: center;
}

.myFans-img {
    width: '';
    height: 100%;
    max-width: 100%;
}

.my-fans-list {
    border-bottom: 10rpx solid #f5f5f5;
    padding: 30rpx 30rpx 30rpx 20rpx;
}

.talentBlock-head {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.talentBlock-head-info {
    flex-direction: row;
    display: inline-flex;
    align-items: center;
}

.talentBlock-head-info-img {
    width: 97rpx;
    height: 97rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.talentBlock-head-info-text .name {
    font-size: 26rpx;
    color: #000;
    padding-bottom: 10rpx;
}

.talentBlock-head-info-text .time {
    font-size: 24rpx;
    color: #999;
}

.talentBlock-head .attention {
    font-size: 26rpx;
    border: 1rpx solid #ddd;
    color: #999;
    text-align: center;
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 6rpx;
}

.talentBlock-head .addAttention {
    color: #2aaefd;
    border: 1rpx solid #2aaefd;
}

.talentBlock-sex {
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    right: 20rpx;
    bottom: 10rpx;
}

.talentBlock-head-info-img-block {
    position: relative;
}