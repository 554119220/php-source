.my-bespoke {
    border-top: 1rpx solid #ddd;
}

.my-bespoke-list {
    border-bottom: 1rpx solid #ddd;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding: 30rpx;
}

.my-bespoke-list-left .title {
    display: flex;
    align-items: center;
    padding-bottom: 10rpx;
    font-size: 26rpx;
}

.fs32 {
    font-size: 32rpx;
    font-weight: 600;
}

.title view:nth-child(2),.title view:nth-child(3) {
    margin-left: 20rpx;
}

.my-bespoke-list-left .cent {
    font-size: 26rpx;
    padding-top: 10rpx;
}

.my-bespoke-list-right {
    width: 200rpx;
    height: 140rpx;
    text-align: right;
    font-size: 26rpx;
}

.icon-top {
    width: 95rpx;
    height: 77rpx;
    margin-bottom: 10rpx;
}

.my-bespoke-list-right .img {
    width: '';
    height: 100%;
    max-width: 100%;
}

.my-bespoke-list-right button {
    background: #e85544;
    width: 200rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: #fff;
    font-size: 30rpx;
    border-radius: 10rpx;
}

.gohome {
    position: fixed;
    bottom: 30rpx;
    right: 30rpx;
    height: 80rpx;
    width: 80rpx;
    z-index: 2;
}

.nav {
    border-top: 1rpx solid #ddd;
    display: flex;
    border-bottom: 1rpx solid #ddd;
    height: 90rpx;
    align-items: center;
    background: #fff;
}

.nav>view {
    font-size: 32rpx;
    width: 50%;
    text-align: center;
}