page {
    height: 100%;
    background-color: #f2f2f2;
}

form {
    margin-top: 20rpx;
}

.session {
    margin-top: 10rpx;
    background-color: #fff;
    padding-left: 20rpx;
}

.session .list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 16rpx 20rpx 16rpx 0;
    width: 710rpx;
    font-size: 30rpx;
    border-bottom: 1rpx solid #ececec;
}

.session .list-remark {
    align-items: flex-start;
}

.session .list:last-child {
    border-bottom: 0rpx solid #ececec;
}

.session .list text {
    white-space: nowrap;
}

.list input {
    text-align: right;
    font-size: 30rpx;
}

.button {
    width: 400rpx;
    height: 80rpx;
    margin-top: 30rpx;
    line-height: 80rpx;
    border-radius: 10rpx;
    background-color: rgb(61,173,221);
    font-size: 36rpx;
    color: #fff;
}