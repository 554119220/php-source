page {
    background-color: #f2f2f2;
}

form {
    display: block;
}

.session {
    margin-top: 10rpx;
    background-color: #fff;
    padding: 0 20rpx;
}

.session .list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 16rpx 0rpx 16rpx 0;
    width: 710rpx;
    font-size: 30rpx;
    border-top: 1rpx solid #ececec;
}

.session .list:first-child {
    border-bottom: 0rpx solid #ececec;
}

.list text {
    white-space: nowrap;
}

.list input {
    text-align: right;
    font-size: 30rpx;
}

.footer {
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100rpx;
    width: 750rpx;
    background-color: #fff;
    border-top: 1rpx solid #ececec;
    z-index: 100;
}

.footer>button {
    width: 400rpx;
    height: 60rpx;
    border-radius: 10rpx;
    background-color: rgb(61,173,221);
    font-size: 30rpx;
    text-align: center;
    line-height: 60rpx;
    color: #fff;
}

.active-cont {
    height: 260rpx;
    width: 710rpx;
}

.picGroup>view {
    display: inline-block;
    width: 750rpx;
    height: 300rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}

.picGroup>view:nth-child(3n) {
    margin-right: 0rpx;
}

.pic {
    width: 750rpx;
    height: 300rpx;
}

.tip {
    margin: 0 20rpx 20rpx;
    line-height: 36rpx;
    font-size: 26rpx;
    color: #999999;
}