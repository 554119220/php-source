page,.container {
    background-color: #f2f2f2;
}

.contain {
    position: relative;
}

.banner-image {
    height: 400rpx;
    width: 750rpx;
}

.wrap {
    padding: 0rpx 20rpx 20rpx;
    background: #fff;
}

.wrap-top {
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
}

.title {
    padding-top: 22rpx;
    line-height: 36rpx;
    font-size: 30rpx;
}

.addrIcon {
    width: 20rpx;
    height: 24rpx;
    margin-right: 10rpx;
}

.address {
    display: flex;
    align-items: center;
    margin-top: 6rpx;
    font-size: 26rpx;
}

.address>text {
    flex: 1;
}

.collect-active {
    flex: 0 0 60rpx;
    width: 60rpx;
    height: 56rpx;
    padding: 12rpx 10rpx;
    line-height: 28rpx;
    border-radius: 10rpx;
    background-color: rgb(61,173,221);
    font-size: 26rpx;
    color: #fff;
    text-align: center;
}

.collect-noactive {
    flex: 0 0 60rpx;
    width: 60rpx;
    height: 56rpx;
    padding: 12rpx 10rpx;
    line-height: 28rpx;
    border-radius: 10rpx;
    background-color: rgb(153,153,153);
    font-size: 26rpx;
    color: #fff;
    text-align: center;
}

.public-user {
    margin-top: 12rpx;
    font-size: 26rpx;
    color: #999999;
}

.wrap-bot {
    display: flex;
    justify-content: space-between;
    margin-top: 12rpx;
    font-size: 26rpx;
    color: #999;
}

.wrap-bot .num {
    display: flex;
}

.wrap-bot .num>view {
    display: flex;
    align-items: center;
    margin-left: 30rpx;
}

.wrap-bot .num>view>image {
    width: 28rpx;
    height: 20rpx;
    margin-right: 6rpx;
}

.session {
    padding: 0rpx 20rpx;
    margin-top: 10rpx;
    background-color: #fff;
    border-bottom: 1rpx solid #ececec;
}

.session:last-child {
    margin-bottom: 30rpx;
}

.list {
    height: 68rpx;
    line-height: 68rpx;
    border-bottom: 1rpx solid #ececec;
    font-size: 30rpx;
}

.list:last-child {
    border-bottom: 0rpx solid #fff;
}

.list .name {
    margin-right: 16rpx;
}

.list .colr {
    color: #3daddd;
}

.fight-list-title-left {
    display: flex;
    align-items: center;
    padding: 20rpx 0 0;
    font-size: 30rpx;
}

.fight-list-title-icon {
    width: 4rpx;
    height: 26rpx;
    background: #2aaefd;
    margin-right: 20rpx;
}

.active-det {
    padding: 20rpx 0;
    line-height: 36rpx;
    font-size: 26rpx;
}

.comment {
    padding-bottom: 120rpx;
}

.comment-list {
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
}

.comment-list:last-child {
    border-bottom: 0rpx solid #ececec;
}

.comment-top {
    display: flex;
    align-items: center;
}

.comment-top .face {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.comment-det {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 10rpx 0rpx;
    margin-left: 20rpx;
    font-size: 30rpx;
}

.comment-det .times {
    font-size: 24rpx;
    color: #989898;
}

.comment-list .cont {
    padding: 20rpx 0 0;
    line-height: 32rpx;
    font-size: 26rpx;
    color: #333333;
}

.fixed-right {
    position: fixed;
    right: 0rpx;
    bottom: 180rpx;
}

.fixed-right>view>image {
    height: 80rpx;
    width: 80rpx;
}

.footer {
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 20rpx;
    height: 100rpx;
    width: 710rpx;
    background-color: #fff;
    border-top: 1rpx solid #cdcdcd;
}

.opear {
    display: flex;
    font-size: 30rpx;
    color: #989898;
}

.opear>view {
    display: flex;
    align-items: center;
    padding-right: 20rpx;
    margin-right: 20rpx;
    border-right: 1rpx solid #ececec;
}

.opear>button {
    display: flex;
    align-items: center;
    border-right: 1rpx solid #ececec;
    background-color: white;
    font-size: 30rpx;
    padding-left: 0rpx;
    color: #989898;
}

.opear>view:last-child {
    margin-right: 0rpx;
    border-right: 0rpx solid #ececec;
}

.opear>view .good {
    color: #e85545;
}

.btnIcon {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
}

.toJoin {
    width: 230rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 10rpx;
    background-color: rgb(61,173,221);
    color: #fff;
    font-size: 30rpx;
    text-align: center;
}

.toJoin1 {
    width: 230rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 10rpx;
    background-color: rgb(153,153,153);
    color: #fff;
    font-size: 30rpx;
    text-align: center;
}

.commSend {
    position: fixed;
    bottom: 0rpx;
    background: #fff;
    height: 160rpx;
    padding: 20rpx 10rpx 20rpx 30rpx;
    display: flex;
    justify-content: space-between;
    width: 710rpx;
}

.commSend input {
    width: 560rpx;
    min-height: 64rpx;
    padding-left: 10rpx;
    border-bottom: 2rpx solid #2bc407;
    margin-bottom: 20rpx;
}

.commSend button {
    width: 100rpx;
    height: 64rpx;
    background: #2bc407;
    font-size: 28rpx;
    color: #fff;
    line-height: 64rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    margin-right: 0rpx;
    margin-left: 0rpx;
}

.commSend .grey {
    border: 1rpx solid #999;
    background: #fff;
    color: #666;
}