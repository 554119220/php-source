<view class="cu-timeline">
    <view bindtap="goNotice" class="cu-item" data-id="{{item.notice_id}}" wx:for="{{list}}" wx:key="{{index}}">
        <view class="cu-time ">{{item.create_time}}</view>
        <view class="content">
      收款：
      <view class="text-red" style="display:inline">￥{{item.account}}</view>
        </view>
    </view>
</view>
<view wx:if="{{list.length==0}}">
    <view class="flex  justify-center">
        <image class="justify-center " mode="widthFix" src="/pages/images/nodata.png" style="width:360rpx;margin-top:200rpx;"></image>
    </view>
    <view class="text-df text-grey flex justify-center margin-top">暂无收款记录</view>
</view>
